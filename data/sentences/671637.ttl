<http://w3id.org/polifonia/pilot/meetups/671637/3/6>
        rdf:type          ont:Sentence ;
        ont:excerpt       "It features originals along with a Townes Van Zandt song titled \"If I Needed You\"." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/671637/3> ;
        ont:person        <http://dbpedia.org/resource/Townes_Van_Zandt> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/671637/3/5> ;
