<http://w3id.org/polifonia/pilot/meetups/726151/3/2>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Harris and Marcia divorced in September 1940, and Harris married Alice Faye in 1941." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/726151/3> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/726151/3/1> ;
