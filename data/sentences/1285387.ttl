<http://w3id.org/polifonia/pilot/meetups/1285387/11/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1974, en route to a meeting with David Rockefeller to discuss a Rudolf Nureyev project, Hurok died of a heart attack." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1285387/11> ;
        ont:person        <http://dbpedia.org/resource/Rudolf_Nureyev> , <http://dbpedia.org/resource/David_Rockefeller> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1285387/11/0> ;
        ont:subject       <http://dbpedia.org/resource/Sol_Hurok> .

<http://w3id.org/polifonia/pilot/meetups/1285387/7/3>
        rdf:type            ont:Sentence ;
        ont:excerpt         "The First Moog Quartet, the first to perform electronic music in Carnegie Hall, was formed in 1970 in response to Hurok's request to hear the Moog synthesizer in a live concert." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1285387/7> ;
        ont:place           <http://dbpedia.org/resource/Carnegie_Hall> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1285387/7/2> ;
        ont:subject         <http://dbpedia.org/resource/Sol_Hurok> ;
        ont:timeExpression  "1970 in" .

