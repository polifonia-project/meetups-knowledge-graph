        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1961, he brought Russia's Kirov Academy of Ballet and the Igor Moiseyev Ballet Company to the U.S." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1285387/7> ;
        ont:place         <http://dbpedia.org/resource/Russia> , <http://dbpedia.org/resource/United_States> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1285387/7/0> ;
        ont:subject       <http://dbpedia.org/resource/Sol_Hurok> .

<http://w3id.org/polifonia/pilot/meetups/1285387/6/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Hurok hoped to reunite the companies, but ultimately was unsuccessful." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1285387/6> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1285387/6/0> ;
        ont:subject       <http://dbpedia.org/resource/Sol_Hurok> .

<http://w3id.org/polifonia/pilot/meetups/1285387/12/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Like Hurok, they just have hearts.\"" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1285387/12> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1285387/12/2> ;
