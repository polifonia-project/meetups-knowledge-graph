<http://w3id.org/polifonia/pilot/meetups/50016/0/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Leonard Alfred Schneider (October 13, 1925 – August 3, 1966), known professionally as Lenny Bruce, was an American stand-up comedian, social critic, and satirist." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/50016/0> ;
        ont:person          <http://dbpedia.org/resource/Lenny_Bruce> ;
        ont:place           <http://dbpedia.org/resource/United_States> ;
