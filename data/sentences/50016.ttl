        ont:subject         <http://dbpedia.org/resource/Lenny_Bruce> ;
        ont:timeExpression  "August 3 , 1966" , "October 13 , 1925" .

<http://w3id.org/polifonia/pilot/meetups/50016/11/8>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Honey and Lenny had a tumultuous relationship." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/50016/11> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/50016/11/7> ;
        ont:subject       <http://dbpedia.org/resource/Lenny_Bruce> .

