<http://w3id.org/polifonia/pilot/meetups/50016/7/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "It's all a trip—and the best of it is you haven't the faintest idea where you're going!" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/50016/7> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/50016/7/0> ;
        ont:subject       <http://dbpedia.org/resource/Lenny_Bruce> .

<http://w3id.org/polifonia/pilot/meetups/50016/15/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Although the jury acquitted him, other law enforcement agencies began monitoring his appearances, resulting in frequent arrests under charges of obscenity." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/50016/15> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/50016/15/0> ;
        ont:subject       <http://dbpedia.org/resource/Lenny_Bruce> .

<http://w3id.org/polifonia/pilot/meetups/50016/72/0>
