        ont:person          <http://dbpedia.org/resource/Dwight_Howard> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/50016/16/9> ;
        ont:subject         <http://dbpedia.org/resource/Lenny_Bruce> ;
        ont:timeExpression  "November 4 , 1964" .

<http://w3id.org/polifonia/pilot/meetups/50016/13/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Bruce had stolen several priests' clergy shirts and a clerical collar while posing as a laundry man." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/50016/13> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/50016/13/2> ;
        ont:subject       <http://dbpedia.org/resource/Lenny_Bruce> .

