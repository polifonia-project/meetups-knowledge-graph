<http://w3id.org/polifonia/pilot/meetups/1272132/7/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1982 he made the album Who Are You Staring At?" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1272132/7> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1272132/7/0> ;
        ont:subject       <http://dbpedia.org/resource/John_Giorno> .

<http://w3id.org/polifonia/pilot/meetups/1272132/2/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "While in his early twenties, he briefly worked in New York City as a stockbroker." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1272132/2> ;
        ont:person        <http://dbpedia.org/resource/Stockbroker> ;
