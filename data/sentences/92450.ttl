        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/92450/33/1> .

<http://w3id.org/polifonia/pilot/meetups/92450/9/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1968, Watson began a relationship with Gallagher Guitars when he started playing their G-50 model." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/92450/9> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/92450/9/0> ;
        ont:subject       <http://dbpedia.org/resource/Doc_Watson> .

