<http://w3id.org/polifonia/pilot/meetups/92450/16/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Watson and Rosa Lee had two children,  Eddy Merle (named after country music legends Eddy Arnold and Merle Travis) in 1949 and Nancy Ellen in 1951." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/92450/16> ;
        ont:person        <http://dbpedia.org/resource/James_Watson> , <http://dbpedia.org/resource/Nancy_Sinatra> , <http://dbpedia.org/resource/Eddy_Arnold> , <http://dbpedia.org/resource/Ellen_Terry> , <http://dbpedia.org/resource/Merle_Travis> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/92450/16/0> ;
        ont:subject       <http://dbpedia.org/resource/Doc_Watson> .

<http://w3id.org/polifonia/pilot/meetups/92450/2/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Early life =" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/92450/2> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/92450/2/0> ;
