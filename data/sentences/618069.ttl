        ont:excerpt       "Starting in 1957, he was organist and choirmaster at the Stuttgart Gedächtniskirche, conducting the choir Figuralchor der Gedächtniskirche Stuttgart." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/618069/3> ;
        ont:place         <http://dbpedia.org/resource/Stuttgart> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/618069/3/0> ;
        ont:subject       <http://dbpedia.org/resource/Helmuth_Rilling> .

<http://w3id.org/polifonia/pilot/meetups/618069/10/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "For Rilling's 75th birthday, his record label Hänssler Classic released his entire Bach edition on iPod." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/618069/10> ;
        ont:person        <http://dbpedia.org/resource/Johann_Sebastian_Bach> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/618069/10/0> ;
        ont:subject       <http://dbpedia.org/resource/Helmuth_Rilling> .

<http://w3id.org/polifonia/pilot/meetups/618069/6/6>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 2001 Rilling created the Festival Ensemble to be part of the European Music Festival Stuttgart (\"Musikfest Stuttgart\")." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/618069/6> ;
        ont:place         <http://dbpedia.org/resource/Stuttgart> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/618069/6/5> ;
        ont:subject       <http://dbpedia.org/resource/Helmuth_Rilling> .

<http://w3id.org/polifonia/pilot/meetups/618069/7/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Rilling was a teacher of chorale conducting at the Frankfurt Musikhochschule from 1965 to 1989." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/618069/7> ;
        ont:place           <http://dbpedia.org/resource/Frankfurt> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/618069/7/0> ;
        ont:subject         <http://dbpedia.org/resource/Helmuth_Rilling> ;
        ont:timeExpression  "from 1965 to 1989" .

<http://w3id.org/polifonia/pilot/meetups/618069/2/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He received his early training at the Protestant Seminaries in Württemberg." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/618069/2> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/618069/2/0> ;
        ont:subject       <http://dbpedia.org/resource/Helmuth_Rilling> .

<http://w3id.org/polifonia/pilot/meetups/618069/6/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He is well known for his performances of the music of Johann Sebastian Bach and his contemporaries." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/618069/6> ;
        ont:person        <http://dbpedia.org/resource/Johann_Sebastian_Bach> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/618069/6/0> ;
