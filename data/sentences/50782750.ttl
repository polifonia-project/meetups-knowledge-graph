        rdf:type            ont:Sentence ;
        ont:excerpt         "Juanita García (Bogota, February 14, 1943 — Morgantown, West Virginia, January 26, 2014) was a Tejano singer." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/50782750/0> ;
        ont:person          <http://dbpedia.org/resource/Juanita_Coulson> ;
