<http://w3id.org/polifonia/pilot/meetups/50782750/2/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Juanita García began her career at the age of eighteen in 1948, despite her parents disapproval of her career choice." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/50782750/2> ;
        ont:person          <http://dbpedia.org/resource/Juanita_Coulson> ;
