        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/51130819/2> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/51130819/2/10> ;
        ont:subject         <http://dbpedia.org/resource/Morris_Broadnax> ;
        ont:timeExpression  "February 17 , 2009" .

<http://w3id.org/polifonia/pilot/meetups/51130819/2/9>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In the late 1980s he also tried, unsuccessfully, to organize a Motown Alumni Association." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/51130819/2> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/51130819/2/8> ;
        ont:subject       <http://dbpedia.org/resource/Morris_Broadnax> .

<http://w3id.org/polifonia/pilot/meetups/51130819/2/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "At Motown, he worked closely with songwriter and record producer Clarence Paul, and with the young Stevie Wonder." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/51130819/2> ;
        ont:person        <http://dbpedia.org/resource/Clarence_Paul> , <http://dbpedia.org/resource/Stevie_Wonder> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/51130819/2/2> ;
