        ont:timeExpression  "one of" .

<http://w3id.org/polifonia/pilot/meetups/52811997/6/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Bishop died on 20 August 2011, after suffering a stroke during a meeting with officials to discuss national cultural presentations." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/52811997/6> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/52811997/6/0> ;
