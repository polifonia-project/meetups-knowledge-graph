<http://w3id.org/polifonia/pilot/meetups/52811997/4/10>
        rdf:type          ont:Sentence ;
        ont:excerpt       "While performing as a singer with the Tripoli Steel Orchestra, Bishop decided to arrange music." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/52811997/4> ;
        ont:place         <http://dbpedia.org/resource/Tripoli> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/52811997/4/9> ;
        ont:subject       <http://dbpedia.org/resource/Patricia_Bishop> .

<http://w3id.org/polifonia/pilot/meetups/52811997/6/6>
        rdf:type          ont:Sentence ;
        ont:excerpt       "The showing was a private exhibit called She Sells Sea Shells by the Sea Shore and featured 20 artworks centred on the imagery of the sea." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/52811997/6> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/52811997/6/5> ;
        ont:subject       <http://dbpedia.org/resource/Patricia_Bishop> .

