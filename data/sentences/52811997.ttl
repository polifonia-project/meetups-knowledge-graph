        rdf:type          ont:Sentence ;
        ont:excerpt       "Beginning with hymns and classical works, she soon turned toward arranging music for steel orchestras." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/52811997/4> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/52811997/4/10> ;
