<http://w3id.org/polifonia/pilot/meetups/52811997/4/1>
        rdf:type            ont:Sentence ;
        ont:excerpt         "She exhibited her first collection of artwork in 1961 in a group show with Jackie Hinkson and Peter Minshall." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/52811997/4> ;
        ont:person          <http://dbpedia.org/resource/Peter_Minshall> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/52811997/4/0> ;
        ont:subject         <http://dbpedia.org/resource/Patricia_Bishop> ;
