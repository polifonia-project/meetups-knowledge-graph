        rdf:type          ont:Sentence ;
        ont:excerpt       "Arranging music for the Desperadoes, she also contracted with major venues gaining a respected reputation in the steelband world, eventually taking them on eight major tours in the United States." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/52811997/4> ;
