<http://w3id.org/polifonia/pilot/meetups/52811997/4/6>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In the late 1960s, Bishop returned to her education, pursuing a master's degree at the University of the West Indies (UWI)." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/52811997/4> ;
        ont:place         <http://dbpedia.org/resource/West_Indies> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/52811997/4/5> ;
        ont:subject       <http://dbpedia.org/resource/Patricia_Bishop> .

<http://w3id.org/polifonia/pilot/meetups/52811997/4/30>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Combining the performances with folk and ballet dancers The Lydians' shows represented the cultural mix of indigenous and European culture that is uniquely Caribbean." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/52811997/4> ;
        ont:place         <http://dbpedia.org/resource/Caribbean> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/52811997/4/29> ;
        ont:subject       <http://dbpedia.org/resource/Patricia_Bishop> .

<http://w3id.org/polifonia/pilot/meetups/52811997/5/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "==" ;
