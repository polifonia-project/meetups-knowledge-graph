<http://w3id.org/polifonia/pilot/meetups/52811997/4/3>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Carlisle Chang, who designed the Flag of Trinidad and Tobago and promoted indigenous art[2] as an aesthetic, became her mentor when she returned to Trinidad." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/52811997/4> ;
        ont:place           <http://dbpedia.org/resource/Carlisle> , <http://dbpedia.org/resource/Trinidad> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/52811997/4/2> ;
        ont:subject         <http://dbpedia.org/resource/Patricia_Bishop> ;
        ont:timeExpression  "2 ]" .

<http://w3id.org/polifonia/pilot/meetups/52811997/8/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "==" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/52811997/8> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/52811997/8/1> ;
        ont:subject       <http://dbpedia.org/resource/Patricia_Bishop> .

<http://w3id.org/polifonia/pilot/meetups/52811997/4/12>
        rdf:type            ont:Sentence ;
        ont:excerpt         "She was one of the first women to arrange music specifically for steelbands." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/52811997/4> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/52811997/4/11> ;
        ont:subject         <http://dbpedia.org/resource/Patricia_Bishop> ;
        ont:timeExpression  "one of" .

<http://w3id.org/polifonia/pilot/meetups/52811997/6/8>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Bishop has artistic works that are part of the permanent collection of the Central Bank of Trinidad and Tobago." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/52811997/6> ;
        ont:place         <http://dbpedia.org/resource/Central_America> , <http://dbpedia.org/resource/Trinidad> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/52811997/6/7> ;
        ont:subject       <http://dbpedia.org/resource/Patricia_Bishop> .

<http://w3id.org/polifonia/pilot/meetups/52811997/3>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/52811997/3/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/52811997/3/1> .

<http://w3id.org/polifonia/pilot/meetups/52811997/4/25>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Bishop directed and prepared some of Trinidad's most noted steelbands in festival competitions and critically reviewed pan music for Radio Trinidad." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/52811997/4> ;
        ont:place         <http://dbpedia.org/resource/Trinidad> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/52811997/4/24> ;
        ont:subject       <http://dbpedia.org/resource/Patricia_Bishop> .

<http://w3id.org/polifonia/pilot/meetups/52811997/2/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "She attended Tranquility Girls' School and then studied at Bishop Anstey High School." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/52811997/2> ;
        ont:person        <http://dbpedia.org/resource/Girl> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/52811997/2/2> ;
        ont:subject       <http://dbpedia.org/resource/Patricia_Bishop> .

<http://w3id.org/polifonia/pilot/meetups/52811997/4/38>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Bishop taught at UWI until her death." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/52811997/4> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/52811997/4/37> ;
        ont:subject       <http://dbpedia.org/resource/Patricia_Bishop> .

<http://w3id.org/polifonia/pilot/meetups/52811997/6/2>
        rdf:type            ont:Sentence ;
        ont:excerpt         "In January 2012, the Panorama Competition featured a presentation by the band Phase II Pan Groove, led by Boogsie Sharpe, featuring lyrics written by Gregory \"GB\" Ballantyne dedicated to Bishop." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/52811997/6> ;
        ont:person          <http://dbpedia.org/resource/Pope_Gregory_I> , <http://dbpedia.org/resource/Lennox_Sharpe> , <http://dbpedia.org/resource/R._M._Ballantyne> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/52811997/6/1> ;
        ont:subject         <http://dbpedia.org/resource/Patricia_Bishop> ;
        ont:timeExpression  "January 2012" .

<http://w3id.org/polifonia/pilot/meetups/52811997/4/8>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Between 1970 and 1972, she taught art history and design as a lecturer at the Jamaica School of Art and Crafts." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/52811997/4> ;
        ont:place         <http://dbpedia.org/resource/Jamaica> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/52811997/4/7> ;
        ont:subject       <http://dbpedia.org/resource/Patricia_Bishop> .

<http://w3id.org/polifonia/pilot/meetups/52811997/10>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/52811997/10/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/52811997/10/1> .

<http://w3id.org/polifonia/pilot/meetups/52811997/4/32>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Also in 1996, Bishop directed The Lydians in its first opera Koanga by Frederick Delius." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/52811997/4> ;
        ont:person        <http://dbpedia.org/resource/Frederick_Delius> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/52811997/4/31> ;
        ont:subject       <http://dbpedia.org/resource/Patricia_Bishop> .

<http://w3id.org/polifonia/pilot/meetups/52811997/4/17>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In addition to arranging music, Bishop composed two full-length folk operas and several choral pieces based on Trinidadian folk traditions." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/52811997/4> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/52811997/4/16> ;
        ont:subject       <http://dbpedia.org/resource/Patricia_Bishop> .

<http://w3id.org/polifonia/pilot/meetups/52811997/9/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Bibliography =" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/52811997/9> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/52811997/9/0> ;
        ont:subject       <http://dbpedia.org/resource/Patricia_Bishop> .

