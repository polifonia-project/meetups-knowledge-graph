<http://w3id.org/polifonia/pilot/meetups/52811997/2/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Bishop had one younger sibling, a sister, Gillian, who would become a jewellery designer." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/52811997/2> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/52811997/2/1> ;
        ont:subject       <http://dbpedia.org/resource/Patricia_Bishop> .

<http://w3id.org/polifonia/pilot/meetups/52811997/4/37>
        rdf:type          ont:Sentence ;
        ont:excerpt       "To preserve the traditions of Trinidad and Tobago, digital and printed format arrangements of music collections were gathered and donated to tertiary institutions for conservation." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/52811997/4> ;
        ont:place         <http://dbpedia.org/resource/Trinidad> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/52811997/4/36> ;
        ont:subject       <http://dbpedia.org/resource/Patricia_Bishop> .

<http://w3id.org/polifonia/pilot/meetups/52811997/6/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Posthumously, the Music Literacy Trust developed the Pat Bishop Scholarship Programme to assist music students and arrangers in furthering their education." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/52811997/6> ;
        ont:person        <http://dbpedia.org/resource/Patricia_Bishop> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/52811997/6/0> ;
        ont:subject       <http://dbpedia.org/resource/Patricia_Bishop> .

<http://w3id.org/polifonia/pilot/meetups/52811997/4/7>
        rdf:type            ont:Sentence ;
        ont:excerpt         "She completed her thesis, Runaway Slaves in Jamaica 1740–1807 in 1970, graduating with a master's in art history." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/52811997/4> ;
        ont:place           <http://dbpedia.org/resource/Jamaica> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/52811997/4/6> ;
        ont:subject         <http://dbpedia.org/resource/Patricia_Bishop> ;
        ont:timeExpression  "Jamaica 1740–1807" .

<http://w3id.org/polifonia/pilot/meetups/52811997/4/31>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1995, Bishop was awarded an honorary doctorate from UWI and the following year, received the highest national honor, when she was awarded the Trinity Cross." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/52811997/4> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/52811997/4/30> ;
        ont:subject       <http://dbpedia.org/resource/Patricia_Bishop> .

<http://w3id.org/polifonia/pilot/meetups/52811997/5/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Death and legacy ==" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/52811997/5> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/52811997/5/0> ;
        ont:subject       <http://dbpedia.org/resource/Patricia_Bishop> .

<http://w3id.org/polifonia/pilot/meetups/52811997/4/16>
        rdf:type          ont:Sentence ;
        ont:excerpt       "She advocated for panning to be taught in schools, so that students could carry on the traditions by learning its history, techniques and the theory behind the performance." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/52811997/4> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/52811997/4/15> ;
        ont:subject       <http://dbpedia.org/resource/Patricia_Bishop> .

