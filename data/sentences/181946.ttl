<http://w3id.org/polifonia/pilot/meetups/181946/8/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Hardy died of tuberculosis in New Orleans, just four days after his 22nd birthday, and was buried in Gretna, Louisiana." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/181946/8> ;
        ont:place           <http://dbpedia.org/resource/Gretna,_Louisiana> , <http://dbpedia.org/resource/New_Orleans> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/181946/8/0> ;
        ont:subject         <http://dbpedia.org/resource/Emmett_Hardy> ;
        ont:timeExpression  "22nd birthday" .

<http://w3id.org/polifonia/pilot/meetups/181946/3/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He belonged to a small band that supported singer Bee Palmer." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/181946/3> ;
        ont:person        <http://dbpedia.org/resource/Bee_Palmer> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/181946/3/0> ;
        ont:subject       <http://dbpedia.org/resource/Emmett_Hardy> .

<http://w3id.org/polifonia/pilot/meetups/181946/7/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Hardy and some of his musician friends made some home recordings on wax phonograph cylinders for their own amusement." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/181946/7> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/181946/7/0> ;
        ont:subject       <http://dbpedia.org/resource/Emmett_Hardy> .

<http://w3id.org/polifonia/pilot/meetups/181946/2/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Hardy was a child prodigy, described as already playing marvelously in his early teens." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/181946/2> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/181946/2/0> ;
        ont:subject       <http://dbpedia.org/resource/Emmett_Hardy> .

<http://w3id.org/polifonia/pilot/meetups/181946/6/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "When advancing tuberculosis started to make his breathing difficult, Hardy taught himself banjo so he could continue playing music." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/181946/6> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/181946/6/0> ;
        ont:subject       <http://dbpedia.org/resource/Emmett_Hardy> .

<http://w3id.org/polifonia/pilot/meetups/181946/1/1>
