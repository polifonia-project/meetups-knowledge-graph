        rdf:_4    <http://w3id.org/polifonia/pilot/meetups/833172/3/3> .

<http://w3id.org/polifonia/pilot/meetups/833172/8/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Von Arnim was the first composer to set the poet Hölderlin's work to music." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/833172/8> ;
        ont:person        <http://dbpedia.org/resource/Poet> , <http://dbpedia.org/resource/Composer> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/833172/8/0> ;
        ont:subject       <http://dbpedia.org/resource/Bettina_von_Arnim> .

<http://w3id.org/polifonia/pilot/meetups/833172/3/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He was a mentor and protector to her and inspired her to read the poetry of the time, especially Goethe." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/833172/3> ;
        ont:person        <http://dbpedia.org/resource/Johann_Wolfgang_von_Goethe> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/833172/3/1> ;
        ont:subject       <http://dbpedia.org/resource/Bettina_von_Arnim> .

