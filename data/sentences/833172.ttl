<http://w3id.org/polifonia/pilot/meetups/833172/9/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "She published two politically dissident works but evaded chastisement because of her friendship with the King of Prussia." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/833172/9> ;
        ont:person        <http://dbpedia.org/resource/Dissident> ;
        ont:place         <http://dbpedia.org/resource/Prussia> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/833172/9/0> ;
        ont:subject       <http://dbpedia.org/resource/Bettina_von_Arnim> .

<http://w3id.org/polifonia/pilot/meetups/833172/4/2>
        rdf:type            ont:Sentence ;
        ont:excerpt         "The two friends acknowledged only natural impulses, laws, and methods of life, and brooded over the \"tyranny\" of conventionalities." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/833172/4> ;
        ont:person          <http://dbpedia.org/resource/Tyrant> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/833172/4/1> ;
        ont:subject         <http://dbpedia.org/resource/Bettina_von_Arnim> ;
        ont:timeExpression  "two" .

<http://w3id.org/polifonia/pilot/meetups/833172/3>
        rdf:type  ont:Paragraph ;
