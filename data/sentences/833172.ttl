<http://w3id.org/polifonia/pilot/meetups/833172/10/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "After the 1831 death of her husband, Bettina continued her dedication to the creative community." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/833172/10> ;
        ont:person          <http://dbpedia.org/resource/Bettina_von_Arnim> ;
