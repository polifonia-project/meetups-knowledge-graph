<http://w3id.org/polifonia/pilot/meetups/833172/7/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "The collection became a touchstone of the Romantic musical and poetic style." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/833172/7> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/833172/7/1> ;
