        rdf:_4    <http://w3id.org/polifonia/pilot/meetups/833172/21/3> .

<http://w3id.org/polifonia/pilot/meetups/833172/7/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Some of the songs were later put to music by a number of composers, among them Gustav Mahler." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/833172/7> ;
        ont:person        <http://dbpedia.org/resource/Gustav_Mahler> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/833172/7/0> ;
        ont:subject       <http://dbpedia.org/resource/Bettina_von_Arnim> .

<http://w3id.org/polifonia/pilot/meetups/833172/19/2>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Correspondence 1832–1844), with commentary, Cotta, Stuttgart 2001, ISBN 3-7681-9809-XHer Armenbuch contains a tale that reflects the social sensibilities of the time in regards to the issue of poverty (German: Erzählung vom Heckebeutel; English: \"Tale of the Lucky Purse\")." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/833172/19> ;
        ont:place           <http://dbpedia.org/resource/Stuttgart> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/833172/19/1> ;
        ont:subject         <http://dbpedia.org/resource/Bettina_von_Arnim> ;
        ont:timeExpression  "Stuttgart 2001" , "Correspondence 1832–1844" , "ISBN 3-7681-9809-XHer" .

<http://w3id.org/polifonia/pilot/meetups/833172/21/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "The community's third founder was Hermann Spiess." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/833172/21> ;
