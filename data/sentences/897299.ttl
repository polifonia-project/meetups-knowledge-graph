<http://w3id.org/polifonia/pilot/meetups/897299/0/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "His performances polished his skills at pantomime and music hall sketches." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/897299/0> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/897299/0/2> ;
