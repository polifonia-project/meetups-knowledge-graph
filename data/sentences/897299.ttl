        ont:timeExpression  "April 1961" , "on the 33rd" .

<http://w3id.org/polifonia/pilot/meetups/897299/18/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Laurel found, to his shock, that he and Hardy were hired only as actors, and were not expected to contribute to the staging, writing, or editing of the productions." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/897299/18> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/897299/18/0> ;
        ont:subject       <http://dbpedia.org/resource/Stan_Laurel> .

<http://w3id.org/polifonia/pilot/meetups/897299/6/5>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Chaplin and Laurel arrived in the United States on the same ship from Britain with the Karno troupe and toured the country." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/897299/6> ;
        ont:person        <http://dbpedia.org/resource/Charlie_Chaplin> , <http://dbpedia.org/resource/Fred_Karno> ;
        ont:place         <http://dbpedia.org/resource/United_States> , <http://dbpedia.org/resource/United_Kingdom> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/897299/6/4> ;
        ont:subject       <http://dbpedia.org/resource/Stan_Laurel> .

<http://w3id.org/polifonia/pilot/meetups/897299/20/5>
        rdf:type          ont:Sentence ;
        ont:excerpt       "During this tour, Laurel fell ill and was unable to perform for several weeks." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/897299/20> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/897299/20/4> ;
        ont:subject       <http://dbpedia.org/resource/Stan_Laurel> .

<http://w3id.org/polifonia/pilot/meetups/897299/27/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "At the time, Laurel's second marriage was in the process of a divorce, with Dahlberg's legal suit adding to Laurel's woes." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/897299/27> ;
        ont:person        <http://dbpedia.org/resource/Divorce> , <http://dbpedia.org/resource/Mikael_Dahlberg> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/897299/27/1> ;
        ont:subject       <http://dbpedia.org/resource/Stan_Laurel> .

