        ont:person          <http://dbpedia.org/resource/Oliver_Hardy> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/897299/8/0> ;
        ont:subject         <http://dbpedia.org/resource/Stan_Laurel> ;
