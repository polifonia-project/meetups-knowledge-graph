<http://w3id.org/polifonia/pilot/meetups/897299/18/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "The team signed another contract with Metro-Goldwyn-Mayer in 1942, resulting in two more features." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/897299/18> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/897299/18/3> ;
        ont:subject       <http://dbpedia.org/resource/Stan_Laurel> .

<http://w3id.org/polifonia/pilot/meetups/897299/22/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Oliver Hardy died on 7 August 1957." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/897299/22> ;
