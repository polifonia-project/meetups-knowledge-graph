        rdf:_3    <http://w3id.org/polifonia/pilot/meetups/897299/41/2> .

<http://w3id.org/polifonia/pilot/meetups/897299/31/4>
        rdf:type            ont:Sentence ;
        ont:excerpt         "In a 2005 UK poll, Comedians' Comedian, Laurel and Hardy were ranked top double act, and seventh overall." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/897299/31> ;
        ont:place           <http://dbpedia.org/resource/United_Kingdom> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/897299/31/3> ;
        ont:subject         <http://dbpedia.org/resource/Stan_Laurel> ;
        ont:timeExpression  "In a 2005" .

