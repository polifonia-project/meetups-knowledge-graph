<http://w3id.org/polifonia/pilot/meetups/897299/20/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1950, Laurel and Hardy were invited to France to make a feature film." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/897299/20> ;
        ont:place         <http://dbpedia.org/resource/France> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/897299/20/0> ;
