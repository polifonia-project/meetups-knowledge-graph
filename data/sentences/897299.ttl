<http://w3id.org/polifonia/pilot/meetups/897299/6/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "He joined Fred Karno's troupe of actors in 1910 with the stage name of \"Stan Jefferson\"; the troupe also included a young Charlie Chaplin." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/897299/6> ;
        ont:person          <http://dbpedia.org/resource/Charlie_Chaplin> , <http://dbpedia.org/resource/Fred_Karno> , <http://dbpedia.org/resource/Stan_Jefferson> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/897299/6/0> ;
        ont:subject         <http://dbpedia.org/resource/Stan_Laurel> ;
        ont:timeExpression  "1910 with" .

<http://w3id.org/polifonia/pilot/meetups/897299/16/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Eventually, the case was dropped and Laurel returned to Roach." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/897299/16> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/897299/16/3> ;
