        ont:subject       <http://dbpedia.org/resource/P._B._Sreenivas> .

<http://w3id.org/polifonia/pilot/meetups/1103482/13/14>
        rdf:type          ont:Sentence ;
        ont:excerpt       "With that song, Rajkumar restarted his singing career, which had stopped after the movie Ohileshwara." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1103482/13> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1103482/13/13> ;
        ont:subject       <http://dbpedia.org/resource/P._B._Sreenivas> .

<http://w3id.org/polifonia/pilot/meetups/1103482/8/13>
        rdf:type          ont:Sentence ;
        ont:excerpt       "They were titled 'Man To Moon', a solo by himself and the other was 'Moon To God' a duet with S. Janaki." ;
