<http://w3id.org/polifonia/pilot/meetups/103549/0/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Jiles Perry Richardson Jr. (October 24, 1930 – February 3, 1959), known as The Big Bopper, was an American musician, songwriter and disc jockey." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/103549/0> ;
        ont:person          <http://dbpedia.org/resource/The_Big_Bopper> , <http://dbpedia.org/resource/Disc_jockey> ;
        ont:place           <http://dbpedia.org/resource/United_States> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/103549/0/0> ;
        ont:subject         <http://dbpedia.org/resource/The_Big_Bopper> ;
        ont:timeExpression  "February 3 , 1959" , "October 24 , 1930" .

<http://w3id.org/polifonia/pilot/meetups/103549/17/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Meanwhile, J.P. Richardson was suffering from the flu and was complaining that the bus was too cold and uncomfortable for him, so Jennings voluntarily surrendered his seat." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/103549/17> ;
        ont:person        <http://dbpedia.org/resource/Justice_of_the_peace> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/103549/17/1> ;
        ont:subject       <http://dbpedia.org/resource/The_Big_Bopper> .

<http://w3id.org/polifonia/pilot/meetups/103549/5/5>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Richardson returned to KTRM radio following his discharge as a corporal in March 1957, where he held down the \"Dishwashers' Serenade\" shift from 11 a.m. to 12:30 p.m., Monday through Friday." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/103549/5> ;
        ont:person          <http://dbpedia.org/resource/Hugh_Edward_Richardson> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/103549/5/4> ;
        ont:subject         <http://dbpedia.org/resource/The_Big_Bopper> ;
        ont:timeExpression  "11 a.m." , "12:30 p.m." , "March 1957" .

<http://w3id.org/polifonia/pilot/meetups/103549/1/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "==" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/103549/1> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/103549/1/0> ;
        ont:subject       <http://dbpedia.org/resource/The_Big_Bopper> .

<http://w3id.org/polifonia/pilot/meetups/103549/11/5>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He toured on the \"Winter Dance Party\" tour with Buddy Holly impersonator John Mueller on some of the same stages where his father had performed." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/103549/11> ;
        ont:person        <http://dbpedia.org/resource/John_Mueller> , <http://dbpedia.org/resource/Buddy_Holly> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/103549/11/4> ;
        ont:subject       <http://dbpedia.org/resource/The_Big_Bopper> .

<http://w3id.org/polifonia/pilot/meetups/103549/18/2>
        rdf:type            ont:Sentence ;
        ont:excerpt         "It slammed into the ground at full throttle shortly after takeoff about 5 miles outside Mason City in the middle of farm country." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/103549/18> ;
        ont:place           <http://dbpedia.org/resource/Mason_City,_Iowa> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/103549/18/1> ;
        ont:subject         <http://dbpedia.org/resource/The_Big_Bopper> ;
