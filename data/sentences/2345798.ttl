        ont:subject       <http://dbpedia.org/resource/Hersal_Thomas> .

<http://w3id.org/polifonia/pilot/meetups/2345798/1/10>
        rdf:type          ont:Sentence ;
        ont:excerpt       "The first three are listed as having been composed by \"Thomas\", though it is not clear if this refers to Hersal or his brother." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2345798/1> ;
        ont:person        <http://dbpedia.org/resource/Listed_building> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2345798/1/9> ;
        ont:subject       <http://dbpedia.org/resource/Hersal_Thomas> .

<http://w3id.org/polifonia/pilot/meetups/2345798/0/1>
        rdf:type            ont:Sentence ;
        ont:excerpt         "He recorded a number of sides for Okeh Records in 1925 and 1926." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/2345798/0> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/2345798/0/0> ;
        ont:subject         <http://dbpedia.org/resource/Hersal_Thomas> ;
        ont:timeExpression  "in 1925 and 1926" .

<http://w3id.org/polifonia/pilot/meetups/2345798/1/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Thomas was born in Houston, Texas, United States, and displayed an early talent for blues playing and composition." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2345798/1> ;
        ont:place         <http://dbpedia.org/resource/Houston> , <http://dbpedia.org/resource/United_States> , <http://dbpedia.org/resource/Texas> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2345798/1/0> ;
        ont:subject       <http://dbpedia.org/resource/Hersal_Thomas> .

<http://w3id.org/polifonia/pilot/meetups/2345798/1/6>
        rdf:type          ont:Sentence ;
        ont:excerpt       "The Thomas brothers also co-wrote \"The Fives\", which Ammons and Lewis cited as an essential boogie-woogie number." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2345798/1> ;
        ont:place         <http://dbpedia.org/resource/Isle_of_Lewis> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2345798/1/5> ;
        ont:subject       <http://dbpedia.org/resource/Hersal_Thomas> .

