<http://w3id.org/polifonia/pilot/meetups/2345798/1/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Albert Ammons and Meade Lux Lewis both cited him as an influence." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2345798/1> ;
        ont:person        <http://dbpedia.org/resource/Albert_Ammons> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2345798/1/3> ;
        ont:subject       <http://dbpedia.org/resource/Hersal_Thomas> .

<http://w3id.org/polifonia/pilot/meetups/2345798/1/13>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Thomas died of food poisoning while working at Penny's Pleasure Inn in Detroit, Michigan." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2345798/1> ;
        ont:place         <http://dbpedia.org/resource/Michigan> , <http://dbpedia.org/resource/Detroit> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2345798/1/12> ;
        ont:subject       <http://dbpedia.org/resource/Hersal_Thomas> .

<http://w3id.org/polifonia/pilot/meetups/2345798/0>
        rdf:type  ont:Paragraph ;
