<http://w3id.org/polifonia/pilot/meetups/2345798/1/11>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He also worked in session with King Oliver." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2345798/1> ;
        ont:person        <http://dbpedia.org/resource/King_Oliver> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2345798/1/10> ;
        ont:subject       <http://dbpedia.org/resource/Hersal_Thomas> .

<http://w3id.org/polifonia/pilot/meetups/2345798/1/1>
        rdf:type            ont:Sentence ;
        ont:excerpt         "He was one of several musicians in his family." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/2345798/1> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/2345798/1/0> ;
        ont:subject         <http://dbpedia.org/resource/Hersal_Thomas> ;
        ont:timeExpression  "one of" .

<http://w3id.org/polifonia/pilot/meetups/2345798/1/7>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Thomas recorded under his own name, and as an accompanist to Hociel Thomas, Sippie Wallace, Lilian Miller and possibly, Sodarisa Miller." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2345798/1> ;
        ont:person        <http://dbpedia.org/resource/Sippie_Wallace> , <http://dbpedia.org/resource/Accompaniment> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2345798/1/6> ;
        ont:subject       <http://dbpedia.org/resource/Hersal_Thomas> .

