<http://w3id.org/polifonia/pilot/meetups/1068160/35/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In July that year, he flew to London, where he stayed in the five-star Westbury Hotel in Mayfair and began a four-day drinking binge during which he extensively damaged his room." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1068160/35> ;
        ont:place         <http://dbpedia.org/resource/London> , <http://dbpedia.org/resource/Westbury_railway_station> , <http://dbpedia.org/resource/Mayfair> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1068160/35/0> ;
        ont:subject       <http://dbpedia.org/resource/Gerry_Rafferty> .

<http://w3id.org/polifonia/pilot/meetups/1068160/28/7>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Billy Connolly agrees that Rafferty had different priorities: \"I wanted success and fame and I got it, to a degree." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1068160/28> ;
        ont:person        <http://dbpedia.org/resource/Billy_Connolly> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1068160/28/6> ;
        ont:subject       <http://dbpedia.org/resource/Gerry_Rafferty> .

