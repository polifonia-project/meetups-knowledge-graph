<http://w3id.org/polifonia/pilot/meetups/1068160/19/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "These were the last two records Rafferty produced with Hugh Murphy, who died in 1998." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1068160/19> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1068160/19/3> ;
