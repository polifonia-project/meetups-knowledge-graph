        rdf:type          ont:Sentence ;
        ont:excerpt       "Gordon Laurie, director of the housing association behind the project, commented: \"It is a fitting tribute to a brilliant musician who lives on through his music." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1068160/41> ;
        ont:person        <http://dbpedia.org/resource/Mike_Gordon> ;
