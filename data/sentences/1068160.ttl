<http://w3id.org/polifonia/pilot/meetups/1068160/11/8>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Ravenscroft went on to play on Rafferty's next two albums." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1068160/11> ;
        ont:person        <http://dbpedia.org/resource/Raphael_Ravenscroft> ;
