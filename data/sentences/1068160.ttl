<http://w3id.org/polifonia/pilot/meetups/1068160/35/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Speaking to The Independent newspaper later, the hotel's director commented: \"It was such a shame." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1068160/35> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1068160/35/1> ;
        ont:subject       <http://dbpedia.org/resource/Gerry_Rafferty> .

<http://w3id.org/polifonia/pilot/meetups/1068160/28/8>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Gerry wanted respect." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1068160/28> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1068160/28/7> ;
        ont:subject       <http://dbpedia.org/resource/Gerry_Rafferty> .

<http://w3id.org/polifonia/pilot/meetups/1068160/12/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "I must admit, I could live off that song alone\"." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1068160/12> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1068160/12/3> ;
        ont:subject       <http://dbpedia.org/resource/Gerry_Rafferty> .

<http://w3id.org/polifonia/pilot/meetups/1068160/35/15>
