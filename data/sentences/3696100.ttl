<http://w3id.org/polifonia/pilot/meetups/3696100/8/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "They returned to the recording studio but their albums, Home is in My Head and Three, failed to sell." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/3696100/8> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/3696100/8/3> ;
