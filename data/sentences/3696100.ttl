        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/3696100/25/0> .

<http://w3id.org/polifonia/pilot/meetups/3696100/4/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Lomax spent two years in the US with The Undertakers and a couple of other groups." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/3696100/4> ;
        ont:person        <http://dbpedia.org/resource/Alan_Lomax> ;
        ont:place         <http://dbpedia.org/resource/United_States> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/3696100/4/3> ;
        ont:subject       <http://dbpedia.org/resource/Jackie_Lomax> .

<http://w3id.org/polifonia/pilot/meetups/3696100/13/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Dicks later wrote the sleeve notes for, and organised, the UK release of Lomax's The Ballad of Liverpool Slim...and Others album." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/3696100/13> ;
        ont:person        <http://dbpedia.org/resource/Alan_Lomax> ;
        ont:place         <http://dbpedia.org/resource/Liverpool> , <http://dbpedia.org/resource/United_Kingdom> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/3696100/13/3> ;
        ont:subject       <http://dbpedia.org/resource/Jackie_Lomax> .

<http://w3id.org/polifonia/pilot/meetups/3696100/2>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/3696100/2/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/3696100/2/1> .

<http://w3id.org/polifonia/pilot/meetups/3696100/8/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He signed to Warner Bros. Records and reunited with members of the Lomax Alliance and The Undertakers." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/3696100/8> ;
        ont:person        <http://dbpedia.org/resource/Alliance> , <http://dbpedia.org/resource/Alan_Lomax> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/3696100/8/2> ;
        ont:subject       <http://dbpedia.org/resource/Jackie_Lomax> .

<http://w3id.org/polifonia/pilot/meetups/3696100/7/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "After a few months the band disintegrated." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/3696100/7> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/3696100/7/2> ;
        ont:subject       <http://dbpedia.org/resource/Jackie_Lomax> .

<http://w3id.org/polifonia/pilot/meetups/3696100/32>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/3696100/32/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/3696100/32/1> .

<http://w3id.org/polifonia/pilot/meetups/3696100/16/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "It featured a reworked version by Irish female singer Lyra." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/3696100/16> ;
        ont:place         <http://dbpedia.org/resource/Lyra> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/3696100/16/2> ;
        ont:subject       <http://dbpedia.org/resource/Jackie_Lomax> .

<http://w3id.org/polifonia/pilot/meetups/3696100/4/13>
        rdf:type          ont:Sentence ;
        ont:excerpt       "By 1970, The Beatles' breakup left the remaining Apple Records artists in limbo." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/3696100/4> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/3696100/4/12> ;
        ont:subject       <http://dbpedia.org/resource/Jackie_Lomax> .

<http://w3id.org/polifonia/pilot/meetups/3696100/17>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/3696100/17/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/3696100/17/1> .

<http://w3id.org/polifonia/pilot/meetups/3696100/6/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "They were beaten to the punch however by the group Skip Bifferty who released their own single as Heavy Jelly, \"I Keep Singing That Same Old Song\" b/w \"Blue\", on Island Records." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/3696100/6> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/3696100/6/2> ;
        ont:subject       <http://dbpedia.org/resource/Jackie_Lomax> .

<http://w3id.org/polifonia/pilot/meetups/3696100/10/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In California, particularly Ventura County, Lomax played live with a succession of line-ups including Tom Petty, drummer Randall Marsh, Jim Calire, Patrick Landreville and Mitch Kashmar." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/3696100/10> ;
        ont:person        <http://dbpedia.org/resource/Tom_Petty> , <http://dbpedia.org/resource/John_Ernest_Randall> , <http://dbpedia.org/resource/Alan_Lomax> , <http://dbpedia.org/resource/Mitch_Albom> ;
        ont:place         <http://dbpedia.org/resource/Ventura_County_Line> , <http://dbpedia.org/resource/Kashmar> , <http://dbpedia.org/resource/California> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/3696100/10/3> ;
