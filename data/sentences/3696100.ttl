<http://w3id.org/polifonia/pilot/meetups/3696100/8/9>
        rdf:type            ont:Sentence ;
        ont:excerpt         "He released two Capitol albums, Livin' for Lovin' and Did You Ever Have That Feeling?, before leaving the label in 1977." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/3696100/8> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/3696100/8/8> ;
        ont:subject         <http://dbpedia.org/resource/Jackie_Lomax> ;
        ont:timeExpression  "label in 1977" .

