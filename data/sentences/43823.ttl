        rdf:type          ont:Sentence ;
        ont:excerpt       "Audiences took some time to appreciate his films, and he did not want to have to satisfy studio officials." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/43823/17> ;
