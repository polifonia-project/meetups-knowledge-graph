        rdf:type            ont:Sentence ;
        ont:excerpt         "Altman was born on February 20, 1925, in Kansas City, Missouri, the son of Helen (née Matthews), a Mayflower descendant from Nebraska, and Bernard Clement Altman, a wealthy insurance salesman and amateur gambler, who came from an upper-class family." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/43823/5> ;
