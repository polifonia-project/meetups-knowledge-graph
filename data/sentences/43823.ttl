        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/43823/30/1> .

<http://w3id.org/polifonia/pilot/meetups/43823/71/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Vilmos Zsigmond's cinematography in McCabe and Mrs. Miller received a nomination by the British Academy Film Awards." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/43823/71> ;
        ont:person        <http://dbpedia.org/resource/Vilmos_Kertész> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/43823/71/0> ;
        ont:subject       <http://dbpedia.org/resource/Robert_Altman> .

<http://w3id.org/polifonia/pilot/meetups/43823/54/5>
        rdf:type          ont:Sentence ;
        ont:excerpt       "We don't just turn people loose.\"" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/43823/54> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/43823/54/4> ;
        ont:subject       <http://dbpedia.org/resource/Robert_Altman> .

<http://w3id.org/polifonia/pilot/meetups/43823/91/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He was known to hate the \"phoniness\" he saw in most mainstream films, and \"he wanted to explode them\" through satire." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/43823/91> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/43823/91/1> ;
        ont:subject       <http://dbpedia.org/resource/Robert_Altman> .

<http://w3id.org/polifonia/pilot/meetups/43823/7/5>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Altman directed some 65 industrial films and documentaries for the Calvin Company." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/43823/7> ;
        ont:person          <http://dbpedia.org/resource/Robert_Altman> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/43823/7/4> ;
        ont:subject         <http://dbpedia.org/resource/Robert_Altman> ;
        ont:timeExpression  "65" .

<http://w3id.org/polifonia/pilot/meetups/43823/1/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Actors especially enjoyed working under his direction because he encouraged them to improvise, thereby inspiring their own creativity." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/43823/1> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/43823/1/1> ;
        ont:subject       <http://dbpedia.org/resource/Robert_Altman> .

