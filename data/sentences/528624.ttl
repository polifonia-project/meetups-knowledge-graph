<http://w3id.org/polifonia/pilot/meetups/528624/0/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He was a classmate of Carl Reinecke, whose father was head of the Leipzig Conservatory." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/528624/0> ;
        ont:person        <http://dbpedia.org/resource/Carl_Reinecke> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/528624/0/0> ;
        ont:subject       <http://dbpedia.org/resource/Cornelius_Gurlitt_(composer)> .

<http://w3id.org/polifonia/pilot/meetups/528624/1/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "In 1842, Gurlitt moved to Hørsholm, where he resided as organist and music teacher for four years." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/528624/1> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/528624/1/0> ;
        ont:subject         <http://dbpedia.org/resource/Cornelius_Gurlitt_(composer)> ;
        ont:timeExpression  "music teacher for four" .

<http://w3id.org/polifonia/pilot/meetups/528624/1/6>
        rdf:type          ont:Sentence ;
        ont:excerpt       "When the Schleswig-Holstein war broke out in 1848, Gurlitt became a military band master." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/528624/1> ;
        ont:place         <http://dbpedia.org/resource/Schleswig-Holstein> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/528624/1/5> ;
        ont:subject       <http://dbpedia.org/resource/Cornelius_Gurlitt_(composer)> .

