        rdf:type            ont:Sentence ;
        ont:excerpt         "Gurlitt studied with Reinecke's father for six years." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/528624/0> ;
        ont:person          <http://dbpedia.org/resource/Carl_Reinecke> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/528624/0/1> ;
        ont:subject         <http://dbpedia.org/resource/Cornelius_Gurlitt_(composer)> ;
        ont:timeExpression  "father for six" .

<http://w3id.org/polifonia/pilot/meetups/528624/1/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He then moved to Leipzig, Germany, where Gade was musical director for the Gewandhaus concerts." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/528624/1> ;
        ont:place         <http://dbpedia.org/resource/Germany> , <http://dbpedia.org/resource/Leipzig> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/528624/1/0> ;
        ont:subject       <http://dbpedia.org/resource/Cornelius_Gurlitt_(composer)> .

<http://w3id.org/polifonia/pilot/meetups/528624/1/7>
        rdf:type          ont:Sentence ;
        ont:excerpt       "His output was prodigious in quantity and breadth, ranging from songs and teaching pieces to operas, cantatas, and symphonies." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/528624/1> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/528624/1/6> ;
        ont:subject       <http://dbpedia.org/resource/Cornelius_Gurlitt_(composer)> .

