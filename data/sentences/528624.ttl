        rdf:type          ont:Sentence ;
        ont:excerpt       "Gurlitt next traveled to Rome, where his brother, Louis Gurlitt, a well-known painter, was studying." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/528624/1> ;
        ont:person        <http://dbpedia.org/resource/Louis_Gurlitt> ;
