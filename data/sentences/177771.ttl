<http://w3id.org/polifonia/pilot/meetups/177771/8/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Hayes's consent to raze the theatre named for her was sought and given, though she had no ownership interest in the building." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/177771/8> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/177771/8/1> ;
        ont:subject       <http://dbpedia.org/resource/Helen_Hayes> .

<http://w3id.org/polifonia/pilot/meetups/177771/3/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Hayes's Catholic maternal grandparents emigrated from Ireland during the Great Famine." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/177771/3> ;
        ont:place         <http://dbpedia.org/resource/Ireland> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/177771/3/2> ;
        ont:subject       <http://dbpedia.org/resource/Helen_Hayes> .

<http://w3id.org/polifonia/pilot/meetups/177771/25>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/177771/25/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/177771/25/1> .

<http://w3id.org/polifonia/pilot/meetups/177771/28/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1983, Hayes received the Award for Greatest Public Service Benefiting the Disadvantaged, an award given out annually by Jefferson Awards." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/177771/28> ;
        ont:person        <http://dbpedia.org/resource/Thomas_Jefferson> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/177771/28/2> ;
        ont:subject       <http://dbpedia.org/resource/Helen_Hayes> .

<http://w3id.org/polifonia/pilot/meetups/177771/7/2>
        rdf:type            ont:Sentence ;
        ont:excerpt         "She returned to Hollywood in the 1950s, and her film star began to rise." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/177771/7> ;
        ont:place           <http://dbpedia.org/resource/Hollywood> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/177771/7/1> ;
        ont:subject         <http://dbpedia.org/resource/Helen_Hayes> ;
        ont:timeExpression  "in the 1950s" .

