<http://w3id.org/polifonia/pilot/meetups/177771/0/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1988, she was awarded the National Medal of Arts." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/177771/0> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/177771/0/3> ;
        ont:subject       <http://dbpedia.org/resource/Helen_Hayes> .

<http://w3id.org/polifonia/pilot/meetups/177771/10/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "It is unclear when or by whom Hayes was called the \"First Lady of the Theatre\"." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/177771/10> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/177771/10/0> ;
        ont:subject       <http://dbpedia.org/resource/Helen_Hayes> .

<http://w3id.org/polifonia/pilot/meetups/177771/9/2>
        rdf:type            ont:Sentence ;
        ont:excerpt         "With that, she convinced Loos to embark on an exploration of all five boroughs of New York." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/177771/9> ;
        ont:place           <http://dbpedia.org/resource/New_York_City> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/177771/9/1> ;
        ont:subject         <http://dbpedia.org/resource/Helen_Hayes> ;
        ont:timeExpression  "of all five" .

