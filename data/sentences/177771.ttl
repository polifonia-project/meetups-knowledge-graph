<http://w3id.org/polifonia/pilot/meetups/177771/10/5>
        rdf:type          ont:Sentence ;
        ont:excerpt       "The Helen Hayes Award for theater in the Washington, DC, area is named in her honor." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/177771/10> ;
        ont:place         <http://dbpedia.org/resource/Washington,_D.C.> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/177771/10/4> ;
        ont:subject       <http://dbpedia.org/resource/Helen_Hayes> .

<http://w3id.org/polifonia/pilot/meetups/177771/1/3>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Helen Hayes is regarded as one of the greatest leading ladies of the 20th-century theatre." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/177771/1> ;
        ont:person          <http://dbpedia.org/resource/Helen_Hayes> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/177771/1/2> ;
        ont:subject         <http://dbpedia.org/resource/Helen_Hayes> ;
        ont:timeExpression  "one of" .

<http://w3id.org/polifonia/pilot/meetups/177771/14/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "She spent most of her last years writing and raising money for organizations that fight asthma." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/177771/14> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/177771/14/3> ;
        ont:subject       <http://dbpedia.org/resource/Helen_Hayes> .

<http://w3id.org/polifonia/pilot/meetups/177771/5/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "But Hayes did not prefer film to the stage." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/177771/5> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/177771/5/1> ;
        ont:subject       <http://dbpedia.org/resource/Helen_Hayes> .

<http://w3id.org/polifonia/pilot/meetups/177771/17>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/177771/17/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/177771/17/1> ;
        rdf:_3    <http://w3id.org/polifonia/pilot/meetups/177771/17/2> .

<http://w3id.org/polifonia/pilot/meetups/177771/0/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Hayes also received the Presidential Medal of Freedom, America's highest civilian honor, from President Ronald Reagan in 1986." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/177771/0> ;
        ont:person        <http://dbpedia.org/resource/President_of_the_United_States> , <http://dbpedia.org/resource/Ronald_Reagan> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/177771/0/2> ;
        ont:subject       <http://dbpedia.org/resource/Helen_Hayes> .

<http://w3id.org/polifonia/pilot/meetups/177771/3/8>
        rdf:type          ont:Sentence ;
        ont:excerpt       "She attended the Academy of the Sacred Heart Convent in Washington and graduated in 1917." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/177771/3> ;
        ont:person        <http://dbpedia.org/resource/Sacred_Heart> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/177771/3/7> ;
        ont:subject       <http://dbpedia.org/resource/Helen_Hayes> .

<http://w3id.org/polifonia/pilot/meetups/177771/20/4>
        rdf:type            ont:Sentence ;
        ont:excerpt         "In the 1970s, she was instrumental in lobbying for funding to transform the hospital into a state-of-the-art facility." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/177771/20> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/177771/20/3> ;
        ont:subject         <http://dbpedia.org/resource/Helen_Hayes> ;
        ont:timeExpression  "In the 1970s" .

<http://w3id.org/polifonia/pilot/meetups/177771/13/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Forever\" and later, in 1980, both appeared in The Love Boat episode \"No Girls for Doc/Marriage of Convenience/The Caller/The Witness\"." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/177771/13> ;
        ont:person        <http://dbpedia.org/resource/Girl> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/177771/13/3> ;
        ont:subject       <http://dbpedia.org/resource/Helen_Hayes> .

<http://w3id.org/polifonia/pilot/meetups/177771/9/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "I'll bet it still is and if I were free next summer, I would prove it.\"" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/177771/9> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/177771/9/0> ;
        ont:subject       <http://dbpedia.org/resource/Helen_Hayes> .

