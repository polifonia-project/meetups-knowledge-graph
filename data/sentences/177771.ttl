<http://w3id.org/polifonia/pilot/meetups/177771/20/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Hayes became involved with the hospital in the 1940s and was named to the Board of Visitors in 1944." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/177771/20> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/177771/20/0> ;
        ont:subject         <http://dbpedia.org/resource/Helen_Hayes> ;
        ont:timeExpression  "in the 1940s" .

<http://w3id.org/polifonia/pilot/meetups/177771/13/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Hayes wrote three memoirs: A Gift of Joy, On Reflection, and My Life in Three Acts." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/177771/13> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/177771/13/0> ;
        ont:subject       <http://dbpedia.org/resource/Helen_Hayes> .

<http://w3id.org/polifonia/pilot/meetups/177771/28/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1979, she received the Laetare Medal from the University of Notre Dame." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/177771/28> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/177771/28/3> ;
        ont:subject       <http://dbpedia.org/resource/Helen_Hayes> .

<http://w3id.org/polifonia/pilot/meetups/177771/7/3>
        rdf:type            ont:Sentence ;
        ont:excerpt         "She starred in My Son John (1952) and Anastasia (1956), and won the Academy Award for Best Supporting Actress for her role as an elderly stowaway in the disaster film Airport (1970)." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/177771/7> ;
        ont:person          <http://dbpedia.org/resource/Stowaway> , <http://dbpedia.org/resource/Grand_Duchess_Anastasia_Nikolaevna_of_Russia> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/177771/7/2> ;
        ont:subject         <http://dbpedia.org/resource/Helen_Hayes> ;
        ont:timeExpression  "1970" , "1956" , "1952" .

