        rdf:type          ont:Sentence ;
        ont:excerpt       "Hayes, who spoke with her good friend Anita Loos almost daily on the phone, told her, \"I used to think New York was the most enthralling place in the world." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/177771/9> ;
        ont:person        <http://dbpedia.org/resource/Anita_Loos> ;
