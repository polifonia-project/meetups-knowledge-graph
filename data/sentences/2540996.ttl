<http://w3id.org/polifonia/pilot/meetups/2540996/2/5>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Apart from vocals and sarangi, he also learned the veena (been), sitar and tabla." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2540996/2> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2540996/2/4> ;
        ont:subject       <http://dbpedia.org/resource/Abdul_Karim_Khan> .

<http://w3id.org/polifonia/pilot/meetups/2540996/6/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He became a frequent visitor to Mysore Palace which conferred on him the title Sangeet Ratna." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2540996/6> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2540996/6/3> ;
        ont:subject       <http://dbpedia.org/resource/Abdul_Karim_Khan> .

<http://w3id.org/polifonia/pilot/meetups/2540996/0>
        rdf:type  ont:Paragraph ;
