        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2540996/12> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2540996/12/0> ;
        ont:subject       <http://dbpedia.org/resource/Abdul_Karim_Khan> .

<http://w3id.org/polifonia/pilot/meetups/2540996/3/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "This is where he met Tarabai Mane, who was the daughter of Sardar Maruti Rao Mane, a member of the royal family." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2540996/3> ;
        ont:person        <http://dbpedia.org/resource/Tarabai> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2540996/3/0> ;
        ont:subject       <http://dbpedia.org/resource/Abdul_Karim_Khan> .

<http://w3id.org/polifonia/pilot/meetups/2540996/6/5>
        rdf:type          ont:Sentence ;
        ont:excerpt       "On the way to Mysore he used to stay with his brother in Dharwad where he taught his most famous disciple Sawai Gandharva." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2540996/6> ;
        ont:person        <http://dbpedia.org/resource/Sawai_Gandharva> ;
        ont:place         <http://dbpedia.org/resource/Dharwad> , <http://dbpedia.org/resource/Mysore> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2540996/6/4> ;
        ont:subject       <http://dbpedia.org/resource/Abdul_Karim_Khan> .

