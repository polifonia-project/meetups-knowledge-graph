        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2540996/20> ;
        ont:person        <http://dbpedia.org/resource/Abdul_Karim_Khan> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2540996/20/0> ;
        ont:subject       <http://dbpedia.org/resource/Abdul_Karim_Khan> .

<http://w3id.org/polifonia/pilot/meetups/2540996/13/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He was on his way to Pondicherry when he experienced severe pain in the chest at Chingleput." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2540996/13> ;
        ont:place         <http://dbpedia.org/resource/Chengalpattu> , <http://dbpedia.org/resource/Pondicherry> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2540996/13/0> ;
        ont:subject       <http://dbpedia.org/resource/Abdul_Karim_Khan> .

