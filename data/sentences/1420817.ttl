<http://w3id.org/polifonia/pilot/meetups/1420817/6/32>
        rdf:type          ont:Sentence ;
        ont:excerpt       "This work made little to no money, but Carter believed it was necessary in order to develop as an artist, and was a way to \"pay her dues\"." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1420817/6> ;
        ont:person        <http://dbpedia.org/resource/Jimmy_Carter> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1420817/6/31> ;
        ont:subject       <http://dbpedia.org/resource/Betty_Carter> .

<http://w3id.org/polifonia/pilot/meetups/1420817/6/11>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Carter's confidence was well founded." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1420817/6> ;
        ont:person        <http://dbpedia.org/resource/Jimmy_Carter> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1420817/6/10> ;
