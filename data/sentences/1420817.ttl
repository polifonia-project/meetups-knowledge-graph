<http://w3id.org/polifonia/pilot/meetups/1420817/6/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Detroit, where Carter grew up, was a hotbed of jazz growth." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1420817/6> ;
        ont:person        <http://dbpedia.org/resource/Jimmy_Carter> ;
        ont:place         <http://dbpedia.org/resource/Detroit> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1420817/6/3> ;
        ont:subject       <http://dbpedia.org/resource/Betty_Carter> .

<http://w3id.org/polifonia/pilot/meetups/1420817/6/46>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Rock and roll, like pop, was steadily becoming more popular and provided cash flow for labels and recording companies." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1420817/6> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1420817/6/45> ;
        ont:subject       <http://dbpedia.org/resource/Betty_Carter> .

<http://w3id.org/polifonia/pilot/meetups/1420817/6/25>
        rdf:type            ont:Sentence ;
        ont:excerpt         "They also performed at the sixth famed Cavalcade of Jazz concert on June 25, 1950." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1420817/6> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1420817/6/24> ;
