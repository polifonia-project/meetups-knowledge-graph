<http://w3id.org/polifonia/pilot/meetups/1420817/9>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/1420817/9/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/1420817/9/1> .

<http://w3id.org/polifonia/pilot/meetups/1420817/2/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Even 30 years after leaving home, Carter was still very aware of and affected by the home life she was raised in, and was quoted saying:" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1420817/2> ;
        ont:person        <http://dbpedia.org/resource/Jimmy_Carter> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1420817/2/1> ;
        ont:subject       <http://dbpedia.org/resource/Betty_Carter> .

<http://w3id.org/polifonia/pilot/meetups/1420817/7/7>
        rdf:type          ont:Sentence ;
        ont:excerpt       "So, that's what I did." ;
