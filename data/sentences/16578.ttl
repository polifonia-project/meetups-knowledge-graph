        ont:subject       <http://dbpedia.org/resource/John_Hay> .

<http://w3id.org/polifonia/pilot/meetups/16578/50/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Senator Hanna bitterly opposed that choice, but nevertheless raised millions for the McKinley/Roosevelt ticket, which was elected." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/16578/50> ;
