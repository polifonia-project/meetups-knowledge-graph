        ont:subject         <http://dbpedia.org/resource/John_Hay> ;
        ont:timeExpression  "1,600 per" , "one presidential secretary" .

<http://w3id.org/polifonia/pilot/meetups/16578/68/9>
        rdf:type          ont:Sentence ;
