        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/16578/39/13> ;
        ont:subject         <http://dbpedia.org/resource/John_Hay> ;
        ont:timeExpression  "October 1897" .

<http://w3id.org/polifonia/pilot/meetups/16578/50/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Under the laws then in force, this made Hay next in line to the presidency should anything happen to McKinley." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/16578/50> ;
        ont:person        <http://dbpedia.org/resource/William_McKinley> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/16578/50/0> ;
        ont:subject       <http://dbpedia.org/resource/John_Hay> .

