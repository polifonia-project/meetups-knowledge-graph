        rdf:type          ont:Sentence ;
        ont:excerpt       "By 1873, Hay was wooing Clara Stone, daughter of Cleveland multimillionaire railroad and banking mogul Amasa Stone." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/16578/23> ;
        ont:person        <http://dbpedia.org/resource/Amasa_Stone> , <http://dbpedia.org/resource/Clara_Stone> ;
