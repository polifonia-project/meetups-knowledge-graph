<http://w3id.org/polifonia/pilot/meetups/354680/11/8>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Danko is featured in the documentary film Festival Express, about an all-star tour by train across Canada in 1970." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/354680/11> ;
        ont:person        <http://dbpedia.org/resource/Rick_Danko> ;
        ont:place         <http://dbpedia.org/resource/Canada> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/354680/11/7> ;
        ont:subject       <http://dbpedia.org/resource/Rick_Danko> .

