        rdf:_4    <http://w3id.org/polifonia/pilot/meetups/354680/20/3> .

<http://w3id.org/polifonia/pilot/meetups/354680/6/7>
        rdf:type          ont:Sentence ;
        ont:excerpt       "The group had been planning to leave Hawkins and strike out together as a band without a frontman, as a team of equal members." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/354680/6> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/354680/6/6> ;
        ont:subject       <http://dbpedia.org/resource/Rick_Danko> .

<http://w3id.org/polifonia/pilot/meetups/354680/14/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In early 1979 Danko opened shows for Boz Scaggs." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/354680/14> ;
        ont:person        <http://dbpedia.org/resource/Rick_Danko> , <http://dbpedia.org/resource/Boz_Scaggs> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/354680/14/0> ;
        ont:subject       <http://dbpedia.org/resource/Rick_Danko> .

<http://w3id.org/polifonia/pilot/meetups/354680/20/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "The Drive-By Truckers' song \"Danko/Manuel,\" written by Jason Isbell, was released on their album The Dirty South in 2004." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/354680/20> ;
        ont:person        <http://dbpedia.org/resource/Rick_Danko> , <http://dbpedia.org/resource/Jason_Isbell> , <http://dbpedia.org/resource/Manuel_I_of_Portugal> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/354680/20/0> ;
        ont:subject       <http://dbpedia.org/resource/Rick_Danko> .

<http://w3id.org/polifonia/pilot/meetups/354680/14/12>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Danko struggled with heroin addiction." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/354680/14> ;
