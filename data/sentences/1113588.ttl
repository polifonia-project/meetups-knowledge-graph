        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1947, while living in Copenhagen, Albertson listened by chance to a Bessie Smith recording on radio; it led to an abiding interest in jazz and blues music." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1113588/3> ;
        ont:person        <http://dbpedia.org/resource/Bessie_Smith> ;
        ont:place         <http://dbpedia.org/resource/Copenhagen> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1113588/3/0> ;
        ont:subject       <http://dbpedia.org/resource/Chris_Albertson> .

