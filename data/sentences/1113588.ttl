        rdf:type          ont:Sentence ;
        ont:excerpt       "He also founded his own production company, supervising sessions with Howard McGhee, Roy Eldridge, Bud Freeman, Ray Bryant, and Elmer Snowden." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1113588/6> ;
        ont:person        <http://dbpedia.org/resource/Ray_Bryant> , <http://dbpedia.org/resource/Elmer_Snowden> , <http://dbpedia.org/resource/Howard_McGhee> , <http://dbpedia.org/resource/Bud_Freeman> , <http://dbpedia.org/resource/Roy_Eldridge> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1113588/6/4> ;
        ont:subject       <http://dbpedia.org/resource/Chris_Albertson> .

