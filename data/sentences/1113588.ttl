        rdf:type          ont:Sentence ;
        ont:excerpt       "His standard work, Bessie, a biography of Bessie Smith, first appeared in 1972, with a revised and expanded version published by Yale University Press in 2003." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1113588/9> ;
        ont:person        <http://dbpedia.org/resource/Bessie_Smith> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1113588/9/0> ;
