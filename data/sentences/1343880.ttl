        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/1343880/7/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/1343880/7/1> .

<http://w3id.org/polifonia/pilot/meetups/1343880/5/3>
        rdf:type            ont:Sentence ;
        ont:excerpt         "He was briefly replaced by Nigel Macara, who quit again in October 1976, to be replaced by Iain McLennan." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1343880/5> ;
        ont:person          <http://dbpedia.org/resource/Grant_McLennan> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1343880/5/2> ;
        ont:subject         <http://dbpedia.org/resource/Mike_Rudd> ;
        ont:timeExpression  "October 1976" .

<http://w3id.org/polifonia/pilot/meetups/1343880/1>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/1343880/1/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/1343880/1/1> .

<http://w3id.org/polifonia/pilot/meetups/1343880/9/2>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Ariel also reformed for occasional gigs with varying line-ups, including a final reunion of the 'Mark II' line-up with Harvey James and John Lee, which took place not long before Lee's untimely death in July 1998." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1343880/9> ;
        ont:person          <http://dbpedia.org/resource/Ana_Rocha> , <http://dbpedia.org/resource/Robert_E._Lee> , <http://dbpedia.org/resource/Harvey_James> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1343880/9/1> ;
        ont:subject         <http://dbpedia.org/resource/Mike_Rudd> ;
        ont:timeExpression  "July 1998" .

<http://w3id.org/polifonia/pilot/meetups/1343880/4/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "The resulting album, Rock'n'Roll Scars, was mixed by the EMI recording engineer Geoff Emerick, who had worked with The Beatles." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1343880/4> ;
        ont:person        <http://dbpedia.org/resource/Geoff_Emerick> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1343880/4/2> ;
        ont:subject       <http://dbpedia.org/resource/Mike_Rudd> .

<http://w3id.org/polifonia/pilot/meetups/1343880/12/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "===" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1343880/12> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1343880/12/0> ;
