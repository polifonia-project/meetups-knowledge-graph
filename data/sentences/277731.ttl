<http://w3id.org/polifonia/pilot/meetups/277731/1/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 2016, he was awarded the National Medal of Arts by President Barack Obama." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/277731/1> ;
        ont:person        <http://dbpedia.org/resource/Barack_Obama> , <http://dbpedia.org/resource/President_of_the_United_States> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/277731/1/0> ;
        ont:subject       <http://dbpedia.org/resource/Berry_Gordy> .

<http://w3id.org/polifonia/pilot/meetups/277731/25/1>
        rdf:type            ont:Sentence ;
        ont:excerpt         "With Jeana Jackson, Gordy had one daughter:" ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/277731/25> ;
        ont:person          <http://dbpedia.org/resource/Michael_Jackson> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/277731/25/0> ;
        ont:subject         <http://dbpedia.org/resource/Berry_Gordy> ;
        ont:timeExpression  "one daughter" .

<http://w3id.org/polifonia/pilot/meetups/277731/10/3>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Gordy wrote or co-wrote 240 of the approximately 15,000 songs in Motown's Jobete music catalogue." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/277731/10> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/277731/10/2> ;
        ont:subject         <http://dbpedia.org/resource/Berry_Gordy> ;
        ont:timeExpression  "240 of" .

<http://w3id.org/polifonia/pilot/meetups/277731/36>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/277731/36/0> .

<http://w3id.org/polifonia/pilot/meetups/277731/17/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "On May 15, 2011, it was announced that Gordy was developing a Broadway musical about Motown." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/277731/17> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/277731/17/0> ;
        ont:subject         <http://dbpedia.org/resource/Berry_Gordy> ;
        ont:timeExpression  "May 15 , 2011" .

