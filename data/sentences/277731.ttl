<http://w3id.org/polifonia/pilot/meetups/277731/15/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Following the funeral of Marvin Gaye on April 5, 1984, Gordy declared Gaye \"the greatest of his time.\"" ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/277731/15> ;
        ont:person          <http://dbpedia.org/resource/Marvin_Gaye> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/277731/15/0> ;
        ont:subject         <http://dbpedia.org/resource/Berry_Gordy> ;
        ont:timeExpression  "April 5 , 1984" .

<http://w3id.org/polifonia/pilot/meetups/277731/0/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He is best known as the founder of the Motown record label and its subsidiaries, which was the highest-earning African-American business for decades." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/277731/0> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/277731/0/0> ;
        ont:subject       <http://dbpedia.org/resource/Berry_Gordy> .

<http://w3id.org/polifonia/pilot/meetups/277731/24/1>
        rdf:type            ont:Sentence ;
        ont:excerpt         "In the spring of 1960 he married Raynoma Mayberry Liles (they were divorced in 1964)." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/277731/24> ;
        ont:person          <http://dbpedia.org/resource/Ray_Singleton> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/277731/24/0> ;
        ont:subject         <http://dbpedia.org/resource/Berry_Gordy> ;
        ont:timeExpression  "spring of 1960" .

<http://w3id.org/polifonia/pilot/meetups/277731/5/6>
        rdf:type            ont:Sentence ;
        ont:excerpt         "\"Come to Me\" by Marv Johnson was issued as Tamla 101." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/277731/5> ;
        ont:person          <http://dbpedia.org/resource/Marv_Johnson> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/277731/5/5> ;
        ont:subject         <http://dbpedia.org/resource/Berry_Gordy> ;
