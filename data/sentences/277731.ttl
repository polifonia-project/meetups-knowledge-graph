<http://w3id.org/polifonia/pilot/meetups/277731/0/3>
        rdf:type            ont:Sentence ;
        ont:excerpt         "As part of The Corporation he wrote many  hit songs for The Jackson 5, including \"I Want You Back\" and \"ABC\"." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/277731/0> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/277731/0/2> ;
