<http://w3id.org/polifonia/pilot/meetups/277731/3/14>
        rdf:type          ont:Sentence ;
        ont:excerpt       "The store was unsuccessful, and Gordy sought work at the Lincoln-Mercury plant, but his family connections put him in touch with Al Green (no relation to the singer Reverend Al Green), owner of the Flame Show Bar Talent Club, where he met the singer Jackie Wilson." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/277731/3> ;
        ont:person        <http://dbpedia.org/resource/Al_Green> , <http://dbpedia.org/resource/Jackie_Wilson> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/277731/3/13> ;
        ont:subject       <http://dbpedia.org/resource/Berry_Gordy> .

<http://w3id.org/polifonia/pilot/meetups/277731/36/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Gordy was portrayed by Obba Babatunde in the 1998 miniseries The Temptations." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/277731/36> ;
        ont:person          <http://dbpedia.org/resource/Obba_Babatund> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/277731/36/0> ;
        ont:subject         <http://dbpedia.org/resource/Berry_Gordy> ;
        ont:timeExpression  "in the 1998" .

