<http://w3id.org/polifonia/pilot/meetups/277731/5/11>
        rdf:type            ont:Sentence ;
        ont:excerpt         "\"Bad Girl\" was a solid hit in 1959 after Chess Records picked it up." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/277731/5> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/277731/5/10> ;
