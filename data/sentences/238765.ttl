        ont:subject       <http://dbpedia.org/resource/Anton_LaVey> .

<http://w3id.org/polifonia/pilot/meetups/238765/3/11>
        rdf:type          ont:Sentence ;
        ont:excerpt       "His \"genius\" on keyboards helped him attain gigs." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/238765/3> ;
