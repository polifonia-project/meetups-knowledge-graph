<http://w3id.org/polifonia/pilot/meetups/1429918/2/16>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Allison played the bar circuit in the United States during this period and spent eight months of the year in Europe at high-profile venues, including the Montreux Jazz Festival." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1429918/2> ;
        ont:person        <http://dbpedia.org/resource/Davey_Allison> ;
        ont:place         <http://dbpedia.org/resource/Europe> , <http://dbpedia.org/resource/United_States> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1429918/2/15> ;
        ont:subject       <http://dbpedia.org/resource/Luther_Allison> .
