<http://w3id.org/polifonia/pilot/meetups/1429918/0/6>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Allison played with the bands of Howlin' Wolf and Freddie King, taking over King's band when King toured nationally." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1429918/0> ;
        ont:person        <http://dbpedia.org/resource/Howlin'_Wolf> , <http://dbpedia.org/resource/Davey_Allison> , <http://dbpedia.org/resource/Freddie_King> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1429918/0/5> ;
        ont:subject       <http://dbpedia.org/resource/Luther_Allison> .

<http://w3id.org/polifonia/pilot/meetups/1429918/2/13>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He moved to France in 1977." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1429918/2> ;
        ont:place         <http://dbpedia.org/resource/France> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1429918/2/12> ;
        ont:subject       <http://dbpedia.org/resource/Luther_Allison> .

<http://w3id.org/polifonia/pilot/meetups/1429918/1/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "==" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1429918/1> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1429918/1/0> ;
        ont:subject       <http://dbpedia.org/resource/Luther_Allison> .

<http://w3id.org/polifonia/pilot/meetups/1429918/0/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Luther Allison (August 17, 1939 – August 12, 1997) was an American blues guitarist." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1429918/0> ;
        ont:person          <http://dbpedia.org/resource/Luther_Allison> ;
        ont:place           <http://dbpedia.org/resource/United_States> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1429918/0/0> ;
        ont:subject         <http://dbpedia.org/resource/Luther_Allison> ;
        ont:timeExpression  "August 17 , 1939" , "August 12 , 1997" .

<http://w3id.org/polifonia/pilot/meetups/1429918/3/5>
        rdf:type          ont:Sentence ;
        ont:excerpt       "With the James Solberg Band backing him, nonstop touring and the release of Blue Streak (featuring the song \"Cherry Red Wine\"), Allison earned more Handy Awards and gained wider recognition." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1429918/3> ;
        ont:person        <http://dbpedia.org/resource/Erna_Solberg> , <http://dbpedia.org/resource/LeBron_James> , <http://dbpedia.org/resource/Davey_Allison> , <http://dbpedia.org/resource/W._C._Handy> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1429918/3/4> ;
        ont:subject       <http://dbpedia.org/resource/Luther_Allison> .

<http://w3id.org/polifonia/pilot/meetups/1429918/2/5>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He worked the club circuit in the late 1950s and early 1960s." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1429918/2> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1429918/2/4> ;
        ont:subject       <http://dbpedia.org/resource/Luther_Allison> .

<http://w3id.org/polifonia/pilot/meetups/1429918/7>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/1429918/7/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/1429918/7/1> .

<http://w3id.org/polifonia/pilot/meetups/1429918/1>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/1429918/1/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/1429918/1/1> .

<http://w3id.org/polifonia/pilot/meetups/1429918/5/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 2000, the Chicago Sun-Times called him \"the Bruce Springsteen of the blues\"." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1429918/5> ;
        ont:person        <http://dbpedia.org/resource/Bruce_Springsteen> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1429918/5/3> ;
        ont:subject       <http://dbpedia.org/resource/Luther_Allison> .

<http://w3id.org/polifonia/pilot/meetups/1429918/0/5>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Three years later he dropped out of school and began hanging around outside blues nightclubs with the hopes of being invited to perform." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1429918/0> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1429918/0/4> ;
        ont:subject       <http://dbpedia.org/resource/Luther_Allison> .

<http://w3id.org/polifonia/pilot/meetups/1429918/2/12>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In the mid-1970s he toured Europe." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1429918/2> ;
        ont:place         <http://dbpedia.org/resource/Europe> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1429918/2/11> ;
        ont:subject       <http://dbpedia.org/resource/Luther_Allison> .

<http://w3id.org/polifonia/pilot/meetups/1429918/3/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He won four W. C. Handy Awards in 1994." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1429918/3> ;
        ont:person        <http://dbpedia.org/resource/W._C._Handy> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1429918/3/3> ;
        ont:subject       <http://dbpedia.org/resource/Luther_Allison> .

<http://w3id.org/polifonia/pilot/meetups/1429918/2/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Freddie King took Allison under his wing, and after King got a record deal, Allison took over his gig in the house band of a club on Chicago's West Side." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1429918/2> ;
        ont:person        <http://dbpedia.org/resource/Freddie_King> ;
        ont:place         <http://dbpedia.org/resource/Chicago> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1429918/2/3> ;
        ont:subject       <http://dbpedia.org/resource/Luther_Allison> .

<http://w3id.org/polifonia/pilot/meetups/1429918/6>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/1429918/6/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/1429918/6/1> .

<http://w3id.org/polifonia/pilot/meetups/1429918/2/17>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1992, he performed with the French rock and roll star Johnny Hallyday in 18 shows in Paris, also playing during the intermission." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1429918/2> ;
        ont:person        <http://dbpedia.org/resource/Johnny_Hallyday> ;
        ont:place         <http://dbpedia.org/resource/France> , <http://dbpedia.org/resource/Paris> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1429918/2/16> ;
        ont:subject       <http://dbpedia.org/resource/Luther_Allison> .

<http://w3id.org/polifonia/pilot/meetups/1429918/0>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/1429918/0/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/1429918/0/1> ;
        rdf:_3    <http://w3id.org/polifonia/pilot/meetups/1429918/0/2> ;
        rdf:_4    <http://w3id.org/polifonia/pilot/meetups/1429918/0/3> ;
        rdf:_5    <http://w3id.org/polifonia/pilot/meetups/1429918/0/4> ;
        rdf:_6    <http://w3id.org/polifonia/pilot/meetups/1429918/0/5> ;
        rdf:_7    <http://w3id.org/polifonia/pilot/meetups/1429918/0/6> ;
        rdf:_8    <http://w3id.org/polifonia/pilot/meetups/1429918/0/7> .

<http://w3id.org/polifonia/pilot/meetups/1429918/5/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Allison was posthumously inducted into the Blues Hall of Fame in 1998." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1429918/5> ;
        ont:person        <http://dbpedia.org/resource/Davey_Allison> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1429918/5/2> ;
        ont:subject       <http://dbpedia.org/resource/Luther_Allison> .

<http://w3id.org/polifonia/pilot/meetups/1429918/0/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He taught himself guitar and began listening to blues extensively." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1429918/0> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1429918/0/3> ;
        ont:subject       <http://dbpedia.org/resource/Luther_Allison> .

<http://w3id.org/polifonia/pilot/meetups/1429918/2/11>
        rdf:type            ont:Sentence ;
        ont:excerpt         "In 1972, he signed with Motown Records, one of the few blues artists on that label." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1429918/2> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1429918/2/10> ;
        ont:subject         <http://dbpedia.org/resource/Luther_Allison> ;
        ont:timeExpression  "one of" .

<http://w3id.org/polifonia/pilot/meetups/1429918/9/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "==" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1429918/9> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1429918/9/1> ;
        ont:subject       <http://dbpedia.org/resource/Luther_Allison> .

<http://w3id.org/polifonia/pilot/meetups/1429918/4/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "His album Reckless had just been released." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1429918/4> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1429918/4/2> ;
        ont:subject       <http://dbpedia.org/resource/Luther_Allison> .

<http://w3id.org/polifonia/pilot/meetups/1429918/2/9>
        rdf:type            ont:Sentence ;
        ont:excerpt         "He performed a well-received set at the 1969 Ann Arbor Blues Festival and as a result was asked to perform there in each of the next three years." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1429918/2> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1429918/2/8> ;
        ont:subject         <http://dbpedia.org/resource/Luther_Allison> ;
        ont:timeExpression  "at the 1969" .

<http://w3id.org/polifonia/pilot/meetups/1429918/3/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "The album Soul Fixin' Man was recorded and released in 1994, and Allison toured the United States and Canada." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1429918/3> ;
        ont:place         <http://dbpedia.org/resource/United_States> , <http://dbpedia.org/resource/Canada> , <http://dbpedia.org/resource/Fixin> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1429918/3/2> ;
        ont:subject       <http://dbpedia.org/resource/Luther_Allison> .

<http://w3id.org/polifonia/pilot/meetups/1429918/2/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "The same year he worked briefly with Jimmy Dawkins, playing in local clubs." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1429918/2> ;
        ont:person        <http://dbpedia.org/resource/Jimmy_Dawkins> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1429918/2/2> ;
        ont:subject       <http://dbpedia.org/resource/Luther_Allison> .

