        rdf:type          ont:Sentence ;
        ont:excerpt       "Allison is buried at Washington Memory Gardens Cemetery in Homewood, Illinois." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1429918/5> ;
