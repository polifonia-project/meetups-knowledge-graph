<http://w3id.org/polifonia/pilot/meetups/1429918/5/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "His son Bernard Allison, at one time a member of his band, is now a solo recording artist." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1429918/5> ;
        ont:person          <http://dbpedia.org/resource/Bernard_Allison> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1429918/5/0> ;
        ont:subject         <http://dbpedia.org/resource/Luther_Allison> ;
        ont:timeExpression  "one time" .

<http://w3id.org/polifonia/pilot/meetups/1429918/0/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He was born in Widener, Arkansas, although some accounts suggest his actual place of birth was Mayflower, Arkansas." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1429918/0> ;
        ont:place         <http://dbpedia.org/resource/Harrison,_Arkansas> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1429918/0/0> ;
        ont:subject       <http://dbpedia.org/resource/Luther_Allison> .

<http://w3id.org/polifonia/pilot/meetups/1429918/3/6>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He won several Living Blues Awards and was featured on the covers of blues publications." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1429918/3> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1429918/3/5> ;
        ont:subject       <http://dbpedia.org/resource/Luther_Allison> .

<http://w3id.org/polifonia/pilot/meetups/1429918/4/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "During his tour in the summer of 1997, Allison checked into a hospital for dizziness and loss of coordination." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1429918/4> ;
        ont:person          <http://dbpedia.org/resource/Davey_Allison> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1429918/4/0> ;
        ont:subject         <http://dbpedia.org/resource/Luther_Allison> ;
        ont:timeExpression  "summer of 1997" .

<http://w3id.org/polifonia/pilot/meetups/1429918/2/6>
        rdf:type          ont:Sentence ;
        ont:excerpt       "During this period, Allison moved to California for a year where he worked with Shakey Jake Harris and Sunnyland Slim." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1429918/2> ;
        ont:person        <http://dbpedia.org/resource/Shakey_Jake_Harris> , <http://dbpedia.org/resource/Davey_Allison> , <http://dbpedia.org/resource/Sunnyland_Slim> ;
        ont:place         <http://dbpedia.org/resource/California> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1429918/2/5> ;
        ont:subject       <http://dbpedia.org/resource/Luther_Allison> .

