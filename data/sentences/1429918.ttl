        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1429918/0/6> ;
        ont:subject       <http://dbpedia.org/resource/Luther_Allison> .

<http://w3id.org/polifonia/pilot/meetups/1429918/2/14>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Allison was known for his powerful concert performances, lengthy soulful guitar solos and crowd walking with his Gibson Les Paul." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1429918/2> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1429918/2/13> ;
