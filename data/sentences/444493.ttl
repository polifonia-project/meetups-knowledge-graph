        ont:timeExpression  "of the 1980s" , "1980" .

<http://w3id.org/polifonia/pilot/meetups/444493/20/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Phillips was a passenger in the car with boyfriend Geoffrey Tozer, and the marijuana was discovered after police searched the couple's vehicle." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/444493/20> ;
        ont:person        <http://dbpedia.org/resource/Geoffrey_Tozer> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/444493/20/2> ;
        ont:subject       <http://dbpedia.org/resource/Michelle_Phillips> .

<http://w3id.org/polifonia/pilot/meetups/444493/7/16>
        rdf:type          ont:Sentence ;
        ont:excerpt       "There, she became a childhood friend of Sue Lyon." ;
