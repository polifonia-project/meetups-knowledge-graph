<http://w3id.org/polifonia/pilot/meetups/444493/20/1>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Phillips continued to appear in the role until the series' 1993 conclusion." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/444493/20> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/444493/20/0> ;
        ont:subject         <http://dbpedia.org/resource/Michelle_Phillips> ;
        ont:timeExpression  "1993 conclusion" .

<http://w3id.org/polifonia/pilot/meetups/444493/7/14>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Phillips recalled that her and her sister's experiences living in a different culture \"helped us get over my mother's death, and instead of grieving, we became very strong, independent, and free." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/444493/7> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/444493/7/13> ;
        ont:subject       <http://dbpedia.org/resource/Michelle_Phillips> .

<http://w3id.org/polifonia/pilot/meetups/444493/12/0>
