        rdf:type          ont:Sentence ;
        ont:excerpt       "References ==" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/444493/33> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/444493/33/0> ;
        ont:subject       <http://dbpedia.org/resource/Michelle_Phillips> .

<http://w3id.org/polifonia/pilot/meetups/444493/25/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Phillips has been noted for her soprano vocals, and was once deemed by Time as \"the purest soprano\" in pop music." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/444493/25> ;
        ont:person        <http://dbpedia.org/resource/Soprano> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/444493/25/0> ;
        ont:subject       <http://dbpedia.org/resource/Michelle_Phillips> .

<http://w3id.org/polifonia/pilot/meetups/444493/10/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "This time, John Phillips was determined to fire his wife." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/444493/10> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/444493/10/1> ;
        ont:subject       <http://dbpedia.org/resource/Michelle_Phillips> .

<http://w3id.org/polifonia/pilot/meetups/444493/20/16>
        rdf:type            ont:Sentence ;
        ont:excerpt         "In late September 1993, Phillips and her friend Aloma Ichinose were robbed at gunpoint outside a restaurant in West Hollywood, California." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/444493/20> ;
        ont:place           <http://dbpedia.org/resource/West_Hollywood,_California> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/444493/20/15> ;
        ont:subject         <http://dbpedia.org/resource/Michelle_Phillips> ;
        ont:timeExpression  "September 1993" .

<http://w3id.org/polifonia/pilot/meetups/444493/23/8>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 2019, Phillips appeared as an interview subject in Andrew Slater's Echo in the Canyon, a documentary on the Laurel Canyon music scene of the 1960s." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/444493/23> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/444493/23/7> ;
        ont:subject       <http://dbpedia.org/resource/Michelle_Phillips> .

<http://w3id.org/polifonia/pilot/meetups/444493/2/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "After the breakup of the Mamas and the Papas and her divorce from John Phillips, she transitioned into acting, appearing in a supporting part in The Last Movie (1971) before being cast as Billie Frechette in the critically acclaimed crime biopic Dillinger (1973), for which she was nominated for a Golden Globe Award for Most Promising Newcomer." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/444493/2> ;
        ont:person          <http://dbpedia.org/resource/John_Dillinger> , <http://dbpedia.org/resource/Divorce> , <http://dbpedia.org/resource/Evelyn_Frechette> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/444493/2/0> ;
        ont:subject         <http://dbpedia.org/resource/Michelle_Phillips> ;
        ont:timeExpression  "1973" , "1971" .

