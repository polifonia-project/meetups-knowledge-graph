<http://w3id.org/polifonia/pilot/meetups/444493/1/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "A native of Long Beach, California, she spent her early life in Los Angeles and Mexico City, raised by her widowed father." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/444493/1> ;
        ont:place         <http://dbpedia.org/resource/Mexico_City> , <http://dbpedia.org/resource/Los_Angeles> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/444493/1/0> ;
        ont:subject       <http://dbpedia.org/resource/Michelle_Phillips> .

