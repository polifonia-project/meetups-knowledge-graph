        ont:subject         <http://dbpedia.org/resource/Michelle_Phillips> ;
        ont:timeExpression  "1966" .

<http://w3id.org/polifonia/pilot/meetups/444493/23/6>
        rdf:type          ont:Sentence ;
        ont:excerpt       "She also appeared in a minor role in the Norwegian historical film Betrayal, which chronicles the German occupation of Norway." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/444493/23> ;
        ont:place         <http://dbpedia.org/resource/Norway> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/444493/23/5> ;
        ont:subject       <http://dbpedia.org/resource/Michelle_Phillips> .

