        rdf:_5    <http://w3id.org/polifonia/pilot/meetups/444493/0/4> .

<http://w3id.org/polifonia/pilot/meetups/444493/24/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Artistry ==" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/444493/24> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/444493/24/0> ;
        ont:subject       <http://dbpedia.org/resource/Michelle_Phillips> .

<http://w3id.org/polifonia/pilot/meetups/444493/13/11>
        rdf:type          ont:Sentence ;
        ont:excerpt       "It was just a wonderful, wonderful experience for me and I had so much support and so much help and so much encouragement." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/444493/13> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/444493/13/10> ;
        ont:subject       <http://dbpedia.org/resource/Michelle_Phillips> .

<http://w3id.org/polifonia/pilot/meetups/444493/29>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/444493/29/0> .

<http://w3id.org/polifonia/pilot/meetups/444493/16/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1977, Phillips released her first and only solo album, Victim of Romance, produced by Jack Nitzsche for A&M Records." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/444493/16> ;
        ont:person        <http://dbpedia.org/resource/Jack_Nitzsche> ;
