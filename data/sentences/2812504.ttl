        ont:timeExpression  "May 1960" .

<http://w3id.org/polifonia/pilot/meetups/2812504/2/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "After graduating from Mount St. Charles Academy in Rhode Island, he won a scholarship to Duquesne University." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2812504/2> ;
        ont:person        <http://dbpedia.org/resource/Charles_II_of_England> ;
        ont:place         <http://dbpedia.org/resource/Rhode_Island> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2812504/2/2> ;
        ont:subject       <http://dbpedia.org/resource/Carl_Betz> .

<http://w3id.org/polifonia/pilot/meetups/2812504/5/8>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1967, producer Paul Monash offered Betz the role of defense lawyer Clinton Judd in the legal drama Judd, for the Defense." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2812504/5> ;
        ont:person        <http://dbpedia.org/resource/Bill_Clinton> , <http://dbpedia.org/resource/Paul_Monash> , <http://dbpedia.org/resource/Hans_Dieter_Betz> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2812504/5/7> ;
        ont:subject       <http://dbpedia.org/resource/Carl_Betz> .

<http://w3id.org/polifonia/pilot/meetups/2812504/5/14>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He also made many guest appearances on a variety of popular television shows such as Mission: Impossible, The Mod Squad, Love, American Style, and Starsky & Hutch." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2812504/5> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2812504/5/13> ;
        ont:subject       <http://dbpedia.org/resource/Carl_Betz> .

