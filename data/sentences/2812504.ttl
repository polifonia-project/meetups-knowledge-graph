<http://w3id.org/polifonia/pilot/meetups/2812504/7/2>
        rdf:type            ont:Sentence ;
        ont:excerpt         "The couple separated in May 1960 and divorced in 1961." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/2812504/7> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/2812504/7/1> ;
