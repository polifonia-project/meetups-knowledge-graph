        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/577798/5/4> ;
        ont:subject       <http://dbpedia.org/resource/Flip_Wilson> .

<http://w3id.org/polifonia/pilot/meetups/577798/0/6>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Wilson kept performing and acting until the 1990s, though at a reduced schedule." ;
