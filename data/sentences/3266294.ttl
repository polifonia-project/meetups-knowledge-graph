<http://w3id.org/polifonia/pilot/meetups/3266294/4/4>
        rdf:type            ont:Sentence ;
        ont:excerpt         "On August 13, 1977, he appeared with St. Elmo’s Fire on The Lil Ol’ Show That Comes on After Monty Python." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/3266294/4> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/3266294/4/3> ;
        ont:subject         <http://dbpedia.org/resource/Richard_Dobson> ;
        ont:timeExpression  "August 13 , 1977" .

<http://w3id.org/polifonia/pilot/meetups/3266294/0>
