<http://w3id.org/polifonia/pilot/meetups/152557/9/7>
        rdf:type            ont:Sentence ;
        ont:excerpt         "During the hard economic times of 1934 and 1935, Tatum mostly played in clubs in Cleveland, but also recorded in New York four times in 1934 and once in the following year." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/152557/9> ;
        ont:person          <http://dbpedia.org/resource/Jayson_Tatum> ;
        ont:place           <http://dbpedia.org/resource/New_York_City> , <http://dbpedia.org/resource/Cleveland> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/152557/9/6> ;
        ont:subject         <http://dbpedia.org/resource/Art_Tatum> ;
        ont:timeExpression  "of 1934 and 1935" , "York four" .

<http://w3id.org/polifonia/pilot/meetups/152557/24/10>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He also increased the frequency of harmonic substitutions and the variety of musical devices played by his left hand, and developed a greater harmonic and contrapuntal balance across the piano's upper and lower registers." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/152557/24> ;
        ont:person        <http://dbpedia.org/resource/Counterpoint> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/152557/24/9> ;
        ont:subject       <http://dbpedia.org/resource/Art_Tatum> .

<http://w3id.org/polifonia/pilot/meetups/152557/28/14>
        rdf:type          ont:Sentence ;
        ont:excerpt       "[...] I just take his phrasing and sing it that way.\"" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/152557/28> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/152557/28/13> ;
        ont:subject       <http://dbpedia.org/resource/Art_Tatum> .

