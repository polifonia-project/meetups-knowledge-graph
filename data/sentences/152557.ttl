        rdf:_3    <http://w3id.org/polifonia/pilot/meetups/152557/36/2> .

<http://w3id.org/polifonia/pilot/meetups/152557/0/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Many pianists attempted to copy him; others questioned their own skills after encountering him, and some even switched instruments in response." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/152557/0> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/152557/0/1> ;
        ont:subject       <http://dbpedia.org/resource/Art_Tatum> .

<http://w3id.org/polifonia/pilot/meetups/152557/24/15>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Pianist Keith Jarrett objected to Tatum playing too many notes, and a criticism of him in a band setting was that he often did not modify his playing, overwhelming the other musicians and appearing to compete with any soloist that he was ostensibly supporting." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/152557/24> ;
        ont:person        <http://dbpedia.org/resource/Art_Tatum> , <http://dbpedia.org/resource/Keith_Jarrett> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/152557/24/14> ;
        ont:subject       <http://dbpedia.org/resource/Art_Tatum> .

<http://w3id.org/polifonia/pilot/meetups/152557/28/19>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Many pianists tried to copy him and attain the same level of ability, hindering their progress towards finding their own style." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/152557/28> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/152557/28/18> ;
        ont:subject       <http://dbpedia.org/resource/Art_Tatum> .

<http://w3id.org/polifonia/pilot/meetups/152557/36/0>
        rdf:type          ont:Sentence ;
