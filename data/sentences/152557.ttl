<http://w3id.org/polifonia/pilot/meetups/152557/28/20>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Others, including trumpeter Rex Stewart and pianists Oscar Peterson and Bobby Short, were overwhelmed and began to question their own abilities." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/152557/28> ;
        ont:person        <http://dbpedia.org/resource/Bobby_Short> , <http://dbpedia.org/resource/Oscar_Peterson> , <http://dbpedia.org/resource/Rex_Stewart> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/152557/28/19> ;
        ont:subject       <http://dbpedia.org/resource/Art_Tatum> .

<http://w3id.org/polifonia/pilot/meetups/152557/20/7>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He did not add to the classical pieces he had used earlier." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/152557/20> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/152557/20/6> ;
        ont:subject       <http://dbpedia.org/resource/Art_Tatum> .

<http://w3id.org/polifonia/pilot/meetups/152557/4/6>
        rdf:type          ont:Sentence ;
        ont:excerpt       "The Tatum family was regarded as conventional and church-going." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/152557/4> ;
        ont:person        <http://dbpedia.org/resource/Jayson_Tatum> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/152557/4/5> ;
        ont:subject       <http://dbpedia.org/resource/Art_Tatum> .

