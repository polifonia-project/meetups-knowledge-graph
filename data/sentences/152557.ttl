        rdf:type          ont:Sentence ;
        ont:excerpt       "\"Among the musicians who said that Tatum could make a bad piano sound good were Billy Taylor and Gerald Wiggins." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/152557/26> ;
