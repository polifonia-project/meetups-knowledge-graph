        rdf:type          ont:Sentence ;
        ont:excerpt       "==" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/3136059/9> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/3136059/9/0> ;
        ont:subject       <http://dbpedia.org/resource/Sidney_Sheinberg> .

