<http://w3id.org/polifonia/pilot/meetups/3136059/4/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "In the summer of 1958, Sheinberg arrived in California where he accepted a teaching position at UCLA School of Law." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/3136059/4> ;
        ont:place           <http://dbpedia.org/resource/California> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/3136059/4/0> ;
        ont:subject         <http://dbpedia.org/resource/Sidney_Sheinberg> ;
        ont:timeExpression  "summer of 1958" .

<http://w3id.org/polifonia/pilot/meetups/3136059/10/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Sheinberg served on the National Board of the National Conference of Christians and Jews." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/3136059/10> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/3136059/10/0> ;
        ont:subject       <http://dbpedia.org/resource/Sidney_Sheinberg> .

