        rdf:_9    <http://w3id.org/polifonia/pilot/meetups/1303365/4/8> .

<http://w3id.org/polifonia/pilot/meetups/1303365/1/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "==" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1303365/1> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1303365/1/0> ;
        ont:subject       <http://dbpedia.org/resource/Lili_Chookasian> .

<http://w3id.org/polifonia/pilot/meetups/1303365/6/24>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1966 she made her first appearances with the Opéra de Montréal and the American Opera Society." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1303365/6> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1303365/6/23> ;
        ont:subject       <http://dbpedia.org/resource/Lili_Chookasian> .

<http://w3id.org/polifonia/pilot/meetups/1303365/4/5>
        rdf:type          ont:Sentence ;
        ont:excerpt       "However, two years later Schippers wanted \"that extraordinary Adalgisa from Arkansas\" for a concert performance of Prokofiev's Alexander Nevsky with the New York Philharmonic." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1303365/4> ;
        ont:person        <http://dbpedia.org/resource/Alexander_Nevsky> , <http://dbpedia.org/resource/Sergei_Prokofiev> ;
        ont:place         <http://dbpedia.org/resource/Arkansas> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1303365/4/4> ;
        ont:subject       <http://dbpedia.org/resource/Lili_Chookasian> .

<http://w3id.org/polifonia/pilot/meetups/1303365/0/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Lili Chookasian (August 1, 1921 – April 9, 2012) was an American contralto of Armenian ethnicity, who appeared with many of the world's major symphony orchestras and opera houses." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1303365/0> ;
        ont:person          <http://dbpedia.org/resource/Contralto> , <http://dbpedia.org/resource/Lili_Chookasian> ;
        ont:place           <http://dbpedia.org/resource/United_States> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1303365/0/0> ;
        ont:subject         <http://dbpedia.org/resource/Lili_Chookasian> ;
        ont:timeExpression  "April 9 , 2012" , "August 1 , 1921" .

<http://w3id.org/polifonia/pilot/meetups/1303365/4/12>
        rdf:type          ont:Sentence ;
        ont:excerpt       "She sang Herodias again just a month later at the Teatro Lirico Giuseppe Verdi and also gave a lauded performance of Mahler's Kindertotenlieder, accompanied by Charles Wadsworth at the Teatro Caio Melisso that summer." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1303365/4> ;
        ont:person        <http://dbpedia.org/resource/Giuseppe_Verdi> , <http://dbpedia.org/resource/Charles_Wadsworth> , <http://dbpedia.org/resource/Giuseppe_Palizzi> , <http://dbpedia.org/resource/Herodias> , <http://dbpedia.org/resource/Gustav_Mahler> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1303365/4/11> ;
        ont:subject       <http://dbpedia.org/resource/Lili_Chookasian> .

<http://w3id.org/polifonia/pilot/meetups/1303365/2/5>
        rdf:type          ont:Sentence ;
        ont:excerpt       "After high school she began studying singing seriously with Philip Manuel and Gavin Williamson, both with whom she took lessons for almost twenty years." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1303365/2> ;
        ont:person        <http://dbpedia.org/resource/Philip_Manuel> , <http://dbpedia.org/resource/Gavin_Williamson> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1303365/2/4> ;
        ont:subject       <http://dbpedia.org/resource/Lili_Chookasian> .

<http://w3id.org/polifonia/pilot/meetups/1303365/2/13>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1956 Chookasian was diagnosed with breast cancer and her physicians gave her six months to live." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1303365/2> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1303365/2/12> ;
        ont:subject       <http://dbpedia.org/resource/Lili_Chookasian> .

<http://w3id.org/polifonia/pilot/meetups/1303365/6/31>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Chookasian was invited to the city to perform in two productions mounted in her honor: Amneris in Aida and the Armenian composer Tigran Chukhajian's opera Arshak II." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1303365/6> ;
        ont:person        <http://dbpedia.org/resource/Tigran_Chukhajian> , <http://dbpedia.org/resource/Composer> , <http://dbpedia.org/resource/Arshak_II> ;
        ont:place         <http://dbpedia.org/resource/Armenia> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1303365/6/30> ;
        ont:subject       <http://dbpedia.org/resource/Lili_Chookasian> .

<http://w3id.org/polifonia/pilot/meetups/1303365/6/16>
        rdf:type            ont:Sentence ;
        ont:excerpt         "She was particularly admired worldwide for her performances in Beethoven's Symphony No. 9, Mahler's Das Lied von der Erde, Schoenberg's Gurre-Lieder and above all Verdi's Requiem." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1303365/6> ;
        ont:person          <http://dbpedia.org/resource/Arnold_Schoenberg> , <http://dbpedia.org/resource/Giuseppe_Verdi> , <http://dbpedia.org/resource/Ludwig_van_Beethoven> , <http://dbpedia.org/resource/Gustav_Mahler> ;
        ont:place           <http://dbpedia.org/resource/Requiem> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1303365/6/15> ;
        ont:subject         <http://dbpedia.org/resource/Lili_Chookasian> ;
        ont:timeExpression  "9" .

<http://w3id.org/polifonia/pilot/meetups/1303365/9>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/1303365/9/0> .

<http://w3id.org/polifonia/pilot/meetups/1303365/6/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "The audience and the critics responded enthusiastically to her performance." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1303365/6> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1303365/6/3> ;
        ont:subject       <http://dbpedia.org/resource/Lili_Chookasian> .

<http://w3id.org/polifonia/pilot/meetups/1303365/6/29>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Chookasian would often bring her husband with her when she traveled as his work in real estate was flexible enough for him to travel." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1303365/6> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1303365/6/28> ;
        ont:subject       <http://dbpedia.org/resource/Lili_Chookasian> .

<http://w3id.org/polifonia/pilot/meetups/1303365/3>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/1303365/3/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/1303365/3/1> .

<http://w3id.org/polifonia/pilot/meetups/1303365/6/10>
        rdf:type          ont:Sentence ;
        ont:excerpt       "During her career at the Met, Chookasian sang with many great singers like Carlo Bergonzi, Richard Cassilly, Franco Corelli, Phyllis Curtin, Mattiwilda Dobbs, William Dooley, Plácido Domingo, Reri Grist, Anna Moffo, Birgit Nilsson, Leontyne Price, Gail Robinson, Leonie Rysanek, Joan Sutherland, Renata Tebaldi, and Richard Tucker among others." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1303365/6> ;
        ont:person        <http://dbpedia.org/resource/Leonie_Rysanek> , <http://dbpedia.org/resource/Mattiwilda_Dobbs> , <http://dbpedia.org/resource/Renata_Tebaldi> , <http://dbpedia.org/resource/Francisco_Franco> , <http://dbpedia.org/resource/Reri_Grist> , <http://dbpedia.org/resource/Leontyne_Price> , <http://dbpedia.org/resource/Anna_Moffo> , <http://dbpedia.org/resource/Birgit_Nilsson> , <http://dbpedia.org/resource/Carlo_Bergonzi> , <http://dbpedia.org/resource/Phyllis_Curtin> , <http://dbpedia.org/resource/Richard_Tucker> , <http://dbpedia.org/resource/Richard_Cassilly> , <http://dbpedia.org/resource/Joan_Sutherland> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1303365/6/9> ;
        ont:subject       <http://dbpedia.org/resource/Lili_Chookasian> .

<http://w3id.org/polifonia/pilot/meetups/1303365/6/23>
        rdf:type            ont:Sentence ;
        ont:excerpt         "She made her first appearance at the Bayreuth festival in 1965 singing in three of her Wagner roles: Mary, the First Norn, and Erda." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1303365/6> ;
        ont:person          <http://dbpedia.org/resource/Richard_Wagner> , <http://dbpedia.org/resource/Mary_I_of_England> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1303365/6/22> ;
        ont:subject         <http://dbpedia.org/resource/Lili_Chookasian> ;
        ont:timeExpression  "three of" , "1965 singing" .

<http://w3id.org/polifonia/pilot/meetups/1303365/4/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "McGuire recalls, \"He was astonished by Lili's voice, but he had nothing in mind for her at the time.\"" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1303365/4> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1303365/4/3> ;
        ont:subject       <http://dbpedia.org/resource/Lili_Chookasian> .

<http://w3id.org/polifonia/pilot/meetups/1303365/4/11>
        rdf:type          ont:Sentence ;
        ont:excerpt       "She also made her European opera debut at the festival under Schippers as Herodias in Salome." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1303365/4> ;
        ont:person        <http://dbpedia.org/resource/Salome> , <http://dbpedia.org/resource/Herodias> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1303365/4/10> ;
        ont:subject       <http://dbpedia.org/resource/Lili_Chookasian> .

<http://w3id.org/polifonia/pilot/meetups/1303365/6/9>
        rdf:type          ont:Sentence ;
        ont:excerpt       "She also notably sang the role of Maharanee in the United States premiere of Gian Carlo Menotti's The Last Savage in a cast that included George London, Nicolai Gedda, Roberta Peters and Teresa Stratas." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1303365/6> ;
        ont:person        <http://dbpedia.org/resource/Nicolai_Gedda> , <http://dbpedia.org/resource/George_Gershwin> , <http://dbpedia.org/resource/Teresa_Stratas> , <http://dbpedia.org/resource/Gian_Carlo_Menotti> ;
        ont:place         <http://dbpedia.org/resource/London> , <http://dbpedia.org/resource/United_States> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1303365/6/8> ;
        ont:subject       <http://dbpedia.org/resource/Lili_Chookasian> .

<http://w3id.org/polifonia/pilot/meetups/1303365/6/30>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Chookasian fondly recounted one of her most memorable trips to Yerevan, Armenia in a 1997 Opera News interview." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1303365/6> ;
        ont:place           <http://dbpedia.org/resource/Yerevan> , <http://dbpedia.org/resource/Armenia> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1303365/6/29> ;
        ont:subject         <http://dbpedia.org/resource/Lili_Chookasian> ;
        ont:timeExpression  "one of" , "in a 1997" .

<http://w3id.org/polifonia/pilot/meetups/1303365/2/12>
        rdf:type          ont:Sentence ;
        ont:excerpt       "During this time she was also on the voice faculty at Northwestern University." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1303365/2> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1303365/2/11> ;
        ont:subject       <http://dbpedia.org/resource/Lili_Chookasian> .

<http://w3id.org/polifonia/pilot/meetups/1303365/2/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Chookasian first became involved with music through singing at local churches and in musical programs at her high school, notably appearing as Buttercup in her school's production of H.M.S. Pinafore." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1303365/2> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1303365/2/3> ;
        ont:subject       <http://dbpedia.org/resource/Lili_Chookasian> .

<http://w3id.org/polifonia/pilot/meetups/1303365/6/15>
        rdf:type            ont:Sentence ;
        ont:excerpt         "She quickly became one of the leading contraltos performing on the international stage during the 1960s and 1970s, singing under Georg Solti, Leonard Bernstein, James Levine, Zubin Mehta, Seiji Ozawa, Fritz Reiner, Karl Böhm, Herbert von Karajan, Lorin Maazel, and many other great conductors, with such ensembles as the Philadelphia Orchestra, the Cleveland Orchestra, the London Symphony Orchestra, and the Vienna Philharmonic among others." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1303365/6> ;
        ont:person          <http://dbpedia.org/resource/Georg_Solti> , <http://dbpedia.org/resource/Zubin_Mehta> , <http://dbpedia.org/resource/Leonard_Bernstein> , <http://dbpedia.org/resource/Seiji_Ozawa> , <http://dbpedia.org/resource/James_Levine> , <http://dbpedia.org/resource/Fritz_Reiner> , <http://dbpedia.org/resource/Herbert_von_Karajan> , <http://dbpedia.org/resource/Lorin_Maazel> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1303365/6/14> ;
        ont:subject         <http://dbpedia.org/resource/Lili_Chookasian> ;
        ont:timeExpression  "one of" .

<http://w3id.org/polifonia/pilot/meetups/1303365/8>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/1303365/8/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/1303365/8/1> .

<http://w3id.org/polifonia/pilot/meetups/1303365/6/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Ponselle, who had developed a strong personal friendship with Chookasian as well as being her opera mentor, recalled \"It not only brought back such wonderful memories, but it was another Force of Destiny.\"" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1303365/6> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1303365/6/2> ;
        ont:subject       <http://dbpedia.org/resource/Lili_Chookasian> .

<http://w3id.org/polifonia/pilot/meetups/1303365/6/28>
        rdf:type          ont:Sentence ;
        ont:excerpt       "She also appeared with the Zurich Opera, the Salzburg Festival, the New Orleans Opera, the Houston Grand Opera, and other companies in the United States and Europe." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1303365/6> ;
        ont:place         <http://dbpedia.org/resource/United_States> , <http://dbpedia.org/resource/Houston> , <http://dbpedia.org/resource/Europe> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1303365/6/27> ;
        ont:subject       <http://dbpedia.org/resource/Lili_Chookasian> .

<http://w3id.org/polifonia/pilot/meetups/1303365/4/9>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Shortly after her NYP debut, Chookasian was offered a contract with the Metropolitan Opera by Rudolf Bing but turned it down because she was afraid it would take too much time away from her family." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1303365/4> ;
        ont:person        <http://dbpedia.org/resource/Rudolf_Bing> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1303365/4/8> ;
        ont:subject       <http://dbpedia.org/resource/Lili_Chookasian> .

<http://w3id.org/polifonia/pilot/meetups/1303365/4/16>
        rdf:type          ont:Sentence ;
        ont:excerpt       "She went through another mastectomy, after which her prognosis was good; the cancer had not spread, and she recovered quickly." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1303365/4> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1303365/4/15> ;
        ont:subject       <http://dbpedia.org/resource/Lili_Chookasian> .

<http://w3id.org/polifonia/pilot/meetups/1303365/0/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "She often chose, against tradition, to sing oratorios from memory." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1303365/0> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1303365/0/3> ;
        ont:subject       <http://dbpedia.org/resource/Lili_Chookasian> .

<http://w3id.org/polifonia/pilot/meetups/1303365/2>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/1303365/2/0> ;
        rdf:_10   <http://w3id.org/polifonia/pilot/meetups/1303365/2/9> ;
        rdf:_11   <http://w3id.org/polifonia/pilot/meetups/1303365/2/10> ;
        rdf:_12   <http://w3id.org/polifonia/pilot/meetups/1303365/2/11> ;
        rdf:_13   <http://w3id.org/polifonia/pilot/meetups/1303365/2/12> ;
        rdf:_14   <http://w3id.org/polifonia/pilot/meetups/1303365/2/13> ;
        rdf:_15   <http://w3id.org/polifonia/pilot/meetups/1303365/2/14> ;
        rdf:_16   <http://w3id.org/polifonia/pilot/meetups/1303365/2/15> ;
        rdf:_17   <http://w3id.org/polifonia/pilot/meetups/1303365/2/16> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/1303365/2/1> ;
        rdf:_3    <http://w3id.org/polifonia/pilot/meetups/1303365/2/2> ;
        rdf:_4    <http://w3id.org/polifonia/pilot/meetups/1303365/2/3> ;
        rdf:_5    <http://w3id.org/polifonia/pilot/meetups/1303365/2/4> ;
        rdf:_6    <http://w3id.org/polifonia/pilot/meetups/1303365/2/5> ;
        rdf:_7    <http://w3id.org/polifonia/pilot/meetups/1303365/2/6> ;
        rdf:_8    <http://w3id.org/polifonia/pilot/meetups/1303365/2/7> ;
        rdf:_9    <http://w3id.org/polifonia/pilot/meetups/1303365/2/8> .

<http://w3id.org/polifonia/pilot/meetups/1303365/6/22>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1963 she made her first of several appearances at the New York City Opera in the title role of Menotti's The Medium." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1303365/6> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1303365/6/21> ;
        ont:subject       <http://dbpedia.org/resource/Lili_Chookasian> .

<http://w3id.org/polifonia/pilot/meetups/1303365/4/3>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Chookasian's debut performance was a resounding success and a recording of that performance was given to conductor Thomas Schippers by McGuire at the Festival of Two Worlds the following summer." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1303365/4> ;
        ont:person          <http://dbpedia.org/resource/Conducting> , <http://dbpedia.org/resource/Thomas_Schippers> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1303365/4/2> ;
        ont:subject         <http://dbpedia.org/resource/Lili_Chookasian> ;
        ont:timeExpression  "Two Worlds" .

<http://w3id.org/polifonia/pilot/meetups/1303365/2/9>
        rdf:type          ont:Sentence ;
        ont:excerpt       "They had several children and eleven grandchildren together." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1303365/2> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1303365/2/8> ;
        ont:subject       <http://dbpedia.org/resource/Lili_Chookasian> .

<http://w3id.org/polifonia/pilot/meetups/1303365/6/35>
        rdf:type            ont:Sentence ;
        ont:excerpt         "She died at her home in Branford at the age of 90 in 2012." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1303365/6> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1303365/6/34> ;
        ont:subject         <http://dbpedia.org/resource/Lili_Chookasian> ;
        ont:timeExpression  "of 90 in 2012" .

<http://w3id.org/polifonia/pilot/meetups/1303365/4/10>
        rdf:type            ont:Sentence ;
        ont:excerpt         "In the summer of 1961 she repeated the music of Amelfa Timoferevna for European debut at the Festival dei Due Mondi, again under the baton of Schippers." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1303365/4> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1303365/4/9> ;
        ont:subject         <http://dbpedia.org/resource/Lili_Chookasian> ;
        ont:timeExpression  "summer of 1961" .

<http://w3id.org/polifonia/pilot/meetups/1303365/6/8>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Her roles included Adalgisa, Amneris in Aida, Auntie in Peter Grimes, Azucena, Death in Le Rossignol, Erda in Das Rheingold and Siegfried, Filippyevna in Eugene Onegin, the First Norn in Götterdämmerung, Frugola in Il tabarro, Geneviève in Pelléas et Mélisande, Gertrud in Hansel and Gretel, the Grandmother in Jenůfa, Leocadia Begbick in Rise and Fall of the City of Mahagonny, Madelon in Andrea Chénier, Mamma Lucia in Cavalleria Rusticana, Marthe in Faust, Mary in Der fliegende Holländer, Mistress Quickly in Falstaff, the Nurse in Boris Godunov, the Princess in Suor Angelica, Teresa in La Sonnambula, Ulrica in Un ballo in maschera, the Witch in Hansel and Gretel, and Zita in Gianni Schicchi among others." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1303365/6> ;
        ont:person        <http://dbpedia.org/resource/Boris_Godunov> , <http://dbpedia.org/resource/Giuseppe_Verdi> , <http://dbpedia.org/resource/Siegfried_Wagner> , <http://dbpedia.org/resource/Leocadia> , <http://dbpedia.org/resource/Mary_I_of_England> , <http://dbpedia.org/resource/Zita> , <http://dbpedia.org/resource/Alioune_Mbaye_Nder> , <http://dbpedia.org/resource/Teresa_Brambilla> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1303365/6/7> ;
        ont:subject       <http://dbpedia.org/resource/Lili_Chookasian> .

<http://w3id.org/polifonia/pilot/meetups/1303365/2/11>
        rdf:type            ont:Sentence ;
        ont:excerpt         "The biggest triumph of her early concert career was in January 1955 when she was chosen by Bruno Walter as the contralto soloist for Mahler's Symphony No. 2, \"Resurrection\" with the Chicago Symphony Orchestra." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1303365/2> ;
        ont:person          <http://dbpedia.org/resource/Contralto> , <http://dbpedia.org/resource/Bruno_Walter> , <http://dbpedia.org/resource/Gustav_Mahler> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1303365/2/10> ;
        ont:subject         <http://dbpedia.org/resource/Lili_Chookasian> ;
        ont:timeExpression  "January 1955" , "2" .

<http://w3id.org/polifonia/pilot/meetups/1303365/2/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "She only became proficient speaking English through attending school as a child." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1303365/2> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1303365/2/2> ;
        ont:subject       <http://dbpedia.org/resource/Lili_Chookasian> .

<http://w3id.org/polifonia/pilot/meetups/1303365/7>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/1303365/7/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/1303365/7/1> .

<http://w3id.org/polifonia/pilot/meetups/1303365/6/14>
        rdf:type          ont:Sentence ;
        ont:excerpt       "While the Met was her major home, Chookasian also remained active as a concert performer and sang in opera houses both in the United States and Europe." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1303365/6> ;
        ont:place         <http://dbpedia.org/resource/Europe> , <http://dbpedia.org/resource/United_States> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1303365/6/13> ;
        ont:subject       <http://dbpedia.org/resource/Lili_Chookasian> .

<http://w3id.org/polifonia/pilot/meetups/1303365/6/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "During the performance Francis Robinson, then assistant general manager at the Met, had the house phones hooked up so Rosa Ponselle could hear Chookasian's entrance over the telephone at her home." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1303365/6> ;
        ont:person        <http://dbpedia.org/resource/Francis_Robinson> , <http://dbpedia.org/resource/Rosa_Ponselle> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1303365/6/1> ;
        ont:subject       <http://dbpedia.org/resource/Lili_Chookasian> .

<http://w3id.org/polifonia/pilot/meetups/1303365/6/27>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1976 she notably sang in the world premiere of Thomas Pasatieri's Inés de Castro and sang Ulrica with the Philadelphia Lyric Opera Company." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1303365/6> ;
        ont:person        <http://dbpedia.org/resource/Thomas_Pasatieri> , <http://dbpedia.org/resource/Fidel_Castro> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1303365/6/26> ;
        ont:subject       <http://dbpedia.org/resource/Lili_Chookasian> .

<http://w3id.org/polifonia/pilot/meetups/1303365/4/8>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Schippers contacted Chookasian to come up for an audition, and after hearing her, she was immediately engaged to sing the music of Amelfa Timoferevna (in Alexander Nevsky) for her New York Philharmonic debut in early 1961." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1303365/4> ;
        ont:person        <http://dbpedia.org/resource/Alexander_Nevsky> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1303365/4/7> ;
        ont:subject       <http://dbpedia.org/resource/Lili_Chookasian> .

<http://w3id.org/polifonia/pilot/meetups/1303365/4/15>
        rdf:type            ont:Sentence ;
        ont:excerpt         "She told no one, fulfilled her commitments in New York, Baltimore, and Europe, and then finally sought a doctor's care in November 1961 after finishing up performance of Aida." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1303365/4> ;
        ont:place           <http://dbpedia.org/resource/Baltimore> , <http://dbpedia.org/resource/New_York_City> , <http://dbpedia.org/resource/Europe> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1303365/4/14> ;
        ont:subject         <http://dbpedia.org/resource/Lili_Chookasian> ;
        ont:timeExpression  "November 1961" .

<http://w3id.org/polifonia/pilot/meetups/1303365/0/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "She was admired for her sonorous, focused tone as well as her excellent musicianship." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1303365/0> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1303365/0/2> ;
        ont:subject       <http://dbpedia.org/resource/Lili_Chookasian> .

<http://w3id.org/polifonia/pilot/meetups/1303365/1>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/1303365/1/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/1303365/1/1> .

<http://w3id.org/polifonia/pilot/meetups/1303365/6/21>
        rdf:type          ont:Sentence ;
        ont:excerpt       "On the opera stage Chookasian returned to the Spoleto Festival in 1962 to sing Clarissa in Prokofiev's The Love for Three Oranges." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1303365/6> ;
        ont:person        <http://dbpedia.org/resource/Sergei_Prokofiev> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1303365/6/20> ;
        ont:subject       <http://dbpedia.org/resource/Lili_Chookasian> .

<http://w3id.org/polifonia/pilot/meetups/1303365/4/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Stevenson gave McGuire a recording of a Messiah that she had sung with Chookasian in Salt Lake City and, after hearing the recording, McGuire knew he wanted Chookasian to sing the part." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1303365/4> ;
        ont:person        <http://dbpedia.org/resource/Robert_Louis_Stevenson> ;
        ont:place         <http://dbpedia.org/resource/Salt_Lake_City> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1303365/4/1> ;
        ont:subject       <http://dbpedia.org/resource/Lili_Chookasian> .

<http://w3id.org/polifonia/pilot/meetups/1303365/6/34>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 2002 she was awarded Yale's Sanford Medal." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1303365/6> ;
        ont:person        <http://dbpedia.org/resource/Samuel_Sanford> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1303365/6/33> ;
        ont:subject       <http://dbpedia.org/resource/Lili_Chookasian> .

<http://w3id.org/polifonia/pilot/meetups/1303365/2/16>
        rdf:type          ont:Sentence ;
        ont:excerpt       "With continuous medical attention and the support of her family she prevailed and her life slowly returned to normal." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1303365/2> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1303365/2/15> ;
        ont:subject       <http://dbpedia.org/resource/Lili_Chookasian> .

<http://w3id.org/polifonia/pilot/meetups/1303365/2/8>
        rdf:type          ont:Sentence ;
        ont:excerpt       "They had a very happy marriage that lasted for forty-six years, ending when Gavejian died in 1987." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1303365/2> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1303365/2/7> ;
        ont:subject       <http://dbpedia.org/resource/Lili_Chookasian> .

<http://w3id.org/polifonia/pilot/meetups/1303365/8/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "External links ==" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1303365/8> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1303365/8/0> ;
        ont:subject       <http://dbpedia.org/resource/Lili_Chookasian> .

<http://w3id.org/polifonia/pilot/meetups/1303365/6/19>
        rdf:type            ont:Sentence ;
        ont:excerpt         "With Leonard Bernstein, she was one of the many soloists in his recording of Mahler's Symphony No. 8." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1303365/6> ;
        ont:person          <http://dbpedia.org/resource/Gustav_Mahler> , <http://dbpedia.org/resource/Leonard_Bernstein> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1303365/6/18> ;
        ont:subject         <http://dbpedia.org/resource/Lili_Chookasian> ;
        ont:timeExpression  "8" , "one of" .

<http://w3id.org/polifonia/pilot/meetups/1303365/6/7>
        rdf:type          ont:Sentence ;
        ont:excerpt       "During her 24-year-long career with the company, Chookasian sang many principal contralto roles and a number of secondary parts." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1303365/6> ;
        ont:person        <http://dbpedia.org/resource/Contralto> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1303365/6/6> ;
        ont:subject       <http://dbpedia.org/resource/Lili_Chookasian> .

<http://w3id.org/polifonia/pilot/meetups/1303365/7/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "References ==" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1303365/7> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1303365/7/0> ;
        ont:subject       <http://dbpedia.org/resource/Lili_Chookasian> .

<http://w3id.org/polifonia/pilot/meetups/1303365/2/10>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Chookasian began performing professionally as an oratorio and concert singer in the 1940s, mostly in Chicago but also occasionally out of town." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1303365/2> ;
        ont:place           <http://dbpedia.org/resource/Chicago> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1303365/2/9> ;
        ont:subject         <http://dbpedia.org/resource/Lili_Chookasian> ;
        ont:timeExpression  "in the 1940s" .

<http://w3id.org/polifonia/pilot/meetups/1303365/2/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Chookasian's first language was Armenian, as her parents spoke that language in the home." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1303365/2> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1303365/2/1> ;
        ont:subject       <http://dbpedia.org/resource/Lili_Chookasian> .

<http://w3id.org/polifonia/pilot/meetups/1303365/6/13>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Her 290th performance with the company, was also her farewell to the opera stage." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1303365/6> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1303365/6/12> ;
        ont:subject       <http://dbpedia.org/resource/Lili_Chookasian> .

