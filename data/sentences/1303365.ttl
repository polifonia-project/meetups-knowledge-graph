<http://w3id.org/polifonia/pilot/meetups/1303365/4/14>
        rdf:type            ont:Sentence ;
        ont:excerpt         "In the midst of Chookasian's breakout 1961 season she discovered another lump in her other breast." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1303365/4> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1303365/4/13> ;
        ont:subject         <http://dbpedia.org/resource/Lili_Chookasian> ;
        ont:timeExpression  "1961 season" .

