        ont:timeExpression  "1 January 1981" .

<http://w3id.org/polifonia/pilot/meetups/3236194/8/5>
        rdf:type          ont:Sentence ;
        ont:excerpt       "This also became the base for The Institute for Social Research, which Sir Richard Hauser ran until his death." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/3236194/8> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/3236194/8/4> ;
