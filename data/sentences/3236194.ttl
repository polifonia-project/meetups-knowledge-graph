<http://w3id.org/polifonia/pilot/meetups/3236194/10/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Hephzibah Menuhin died in London on 1 January 1981, following a lengthy illness." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/3236194/10> ;
        ont:person          <http://dbpedia.org/resource/Hephzibah_Menuhin> ;
