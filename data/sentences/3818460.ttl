<http://w3id.org/polifonia/pilot/meetups/3818460/9/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "===" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/3818460/9> ;
