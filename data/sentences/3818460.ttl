<http://w3id.org/polifonia/pilot/meetups/3818460/4/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He specialized in Baroque music and received the Prix d'Excellence for both organ and harpsichord." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/3818460/4> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/3818460/4/0> ;
        ont:subject       <http://dbpedia.org/resource/Ton_Koopman> .

<http://w3id.org/polifonia/pilot/meetups/3818460/8/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Among Koopman's most ambitious projects was the recording of the complete cycle of all of Bach's cantatas, a project completed in 2005." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/3818460/8> ;
        ont:person        <http://dbpedia.org/resource/Johann_Sebastian_Bach> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/3818460/8/0> ;
        ont:subject       <http://dbpedia.org/resource/Ton_Koopman> .

