        rdf:type          ont:Sentence ;
        ont:excerpt       "This is an authentic Baroque practice." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/3818460/5> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/3818460/5/0> ;
