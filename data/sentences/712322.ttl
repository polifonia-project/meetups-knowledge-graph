        rdf:type            ont:Sentence ;
        ont:excerpt         "In September 2007, 12 received a special Golden Lion for the “consistent brilliance” of its work and was praised by many critics at the Venice Film Festival." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/712322/20> ;
