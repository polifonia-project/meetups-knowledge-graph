        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/712322/28/8> ;
        ont:subject         <http://dbpedia.org/resource/Nikita_Mikhalkov> ;
        ont:timeExpression  "April 2010" .

<http://w3id.org/polifonia/pilot/meetups/712322/28/11>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Despite his support for the annexation of Crimea he also called for the release of imprisoned Ukrainian filmmaker Oleg Sentsov." ;
