        ont:subject         <http://dbpedia.org/resource/Nikita_Mikhalkov> ;
        ont:timeExpression  "for the 2014" .

<http://w3id.org/polifonia/pilot/meetups/712322/28/8>
        rdf:type            ont:Sentence ;
        ont:excerpt         "In October 2007, Mikhalkov, who produced a television program for Putin's 55th birthday, co-signed an open letter asking Putin not to step down after the expiry of his term in office." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/712322/28> ;
        ont:person          <http://dbpedia.org/resource/Vladimir_Putin> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/712322/28/7> ;
        ont:subject         <http://dbpedia.org/resource/Nikita_Mikhalkov> ;
        ont:timeExpression  "October 2007" .

<http://w3id.org/polifonia/pilot/meetups/712322/19>
        rdf:type  ont:Paragraph ;
