        ont:subject         <http://dbpedia.org/resource/Nikita_Mikhalkov> ;
        ont:timeExpression  "1984" , "1982" .

<http://w3id.org/polifonia/pilot/meetups/712322/8/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "While continuing to pursue his acting career, he entered VGIK, the state film school in Moscow, where he studied directing under filmmaker Mikhail Romm, teacher to his brother and Andrei Tarkovsky." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/712322/8> ;
