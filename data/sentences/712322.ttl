<http://w3id.org/polifonia/pilot/meetups/712322/0/2>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Mikhalkov is a three-time laureate of the State Prize of the Russian Federation (1993, 1995, 1999) and is a Full Cavalier of the Order \"For Merit to the Fatherland\"." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/712322/0> ;
        ont:place           <http://dbpedia.org/resource/Russia> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/712322/0/1> ;
        ont:subject         <http://dbpedia.org/resource/Nikita_Mikhalkov> ;
        ont:timeExpression  "1993" , "1999" .

<http://w3id.org/polifonia/pilot/meetups/712322/48/2>
        rdf:type          ont:Sentence ;
