<http://w3id.org/polifonia/pilot/meetups/712322/10/5>
        rdf:type            ont:Sentence ;
        ont:excerpt         "The film won the Prix FIPRESCI at the 13th Moscow International Film Festival." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/712322/10> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/712322/10/4> ;
        ont:subject         <http://dbpedia.org/resource/Nikita_Mikhalkov> ;
        ont:timeExpression  "at the 13th" .

<http://w3id.org/polifonia/pilot/meetups/712322/17/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "It featured Julia Ormond and Oleg Menshikov, who regularly appears in Mikhalkov's films, in the leading roles." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/712322/17> ;
