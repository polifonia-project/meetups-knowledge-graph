<http://w3id.org/polifonia/pilot/meetups/712322/1/2>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Mikhalkov received the \"Special Lion\" of the Venice Film Festival (2007) for his contribution to the cinematography and nominated for an Academy Award for the film 12 (2007)." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/712322/1> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/712322/1/1> ;
