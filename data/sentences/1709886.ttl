<http://w3id.org/polifonia/pilot/meetups/1709886/2/9>
        rdf:type            ont:Sentence ;
        ont:excerpt         "\"In 1969, Peter Schjeldahl compared Grooms to Marcel Duchamp, because both embodied \"a movement of one man that is open to everybody.\"" ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1709886/2> ;
        ont:person          <http://dbpedia.org/resource/Peter_Schjeldahl> , <http://dbpedia.org/resource/Marcel_Duchamp> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1709886/2/8> ;
        ont:subject         <http://dbpedia.org/resource/Red_Grooms> ;
        ont:timeExpression  "one man" .

