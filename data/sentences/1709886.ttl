        ont:excerpt       "Today Grooms is recognized as a pioneer of site-specific sculpture and installation art." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1709886/6> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1709886/6/1> ;
