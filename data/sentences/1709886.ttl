        ont:place           <http://dbpedia.org/resource/Chicago> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1709886/8/3> ;
        ont:subject         <http://dbpedia.org/resource/Red_Grooms> ;
        ont:timeExpression  "1967" , "1975" .

<http://w3id.org/polifonia/pilot/meetups/1709886/2/5>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Red Grooms belongs to a generation of artists who, in G. R. Swenson's words, \"took the world too seriously not to be amused by it.\"" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1709886/2> ;
        ont:person        <http://dbpedia.org/resource/Red_Grooms> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1709886/2/4> ;
        ont:subject       <http://dbpedia.org/resource/Red_Grooms> .

<http://w3id.org/polifonia/pilot/meetups/1709886/12/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "It turns out to be easier to work with than less durable materials.\"" ;
