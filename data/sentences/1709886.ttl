        ont:person        <http://dbpedia.org/resource/Grace_Glueck> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1709886/12/1> ;
        ont:subject       <http://dbpedia.org/resource/Red_Grooms> .

<http://w3id.org/polifonia/pilot/meetups/1709886/6/3>
        rdf:type            ont:Sentence ;
        ont:excerpt         "City of Chicago (1967), a room-sized, walk-through \"sculpto-pictorama,\" features sky-scraper-proportioned sculptures of Mayor Daley and Hugh Hefner \"joined by such historical figures as Abraham Lincoln, Al Capone, and fan-dancer Sally Rand, accompanied by a sound track featuring gunfire and burlesque music." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1709886/6> ;
        ont:person          <http://dbpedia.org/resource/Sally_Rand> , <http://dbpedia.org/resource/Hugh_Hefner> , <http://dbpedia.org/resource/Richard_M._Daley> , <http://dbpedia.org/resource/Al_Capone> , <http://dbpedia.org/resource/Abraham_Lincoln> ;
        ont:place           <http://dbpedia.org/resource/Chicago> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1709886/6/2> ;
        ont:subject         <http://dbpedia.org/resource/Red_Grooms> ;
        ont:timeExpression  "1967" .

