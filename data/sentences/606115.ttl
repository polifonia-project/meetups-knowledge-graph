        rdf:type          ont:Sentence ;
        ont:excerpt       "The Scotsman newspaper has described her as Scotland's best-selling female singer in terms of the numbers of hit chart singles and albums she has achieved in the UK since 1976." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/606115/0> ;
