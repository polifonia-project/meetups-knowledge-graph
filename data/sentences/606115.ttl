        ont:person          <http://dbpedia.org/resource/Laurence_Olivier> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/606115/10/2> ;
        ont:subject         <http://dbpedia.org/resource/Barbara_Dickson> ;
        ont:timeExpression  "at the 2000" .

