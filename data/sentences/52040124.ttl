<http://w3id.org/polifonia/pilot/meetups/52040124/2/5>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Adams toured widely in the early 1950s on shows featuring Johnny Ace, Jimmy Forrest, B.B. King, Clarence \"Gatemouth\" Brown, and Lloyd Price." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/52040124/2> ;
        ont:person          <http://dbpedia.org/resource/Lloyd_Price> , <http://dbpedia.org/resource/Jimmy_Forrest> , <http://dbpedia.org/resource/Johnny_Ace> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/52040124/2/4> ;
        ont:subject         <http://dbpedia.org/resource/Marie_Adams> ;
        ont:timeExpression  "1950s on" .

<http://w3id.org/polifonia/pilot/meetups/52040124/2/14>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Although the record did not make the charts in the US, it was released in the UK and rose to number 2 on the UK Singles Chart in early 1958." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/52040124/2> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/52040124/2/13> ;
        ont:subject         <http://dbpedia.org/resource/Marie_Adams> ;
        ont:timeExpression  "2 on" .

