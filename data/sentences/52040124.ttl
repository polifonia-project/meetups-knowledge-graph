<http://w3id.org/polifonia/pilot/meetups/52040124/2/10>
        rdf:type          ont:Sentence ;
        ont:excerpt       "\"In 1957, Johnny Otis signed to Capitol Records." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/52040124/2> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/52040124/2/9> ;
