<http://w3id.org/polifonia/pilot/meetups/52040124/2/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "She was born in Linden, Texas, and sang in gospel groups in her youth." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/52040124/2> ;
        ont:place         <http://dbpedia.org/resource/Linden,_Texas> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/52040124/2/0> ;
        ont:subject       <http://dbpedia.org/resource/Marie_Adams> .

<http://w3id.org/polifonia/pilot/meetups/52040124/2/6>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1953, she joined the Johnny Otis band as a featured singer, and moved to Los Angeles." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/52040124/2> ;
        ont:place         <http://dbpedia.org/resource/Los_Angeles> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/52040124/2/5> ;
        ont:subject       <http://dbpedia.org/resource/Marie_Adams> .

<http://w3id.org/polifonia/pilot/meetups/52040124/2/15>
        rdf:type            ont:Sentence ;
        ont:excerpt         "To follow up its success, Adams and Otis recorded a duet, \"Bye Bye Baby\", which reached number 20 in Britain." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/52040124/2> ;
        ont:place           <http://dbpedia.org/resource/United_Kingdom> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/52040124/2/14> ;
        ont:subject         <http://dbpedia.org/resource/Marie_Adams> ;
        ont:timeExpression  "20 in" .

