        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/87603/18/6> ;
        ont:subject         <http://dbpedia.org/resource/Robert_Mitchum> ;
        ont:timeExpression  "1956" , "in the 1962" .

<http://w3id.org/polifonia/pilot/meetups/87603/22/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Though the film was nominated for four Academy Awards (winning two) and Mitchum was much publicized as a contender for a Best Actor nomination, he was not nominated." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/87603/22> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/87603/22/2> ;
        ont:subject       <http://dbpedia.org/resource/Robert_Mitchum> .

<http://w3id.org/polifonia/pilot/meetups/87603/29/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1991, Mitchum was given a lifetime achievement award from the National Board of Review of Motion Pictures, in the same year he received the Telegatto award and in 1992 the Cecil B. DeMille Award from the Golden Globe Awards." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/87603/29> ;
