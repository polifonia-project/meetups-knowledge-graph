        ont:person          <http://dbpedia.org/resource/Deborah_Kerr> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/87603/18/1> ;
        ont:subject         <http://dbpedia.org/resource/Robert_Mitchum> ;
        ont:timeExpression  "1956" .

<http://w3id.org/polifonia/pilot/meetups/87603/35/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He was five weeks shy of his 80th birthday." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/87603/35> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/87603/35/3> ;
