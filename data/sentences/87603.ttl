<http://w3id.org/polifonia/pilot/meetups/87603/35/5>
        rdf:type          ont:Sentence ;
        ont:excerpt       "His body was cremated and his ashes scattered at sea, though there is a plot marker in the Odd Fellows Cemetery in Delaware." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/87603/35> ;
        ont:place         <http://dbpedia.org/resource/Delaware> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/87603/35/4> ;
        ont:subject       <http://dbpedia.org/resource/Robert_Mitchum> .

<http://w3id.org/polifonia/pilot/meetups/87603/31/11>
        rdf:type            ont:Sentence ;
        ont:excerpt         "\"Little Old Wine Drinker Me\", the first single, was a top-10 hit at country radio, reaching number nine there, and crossed over onto mainstream radio, where it peaked at number 96." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/87603/31> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/87603/31/10> ;
        ont:subject         <http://dbpedia.org/resource/Robert_Mitchum> ;
        ont:timeExpression  "at number 96" .

