<http://w3id.org/polifonia/pilot/meetups/87603/5/8>
        rdf:type            ont:Sentence ;
        ont:excerpt         "He later stated that at age 14 in Savannah, Georgia, he was arrested for vagrancy and put in a local chain gang." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/87603/5> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/87603/5/7> ;
        ont:subject         <http://dbpedia.org/resource/Robert_Mitchum> ;
        ont:timeExpression  "14 in" .

<http://w3id.org/polifonia/pilot/meetups/87603/27/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "A hardened con (Mitchum), being transferred from a federal penitentiary to a Texas institution to finish a life sentence as a habitual criminal, is freed at gunpoint by his niece (played by Kathleen York)." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/87603/27> ;
        ont:person        <http://dbpedia.org/resource/Habitual_offender> , <http://dbpedia.org/resource/Kathleen_York> ;
        ont:place         <http://dbpedia.org/resource/Texas> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/87603/27/0> ;
        ont:subject       <http://dbpedia.org/resource/Robert_Mitchum> .

<http://w3id.org/polifonia/pilot/meetups/87603/49>
        rdf:type  ont:Paragraph ;
