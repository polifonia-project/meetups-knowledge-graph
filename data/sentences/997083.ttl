<http://w3id.org/polifonia/pilot/meetups/997083/3/1>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Simon scored again in 1965 on the Chicago-based label with \"Let's Do It Over\", which landed a #13 spot on the US Billboard R&B chart." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/997083/3> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/997083/3/0> ;
