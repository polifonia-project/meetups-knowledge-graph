        ont:timeExpression  "November 30 , 1963" , "January 23 , 1965" .

<http://w3id.org/polifonia/pilot/meetups/997083/4/6>
        rdf:type            ont:Sentence ;
        ont:excerpt         "The team scored a #3 R&B hit in 1971 with \"Drowning in the Sea of Love\" and a #1 R&B hit in the summer of 1972 with \"Power of Love\"." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/997083/4> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/997083/4/5> ;
        ont:subject         <http://dbpedia.org/resource/Joe_Simon_(musician)> ;
        ont:timeExpression  "summer of 1972" , "hit in 1971" .

