<http://w3id.org/polifonia/pilot/meetups/997083/2/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "With this, the group decided to turn secular and recorded \"Little Island Girl\" as the Golden Tones in 1959." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/997083/2> ;
        ont:place         <http://dbpedia.org/resource/Little_Island,_Cork> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/997083/2/3> ;
        ont:subject       <http://dbpedia.org/resource/Joe_Simon_(musician)> .

<http://w3id.org/polifonia/pilot/meetups/997083/4/9>
        rdf:type            ont:Sentence ;
        ont:excerpt         "\"Power of Love\", written by Gamble, Huff and Simon was Simon's third million seller, and the R.I.A.A. awarded gold disc status on August 29, 1972." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/997083/4> ;
        ont:person          <http://dbpedia.org/resource/Gamble_and_Huff> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/997083/4/8> ;
        ont:subject         <http://dbpedia.org/resource/Joe_Simon_(musician)> ;
        ont:timeExpression  "August 29 , 1972" , "million seller" .

<http://w3id.org/polifonia/pilot/meetups/997083/4/12>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In the late 1970s/early 1980s, Simon decided to remove his tenor/bass-baritone voice from the secular music world and devote it and other parts of his life to Christianity." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/997083/4> ;
        ont:person        <http://dbpedia.org/resource/Bass-baritone> , <http://dbpedia.org/resource/Tenor> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/997083/4/11> ;
        ont:subject       <http://dbpedia.org/resource/Joe_Simon_(musician)> .

<http://w3id.org/polifonia/pilot/meetups/997083/4/3>
        rdf:type            ont:Sentence ;
        ont:excerpt         "\"The Chokin' Kind\" was written by Harlan Howard, spent 12 weeks in the charts, and had sold one million copies by June 16, 1969." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/997083/4> ;
        ont:person          <http://dbpedia.org/resource/John_Marshall_Harlan_(1899â€“1971)> , <http://dbpedia.org/resource/Dwight_Howard> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/997083/4/2> ;
        ont:subject         <http://dbpedia.org/resource/Joe_Simon_(musician)> ;
        ont:timeExpression  "June 16 , 1969" .

