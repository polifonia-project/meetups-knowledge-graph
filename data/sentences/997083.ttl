        ont:timeExpression  "1965 on" , "13 spot" .

<http://w3id.org/polifonia/pilot/meetups/997083/4/15>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Simon released a gospel album titled This Story Must Be Told in the late 1990s." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/997083/4> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/997083/4/14> ;
