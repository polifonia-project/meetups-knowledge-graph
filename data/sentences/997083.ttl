<http://w3id.org/polifonia/pilot/meetups/997083/2/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Similar to many other African-American artists from the era, Simon began singing in his father's Baptist church." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/997083/2> ;
        ont:person        <http://dbpedia.org/resource/Baptists> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/997083/2/0> ;
        ont:subject       <http://dbpedia.org/resource/Joe_Simon_(musician)> .

<http://w3id.org/polifonia/pilot/meetups/997083/6/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Simon has had a number of his songs sampled by other artists, including OutKast, who sampled \"Before the Night is Over\" in their hit \"So Fresh, So Clean\" and Lil' Kim, who sampled Simon's \"It Be's That Way Sometimes\" in \"Magic Stick\", featuring 50 Cent." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/997083/6> ;
        ont:person          <http://dbpedia.org/resource/50_Cent> , <http://dbpedia.org/resource/Lil'_Kim> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/997083/6/0> ;
        ont:subject         <http://dbpedia.org/resource/Joe_Simon_(musician)> ;
        ont:timeExpression  "50 Cent" .

