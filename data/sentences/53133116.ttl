        rdf:type          ont:Sentence ;
        ont:excerpt       "He received his primary and secondary education from the Meepavala Buddhist School, Richmond College and Mahinda College." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/53133116/2> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/53133116/2/0> ;
