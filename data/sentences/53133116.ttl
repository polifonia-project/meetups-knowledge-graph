        rdf:type          ont:Sentence ;
        ont:excerpt       "He then studied at the Visva-Bharati University in Santiniketan, West Bengal, where he was a classmate of Indira Gandhi." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/53133116/2> ;
