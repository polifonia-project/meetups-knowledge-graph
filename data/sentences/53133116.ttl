        ont:place           <http://dbpedia.org/resource/Baddegama> , <http://dbpedia.org/resource/Southern_Province,_Sri_Lanka> , <http://dbpedia.org/resource/Galle_District> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/53133116/2/0> ;
        ont:subject         <http://dbpedia.org/resource/Lionel_Edirisinghe> ;
        ont:timeExpression  "17 January 1913" .

<http://w3id.org/polifonia/pilot/meetups/53133116/3/5>
        rdf:type          ont:Sentence ;
        ont:excerpt       "His talents were acknowledged by greats such as Rabindranath Tagore and Mahatma Gandhi." ;
