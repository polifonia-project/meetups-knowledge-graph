        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/1530722/5/0> .

<http://w3id.org/polifonia/pilot/meetups/1530722/0/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Vadim Alekseyevich Kozin (Russian: Вадим Алексеевич Козин; March 21, 1903 – December 19, 1994) was a Russian tenor, songwriter, and an openly homosexual man until 1934 when male homosexuality became a crime in USSR." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1530722/0> ;
        ont:person          <http://dbpedia.org/resource/Tenor> , <http://dbpedia.org/resource/Roger_Vadim> ;
        ont:place           <http://dbpedia.org/resource/Soviet_Union> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1530722/0/0> ;
        ont:subject         <http://dbpedia.org/resource/Vadim_Kozin> ;
        ont:timeExpression  "man until 1934" , "March 21 , 1903" , "December 19 , 1994" .

<http://w3id.org/polifonia/pilot/meetups/1530722/2/5>
        rdf:type          ont:Sentence ;
        ont:excerpt       "he called it- until his death." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1530722/2> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1530722/2/4> ;
        ont:subject       <http://dbpedia.org/resource/Vadim_Kozin> .
