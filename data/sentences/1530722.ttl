<http://w3id.org/polifonia/pilot/meetups/1530722/3/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He even began burning his own records, to the point where his friends were forced to hide their own copies from him in order to preserve them." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1530722/3> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1530722/3/0> ;
        ont:subject       <http://dbpedia.org/resource/Vadim_Kozin> .

<http://w3id.org/polifonia/pilot/meetups/1530722/7/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Marc Almond included songs from Kozin's repertoire on his 2003 album Heart on Snow and went on to record the album Orpheus in Exile  on which he covers Kozin's songs exclusively." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1530722/7> ;
        ont:person        <http://dbpedia.org/resource/Marc_Almond> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1530722/7/0> ;
        ont:subject       <http://dbpedia.org/resource/Vadim_Kozin> .

<http://w3id.org/polifonia/pilot/meetups/1530722/2/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Kozin famously replied that songs about Stalin were not suited for tenor voices." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1530722/2> ;
        ont:person        <http://dbpedia.org/resource/Joseph_Stalin> , <http://dbpedia.org/resource/Tenor> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1530722/2/0> ;
        ont:subject       <http://dbpedia.org/resource/Vadim_Kozin> .

<http://w3id.org/polifonia/pilot/meetups/1530722/6/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "He died at the age of 91 in 1994." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1530722/6> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1530722/6/0> ;
        ont:subject         <http://dbpedia.org/resource/Vadim_Kozin> ;
        ont:timeExpression  "of 91 in 1994" .

