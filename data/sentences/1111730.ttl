<http://w3id.org/polifonia/pilot/meetups/1111730/5/10>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Wallace recorded an album, Women Be Wise, on October 31, 1966, in Copenhagen, Denmark, with Roosevelt Sykes and Little Brother Montgomery playing the piano." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1111730/5> ;
        ont:person          <http://dbpedia.org/resource/Roosevelt_Sykes> , <http://dbpedia.org/resource/Little_Brother_Montgomery> , <http://dbpedia.org/resource/Henry_A._Wallace> ;
        ont:place           <http://dbpedia.org/resource/Copenhagen> , <http://dbpedia.org/resource/Denmark> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1111730/5/9> ;
        ont:subject         <http://dbpedia.org/resource/Sippie_Wallace> ;
        ont:timeExpression  "October 31 , 1966" .

<http://w3id.org/polifonia/pilot/meetups/1111730/3/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "By the time she was in her mid-teens, they were playing in those tent shows." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1111730/3> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1111730/3/3> ;
        ont:subject       <http://dbpedia.org/resource/Sippie_Wallace> .

