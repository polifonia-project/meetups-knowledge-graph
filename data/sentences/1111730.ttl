        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/1111730/13/1> .

<http://w3id.org/polifonia/pilot/meetups/1111730/8/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Death ==" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1111730/8> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1111730/8/0> ;
