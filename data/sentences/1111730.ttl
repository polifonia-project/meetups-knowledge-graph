<http://w3id.org/polifonia/pilot/meetups/1111730/5/6>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Matt Wallace died in 1936 and George Thomas Washington died on March 6, 1937." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1111730/5> ;
        ont:person          <http://dbpedia.org/resource/Thomas_Washington> , <http://dbpedia.org/resource/Matt_Wallace> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1111730/5/5> ;
