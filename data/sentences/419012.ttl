<http://w3id.org/polifonia/pilot/meetups/419012/7/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Over the years, these record labels were consolidated and the recordings are now controlled by Universal Music." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/419012/7> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/419012/7/2> ;
