<http://w3id.org/polifonia/pilot/meetups/419012/3/10>
        rdf:type          ont:Sentence ;
        ont:excerpt       "The caravan was in Dallas, Texas, on the day of the assassination of President Kennedy in 1963." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/419012/3> ;
        ont:person        <http://dbpedia.org/resource/John_F._Kennedy> ;
        ont:place         <http://dbpedia.org/resource/Dallas> , <http://dbpedia.org/resource/Texas> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/419012/3/9> ;
        ont:subject       <http://dbpedia.org/resource/Brian_Hyland> .

<http://w3id.org/polifonia/pilot/meetups/419012/5/3>
        rdf:type            ont:Sentence ;
        ont:excerpt         "\"Gypsy Woman\" reached No. 3 on the 1970 U.S. pop chart, making it the second-biggest hit of his career, selling over one million copies, and being certified gold by the RIAA in January 1971." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/419012/5> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/419012/5/2> ;
        ont:subject         <http://dbpedia.org/resource/Brian_Hyland> ;
        ont:timeExpression  "on the 1970" , "January 1971" .

<http://w3id.org/polifonia/pilot/meetups/419012/32>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/419012/32/0> .

<http://w3id.org/polifonia/pilot/meetups/419012/12/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "==" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/419012/12> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/419012/12/0> ;
        ont:subject       <http://dbpedia.org/resource/Brian_Hyland> .

<http://w3id.org/polifonia/pilot/meetups/419012/3/9>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Hyland appeared on national television programs such as American Bandstand  and The Jackie Gleason Show, and toured both internationally and around America with Dick Clark in the Caravan of Stars." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/419012/3> ;
        ont:person        <http://dbpedia.org/resource/Dick_Clark> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/419012/3/8> ;
        ont:subject       <http://dbpedia.org/resource/Brian_Hyland> .

<http://w3id.org/polifonia/pilot/meetups/419012/4/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Songs such as \"I'm Afraid To Go Home\" and \"Two Brothers\" had an American Civil War theme." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/419012/4> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/419012/4/2> ;
        ont:subject       <http://dbpedia.org/resource/Brian_Hyland> .

<http://w3id.org/polifonia/pilot/meetups/419012/17>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/419012/17/0> .

<http://w3id.org/polifonia/pilot/meetups/419012/11/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "1998 – MCA parent Universal Music buys Philips Records (\"The Joker Went Wild\") owner PolyGram completing the catalog consolidation" ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/419012/11> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/419012/11/0> ;
        ont:subject         <http://dbpedia.org/resource/Brian_Hyland> ;
        ont:timeExpression  "1998 –" .

<http://w3id.org/polifonia/pilot/meetups/419012/3/3>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Hyland's other major hit during this period was 1962's \"Sealed with a Kiss\", which reached No. 3 in 1962 on both the American and UK Singles Chart." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/419012/3> ;
        ont:place           <http://dbpedia.org/resource/United_States> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/419012/3/2> ;
        ont:subject         <http://dbpedia.org/resource/Brian_Hyland> ;
        ont:timeExpression  "3 in" , "1962 on" .

<http://w3id.org/polifonia/pilot/meetups/419012/10/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "1979 – MCA Records buys ABC Records" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/419012/10> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/419012/10/0> ;
        ont:subject       <http://dbpedia.org/resource/Brian_Hyland> .

