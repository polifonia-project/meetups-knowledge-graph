        ont:person          <http://dbpedia.org/resource/Groucho_Marx> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/150095/7/0> ;
        ont:subject         <http://dbpedia.org/resource/Harpo_Marx> ;
        ont:timeExpression  "1921" .

