<http://w3id.org/polifonia/pilot/meetups/1320240/4/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Dowd also played in a band at New York's Columbia University, where he became a conductor." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1320240/4> ;
        ont:person        <http://dbpedia.org/resource/Conducting> ;
        ont:place         <http://dbpedia.org/resource/New_York_City> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1320240/4/1> ;
