        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1904, he accepted a position as the treasurer of the Colored Republican Club, started by Charles W. Anderson." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/209276/10> ;
        ont:person        <http://dbpedia.org/resource/Charles_I_of_England> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/209276/10/0> ;
        ont:subject       <http://dbpedia.org/resource/James_Weldon_Johnson> .

