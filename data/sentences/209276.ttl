<http://w3id.org/polifonia/pilot/meetups/209276/9/2>
        rdf:type            ont:Sentence ;
        ont:excerpt         "He founded and edited the Daily American newspaper in 1895." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/209276/9> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/209276/9/1> ;
