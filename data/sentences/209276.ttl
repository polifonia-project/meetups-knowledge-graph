        ont:timeExpression  "newspaper in 1895" .

<http://w3id.org/polifonia/pilot/meetups/209276/15/1>
        rdf:type            ont:Sentence ;
        ont:excerpt         "\"In all of my experience there has been no period so brief that has meant so much in my education for life as the three months I spent in the backwoods of Georgia,\" Johnson wrote." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/209276/15> ;
        ont:person          <http://dbpedia.org/resource/Lyndon_B._Johnson> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/209276/15/0> ;
        ont:subject         <http://dbpedia.org/resource/James_Weldon_Johnson> ;
        ont:timeExpression  "as the three" .

