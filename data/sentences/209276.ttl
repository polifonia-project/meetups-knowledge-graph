        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/209276/3/12> ;
        ont:subject       <http://dbpedia.org/resource/James_Weldon_Johnson> .

<http://w3id.org/polifonia/pilot/meetups/209276/6/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Civil rights activism ==" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/209276/6> ;
        ont:person        <http://dbpedia.org/resource/Civil_and_political_rights> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/209276/6/0> ;
        ont:subject       <http://dbpedia.org/resource/James_Weldon_Johnson> .

<http://w3id.org/polifonia/pilot/meetups/209276/12/0>
        rdf:type          ont:Sentence ;
