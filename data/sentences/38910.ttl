        rdf:type            ont:Sentence ;
        ont:excerpt         "He is recognized today as one of the foremost exponents of a sophisticated style of largely instrumental music that combines elements of lounge music and jazz with Latin flavors." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/38910/0> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/38910/0/0> ;
