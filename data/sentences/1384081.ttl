        rdf:type          ont:Sentence ;
        ont:excerpt       "\"Hawes influenced a great number of prominent pianists, including André Previn, Peterson, Horace Silver, Claude Williamson, Pete Jolly, and Toshiko Akiyoshi." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1384081/10> ;
        ont:person        <http://dbpedia.org/resource/Claude_Williamson> , <http://dbpedia.org/resource/Pete_Jolly> , <http://dbpedia.org/resource/Toshiko_Akiyoshi> , <http://dbpedia.org/resource/Horace> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1384081/10/4> ;
        ont:subject       <http://dbpedia.org/resource/Hampton_Hawes> .

