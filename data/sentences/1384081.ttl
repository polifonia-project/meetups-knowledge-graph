        ont:subject         <http://dbpedia.org/resource/Hampton_Hawes> ;
        ont:timeExpression  "1957" .

<http://w3id.org/polifonia/pilot/meetups/1384081/10/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Hawes' playing style developed in the early 1950s" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1384081/10> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1384081/10/0> ;
