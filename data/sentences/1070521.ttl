        ont:timeExpression  "April 8 , 2008" .

<http://w3id.org/polifonia/pilot/meetups/1070521/14/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Later, after retirement, Kyser and Henry got together to share music world memories." ;
