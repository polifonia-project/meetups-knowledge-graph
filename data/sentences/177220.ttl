<http://w3id.org/polifonia/pilot/meetups/177220/7/8>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Bob Dylan called \"Vietnam\" the best protest song he had ever heard." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/177220/7> ;
        ont:person        <http://dbpedia.org/resource/Bob_Dylan> ;
