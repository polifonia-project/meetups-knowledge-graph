<http://w3id.org/polifonia/pilot/meetups/177220/6/4>
        rdf:type            ont:Sentence ;
        ont:excerpt         "It was produced by Kong, with whom Cliff remained until Kong's death from a heart attack in 1971." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/177220/6> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/177220/6/3> ;
        ont:subject         <http://dbpedia.org/resource/Jimmy_Cliff> ;
        ont:timeExpression  "heart attack in 1971" .

<http://w3id.org/polifonia/pilot/meetups/177220/15/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Rebirth was nominated for a Grammy Award for 'Best Reggae Album'." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/177220/15> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/177220/15/1> ;
        ont:subject       <http://dbpedia.org/resource/Jimmy_Cliff> .

<http://w3id.org/polifonia/pilot/meetups/177220/10/3>
        rdf:type            ont:Sentence ;
        ont:excerpt         "The follow-up, Cliff Hanger (1985), won a Grammy Award for 'Best Reggae Album', though it was his last major success in the United States until 1993." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/177220/10> ;
        ont:place           <http://dbpedia.org/resource/United_States> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/177220/10/2> ;
        ont:subject         <http://dbpedia.org/resource/Jimmy_Cliff> ;
        ont:timeExpression  "1985" .

