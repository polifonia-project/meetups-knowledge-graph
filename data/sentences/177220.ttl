        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1997, Cliff was a guest star in an episode of the Cartoon Network talk show Space Ghost Coast to Coast." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/177220/11> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/177220/11/4> ;
        ont:subject       <http://dbpedia.org/resource/Jimmy_Cliff> .

<http://w3id.org/polifonia/pilot/meetups/177220/8/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1972, Cliff starred as Ivanhoe \"Ivan\" Martin in the classic reggae film, The Harder They Come, directed by Perry Henzell." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/177220/8> ;
