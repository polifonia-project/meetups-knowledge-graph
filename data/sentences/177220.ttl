<http://w3id.org/polifonia/pilot/meetups/177220/8/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Arriving in Kingston from the country, he tries to make it in the recording business, but without success." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/177220/8> ;
        ont:place         <http://dbpedia.org/resource/Kingston_upon_Thames> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/177220/8/1> ;
        ont:subject       <http://dbpedia.org/resource/Jimmy_Cliff> .

<http://w3id.org/polifonia/pilot/meetups/177220/17/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Cliff is not a member of the Rastafari movement, although he briefly was before converting to Islam from Christianity." ;
