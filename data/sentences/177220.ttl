<http://w3id.org/polifonia/pilot/meetups/177220/11/1>
        rdf:type            ont:Sentence ;
        ont:excerpt         "His recording of \"You Can Get It If You Really Want\" was used as a campaign anthem by the Sandinista National Liberation Front in the 1990 election in Nicaragua." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/177220/11> ;
        ont:place           <http://dbpedia.org/resource/Nicaragua> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/177220/11/0> ;
        ont:subject         <http://dbpedia.org/resource/Jimmy_Cliff> ;
        ont:timeExpression  "1990 election" .

<http://w3id.org/polifonia/pilot/meetups/177220/14/6>
        rdf:type          ont:Sentence ;
        ont:excerpt       "I don't support any politician." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/177220/14> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/177220/14/5> ;
        ont:subject       <http://dbpedia.org/resource/Jimmy_Cliff> .

<http://w3id.org/polifonia/pilot/meetups/177220/6/2>
        rdf:type            ont:Sentence ;
        ont:excerpt         "\"One night I was walking past a record store and restaurant as they were closing, pushed myself in and convinced one of them, Leslie Kong, to go into the recording business, starting with me,\" he writes in his own website biography." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/177220/6> ;
        ont:person          <http://dbpedia.org/resource/Leslie_Kong> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/177220/6/1> ;
        ont:subject         <http://dbpedia.org/resource/Jimmy_Cliff> ;
        ont:timeExpression  "one of" , "One night" .

<http://w3id.org/polifonia/pilot/meetups/177220/3>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/177220/3/0> ;
