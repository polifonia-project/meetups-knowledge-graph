        ont:subject       <http://dbpedia.org/resource/Jimmy_Cliff> .

<http://w3id.org/polifonia/pilot/meetups/177220/11/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1991, he performed at the second Rock in Rio festival in Estádio do Maracanã in Rio de Janeiro, Brazil." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/177220/11> ;
        ont:place         <http://dbpedia.org/resource/Rio_de_Janeiro> , <http://dbpedia.org/resource/Maracanã_Stadium> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/177220/11/1> ;
        ont:subject       <http://dbpedia.org/resource/Jimmy_Cliff> .

<http://w3id.org/polifonia/pilot/meetups/177220/14/7>
        rdf:type          ont:Sentence ;
        ont:excerpt       "I just believe in right or wrong." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/177220/14> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/177220/14/6> ;
