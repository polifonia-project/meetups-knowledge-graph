<http://w3id.org/polifonia/pilot/meetups/741791/18/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "In 1948, Davis married actress Ruby Dee, whom he had met on the set of Robert Ardrey's 1946 play Jeb." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/741791/18> ;
        ont:person          <http://dbpedia.org/resource/Robert_Ardrey> , <http://dbpedia.org/resource/Ruby_Dee> ;
