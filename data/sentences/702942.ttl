<http://w3id.org/polifonia/pilot/meetups/702942/1/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In the early 1960s, the Cambridge, Massachusetts-born Sharpe collaborated with Jim Coyle to create a series of comic on-the-street interviews for San Francisco radio station KGO." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/702942/1> ;
        ont:place         <http://dbpedia.org/resource/Cambridge> , <http://dbpedia.org/resource/San_Francisco> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/702942/1/0> ;
        ont:subject       <http://dbpedia.org/resource/Mal_Sharpe> .

<http://w3id.org/polifonia/pilot/meetups/702942/8/0>
        rdf:type          ont:Sentence ;
