        rdf:type            ont:Sentence ;
        ont:excerpt         "During 2004, he released a DVD and a live 16-track CD Greatest Hits Live that was recorded August 2003 at the Great American Music Hall in San Francisco." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/630138/11> ;
        ont:place           <http://dbpedia.org/resource/San_Francisco> , <http://dbpedia.org/resource/Great_American_Music_Hall> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/630138/11/2> ;
        ont:subject         <http://dbpedia.org/resource/Boz_Scaggs> ;
        ont:timeExpression  "August 2003" .

