        rdf:type            ont:Sentence ;
        ont:excerpt         "He became prominent for his series of albums in the late 1970s, and songs \"Lido Shuffle\" and \"Lowdown\" from Silk Degrees (1976), which peaked at No. 2 on the Billboard 200." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/630138/0> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/630138/0/0> ;
        ont:subject         <http://dbpedia.org/resource/Boz_Scaggs> ;
        ont:timeExpression  "Billboard 200" , "2 on" , "1976" .

