        rdf:type            ont:Sentence ;
        ont:excerpt         "This version, however, only bubbled under the Billboard 200, reaching No. 209." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/630138/18> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/630138/18/1> ;
