<http://w3id.org/polifonia/pilot/meetups/1392224/4/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "The pop art culture in the 1960s embraced Marisol as one of its members, enhancing her recognition and popularity." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1392224/4> ;
        ont:person          <http://dbpedia.org/resource/Marisol_Escobar> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1392224/4/0> ;
        ont:subject         <http://dbpedia.org/resource/Marisol_Escobar> ;
        ont:timeExpression  "one of" .

<http://w3id.org/polifonia/pilot/meetups/1392224/2/8>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Although Marisol was deeply traumatized, this did not affect her artistic talents." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1392224/2> ;
        ont:person        <http://dbpedia.org/resource/Marisol_Escobar> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1392224/2/7> ;
        ont:subject       <http://dbpedia.org/resource/Marisol_Escobar> .

<http://w3id.org/polifonia/pilot/meetups/1392224/12/21>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Marisol's wit was disregarded as feminine playfulness, therefore, lacking the objectivity and expressionless attitude of male pop artists." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1392224/12> ;
        ont:person        <http://dbpedia.org/resource/Marisol_Escobar> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1392224/12/20> ;
        ont:subject       <http://dbpedia.org/resource/Marisol_Escobar> .

