        rdf:_3    <http://w3id.org/polifonia/pilot/meetups/1392224/18/2> .

<http://w3id.org/polifonia/pilot/meetups/1392224/12/10>
        rdf:type          ont:Sentence ;
        ont:excerpt       "At this time, her sculpture was recognized relative to certain pop objectives." ;
