<http://w3id.org/polifonia/pilot/meetups/1392224/10/19>
        rdf:type            ont:Sentence ;
        ont:excerpt         "The sculpture was featured on the March 3, 1967 cover of Time magazine." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1392224/10> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1392224/10/18> ;
        ont:subject         <http://dbpedia.org/resource/Marisol_Escobar> ;
        ont:timeExpression  "March 3 , 1967" .

<http://w3id.org/polifonia/pilot/meetups/1392224/2/11>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Marisol additionally displayed talent in embroidery, spending at least three years embroidering the corner of a tablecloth (including going to school on Sundays in order to work)." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1392224/2> ;
        ont:person        <http://dbpedia.org/resource/Marisol_Escobar> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1392224/2/10> ;
        ont:subject       <http://dbpedia.org/resource/Marisol_Escobar> .

<http://w3id.org/polifonia/pilot/meetups/1392224/6/5>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Through a crude combination of materials, Marisol symbolized the artist's denial of any consistent existence of \"essential\" femininity." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1392224/6> ;
        ont:person        <http://dbpedia.org/resource/Marisol_Escobar> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1392224/6/4> ;
        ont:subject       <http://dbpedia.org/resource/Marisol_Escobar> .

<http://w3id.org/polifonia/pilot/meetups/1392224/2/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Maria Sol Escobar was born on May 22, 1930, to Venezuelan parents in Paris, France." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1392224/2> ;
        ont:person          <http://dbpedia.org/resource/Kelvim_Escobar> ;
        ont:place           <http://dbpedia.org/resource/France> , <http://dbpedia.org/resource/Paris> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1392224/2/0> ;
        ont:subject         <http://dbpedia.org/resource/Marisol_Escobar> ;
        ont:timeExpression  "May 22 , 1930" .

<http://w3id.org/polifonia/pilot/meetups/1392224/21>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/1392224/21/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/1392224/21/1> .

<http://w3id.org/polifonia/pilot/meetups/1392224/12/13>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Through an objective attitude, she claimed an artist could maintain a position of 'masculine' detachment from the subjects being depicted." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1392224/12> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1392224/12/12> ;
        ont:subject       <http://dbpedia.org/resource/Marisol_Escobar> .

<http://w3id.org/polifonia/pilot/meetups/1392224/20/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Her work is included in the collections of the Albright-Knox Art Gallery, The Metropolitan Museum of Art, the Currier Museum of Art, ICA Boston, and the Museum of Modern Art." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1392224/20> ;
        ont:place         <http://dbpedia.org/resource/Institute_of_Contemporary_Art,_Boston> , <http://dbpedia.org/resource/Albrightâ€“Knox_Art_Gallery> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1392224/20/0> ;
        ont:subject       <http://dbpedia.org/resource/Marisol_Escobar> .

<http://w3id.org/polifonia/pilot/meetups/1392224/8/39>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Manipulating his crucial characteristics, mannerisms, and attributes to effectively subvert his position of power as one of vulnerability." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1392224/8> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1392224/8/38> ;
        ont:subject         <http://dbpedia.org/resource/Marisol_Escobar> ;
        ont:timeExpression  "power as one" .

