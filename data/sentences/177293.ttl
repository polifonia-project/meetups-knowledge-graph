<http://w3id.org/polifonia/pilot/meetups/177293/15/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "The first music he was exposed to included The Beatles and street performers in various metropolitan areas of Bahia." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/177293/15> ;
        ont:place         <http://dbpedia.org/resource/Bahia> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/177293/15/2> ;
        ont:subject       <http://dbpedia.org/resource/Gilberto_Gil> .

<http://w3id.org/polifonia/pilot/meetups/177293/3/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "His father, José Gil Moreira, was a doctor; his mother, Claudina Passos Gil Moreira, an elementary school teacher." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/177293/3> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/177293/3/1> ;
        ont:subject       <http://dbpedia.org/resource/Gilberto_Gil> .

<http://w3id.org/polifonia/pilot/meetups/177293/0>
