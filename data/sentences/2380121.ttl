        rdf:type            ont:Sentence ;
        ont:excerpt         "He also conducted the first recording of Barry Cooper's realisation of Ludwig van Beethoven's Symphony No. 10 in 1988." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/2380121/2> ;
        ont:person          <http://dbpedia.org/resource/Ludwig_van_Beethoven> , <http://dbpedia.org/resource/Barry_Cooper_(musicologist)> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/2380121/2/3> ;
        ont:subject         <http://dbpedia.org/resource/Wyn_Morris> ;
        ont:timeExpression  "10 in" .

