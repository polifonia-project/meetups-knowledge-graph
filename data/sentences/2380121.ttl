        rdf:type          ont:Sentence ;
        ont:excerpt       "Others observed that he was essentially a maestro out of time; his wilful, demanding behaviour belonged to an era when conductors could command orchestras in such a manner, and hence ultimately found himself out of favour." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2380121/5> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2380121/5/1> ;
