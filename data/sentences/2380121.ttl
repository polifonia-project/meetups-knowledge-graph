        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2380121/6> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2380121/6/0> ;
        ont:subject       <http://dbpedia.org/resource/Wyn_Morris> .

<http://w3id.org/polifonia/pilot/meetups/2380121/2/3>
        rdf:type            ont:Sentence ;
        ont:excerpt         "He was the first to record Deryck Cooke's second performing version of Mahler's Symphony No. 10 in 1972, only the third time a recording of the work had been made." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/2380121/2> ;
        ont:person          <http://dbpedia.org/resource/Deryck_Harrison> , <http://dbpedia.org/resource/Gustav_Mahler> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/2380121/2/2> ;
        ont:subject         <http://dbpedia.org/resource/Wyn_Morris> ;
        ont:timeExpression  "10 in" .

<http://w3id.org/polifonia/pilot/meetups/2380121/4>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/2380121/4/0> .

<http://w3id.org/polifonia/pilot/meetups/2380121/5/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Morris could be somewhat abrasive in style, both with orchestras and administrators." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2380121/5> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2380121/5/0> ;
        ont:subject       <http://dbpedia.org/resource/Wyn_Morris> .

<http://w3id.org/polifonia/pilot/meetups/2380121/2/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He was especially acknowledged for his interpretations of Gustav Mahler's works, which he recorded almost complete during  the 1960s and 1970s." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2380121/2> ;
        ont:person        <http://dbpedia.org/resource/Gustav_Mahler> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2380121/2/1> ;
        ont:subject       <http://dbpedia.org/resource/Wyn_Morris> .

<http://w3id.org/polifonia/pilot/meetups/2380121/3/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He was responsible for the first British performance of Sergei Rachmaninoff's All-Night Vigil (often referred to as his Vespers)." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2380121/3> ;
        ont:person        <http://dbpedia.org/resource/Sergei_Rachmaninoff> ;
        ont:place         <http://dbpedia.org/resource/United_Kingdom> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2380121/3/0> ;
        ont:subject       <http://dbpedia.org/resource/Wyn_Morris> .

<http://w3id.org/polifonia/pilot/meetups/2380121/4/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He founded the Symphonica of London in 1965, with which he recorded several of the Mahler symphonies." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2380121/4> ;
        ont:person        <http://dbpedia.org/resource/Gustav_Mahler> ;
        ont:place         <http://dbpedia.org/resource/London> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2380121/4/0> ;
        ont:subject       <http://dbpedia.org/resource/Wyn_Morris> .

