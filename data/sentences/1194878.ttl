        rdf:_4    <http://w3id.org/polifonia/pilot/meetups/1194878/11/3> ;
        rdf:_5    <http://w3id.org/polifonia/pilot/meetups/1194878/11/4> ;
        rdf:_6    <http://w3id.org/polifonia/pilot/meetups/1194878/11/5> .

<http://w3id.org/polifonia/pilot/meetups/1194878/9/5>
        rdf:type          ont:Sentence ;
        ont:excerpt       "During this time, Melcher dabbled in real estate and served as the executive producer of his mother's CBS series, The Doris Day Show." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1194878/9> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1194878/9/4> ;
        ont:subject       <http://dbpedia.org/resource/Terry_Melcher> .

<http://w3id.org/polifonia/pilot/meetups/1194878/11/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "The single was certified gold for U.S. sales of more than one million copies." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1194878/11> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1194878/11/3> ;
        ont:subject       <http://dbpedia.org/resource/Terry_Melcher> .

<http://w3id.org/polifonia/pilot/meetups/1194878/0/1>
        rdf:type            ont:Sentence ;
        ont:excerpt         "His best-known contributions were producing the Byrds' first two albums Mr. Tambourine Man (1965) and Turn!" ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1194878/0> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1194878/0/0> ;
        ont:subject         <http://dbpedia.org/resource/Terry_Melcher> ;
        ont:timeExpression  "1965" .

<http://w3id.org/polifonia/pilot/meetups/1194878/5/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Melcher has been credited with helping to shape the sound of 1960s surf music in California." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1194878/5> ;
        ont:place           <http://dbpedia.org/resource/California> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1194878/5/0> ;
        ont:subject         <http://dbpedia.org/resource/Terry_Melcher> ;
        ont:timeExpression  "sound of 1960s" .

