<http://w3id.org/polifonia/pilot/meetups/1194878/8/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "==" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1194878/8> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1194878/8/0> ;
        ont:subject       <http://dbpedia.org/resource/Terry_Melcher> .

<http://w3id.org/polifonia/pilot/meetups/1194878/3/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Known as \"Terry\", the boy was named by his mother after the hero of her favorite childhood comic strip, Terry and the Pirates." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1194878/3> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1194878/3/0> ;
        ont:subject       <http://dbpedia.org/resource/Terry_Melcher> .

<http://w3id.org/polifonia/pilot/meetups/1194878/17>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/1194878/17/0> .

<http://w3id.org/polifonia/pilot/meetups/1194878/7/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1968, Beach Boy Dennis Wilson introduced Melcher to ex-con and aspiring musician Charles Manson." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1194878/7> ;
        ont:person        <http://dbpedia.org/resource/Charles_Manson> , <http://dbpedia.org/resource/Woodrow_Wilson> , <http://dbpedia.org/resource/Dennis_Wilson> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1194878/7/0> ;
        ont:subject       <http://dbpedia.org/resource/Terry_Melcher> .

<http://w3id.org/polifonia/pilot/meetups/1194878/2/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Background ==" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1194878/2> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1194878/2/0> ;
        ont:subject       <http://dbpedia.org/resource/Terry_Melcher> .

<http://w3id.org/polifonia/pilot/meetups/1194878/5/6>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He produced their hit cover versions of Bob Dylan's \"Mr. Tambourine Man\" and Pete Seeger's \"Turn!" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1194878/5> ;
        ont:person        <http://dbpedia.org/resource/Bob_Dylan> , <http://dbpedia.org/resource/Pete_Seeger> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1194878/5/5> ;
        ont:subject       <http://dbpedia.org/resource/Terry_Melcher> .

<http://w3id.org/polifonia/pilot/meetups/1194878/7/17>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Manson family member Susan Atkins, who admitted her part in the murders, stated to police and before a grand jury that the house was chosen as the scene for the murders \"to instill fear into Terry Melcher because Terry had given us his word on a few things and never came through with them\"." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1194878/7> ;
        ont:person        <http://dbpedia.org/resource/Terry_Melcher> , <http://dbpedia.org/resource/Susan_Atkins> ;
        ont:place         <http://dbpedia.org/resource/Manson_Family> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1194878/7/16> ;
        ont:subject       <http://dbpedia.org/resource/Terry_Melcher> .

<http://w3id.org/polifonia/pilot/meetups/1194878/6/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "==" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1194878/6> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1194878/6/0> ;
        ont:subject       <http://dbpedia.org/resource/Terry_Melcher> .

<http://w3id.org/polifonia/pilot/meetups/1194878/1/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Most of his early recordings were with the vocal surf acts the Rip Chords and Bruce & Terry." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1194878/1> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1194878/1/0> ;
        ont:subject       <http://dbpedia.org/resource/Terry_Melcher> .

<http://w3id.org/polifonia/pilot/meetups/1194878/11>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/1194878/11/0> ;
