        ont:timeExpression  "1991" .

<http://w3id.org/polifonia/pilot/meetups/125311/8/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Sutcliffe was smitten and began dating Kirchherr shortly afterwards." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/125311/8> ;
