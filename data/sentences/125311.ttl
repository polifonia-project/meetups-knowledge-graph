        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/125311/0/1> ;
        ont:subject       <http://dbpedia.org/resource/Stu_Sutcliffe> .

<http://w3id.org/polifonia/pilot/meetups/125311/12/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Kirchherr's mother had German doctors examine him, but they were unable to diagnose the exact cause of the headaches." ;
