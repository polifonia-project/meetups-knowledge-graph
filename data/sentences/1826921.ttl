        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1826921/5/5> ;
        ont:subject         <http://dbpedia.org/resource/Tillman_Franks> ;
        ont:timeExpression  "November 5 , 1960" .

<http://w3id.org/polifonia/pilot/meetups/1826921/6/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Franks' contribution to rock and roll music has been recognized by his induction into the Rockabilly Hall of Fame, the Louisiana Hall of Fame, and his induction in 2003 into the Northwest Louisiana Walk of Stars where his feet and hand impressions are in concrete beside other talents, such as Elvis Presley, Terry Bradshaw, Kix Brooks, David Toms, and Franks' longtime friend Claude King." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1826921/6> ;
        ont:person          <http://dbpedia.org/resource/Kix_Brooks> , <http://dbpedia.org/resource/Elvis_Presley> , <http://dbpedia.org/resource/David_Toms> , <http://dbpedia.org/resource/Terry_Bradshaw> , <http://dbpedia.org/resource/Claude_King> ;
        ont:place           <http://dbpedia.org/resource/Louisiana> , <http://dbpedia.org/resource/Rockabilly_Hall_of_Fame> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1826921/6/0> ;
        ont:subject         <http://dbpedia.org/resource/Tillman_Franks> ;
        ont:timeExpression  "induction in 2003" .

