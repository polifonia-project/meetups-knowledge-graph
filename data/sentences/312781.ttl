<http://w3id.org/polifonia/pilot/meetups/312781/5/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "He entered the 1933 Vienna International Piano Competition and finished second to Polish pianist Bolesław Kon, some say controversially." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/312781/5> ;
        ont:place           <http://dbpedia.org/resource/Vienna> , <http://dbpedia.org/resource/Poland> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/312781/5/0> ;
        ont:subject         <http://dbpedia.org/resource/Dinu_Lipatti> ;
        ont:timeExpression  "1933" .

<http://w3id.org/polifonia/pilot/meetups/312781/49>
        rdf:type  ont:Paragraph ;
