        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/312781/28/0> .

<http://w3id.org/polifonia/pilot/meetups/312781/12/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "In recognition of his outstanding contributions to classical music interpretation and composition he was posthumously elected in 1997 as a member of the Romanian Academy." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/312781/12> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/312781/12/0> ;
        ont:subject         <http://dbpedia.org/resource/Dinu_Lipatti> ;
        ont:timeExpression  "1997 as" .

<http://w3id.org/polifonia/pilot/meetups/312781/10/8>
        rdf:type            ont:Sentence ;
        ont:excerpt         "A recording of Chopin's Piano Concerto No. 1 in E minor, originally released under Lipatti's name, and said to have been a recording of a live performance in Switzerland in May 1948, proved not to be his contribution at all." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/312781/10> ;
        ont:person          <http://dbpedia.org/resource/Frédéric_Chopin> ;
        ont:place           <http://dbpedia.org/resource/Switzerland> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/312781/10/7> ;
        ont:subject         <http://dbpedia.org/resource/Dinu_Lipatti> ;
        ont:timeExpression  "May 1948" , "1 in" .

<http://w3id.org/polifonia/pilot/meetups/312781/6/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Lipatti's career was interrupted by World War II." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/312781/6> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/312781/6/0> ;
