<http://w3id.org/polifonia/pilot/meetups/312781/6/1>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Although he gave concerts across the Nazi-occupied territories, as the war grew closer he fled his native Romania in September 1943 with his companion and fellow pianist, Madeleine Cantacuzene." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/312781/6> ;
        ont:person          <http://dbpedia.org/resource/Madeleine_of_Valois> ;
        ont:place           <http://dbpedia.org/resource/Romania> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/312781/6/0> ;
        ont:subject         <http://dbpedia.org/resource/Dinu_Lipatti> ;
        ont:timeExpression  "September 1943" .

<http://w3id.org/polifonia/pilot/meetups/312781/10/10>
        rdf:type          ont:Sentence ;
        ont:excerpt       "However, later on, an authentic recording by Lipatti of the Chopin Concerto was found." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/312781/10> ;
        ont:person        <http://dbpedia.org/resource/Frédéric_Chopin> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/312781/10/9> ;
        ont:subject       <http://dbpedia.org/resource/Dinu_Lipatti> .
