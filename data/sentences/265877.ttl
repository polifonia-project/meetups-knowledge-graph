<http://w3id.org/polifonia/pilot/meetups/265877/11/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Garden's performances met with considerable critical acclaim." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/265877/11> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/265877/11/3> ;
