<http://w3id.org/polifonia/pilot/meetups/265877/0/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Mary Garden (20 February 1874 – 3 January 1967) was a Scottish operatic soprano with a substantial career in France and America in the first third of the 20th century." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/265877/0> ;
        ont:person          <http://dbpedia.org/resource/Mary_Garden> , <http://dbpedia.org/resource/Soprano> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/265877/0/0> ;
        ont:subject         <http://dbpedia.org/resource/Mary_Garden> ;
        ont:timeExpression  "20 February 1874" , "3 January 1967" .

<http://w3id.org/polifonia/pilot/meetups/265877/0/11>
        rdf:type            ont:Sentence ;
        ont:excerpt         "She first worked with the Chicago Grand Opera Company (1910–1913) and then joined the Chicago Opera Association in 1915, ultimately becoming the company's director in 1921." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/265877/0> ;
        ont:place           <http://dbpedia.org/resource/Chicago_Grand_Opera_Company> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/265877/0/10> ;
        ont:subject         <http://dbpedia.org/resource/Mary_Garden> ;
        ont:timeExpression  "1910–1913" , "director in 1921" .

<http://w3id.org/polifonia/pilot/meetups/265877/23>
