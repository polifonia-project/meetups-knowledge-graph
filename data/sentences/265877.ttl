<http://w3id.org/polifonia/pilot/meetups/265877/15/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "By 1910, Garden had become a household name within America." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/265877/15> ;
