<http://w3id.org/polifonia/pilot/meetups/265877/0/5>
        rdf:type            ont:Sentence ;
        ont:excerpt         "She became the leading soprano at the Opéra-Comique; notably portraying roles in several world premieres, including Mélisande in Claude Debussy's Pelléas et Mélisande (1902)." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/265877/0> ;
        ont:person          <http://dbpedia.org/resource/Claude_Debussy> , <http://dbpedia.org/resource/Soprano> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/265877/0/4> ;
        ont:subject         <http://dbpedia.org/resource/Mary_Garden> ;
        ont:timeExpression  "1902" .

<http://w3id.org/polifonia/pilot/meetups/265877/26/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Garden and Ruby both died on January 3, 1967." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/265877/26> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/265877/26/0> ;
