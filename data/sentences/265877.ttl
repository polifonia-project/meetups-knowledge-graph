        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1930 she sang in the world premiere of Hamilton Forrest's Camille, an opera based on Dumas' The Lady of the Camellias." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/265877/16> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/265877/16/7> ;
