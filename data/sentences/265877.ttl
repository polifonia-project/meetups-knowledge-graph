<http://w3id.org/polifonia/pilot/meetups/265877/22/6>
        rdf:type            ont:Sentence ;
        ont:excerpt         "In the 1930s, Garden appeared on Cecil B. DeMille's Lux Radio Theater." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/265877/22> ;
        ont:person          <http://dbpedia.org/resource/Cecil_Rhodes> , <http://dbpedia.org/resource/Cecil_B._DeMille> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/265877/22/5> ;
        ont:subject         <http://dbpedia.org/resource/Mary_Garden> ;
        ont:timeExpression  "In the 1930s" .

<http://w3id.org/polifonia/pilot/meetups/265877/5>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/265877/5/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/265877/5/1> ;
        rdf:_3    <http://w3id.org/polifonia/pilot/meetups/265877/5/2> .

<http://w3id.org/polifonia/pilot/meetups/265877/6/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Mary Garden was born on 20 February 1874 at 35 Charlotte Street, Aberdeen, Scotland, the oldest of the four daughters of Robert Davidson Garden (b. 1851), a cashier at the Blaikie ironworks, and his wife Mary (née Joss) (1852–1948)." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/265877/6> ;
        ont:person          <http://dbpedia.org/resource/Robert_Davidson_(artist)> , <http://dbpedia.org/resource/Mary_Garden> , <http://dbpedia.org/resource/Mary_II_of_England> ;
        ont:place           <http://dbpedia.org/resource/Aberdeen> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/265877/6/0> ;
        ont:subject         <http://dbpedia.org/resource/Mary_Garden> ;
        ont:timeExpression  "20 February 1874" , "1852–1948" , "1851" , "of the four" .

<http://w3id.org/polifonia/pilot/meetups/265877/10/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Impressed with her voice, Carré invited her to join the roster at the Opéra-Comique in 1900." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/265877/10> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/265877/10/0> ;
        ont:subject       <http://dbpedia.org/resource/Mary_Garden> .

<http://w3id.org/polifonia/pilot/meetups/265877/11/11>
        rdf:type          ont:Sentence ;
        ont:excerpt       "The following year she returned to the Opéra-Comique to sing Chrysis in the world premiere of Camille Erlanger's Aphrodite." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/265877/11> ;
        ont:person        <http://dbpedia.org/resource/Camille_Erlanger> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/265877/11/10> ;
        ont:subject       <http://dbpedia.org/resource/Mary_Garden> .

<http://w3id.org/polifonia/pilot/meetups/265877/0/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "She was particularly admired for her nuanced performances which employed interesting uses of vocal color." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/265877/0> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/265877/0/2> ;
        ont:subject       <http://dbpedia.org/resource/Mary_Garden> .

<http://w3id.org/polifonia/pilot/meetups/265877/0/14>
        rdf:type          ont:Sentence ;
        ont:excerpt       "She sang roles at the Civic Opera until 1931, notably in several United States and world premieres." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/265877/0> ;
        ont:place         <http://dbpedia.org/resource/United_States> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/265877/0/13> ;
        ont:subject       <http://dbpedia.org/resource/Mary_Garden> .

<http://w3id.org/polifonia/pilot/meetups/265877/26>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/265877/26/0> .

<http://w3id.org/polifonia/pilot/meetups/265877/7/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "====" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/265877/7> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/265877/7/0> ;
        ont:subject       <http://dbpedia.org/resource/Mary_Garden> .

<http://w3id.org/polifonia/pilot/meetups/265877/11/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "After her debut, Garden quickly became one of the leading sopranos at the Opéra-Comique." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/265877/11> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/265877/11/0> ;
        ont:subject         <http://dbpedia.org/resource/Mary_Garden> ;
        ont:timeExpression  "one of" .

<http://w3id.org/polifonia/pilot/meetups/265877/16/5>
        rdf:type            ont:Sentence ;
        ont:excerpt         "During World War I she was decorated by the French and Serbian governments and made a Chevalier (knight) of the Légion d'Honneur in 1921." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/265877/16> ;
        ont:place           <http://dbpedia.org/resource/France> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/265877/16/4> ;
        ont:subject         <http://dbpedia.org/resource/Mary_Garden> ;
        ont:timeExpression  "d'Honneur in 1921" .

<http://w3id.org/polifonia/pilot/meetups/265877/1/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "She retired to Scotland and in 1951 published a successful autobiography, Mary Garden's Story." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/265877/1> ;
        ont:person        <http://dbpedia.org/resource/Mary_Garden> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/265877/1/2> ;
        ont:subject       <http://dbpedia.org/resource/Mary_Garden> .

<http://w3id.org/polifonia/pilot/meetups/265877/20/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Always prone to embellish and exaggerate, Garden was already succumbing to dementia when the manuscript was being prepared." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/265877/20> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/265877/20/0> ;
        ont:subject       <http://dbpedia.org/resource/Mary_Garden> .

<http://w3id.org/polifonia/pilot/meetups/265877/8/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "She showed promise as a young singer, and studied with Sarah Robinson-Duff in Chicago under the financial support of wealthy patrons David and Florence Mayer." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/265877/8> ;
        ont:person        <http://dbpedia.org/resource/Sarah> , <http://dbpedia.org/resource/John_Mayer> ;
        ont:place         <http://dbpedia.org/resource/Chicago> , <http://dbpedia.org/resource/Florence> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/265877/8/0> ;
        ont:subject       <http://dbpedia.org/resource/Mary_Garden> .

