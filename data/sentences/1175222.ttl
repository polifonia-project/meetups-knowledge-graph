        ont:place         <http://dbpedia.org/resource/Nazi_Germany> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1175222/4/0> ;
        ont:subject       <http://dbpedia.org/resource/Hans_Albers> .

<http://w3id.org/polifonia/pilot/meetups/1175222/2/10>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Albers himself shot to fame in 1930 with the movie The Copper and constantly enhanced his star status with similar daredevil roles in the 1930s." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1175222/2> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1175222/2/9> ;
        ont:subject         <http://dbpedia.org/resource/Hans_Albers> ;
        ont:timeExpression  "in the 1930s" , "1930 with" .

<http://w3id.org/polifonia/pilot/meetups/1175222/2/9>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Soon thereafter, Albers played big-mouthed strong man Mazeppa alongside Marlene Dietrich in her star-making classic Der blaue Engel (The Blue Angel)." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1175222/2> ;
        ont:person        <http://dbpedia.org/resource/Marlene_Dietrich> , <http://dbpedia.org/resource/Ivan_Mazepa> ;
