        ont:person          <http://dbpedia.org/resource/John_Peel> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/984155/4/0> ;
        ont:subject         <http://dbpedia.org/resource/Mike_Read> ;
        ont:timeExpression  "Radio 1" , "November 1978" .

