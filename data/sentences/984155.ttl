        rdf:type            ont:Sentence ;
        ont:excerpt         "In 1979, Read wrote and performed the pop-punk song \"High Rise\" (upon which his Radio 1 intro jingle was subsequently based) under the guise of The Trainspotters; following this in 1980 with \"My Town\" by his next studio group, The Ghosts." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/984155/18> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/984155/18/0> ;
        ont:subject         <http://dbpedia.org/resource/Mike_Read> ;
        ont:timeExpression  "1980 with" , "1 intro jingle" .

