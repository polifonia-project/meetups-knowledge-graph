        rdf:type            ont:Sentence ;
        ont:excerpt         "Having spoken three times at Conservative Party conferences, including entertaining guests at a Tory conference dinner in 2006 with a ten-minute political rap, Read said that he was asked to run for the Conservative Party nomination for the London Mayoral elections in 2008." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/984155/31> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/984155/31/0> ;
        ont:subject         <http://dbpedia.org/resource/Mike_Read> ;
        ont:timeExpression  "conference dinner in 2006" .

<http://w3id.org/polifonia/pilot/meetups/984155/19/3>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Performed by an ensemble of artists named One World Project, the single reached No. 4 in the UK singles chart." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/984155/19> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/984155/19/2> ;
        ont:subject         <http://dbpedia.org/resource/Mike_Read> ;
        ont:timeExpression  "4 in" .

