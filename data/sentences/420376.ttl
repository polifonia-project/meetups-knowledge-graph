<http://w3id.org/polifonia/pilot/meetups/420376/18/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "After the international success of \"Ue o Muite Arukō\", Sakamoto went on a world tour that lasted from summer of 1963 to the beginning of 1964." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/420376/18> ;
        ont:place           <http://dbpedia.org/resource/Asagiri,_Kumamoto> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/420376/18/0> ;
        ont:subject         <http://dbpedia.org/resource/Kyu_Sakamoto> ;
        ont:timeExpression  "beginning of 1964" , "summer of 1963" .

<http://w3id.org/polifonia/pilot/meetups/420376/27/1>
        rdf:type            ont:Sentence ;
        ont:excerpt         "It was also the first-ever Japanese language song to enter the Australian charts, where it reached #2." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/420376/27> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/420376/27/0> ;
        ont:subject         <http://dbpedia.org/resource/Kyu_Sakamoto> ;
        ont:timeExpression  "2" .

<http://w3id.org/polifonia/pilot/meetups/420376/12/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Sakamoto's career began to rise to expectations, where he ended his studies and left school." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/420376/12> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/420376/12/2> ;
        ont:subject       <http://dbpedia.org/resource/Kyu_Sakamoto> .

<http://w3id.org/polifonia/pilot/meetups/420376/19/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "He received his sole foreign Gold Record of the Recording Industry Association of America (RIAA) by Capitol Records on 15 May 1964 in Hotel Okura, Tokyo." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/420376/19> ;
        ont:place           <http://dbpedia.org/resource/Tokyo> , <http://dbpedia.org/resource/Hotel_Okura_Tokyo> ;
