<http://w3id.org/polifonia/pilot/meetups/420376/0/1>
        rdf:type            ont:Sentence ;
        ont:excerpt         "It reached number one in the United States Billboard Hot 100 in June 1963, making Sakamoto the first Asian recording artist to have a number one song on the chart." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/420376/0> ;
        ont:place           <http://dbpedia.org/resource/United_States> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/420376/0/0> ;
        ont:subject         <http://dbpedia.org/resource/Kyu_Sakamoto> ;
        ont:timeExpression  "one song" , "June 1963" , "one in" , "Hot 100" .

<http://w3id.org/polifonia/pilot/meetups/420376/17/2>
        rdf:type            ont:Sentence ;
        ont:excerpt         "In 1963, Capitol Records released the song in the US with the alternate title, eventually selling over one million copies, and remaining number one on the Billboard Hot 100 number one single for three weeks in June 1963." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/420376/17> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/420376/17/1> ;
        ont:subject         <http://dbpedia.org/resource/Kyu_Sakamoto> ;
        ont:timeExpression  "100 number" , "June 1963" , "one on" .

