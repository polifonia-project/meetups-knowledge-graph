        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/371878/19/0> .

<http://w3id.org/polifonia/pilot/meetups/371878/7/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Williams sang lead on several of the group's songs, and served as the main lead singer during the group's early years." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/371878/7> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/371878/7/2> ;
        ont:subject       <http://dbpedia.org/resource/Paul_Williams_(The_Temptations_singer)> .

<http://w3id.org/polifonia/pilot/meetups/371878/2/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "As teenagers, Williams, Kendricks, and Kell Osborne and Willie Waller performed in a secular singing group known as The Cavaliers, with dreams of making it big in the music industry." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/371878/2> ;
        ont:person        <http://dbpedia.org/resource/Kell_Osborne> , <http://dbpedia.org/resource/Fats_Waller> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/371878/2/3> ;
        ont:subject       <http://dbpedia.org/resource/Paul_Williams_(The_Temptations_singer)> .

<http://w3id.org/polifonia/pilot/meetups/371878/13>
        rdf:type  ont:Paragraph ;
