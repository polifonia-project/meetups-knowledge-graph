        ont:subject       <http://dbpedia.org/resource/Paul_Williams_(The_Temptations_singer)> .

<http://w3id.org/polifonia/pilot/meetups/371878/14/5>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He was survived by his wife, Mary Agnes Williams, and five children: Sarita, Kenneth, Paula, Mary and Paul Jr." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/371878/14> ;
        ont:person        <http://dbpedia.org/resource/Paula_Hitler> , <http://dbpedia.org/resource/Mary_I_of_England> , <http://dbpedia.org/resource/Sarah_Stock> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/371878/14/4> ;
        ont:subject       <http://dbpedia.org/resource/Paul_Williams_(The_Temptations_singer)> .

