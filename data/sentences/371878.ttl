<http://w3id.org/polifonia/pilot/meetups/371878/10/5>
        rdf:type            ont:Sentence ;
        ont:excerpt         "\"In the spring of 1969, Williams and Brown opened the Celebrity House West, a celebrity fashion boutique, in downtown Detroit." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/371878/10> ;
        ont:place           <http://dbpedia.org/resource/Detroit> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/371878/10/4> ;
        ont:subject         <http://dbpedia.org/resource/Paul_Williams_(The_Temptations_singer)> ;
        ont:timeExpression  "spring of 1969" .

<http://w3id.org/polifonia/pilot/meetups/371878/10/12>
        rdf:type          ont:Sentence ;
        ont:excerpt       "When Williams was not well enough to go on, Street took his place onstage." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/371878/10> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/371878/10/11> ;
        ont:subject       <http://dbpedia.org/resource/Paul_Williams_(The_Temptations_singer)> .

<http://w3id.org/polifonia/pilot/meetups/371878/20>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/371878/20/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/371878/20/1> .

<http://w3id.org/polifonia/pilot/meetups/371878/8/2>
        rdf:type            ont:Sentence ;
        ont:excerpt         "The live version of the song \"Don't Look Back\" is also frequently cited as one of his standout performances." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/371878/8> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/371878/8/1> ;
        ont:subject         <http://dbpedia.org/resource/Paul_Williams_(The_Temptations_singer)> ;
        ont:timeExpression  "one of" .

<http://w3id.org/polifonia/pilot/meetups/371878/3/3>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Paul Williams and Kendricks joined Otis Williams, Melvin Franklin, and Elbridge Bryant to form The Elgins, who signed to the local Motown label in 1961, after first changing their name to The Temptations." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/371878/3> ;
        ont:person          <http://dbpedia.org/resource/Melvin_Franklin> , <http://dbpedia.org/resource/Elbridge_Bryant> , <http://dbpedia.org/resource/Paul_R._Williams> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/371878/3/2> ;
        ont:subject         <http://dbpedia.org/resource/Paul_Williams_(The_Temptations_singer)> ;
        ont:timeExpression  "label in 1961" .

<http://w3id.org/polifonia/pilot/meetups/371878/14/4>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Williams's funeral was held on August 24, with his family and former bandmates in attendance." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/371878/14> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/371878/14/3> ;
        ont:subject         <http://dbpedia.org/resource/Paul_Williams_(The_Temptations_singer)> ;
        ont:timeExpression  "August 24" .

