        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/371878/13/1> .

<http://w3id.org/polifonia/pilot/meetups/371878/0/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He was found dead two years later as the result of an apparent suicide." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/371878/0> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/371878/0/3> ;
