        rdf:type            ont:Sentence ;
        ont:excerpt         "In response, he was given lead vocals on the song, \"Don't Look Back\" (1965)." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/371878/7> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/371878/7/1> ;
