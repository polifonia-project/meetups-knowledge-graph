        ont:excerpt       "Personal problems and failing health forced Williams to retire in 1971." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/371878/0> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/371878/0/2> ;
