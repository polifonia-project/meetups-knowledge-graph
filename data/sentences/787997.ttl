<http://w3id.org/polifonia/pilot/meetups/787997/0/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Kenneth Earl Burrell (born July 31, 1931) is an American jazz guitarist known for his work on numerous top jazz labels: Prestige, Blue Note, Verve, CTI, Muse, and Concord." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/787997/0> ;
        ont:person          <http://dbpedia.org/resource/Earl> ;
        ont:place           <http://dbpedia.org/resource/United_States> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/787997/0/0> ;
        ont:subject         <http://dbpedia.org/resource/Kenny_Burrell> ;
        ont:timeExpression  "July 31 , 1931" .

