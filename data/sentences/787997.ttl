<http://w3id.org/polifonia/pilot/meetups/787997/0/1>
        rdf:type            ont:Sentence ;
        ont:excerpt         "His collaborations with Jimmy Smith were notable, and produced the 1965 Billboard Top Twenty hit Verve album Organ Grinder Swing." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/787997/0> ;
        ont:person          <http://dbpedia.org/resource/Jimmy_Smith_(musician)> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/787997/0/0> ;
        ont:subject         <http://dbpedia.org/resource/Kenny_Burrell> ;
        ont:timeExpression  "1965" .

<http://w3id.org/polifonia/pilot/meetups/787997/4/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Burrell toured with Oscar Peterson after graduating in 1955 and then moved to New York City in 1956 with pianist Tommy Flanagan." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/787997/4> ;
        ont:person          <http://dbpedia.org/resource/Tommy_Flanagan> , <http://dbpedia.org/resource/Dave_Burrell> , <http://dbpedia.org/resource/Oscar_Peterson> ;
        ont:place           <http://dbpedia.org/resource/New_York_City> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/787997/4/0> ;
        ont:subject         <http://dbpedia.org/resource/Kenny_Burrell> ;
        ont:timeExpression  "1956 with" .

<http://w3id.org/polifonia/pilot/meetups/787997/2/6>
        rdf:type          ont:Sentence ;
        ont:excerpt       "While in college, Burrell founded the New World Music Society collective with fellow Detroit musicians Pepper Adams, Donald Byrd, Elvin Jones, and Yusef Lateef." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/787997/2> ;
        ont:person        <http://dbpedia.org/resource/Yusef_Lateef> , <http://dbpedia.org/resource/Donald_Byrd> , <http://dbpedia.org/resource/Elvin_Jones> , <http://dbpedia.org/resource/Pepper_Adams> ;
        ont:place         <http://dbpedia.org/resource/Detroit> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/787997/2/5> ;
