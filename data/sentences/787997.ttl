<http://w3id.org/polifonia/pilot/meetups/787997/2/1>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Both his parents played instruments, and he began playing guitar at the age of 12 after listening to Charlie Christian's recordings." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/787997/2> ;
        ont:person          <http://dbpedia.org/resource/Charlie_Christian> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/787997/2/0> ;
        ont:subject         <http://dbpedia.org/resource/Kenny_Burrell> ;
        ont:timeExpression  "age of 12" .

<http://w3id.org/polifonia/pilot/meetups/787997/6/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Burrell wrote, arranged, and performed on the 1998 Grammy Award-winning album Dear Ella by Dee Dee Bridgewater, received the 2004 Jazz Educator of the Year Award from Down Beat, and was named a 2005 NEA Jazz Master." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/787997/6> ;
        ont:person          <http://dbpedia.org/resource/Dave_Burrell> , <http://dbpedia.org/resource/Dee_Dee_Bridgewater> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/787997/6/0> ;
        ont:subject         <http://dbpedia.org/resource/Kenny_Burrell> ;
        ont:timeExpression  "2004" , "on the 1998" , "2005" .

