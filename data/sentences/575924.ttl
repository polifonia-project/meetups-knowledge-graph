<http://w3id.org/polifonia/pilot/meetups/575924/12/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "From the later 1880s up to 1903, Tárrega continued composing and traveling, but limited his concerts to Spain." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/575924/12> ;
        ont:place           <http://dbpedia.org/resource/Spain> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/575924/12/0> ;
        ont:subject         <http://dbpedia.org/resource/Francisco_Tárrega> ;
        ont:timeExpression  "1880s up" .

<http://w3id.org/polifonia/pilot/meetups/575924/3/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Both his first music teachers, Eugeni Ruiz and Manuel González, were blind." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/575924/3> ;
        ont:person        <http://dbpedia.org/resource/Manuel_II_Palaiologos> , <http://dbpedia.org/resource/Eugeni_Valderrama> ;
        ont:place         <http://dbpedia.org/resource/Lez_(river)> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/575924/3/1> ;
        ont:subject       <http://dbpedia.org/resource/Francisco_Tárrega> .

<http://w3id.org/polifonia/pilot/meetups/575924/50/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "\"Kyuhee Park | Recuerdos de la Alhambra (F. Tárrega)\"" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/575924/50> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/575924/50/0> ;
        ont:subject       <http://dbpedia.org/resource/Francisco_Tárrega> .

<http://w3id.org/polifonia/pilot/meetups/575924/13/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "In January 1906, he was afflicted with paralysis on his right side, and though he would eventually return to performing, he never completely recovered." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/575924/13> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/575924/13/0> ;
        ont:subject         <http://dbpedia.org/resource/Francisco_Tárrega> ;
        ont:timeExpression  "January 1906" .

<http://w3id.org/polifonia/pilot/meetups/575924/4/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "The guitar was viewed as an instrument to accompany singers, while the piano was quite popular throughout Europe." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/575924/4> ;
        ont:place         <http://dbpedia.org/resource/Europe> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/575924/4/1> ;
        ont:subject       <http://dbpedia.org/resource/Francisco_Tárrega> .

<http://w3id.org/polifonia/pilot/meetups/575924/9/7>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Your family, perhaps?\"" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/575924/9> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/575924/9/6> ;
        ont:subject       <http://dbpedia.org/resource/Francisco_Tárrega> .

