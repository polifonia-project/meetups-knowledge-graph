        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/575924/20/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/575924/20/1> .

<http://w3id.org/polifonia/pilot/meetups/575924/30/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Danzas y cantos—terminología by Domingo Prat  (Buenos Aires: Romero y Fernández" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/575924/30> ;
        ont:person        <http://dbpedia.org/resource/J._C._Romero> ;
        ont:place         <http://dbpedia.org/resource/Buenos_Aires> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/575924/30/0> ;
        ont:subject       <http://dbpedia.org/resource/Francisco_Tárrega> .

<http://w3id.org/polifonia/pilot/meetups/575924/7/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "By this time he was composing his first works for guitar, which he played in addition to works of other composers." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/575924/7> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/575924/7/1> ;
        ont:subject       <http://dbpedia.org/resource/Francisco_Tárrega> .

<http://w3id.org/polifonia/pilot/meetups/575924/22/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "==" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/575924/22> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/575924/22/0> ;
        ont:subject       <http://dbpedia.org/resource/Francisco_Tárrega> .

<http://w3id.org/polifonia/pilot/meetups/575924/17/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Tárrega is considered to have laid the foundations for 20th-century classical guitar and for increasing interest in the guitar as a recital instrument." ;
