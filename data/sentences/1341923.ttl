        rdf:type          ont:Sentence ;
        ont:excerpt       "The couple had two children: Joshua and Ellen Rachel." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1341923/13> ;
        ont:person        <http://dbpedia.org/resource/Rachel> , <http://dbpedia.org/resource/Joshua> ;
