<http://w3id.org/polifonia/pilot/meetups/1341923/11/7>
        rdf:type            ont:Sentence ;
        ont:excerpt         "He co-hosted the 29th Tony Awards telecast that aired on ABC on April 20, 1975." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1341923/11> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1341923/11/6> ;
        ont:subject         <http://dbpedia.org/resource/Larry_Blyden> ;
        ont:timeExpression  "April 20 , 1975" , "29th" .

<http://w3id.org/polifonia/pilot/meetups/1341923/0/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1972, he won the Tony Award for Best Featured Actor in a Musical for his performance in the revival of A Funny Thing Happened on the Way to the Forum which he also produced." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1341923/0> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1341923/0/1> ;
        ont:subject       <http://dbpedia.org/resource/Larry_Blyden> .

<http://w3id.org/polifonia/pilot/meetups/1341923/16/10>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Goodson said that his production company, Goodson–Todman Productions, was initially informed that Blyden had been involved in a car accident while driving to Marrakesh and was hospitalized with serious injuries." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1341923/16> ;
        ont:person        <http://dbpedia.org/resource/Bill_Todman> ;
        ont:place         <http://dbpedia.org/resource/Marrakesh> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1341923/16/9> ;
        ont:subject       <http://dbpedia.org/resource/Larry_Blyden> .

<http://w3id.org/polifonia/pilot/meetups/1341923/8/6>
        rdf:type            ont:Sentence ;
        ont:excerpt         "He remained with the production for 250 performances." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1341923/8> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1341923/8/5> ;
        ont:subject         <http://dbpedia.org/resource/Larry_Blyden> ;
        ont:timeExpression  "production for 250" .

