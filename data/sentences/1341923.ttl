<http://w3id.org/polifonia/pilot/meetups/1341923/10/5>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Although the strike lasted just ten days, production on the series ceased." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1341923/10> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1341923/10/4> ;
        ont:subject       <http://dbpedia.org/resource/Larry_Blyden> .

<http://w3id.org/polifonia/pilot/meetups/1341923/7/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1958, Blyden replaced Larry Storch as \"Sammy Fong\" in the out-of-town tryouts for the musical Flower Drum Song." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1341923/7> ;
        ont:person        <http://dbpedia.org/resource/Scott_Storch> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1341923/7/3> ;
        ont:subject       <http://dbpedia.org/resource/Larry_Blyden> .

<http://w3id.org/polifonia/pilot/meetups/1341923/10/10>
        rdf:type          ont:Sentence ;
