<http://w3id.org/polifonia/pilot/meetups/1341923/4/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "After graduating with a Bachelor of Arts in 1948, Blyden moved to New York City to pursue an acting career." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1341923/4> ;
        ont:place         <http://dbpedia.org/resource/New_York_City> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1341923/4/2> ;
        ont:subject       <http://dbpedia.org/resource/Larry_Blyden> .

<http://w3id.org/polifonia/pilot/meetups/1341923/21>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/1341923/21/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/1341923/21/1> ;
        rdf:_3    <http://w3id.org/polifonia/pilot/meetups/1341923/21/2> ;
        rdf:_4    <http://w3id.org/polifonia/pilot/meetups/1341923/21/3> .

<http://w3id.org/polifonia/pilot/meetups/1341923/7/8>
        rdf:type            ont:Sentence ;
        ont:excerpt         "In November 1962, Blyden tried his hand at stage directing in the Broadway production of Harold, starring Anthony Perkins and Don Adams." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1341923/7> ;
        ont:person          <http://dbpedia.org/resource/Don_Adams> , <http://dbpedia.org/resource/Anthony_Perkins> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1341923/7/7> ;
        ont:subject         <http://dbpedia.org/resource/Larry_Blyden> ;
        ont:timeExpression  "November 1962" .

<http://w3id.org/polifonia/pilot/meetups/1341923/11/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "What's My Line?" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1341923/11> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1341923/11/2> ;
        ont:subject       <http://dbpedia.org/resource/Larry_Blyden> .

<http://w3id.org/polifonia/pilot/meetups/1341923/10/14>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1963, Blyden was cast to star in a second sitcom, NBC's Harry's Girls." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1341923/10> ;
        ont:person        <http://dbpedia.org/resource/Girl> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1341923/10/13> ;
        ont:subject       <http://dbpedia.org/resource/Larry_Blyden> .

<http://w3id.org/polifonia/pilot/meetups/1341923/8/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1974, Blyden appeared as \"Dionysos\" with the Yale Repertory Theatre in the musical comedy The Frogs, in New Haven, Connecticut." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1341923/8> ;
        ont:person        <http://dbpedia.org/resource/Dionysus> ;
        ont:place         <http://dbpedia.org/resource/New_Haven_Colony> , <http://dbpedia.org/resource/Yale_Repertory_Theatre> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1341923/8/1> ;
        ont:subject       <http://dbpedia.org/resource/Larry_Blyden> .

<http://w3id.org/polifonia/pilot/meetups/1341923/16/9>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Mark Goodson, the producer of Showoffs, later told Variety that he felt the circumstances leading to Blyden's death were unclear." ;
