<http://w3id.org/polifonia/pilot/meetups/1341923/13/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Blyden and Haney were divorced in 1962." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1341923/13> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1341923/13/1> ;
