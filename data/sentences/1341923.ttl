        ont:timeExpression  "September 1942" , "September 20 , 1955" , "February 1941" .

<http://w3id.org/polifonia/pilot/meetups/1341923/7/2>
        rdf:type            ont:Sentence ;
        ont:excerpt         "He was then cast in the larger role of \"Ensign Pulver\", and remained with the production until 1951." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1341923/7> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1341923/7/1> ;
        ont:subject         <http://dbpedia.org/resource/Larry_Blyden> ;
        ont:timeExpression  "production until 1951" .

<http://w3id.org/polifonia/pilot/meetups/1341923/16/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Before production was set to begin, Blyden was granted a two-week vacation and decided to fly to Marrakesh, Morocco." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1341923/16> ;
        ont:place         <http://dbpedia.org/resource/Marrakesh> , <http://dbpedia.org/resource/Morocco> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1341923/16/2> ;
        ont:subject       <http://dbpedia.org/resource/Larry_Blyden> .

