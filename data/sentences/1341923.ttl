        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/1341923/25/0> .

<http://w3id.org/polifonia/pilot/meetups/1341923/7/10>
        rdf:type            ont:Sentence ;
        ont:excerpt         "In February 1967, Blyden replaced Martin Balsam in the Broadway production of You Know I Can't Hear You When the Water's Running." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1341923/7> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1341923/7/9> ;
        ont:subject         <http://dbpedia.org/resource/Larry_Blyden> ;
        ont:timeExpression  "February 1967" .

<http://w3id.org/polifonia/pilot/meetups/1341923/10/18>
        rdf:type          ont:Sentence ;
        ont:excerpt       "For the remainder of the decade, Blyden continued with guest roles on television including spots on Alfred Hitchcock Presents, The Defenders,  The Fugitive, and The Man from U.N.C.L.E." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1341923/10> ;
        ont:person        <http://dbpedia.org/resource/The_Man> , <http://dbpedia.org/resource/Alfred_Hitchcock> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1341923/10/17> ;
        ont:subject       <http://dbpedia.org/resource/Larry_Blyden> .

<http://w3id.org/polifonia/pilot/meetups/1341923/5/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Career ==" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1341923/5> ;
