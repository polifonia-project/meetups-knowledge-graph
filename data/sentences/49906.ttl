<http://w3id.org/polifonia/pilot/meetups/49906/4/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1948, Granz signed an agreement with Mercury Records for the promotion and the distribution of the JATP recordings and other recordings." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/49906/4> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/49906/4/0> ;
        ont:subject       <http://dbpedia.org/resource/Norman_Granz> .

<http://w3id.org/polifonia/pilot/meetups/49906/2/6>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Only one copy of the very first concert program is known to exist." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/49906/2> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/49906/2/5> ;
        ont:subject         <http://dbpedia.org/resource/Norman_Granz> ;
        ont:timeExpression  "one copy" .

<http://w3id.org/polifonia/pilot/meetups/49906/3/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Later known as JATP, the ever-changing group recorded and toured extensively, with Granz producing some of the first live jam session recordings to be distributed to a wide market." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/49906/3> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/49906/3/0> ;
        ont:subject       <http://dbpedia.org/resource/Norman_Granz> .

<http://w3id.org/polifonia/pilot/meetups/49906/6/5>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1965, he married Hannelore Granz, a former airline stewardess from Germany." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/49906/6> ;
        ont:place         <http://dbpedia.org/resource/Germany> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/49906/6/4> ;
        ont:subject       <http://dbpedia.org/resource/Norman_Granz> .

<http://w3id.org/polifonia/pilot/meetups/49906/2/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Born in Los Angeles, Granz was the son of Jewish immigrants from Tiraspol." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/49906/2> ;
        ont:place         <http://dbpedia.org/resource/Tiraspol> , <http://dbpedia.org/resource/Los_Angeles> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/49906/2/0> ;
        ont:subject       <http://dbpedia.org/resource/Norman_Granz> .

<http://w3id.org/polifonia/pilot/meetups/49906/14>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/49906/14/0> .

<http://w3id.org/polifonia/pilot/meetups/49906/12/4>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Granz, incensed by the incident, insisted on successfully fighting the charges, which cost him over $2,000." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/49906/12> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/49906/12/3> ;
        ont:subject         <http://dbpedia.org/resource/Norman_Granz> ;
        ont:timeExpression  "2,000" .

<http://w3id.org/polifonia/pilot/meetups/49906/1/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "==" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/49906/1> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/49906/1/0> ;
        ont:subject       <http://dbpedia.org/resource/Norman_Granz> .

<http://w3id.org/polifonia/pilot/meetups/49906/9/10>
        rdf:type            ont:Sentence ;
        ont:excerpt         "In December 1960, Verve Records was sold to Metro-Goldwyn-Mayer." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/49906/9> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/49906/9/9> ;
        ont:subject         <http://dbpedia.org/resource/Norman_Granz> ;
        ont:timeExpression  "December 1960" .

<http://w3id.org/polifonia/pilot/meetups/49906/9>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/49906/9/0> ;
        rdf:_10   <http://w3id.org/polifonia/pilot/meetups/49906/9/9> ;
        rdf:_11   <http://w3id.org/polifonia/pilot/meetups/49906/9/10> ;
