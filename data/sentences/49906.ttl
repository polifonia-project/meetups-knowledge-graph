        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/49906/5> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/49906/5/0> ;
        ont:subject       <http://dbpedia.org/resource/Norman_Granz> .

<http://w3id.org/polifonia/pilot/meetups/49906/0/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He founded the record labels Clef, Norgran, Down Home, Verve, and Pablo." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/49906/0> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/49906/0/0> ;
