<http://w3id.org/polifonia/pilot/meetups/49906/4/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Down Home Records was intended for traditional jazz works." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/49906/4> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/49906/4/1> ;
        ont:subject       <http://dbpedia.org/resource/Norman_Granz> .

<http://w3id.org/polifonia/pilot/meetups/49906/11/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Opposing racism ==" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/49906/11> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/49906/11/0> ;
        ont:subject       <http://dbpedia.org/resource/Norman_Granz> .

<http://w3id.org/polifonia/pilot/meetups/49906/3/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "They featured swing and bop musicians and were among the first high-profile performances to feature racially integrated bands." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/49906/3> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/49906/3/1> ;
        ont:subject       <http://dbpedia.org/resource/Norman_Granz> .

<http://w3id.org/polifonia/pilot/meetups/49906/6/7>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Granz died of cancer on November 22, 2001, at the age of 83 in Geneva, Switzerland." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/49906/6> ;
        ont:place           <http://dbpedia.org/resource/Geneva> , <http://dbpedia.org/resource/Switzerland> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/49906/6/6> ;
        ont:subject         <http://dbpedia.org/resource/Norman_Granz> ;
        ont:timeExpression  "November 22 , 2001" , "age of 83" .

