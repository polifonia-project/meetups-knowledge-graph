<http://w3id.org/polifonia/pilot/meetups/363631/8/10>
        rdf:type            ont:Sentence ;
        ont:excerpt         "He also earned 12 Top 10 hits and 26 Top 40 hits from 1959 to 1986." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/363631/8> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/363631/8/9> ;
