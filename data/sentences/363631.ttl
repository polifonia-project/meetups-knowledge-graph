<http://w3id.org/polifonia/pilot/meetups/363631/7/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "In May 1960, King left the Drifters, assuming the stage name Ben E. King in preparation for a solo career." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/363631/7> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/363631/7/0> ;
        ont:subject         <http://dbpedia.org/resource/Ben_E._King> ;
        ont:timeExpression  "May 1960" .

<http://w3id.org/polifonia/pilot/meetups/363631/2/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "King began singing in church choirs, and in high school formed the Four B's, a doo-wop group that occasionally performed at the Apollo." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/363631/2> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/363631/2/0> ;
        ont:subject       <http://dbpedia.org/resource/Ben_E._King> .

<http://w3id.org/polifonia/pilot/meetups/363631/5/6>
        rdf:type            ont:Sentence ;
        ont:excerpt         "The last of the King-led Drifters singles to be released was \"Sometimes I Wonder\", which was recorded May 19, 1960, but not issued until June 1962." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/363631/5> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/363631/5/5> ;
        ont:subject         <http://dbpedia.org/resource/Ben_E._King> ;
        ont:timeExpression  "June 1962" , "May 19 , 1960" .

