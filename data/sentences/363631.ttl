        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/363631/5/0> ;
        ont:subject       <http://dbpedia.org/resource/Ben_E._King> .

<http://w3id.org/polifonia/pilot/meetups/363631/0/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He is best known as the singer and co-composer of \"Stand by" ;
