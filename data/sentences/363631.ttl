        ont:place           <http://dbpedia.org/resource/East_Harlem> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/363631/7/0> ;
        ont:subject         <http://dbpedia.org/resource/Ben_E._King> ;
        ont:timeExpression  "1961" .

<http://w3id.org/polifonia/pilot/meetups/363631/5/7>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Due to contract disputes with Treadwell in which King and his manager, Lover Patterson, demanded greater compensation, King rarely performed with the Drifters on tour or on television." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/363631/5> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/363631/5/6> ;
        ont:subject       <http://dbpedia.org/resource/Ben_E._King> .

<http://w3id.org/polifonia/pilot/meetups/363631/13/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "King has been covered by acts from several genres." ;
