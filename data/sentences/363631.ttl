<http://w3id.org/polifonia/pilot/meetups/363631/11/3>
        rdf:type            ont:Sentence ;
        ont:excerpt         "On November 11, 2010, he performed \"Stand By Me\" at the Latin Grammys with Prince Royce." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/363631/11> ;
        ont:person          <http://dbpedia.org/resource/Prince_Royce> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/363631/11/2> ;
        ont:subject         <http://dbpedia.org/resource/Ben_E._King> ;
        ont:timeExpression  "November 11 , 2010" .

<http://w3id.org/polifonia/pilot/meetups/363631/7/9>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Some of these hits include \"What is Soul?\", \"Tears, Tears, Tears\" , \"So Much Love\" , and \"Til I Can't Take It Anymore\"." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/363631/7> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/363631/7/8> ;
        ont:subject       <http://dbpedia.org/resource/Ben_E._King> .

