        rdf:type            ont:Sentence ;
        ont:excerpt         "In the summer of 1963, King had a Top 30 hit with \"I (Who Have Nothing)\", which reached the Top 10 on New York's radio station, WMCA." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/363631/7> ;
        ont:place           <http://dbpedia.org/resource/New_York_City> ;
