<http://w3id.org/polifonia/pilot/meetups/3585224/17/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "At first, lines in Marden's paintings and drawings were arranged in neat rows, like Chinese writing." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/3585224/17> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/3585224/17/0> ;
