        ont:timeExpression  "Between 1981-87" , "total of 31" .

<http://w3id.org/polifonia/pilot/meetups/3585224/28/13>
        rdf:type            ont:Sentence ;
        ont:excerpt         "On Hydra, Marden and his wife traded up houses (as they did elsewhere), moving into the current one in 1989." ;
