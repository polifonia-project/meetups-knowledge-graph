<http://w3id.org/polifonia/pilot/meetups/3585224/0/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Brice Marden (born October 15, 1938) is an American artist generally described as Minimalist, although his work may be hard to categorize." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/3585224/0> ;
        ont:person          <http://dbpedia.org/resource/Brice_Marden> ;
        ont:place           <http://dbpedia.org/resource/United_States> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/3585224/0/0> ;
        ont:subject         <http://dbpedia.org/resource/Brice_Marden> ;
        ont:timeExpression  "October 15 , 1938" .

<http://w3id.org/polifonia/pilot/meetups/3585224/20/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Marden has participated in hundreds of group exhibitions, and has also been the subject of numerous one-person shows and retrospectives." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/3585224/20> ;
        ont:place         <http://dbpedia.org/resource/River_Marden> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/3585224/20/0> ;
        ont:subject       <http://dbpedia.org/resource/Brice_Marden> .

<http://w3id.org/polifonia/pilot/meetups/3585224/20/11>
        rdf:type          ont:Sentence ;
        ont:excerpt       "It allowed the artist to reassess his previous works and focus on future works." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/3585224/20> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/3585224/20/10> ;
        ont:subject       <http://dbpedia.org/resource/Brice_Marden> .

