        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/3585224/2/0> ;
        ont:subject         <http://dbpedia.org/resource/Brice_Marden> ;
        ont:timeExpression  "from 1957 to 1958" .

<http://w3id.org/polifonia/pilot/meetups/3585224/22/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 2000, Brown University awarded the artist an honorary degree of Doctor of Fine Arts." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/3585224/22> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/3585224/22/0> ;
        ont:subject       <http://dbpedia.org/resource/Brice_Marden> .

<http://w3id.org/polifonia/pilot/meetups/3585224/20/9>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Originally, Marden was not enthusiastic about the idea." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/3585224/20> ;
