        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1966, at Dorothea Rockburne's suggestion, Marden was hired by Robert Rauschenberg to work as his assistant." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/3585224/6> ;
        ont:person        <http://dbpedia.org/resource/Dorothea_Rockburne> , <http://dbpedia.org/resource/Robert_Rauschenberg> ;
        ont:place         <http://dbpedia.org/resource/River_Marden> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/3585224/6/0> ;
        ont:subject       <http://dbpedia.org/resource/Brice_Marden> .
