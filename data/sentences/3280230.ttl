<http://w3id.org/polifonia/pilot/meetups/3280230/6/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Gesang remained in the city of his birth, continuing to compose and sing, his fame spreading through the decades." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/3280230/6> ;
        ont:person        <http://dbpedia.org/resource/Gesang_Martohartono> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/3280230/6/0> ;
        ont:subject       <http://dbpedia.org/resource/Gesang_Martohartono> .

<http://w3id.org/polifonia/pilot/meetups/3280230/1/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "His father owned a batik-fabric business, which went bankrupt when Gesang was teens, plunging the family into poverty." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/3280230/1> ;
        ont:person        <http://dbpedia.org/resource/Gesang_Martohartono> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/3280230/1/0> ;
        ont:subject       <http://dbpedia.org/resource/Gesang_Martohartono> .

<http://w3id.org/polifonia/pilot/meetups/3280230/4/6>
        rdf:type          ont:Sentence ;
        ont:excerpt       "It also found an appreciative audience among the Japanese occupation forces, some of whom took to singing it with lyrics translated into Japanese." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/3280230/4> ;
        ont:place         <http://dbpedia.org/resource/Japanese_occupation_of_the_Dutch_East_Indies> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/3280230/4/5> ;
        ont:subject       <http://dbpedia.org/resource/Gesang_Martohartono> .

<http://w3id.org/polifonia/pilot/meetups/3280230/5/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "As World War II drew to a close, the returning soldiers of the Japanese Imperial Army brought the song back to Japan." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/3280230/5> ;
        ont:place         <http://dbpedia.org/resource/Japan> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/3280230/5/0> ;
        ont:subject       <http://dbpedia.org/resource/Gesang_Martohartono> .

<http://w3id.org/polifonia/pilot/meetups/3280230/0/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He is the composer of the \"Bengawan Solo,\" famous song throughout Indonesia, Japan, part of Asia, and some other countries." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/3280230/0> ;
        ont:person        <http://dbpedia.org/resource/Composer> ;
        ont:place         <http://dbpedia.org/resource/Solo_River> , <http://dbpedia.org/resource/Asia> , <http://dbpedia.org/resource/Indonesia> , <http://dbpedia.org/resource/Japan> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/3280230/0/0> ;
        ont:subject       <http://dbpedia.org/resource/Gesang_Martohartono> .

