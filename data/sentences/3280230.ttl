        ont:subject       <http://dbpedia.org/resource/Gesang_Martohartono> .

<http://w3id.org/polifonia/pilot/meetups/3280230/8/2>
        rdf:type            ont:Sentence ;
        ont:excerpt         "He died from starvation and was already reported dead previously on 18 May 2010, but his family denied it." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/3280230/8> ;
        ont:person          <http://dbpedia.org/resource/Paul_is_dead> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/3280230/8/1> ;
        ont:subject         <http://dbpedia.org/resource/Gesang_Martohartono> ;
        ont:timeExpression  "18 May 2010" .

<http://w3id.org/polifonia/pilot/meetups/3280230/2>
