        rdf:type          ont:Sentence ;
        ont:excerpt       "It was periodically re-released by popular artists, and the song soon became almost synonymous in Japan with Indonesian music, with many assuming that it was a centuries-old traditional song." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/3280230/5> ;
        ont:place         <http://dbpedia.org/resource/Japan> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/3280230/5/2> ;
