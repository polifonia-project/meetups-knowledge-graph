<http://w3id.org/polifonia/pilot/meetups/3280230/1/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Gesang, a self-taught musician who was illiterate in musical notation, supported himself and his family by writing songs and singing at local functions such as weddings and other formal occasions." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/3280230/1> ;
        ont:person        <http://dbpedia.org/resource/Gesang_Martohartono> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/3280230/1/1> ;
        ont:subject       <http://dbpedia.org/resource/Gesang_Martohartono> .

<http://w3id.org/polifonia/pilot/meetups/3280230/4/7>
        rdf:type          ont:Sentence ;
        ont:excerpt       "It was popular among the non-Javanese prisoners (principally Dutch civilians) of the Japanese internment camps, many of whom spoke Indonesian." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/3280230/4> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/3280230/4/6> ;
