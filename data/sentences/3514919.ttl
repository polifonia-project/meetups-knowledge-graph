<http://w3id.org/polifonia/pilot/meetups/3514919/2/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "While growing up, Hairston heard his grandmother and her friends talking and singing about life on the plantations of the southern US." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/3514919/2> ;
        ont:place         <http://dbpedia.org/resource/United_States> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/3514919/2/3> ;
        ont:subject       <http://dbpedia.org/resource/Jester_Hairston> .

<http://w3id.org/polifonia/pilot/meetups/3514919/5/9>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He had the role of Leroy on the radio program and as Henry Van Porter on the television program." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/3514919/5> ;
        ont:person        <http://dbpedia.org/resource/Cole_Porter> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/3514919/5/8> ;
        ont:subject       <http://dbpedia.org/resource/Jester_Hairston> .

<http://w3id.org/polifonia/pilot/meetups/3514919/5/14>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In his later years, Hairston served as a cultural ambassador for American music, traveling to numerous countries with choral groups that he had assembled." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/3514919/5> ;
        ont:person        <http://dbpedia.org/resource/Ambassador> ;
        ont:place         <http://dbpedia.org/resource/United_States> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/3514919/5/13> ;
        ont:subject       <http://dbpedia.org/resource/Jester_Hairston> .

