<http://w3id.org/polifonia/pilot/meetups/3514919/4/9>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Hairston wrote the song \"Mary's Boy Child\" in 1956." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/3514919/4> ;
        ont:person        <http://dbpedia.org/resource/Mary_I_of_England> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/3514919/4/8> ;
        ont:subject       <http://dbpedia.org/resource/Jester_Hairston> .

<http://w3id.org/polifonia/pilot/meetups/3514919/2/10>
        rdf:type            ont:Sentence ;
        ont:excerpt         "He was one of the first black students admitted to Tufts." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/3514919/2> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/3514919/2/9> ;
        ont:subject         <http://dbpedia.org/resource/Jester_Hairston> ;
        ont:timeExpression  "one of" .

<http://w3id.org/polifonia/pilot/meetups/3514919/5/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "His banter about the history of the songs along with his engaging personality and sense of humor endeared him to many students." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/3514919/5> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/3514919/5/2> ;
        ont:subject       <http://dbpedia.org/resource/Jester_Hairston> .

