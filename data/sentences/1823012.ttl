<http://w3id.org/polifonia/pilot/meetups/1823012/2/9>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Clay was pregnant, at the time, with her first child by her husband, jazz drummer Leo Gatewood." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1823012/2> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1823012/2/8> ;
        ont:subject       <http://dbpedia.org/resource/Judy_Clay> .

<http://w3id.org/polifonia/pilot/meetups/1823012/3/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "She was survived by two sons, Todd and Leo Gatewood, a brother, Raymond Guions, and her sister, Sylvia Shemwell." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1823012/3> ;
        ont:person        <http://dbpedia.org/resource/Sylvia_Pankhurst> , <http://dbpedia.org/resource/Raymond_of_Poitiers> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1823012/3/1> ;
        ont:subject       <http://dbpedia.org/resource/Judy_Clay> .

