<http://w3id.org/polifonia/pilot/meetups/52114060/0/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "A flamboyant character, commonly known as the Midnight Cowboy, Rodgers worked part-time as a musician for most of his lifetime." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/52114060/0> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/52114060/0/0> ;
