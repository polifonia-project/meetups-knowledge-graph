<http://w3id.org/polifonia/pilot/meetups/876268/4/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1992, he performed a benefit concert in Washington, D.C. for then-jailed presidential candidate Lyndon LaRouche." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/876268/4> ;
        ont:person        <http://dbpedia.org/resource/George_Washington> , <http://dbpedia.org/resource/Lyndon_LaRouche> ;
        ont:place         <http://dbpedia.org/resource/Washington,_D.C.> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/876268/4/1> ;
        ont:subject       <http://dbpedia.org/resource/Norbert_Brainin> .

<http://w3id.org/polifonia/pilot/meetups/876268/0/5>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Finally Schidlof and Nissel were released, and the three were able to study with violin pedagogue Max Rostal, who taught them free of charge." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/876268/0> ;
        ont:person          <http://dbpedia.org/resource/Max_Rostal> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/876268/0/4> ;
        ont:subject         <http://dbpedia.org/resource/Norbert_Brainin> ;
        ont:timeExpression  "three" .

