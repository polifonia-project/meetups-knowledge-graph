<http://w3id.org/polifonia/pilot/meetups/341837/29/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Wuorinen's works have influenced a number of other composers." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/341837/29> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/341837/29/0> ;
