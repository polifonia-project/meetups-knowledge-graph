        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/8300/26/17> ;
        ont:subject         <http://dbpedia.org/resource/Doris_Day> ;
        ont:timeExpression  "career achievement in 1989" .

<http://w3id.org/polifonia/pilot/meetups/8300/1/3>
        rdf:type            ont:Sentence ;
        ont:excerpt         "She played the title role in Calamity Jane (1953) and starred in Alfred Hitchcock's The Man Who Knew Too Much (1956) with James Stewart." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/8300/1> ;
        ont:person          <http://dbpedia.org/resource/Alfred_Hitchcock> , <http://dbpedia.org/resource/James_Stewart> , <http://dbpedia.org/resource/Calamity_Jane> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/8300/1/2> ;
        ont:subject         <http://dbpedia.org/resource/Doris_Day> ;
        ont:timeExpression  "1953" , "1956" .

<http://w3id.org/polifonia/pilot/meetups/8300/36/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1971, she co-founded Actors and Others for Animals, and appeared in a series of newspaper advertisements denouncing the wearing of fur, alongside Mary Tyler Moore, Angie Dickinson, and Jayne Meadows." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/8300/36> ;
        ont:person        <http://dbpedia.org/resource/Mary_Tyler_Moore> , <http://dbpedia.org/resource/Jayne_Meadows> , <http://dbpedia.org/resource/Angie_Dickinson> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/8300/36/0> ;
        ont:subject       <http://dbpedia.org/resource/Doris_Day> .

