        rdf:type            ont:Sentence ;
        ont:excerpt         "In June 1987, Rosenthal filed a $30 million lawsuit against lawyers he claimed cheated him out of millions of dollars in real estate investments." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/8300/26> ;
        ont:person          <http://dbpedia.org/resource/Trevor_Rosenthal> ;
