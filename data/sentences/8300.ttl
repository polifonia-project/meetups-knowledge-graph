<http://w3id.org/polifonia/pilot/meetups/8300/21/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "But he'd signed me up for a series." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/8300/21> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/8300/21/1> ;
        ont:subject       <http://dbpedia.org/resource/Doris_Day> .

