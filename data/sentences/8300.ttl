        ont:place         <http://dbpedia.org/resource/United_States> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/8300/4/6> ;
        ont:subject       <http://dbpedia.org/resource/Doris_Day> .

<http://w3id.org/polifonia/pilot/meetups/8300/29/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "She starred on screen with leading men from Jimmy Stewart to Ronald Reagan, from Rock Hudson to James Garner." ;
