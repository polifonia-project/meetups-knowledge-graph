<http://w3id.org/polifonia/pilot/meetups/8300/34/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "While recovering from an automobile accident, she took her dog Tiny for a walk without a leash." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/8300/34> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/8300/34/0> ;
