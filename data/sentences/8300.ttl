<http://w3id.org/polifonia/pilot/meetups/8300/35/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "She was so appalled at the conditions the animals used in filming were kept in that she refused to work unless they were properly fed and cared for." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/8300/35> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/8300/35/0> ;
