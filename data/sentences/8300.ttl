        rdf:type          ont:Sentence ;
        ont:excerpt       "Day actively lobbied the United States Congress in support of legislation designed to safeguard animal welfare on a number of occasions, and in 1995 she originated the annual Spay Day USA." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/8300/36> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/8300/36/4> ;
