<http://w3id.org/polifonia/pilot/meetups/8300/32/4>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Day became the oldest artist to score a UK Top 10 with an album featuring new material." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/8300/32> ;
        ont:place           <http://dbpedia.org/resource/United_Kingdom> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/8300/32/3> ;
        ont:subject         <http://dbpedia.org/resource/Doris_Day> ;
        ont:timeExpression  "Top 10" .

<http://w3id.org/polifonia/pilot/meetups/8300/7/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "There was a quality to her voice that fascinated me, and I'd sing along with her, trying to catch the subtle ways she shaded her voice, the casual yet clean way she sang the words.\"" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/8300/7> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/8300/7/2> ;
