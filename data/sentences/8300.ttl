<http://w3id.org/polifonia/pilot/meetups/8300/18/6>
        rdf:type            ont:Sentence ;
        ont:excerpt         "During 1960 and the 1962 to 1964 period, she ranked number one at the box office, the second woman to be number one four times, an accomplishment equalled by no other actress except Shirley Temple." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/8300/18> ;
        ont:person          <http://dbpedia.org/resource/Shirley_Temple> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/8300/18/5> ;
        ont:subject         <http://dbpedia.org/resource/Doris_Day> ;
        ont:timeExpression  "1962" , "1964 period" , "one at" .

<http://w3id.org/polifonia/pilot/meetups/8300/22/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "It is remembered today for its abrupt season-to-season changes in casting and premise." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/8300/22> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/8300/22/3> ;
        ont:subject       <http://dbpedia.org/resource/Doris_Day> .

<http://w3id.org/polifonia/pilot/meetups/8300/32/5>
        rdf:type            ont:Sentence ;
        ont:excerpt         "In January 2012, the Los Angeles Film Critics Association presented Day with a Lifetime Achievement Award." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/8300/32> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/8300/32/4> ;
        ont:subject         <http://dbpedia.org/resource/Doris_Day> ;
        ont:timeExpression  "January 2012" .

