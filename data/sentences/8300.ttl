        rdf:type          ont:Sentence ;
        ont:excerpt       "He later complained that she cared more for her \"animal friends\" than she did for him." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/8300/41> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/8300/41/8> ;
