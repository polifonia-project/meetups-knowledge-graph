        ont:person        <http://dbpedia.org/resource/James_Garner> , <http://dbpedia.org/resource/James_Stewart> , <http://dbpedia.org/resource/Ronald_Reagan> ;
        ont:place         <http://dbpedia.org/resource/Hudson_River> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/8300/29/2> ;
        ont:subject       <http://dbpedia.org/resource/Doris_Day> .

<http://w3id.org/polifonia/pilot/meetups/8300/8/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Observing her daughter sing rekindled Alma's interest in show business, and she decided Doris must have singing lessons." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/8300/8> ;
        ont:person        <http://dbpedia.org/resource/Alma_Mahler> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/8300/8/0> ;
        ont:subject       <http://dbpedia.org/resource/Doris_Day> .

