        ont:subject         <http://dbpedia.org/resource/Doris_Day> ;
        ont:timeExpression  "Since the 1980s" .

<http://w3id.org/polifonia/pilot/meetups/8300/7/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "While recovering from her car accident, Kappelhoff started to sing along with the radio and discovered a talent she did not know she had." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/8300/7> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/8300/7/0> ;
        ont:subject       <http://dbpedia.org/resource/Doris_Day> .

<http://w3id.org/polifonia/pilot/meetups/8300/22/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Day hated the idea of performing on television, but felt obligated to do it." ;
