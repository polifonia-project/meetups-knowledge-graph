<http://w3id.org/polifonia/pilot/meetups/8300/18/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "It was the only Oscar nomination she received in her career." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/8300/18> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/8300/18/2> ;
