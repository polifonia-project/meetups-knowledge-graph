        ont:person          <http://dbpedia.org/resource/Mary_II_of_England> , <http://dbpedia.org/resource/Sophia_of_Hanover> , <http://dbpedia.org/resource/William_Joseph_Denison> , <http://dbpedia.org/resource/Alma_Mahler> ;
        ont:place           <http://dbpedia.org/resource/Ohio> , <http://dbpedia.org/resource/Cincinnati> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/8300/4/0> ;
        ont:subject         <http://dbpedia.org/resource/Doris_Day> ;
        ont:timeExpression  "1895–1976" , "April 3 , 1922" , "1892–1967" .

<http://w3id.org/polifonia/pilot/meetups/8300/32/8>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Clint Eastwood offered Day a role in a film he was planning to direct in 2015." ;
