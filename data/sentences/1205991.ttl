        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1205991/4/0> ;
        ont:subject       <http://dbpedia.org/resource/Donna_McKechnie> .

<http://w3id.org/polifonia/pilot/meetups/1205991/2/6>
        rdf:type          ont:Sentence ;
