<http://w3id.org/polifonia/pilot/meetups/1291198/0/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Donald Andrew Hall Jr. (September 20, 1928 – June 23, 2018) was an American poet, writer, editor and literary critic." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1291198/0> ;
        ont:person          <http://dbpedia.org/resource/Andrew_Hall> , <http://dbpedia.org/resource/Poet> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1291198/0/0> ;
        ont:subject         <http://dbpedia.org/resource/Donald_Hall> ;
        ont:timeExpression  "June 23 , 2018" , "September 20 , 1928" .

<http://w3id.org/polifonia/pilot/meetups/1291198/1/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "On June 14, 2006, Hall was appointed as the Library of Congress's 14th Poet Laureate Consultant in Poetry (commonly known as \"Poet Laureate of the United States\")." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1291198/1> ;
        ont:person          <http://dbpedia.org/resource/Poet_laureate> ;
        ont:place           <http://dbpedia.org/resource/United_States> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1291198/1/0> ;
        ont:subject         <http://dbpedia.org/resource/Donald_Hall> ;
        ont:timeExpression  "June 14 , 2006" .

<http://w3id.org/polifonia/pilot/meetups/1291198/6/5>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Following his year at Stanford, Hall went back to Harvard, where he spent three years in the Society of Fellows." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1291198/6> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1291198/6/4> ;
        ont:subject       <http://dbpedia.org/resource/Donald_Hall> .

<http://w3id.org/polifonia/pilot/meetups/1291198/10/6>
        rdf:type            ont:Sentence ;
        ont:excerpt         "His last book A Carnival of Losses:  Essays Nearing Ninety was published on July 10, 2018." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1291198/10> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1291198/10/5> ;
        ont:subject         <http://dbpedia.org/resource/Donald_Hall> ;
        ont:timeExpression  "July 10 , 2018" .

<http://w3id.org/polifonia/pilot/meetups/1291198/6/19>
        rdf:type          ont:Sentence ;
        ont:excerpt       "The book's first poem, 'Kill the Day,' stands among the best Hall has ever written." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1291198/6> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1291198/6/18> ;
        ont:subject       <http://dbpedia.org/resource/Donald_Hall> .

