        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/460378/17/0> ;
        ont:subject       <http://dbpedia.org/resource/Alessandro_Moreschi> .

<http://w3id.org/polifonia/pilot/meetups/460378/12/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He is of medium or rather small stature." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/460378/12> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/460378/12/0> ;
        ont:subject       <http://dbpedia.org/resource/Alessandro_Moreschi> .

<http://w3id.org/polifonia/pilot/meetups/460378/7/0>
