<http://w3id.org/polifonia/pilot/meetups/460378/10/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "In retirement, Moreschi lived in his apartment at 19 Via Plinio, a few minutes' walk from the Vatican, where he died at the age of 63, possibly of pneumonia." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/460378/10> ;
        ont:place           <http://dbpedia.org/resource/Vatican_City> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/460378/10/0> ;
        ont:subject         <http://dbpedia.org/resource/Alessandro_Moreschi> ;
        ont:timeExpression  "apartment at 19" , "age of 63" .

<http://w3id.org/polifonia/pilot/meetups/460378/8/12>
