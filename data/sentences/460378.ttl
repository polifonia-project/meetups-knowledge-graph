<http://w3id.org/polifonia/pilot/meetups/460378/8/22>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Mancini became a professional double-bass player." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/460378/8> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/460378/8/21> ;
