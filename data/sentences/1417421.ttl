<http://w3id.org/polifonia/pilot/meetups/1417421/17/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Their daughter, Erin Perry, worked with Bob Stewart Productions as an associate producer to The $25,000 Pyramid, and also served with Pat Finn's production company, In-Finn-ity Productions, as its vice-president." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1417421/17> ;
        ont:person        <http://dbpedia.org/resource/Robertson_Stewart> , <http://dbpedia.org/resource/Pat_Finn> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1417421/17/0> ;
        ont:subject       <http://dbpedia.org/resource/Jim_Perry_(television_personality)> .

<http://w3id.org/polifonia/pilot/meetups/1417421/12/2>
        rdf:type            ont:Sentence ;
        ont:excerpt         "His daughter Erin also appeared on several of these telethons, and on one occasion they performed a song together." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1417421/12> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1417421/12/1> ;
        ont:subject         <http://dbpedia.org/resource/Jim_Perry_(television_personality)> ;
        ont:timeExpression  "one occasion" .

