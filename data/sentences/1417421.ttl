        ont:subject       <http://dbpedia.org/resource/Jim_Perry_(television_personality)> .

<http://w3id.org/polifonia/pilot/meetups/1417421/4/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Due to a name conflict with AFTRA, he adopted his mother's maiden name of Perry shortly after beginning his television work." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1417421/4> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1417421/4/3> ;
