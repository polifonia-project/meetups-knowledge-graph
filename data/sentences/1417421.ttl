<http://w3id.org/polifonia/pilot/meetups/1417421/6/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Perry presided as emcee of the annual Miss Canada Pageant, a job he held from 1967 until 1990, about the same length of time his U.S. counterpart Bob Barker presided over the Miss USA Pageant on CBS." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1417421/6> ;
        ont:person          <http://dbpedia.org/resource/Bob_Barker> , <http://dbpedia.org/resource/Master_of_ceremonies> ;
        ont:place           <http://dbpedia.org/resource/United_States> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1417421/6/0> ;
        ont:subject         <http://dbpedia.org/resource/Jim_Perry_(television_personality)> ;
        ont:timeExpression  "from 1967 until 1990" .

<http://w3id.org/polifonia/pilot/meetups/1417421/1/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He was the host of the American game shows Card Sharks and $ale of the Century, as well as the Canadian game shows Definition and Headline Hunters." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1417421/1> ;
        ont:place         <http://dbpedia.org/resource/United_States> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1417421/1/0> ;
        ont:subject       <http://dbpedia.org/resource/Jim_Perry_(television_personality)> .

<http://w3id.org/polifonia/pilot/meetups/1417421/5/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "His first appearance as a TV host came in Canada with the popular game show Fractured Phrases (1965)." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1417421/5> ;
        ont:place           <http://dbpedia.org/resource/Canada> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1417421/5/0> ;
        ont:subject         <http://dbpedia.org/resource/Jim_Perry_(television_personality)> ;
        ont:timeExpression  "1965" .

