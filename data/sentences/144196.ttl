        rdf:type            ont:Sentence ;
        ont:excerpt         "The five-nights-a-week taped show was broadcast from an old vaudeville theater at 1228 North Vine Street in Hollywood that was renamed The Steve Allen Playhouse." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/144196/25> ;
        ont:place           <http://dbpedia.org/resource/Vine_Street> , <http://dbpedia.org/resource/The_Steve_Allen_Playhouse> , <http://dbpedia.org/resource/Hollywood> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/144196/25/0> ;
        ont:subject         <http://dbpedia.org/resource/Steve_Allen> ;
        ont:timeExpression  "vaudeville theater at 1228" .

<http://w3id.org/polifonia/pilot/meetups/144196/82/0>
