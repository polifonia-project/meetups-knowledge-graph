        ont:subject         <http://dbpedia.org/resource/Steve_Allen> ;
        ont:timeExpression  "in the 1990s" , "in season 6" , "voice work in two" , "in season 3" .

<http://w3id.org/polifonia/pilot/meetups/144196/68/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He assured viewers that he would not allow Presley 'to do anything that will offend anyone.'" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/144196/68> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/144196/68/0> ;
        ont:subject       <http://dbpedia.org/resource/Steve_Allen> .

<http://w3id.org/polifonia/pilot/meetups/144196/35/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Their dialogue and arguments covered issues such as racism, women's rights, crime and punishment, slavery, and religious tolerance." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/144196/35> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/144196/35/1> ;
        ont:subject       <http://dbpedia.org/resource/Steve_Allen> .

<http://w3id.org/polifonia/pilot/meetups/144196/15/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Leaving CBS, Allen created a late-night New York talk/variety television program that debuted in July 1953 on local station WNBT-TV (now WNBC-TV)." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/144196/15> ;
        ont:person          <http://dbpedia.org/resource/Steve_Allen> ;
        ont:place           <http://dbpedia.org/resource/New_York_City> ;
