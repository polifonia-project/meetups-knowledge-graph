        rdf:type          ont:Sentence ;
        ont:excerpt       "Lennon did so as a humorous reference to Smith's unhurried, unflappable and amiable nature." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/470115/4> ;
        ont:person        <http://dbpedia.org/resource/John_Lennon> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/470115/4/2> ;
        ont:subject       <http://dbpedia.org/resource/Norman_Smith_(record_producer)> .

