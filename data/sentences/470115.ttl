<http://w3id.org/polifonia/pilot/meetups/470115/6/8>
        rdf:type          ont:Sentence ;
        ont:excerpt       "However, his subsequent attempts at producing successful recordings proved elusive." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/470115/6> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/470115/6/7> ;
