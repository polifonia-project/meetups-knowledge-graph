        ont:timeExpression  "7 on" , "hit in 1945" , "chart for eight" , "1 on" .

<http://w3id.org/polifonia/pilot/meetups/264312/2/3>
        rdf:type            ont:Sentence ;
        ont:excerpt         "He first fronted a band in 1931 for an RKO theater tour, and in 1932 took over the leadership of Doc Crawford's orchestra in Harlem." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/264312/2> ;
        ont:person          <http://dbpedia.org/resource/Jordan_Crawford> ;
