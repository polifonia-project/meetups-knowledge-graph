        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/264312/13/1> .

<http://w3id.org/polifonia/pilot/meetups/264312/1/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Early career ==" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/264312/1> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/264312/1/0> ;
