<http://w3id.org/polifonia/pilot/meetups/264312/1>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/264312/1/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/264312/1/1> .

<http://w3id.org/polifonia/pilot/meetups/264312/5/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1938, he teamed-up with pianist Bill Doggett to front Doggett's group." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/264312/5> ;
        ont:person        <http://dbpedia.org/resource/Bill_Doggett> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/264312/5/0> ;
        ont:subject       <http://dbpedia.org/resource/Lucky_Millinder> .

<http://w3id.org/polifonia/pilot/meetups/264312/0/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Although he could not read or write music, did not play an instrument and rarely sang, his showmanship and musical taste made his bands successful." ;
