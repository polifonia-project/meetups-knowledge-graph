        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/264312/27/0> .

<http://w3id.org/polifonia/pilot/meetups/264312/2>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/264312/2/0> ;
