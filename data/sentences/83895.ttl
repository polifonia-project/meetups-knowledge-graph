        rdf:type          ont:Sentence ;
        ont:excerpt       "I am that veritable priest, who, after all this, began to speak against, not only the Catholic faith, but the moral law, and perverted others by my teaching." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/83895/58> ;
