<http://w3id.org/polifonia/pilot/meetups/83895/18/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Its aim was to secure for the Church of England a definite basis of doctrine and discipline." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/83895/18> ;
