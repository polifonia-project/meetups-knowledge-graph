<http://w3id.org/polifonia/pilot/meetups/83895/8/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He was, however, sent shortly to Trinity College, Oxford, where he studied widely." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/83895/8> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/83895/8/0> ;
