        ont:subject         <http://dbpedia.org/resource/John_Henry_Newman> ;
        ont:timeExpression  "February 1843" .

<http://w3id.org/polifonia/pilot/meetups/83895/69/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1854, at the request of the Irish Catholic bishops, Newman went to Dublin as rector of the newly established Catholic University of Ireland, now University College, Dublin." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/83895/69> ;
