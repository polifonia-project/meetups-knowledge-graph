<http://w3id.org/polifonia/pilot/meetups/83895/93/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Natural religion refers to the knowledge of God and divine things that has been acquired outside the Christian revelation." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/83895/93> ;
        ont:person        <http://dbpedia.org/resource/God> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/83895/93/1> ;
