<http://w3id.org/polifonia/pilot/meetups/83895/101/21>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Men born in the first decades of the nineteenth century had a capacity, which did not survive into later generations, for intense male friendships." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/83895/101> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/83895/101/20> ;
