        ont:person        <http://dbpedia.org/resource/Frederick_William_Faber> , <http://dbpedia.org/resource/John_Henry_Newman> ;
        ont:place         <http://dbpedia.org/resource/Edgbaston> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/83895/30/0> ;
        ont:subject       <http://dbpedia.org/resource/John_Henry_Newman> .

<http://w3id.org/polifonia/pilot/meetups/83895/99/8>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Newman wrote after St John's death: \"I have ever thought no bereavement was equal to that of a husband's or a wife's, but I feel it difficult to believe that any can be greater, or any one's sorrow greater, than mine\"." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/83895/99> ;
        ont:person          <http://dbpedia.org/resource/John_Henry_Newman> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/83895/99/7> ;
        ont:subject         <http://dbpedia.org/resource/John_Henry_Newman> ;
        ont:timeExpression  "one" .

<http://w3id.org/polifonia/pilot/meetups/83895/80/7>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In answer to Kingsley, again encouraged by Badeley, Newman published in bi-monthly parts his Apologia Pro Vita Sua, a religious autobiography." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/83895/80> ;
        ont:person        <http://dbpedia.org/resource/John_Henry_Newman> , <http://dbpedia.org/resource/Charles_Kingsley> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/83895/80/6> ;
        ont:subject       <http://dbpedia.org/resource/John_Henry_Newman> .

<http://w3id.org/polifonia/pilot/meetups/83895/97/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "To exponents of muscular Christianity such as Charles Kingsley, celibacy was synonymous with unmanliness." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/83895/97> ;
        ont:person        <http://dbpedia.org/resource/Charles_Kingsley> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/83895/97/0> ;
        ont:subject       <http://dbpedia.org/resource/John_Henry_Newman> .

