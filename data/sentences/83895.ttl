        rdf:type          ont:Sentence ;
        ont:excerpt       "Its tone changed the popular estimate of its author, by explaining the convictions which had led him into the Catholic Church." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/83895/80> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/83895/80/7> ;
