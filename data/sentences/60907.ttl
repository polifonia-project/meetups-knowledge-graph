        rdf:_6    <http://w3id.org/polifonia/pilot/meetups/60907/12/5> .

<http://w3id.org/polifonia/pilot/meetups/60907/13/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "It soon became the highest-rated show on daytime television." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/60907/13> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/60907/13/3> ;
        ont:subject       <http://dbpedia.org/resource/Gene_Rayburn> .

<http://w3id.org/polifonia/pilot/meetups/60907/25>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/60907/25/0> .

<http://w3id.org/polifonia/pilot/meetups/60907/17/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He once hosted a local New York City show on WNEW-TV (now WNYW), Helluva Town, and between game show stints in 1982–1983, he returned to WNEW as host of a weekly local talk and lifestyles show titled Saturday Morning Live." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/60907/17> ;
        ont:place         <http://dbpedia.org/resource/New_York_City> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/60907/17/2> ;
        ont:subject       <http://dbpedia.org/resource/Gene_Rayburn> .

<http://w3id.org/polifonia/pilot/meetups/60907/12/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Because it was a live show, very few episodes were recorded; only four are known to exist." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/60907/12> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/60907/12/3> ;
        ont:subject       <http://dbpedia.org/resource/Gene_Rayburn> .

<http://w3id.org/polifonia/pilot/meetups/60907/8/2>
        rdf:type            ont:Sentence ;
        ont:excerpt         "At one point in his stage career, Rayburn's stand-in was future Match Game panelist Charles Nelson Reilly." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/60907/8> ;
        ont:person          <http://dbpedia.org/resource/Charles_Nelson_Reilly> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/60907/8/1> ;
        ont:subject         <http://dbpedia.org/resource/Gene_Rayburn> ;
        ont:timeExpression  "one point" .

<http://w3id.org/polifonia/pilot/meetups/60907/3/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "After three years in that position, Rayburn began announcing at various radio stations, eventually landing back in New York at WNEW." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/60907/3> ;
        ont:place         <http://dbpedia.org/resource/New_York_City> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/60907/3/2> ;
        ont:subject       <http://dbpedia.org/resource/Gene_Rayburn> .

<http://w3id.org/polifonia/pilot/meetups/60907/6>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/60907/6/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/60907/6/1> ;
        rdf:_3    <http://w3id.org/polifonia/pilot/meetups/60907/6/2> ;
