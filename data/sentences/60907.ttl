<http://w3id.org/polifonia/pilot/meetups/60907/19/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Rayburn identified as a liberal politically, and was a supporter of Planned Parenthood." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/60907/19> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/60907/19/3> ;
        ont:subject       <http://dbpedia.org/resource/Gene_Rayburn> .

<http://w3id.org/polifonia/pilot/meetups/60907/14/5>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Rayburn, by most accounts, was disappointed with how the show turned out." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/60907/14> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/60907/14/4> ;
        ont:subject       <http://dbpedia.org/resource/Gene_Rayburn> .

<http://w3id.org/polifonia/pilot/meetups/60907/10/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Breaking into television as the original announcer on Steve Allen's Tonight, Rayburn began a long association with game show producers Mark Goodson and Bill Todman in 1953." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/60907/10> ;
        ont:person        <http://dbpedia.org/resource/Mark_Goodson> , <http://dbpedia.org/resource/Steve_Allen> , <http://dbpedia.org/resource/Bill_Todman> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/60907/10/0> ;
        ont:subject       <http://dbpedia.org/resource/Gene_Rayburn> .

<http://w3id.org/polifonia/pilot/meetups/60907/13/5>
        rdf:type            ont:Sentence ;
        ont:excerpt         "From 1973 to 1977, Match Game was number one among all daytime network game shows—three of those years it was the highest rated of all daytime shows." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/60907/13> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/60907/13/4> ;
        ont:subject         <http://dbpedia.org/resource/Gene_Rayburn> ;
        ont:timeExpression  "From 1973 to 1977" , "one among" .

<http://w3id.org/polifonia/pilot/meetups/60907/13>
        rdf:type  ont:Paragraph ;
