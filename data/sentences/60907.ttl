<http://w3id.org/polifonia/pilot/meetups/60907/13/6>
        rdf:type            ont:Sentence ;
        ont:excerpt         "The daytime revival of Match Game, which featured regular panelists Richard Dawson, Brett Somers, and Charles Nelson Reilly, ran until 1979 on CBS and another three years in first-run syndication." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/60907/13> ;
        ont:person          <http://dbpedia.org/resource/Brett_Somers> , <http://dbpedia.org/resource/Richard_Dawson> , <http://dbpedia.org/resource/Charles_Nelson_Reilly> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/60907/13/5> ;
        ont:subject         <http://dbpedia.org/resource/Gene_Rayburn> ;
        ont:timeExpression  "three" , "1979 on" .

