        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/60907/26/0> .

<http://w3id.org/polifonia/pilot/meetups/60907/17/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He ended his brief tenure to return as co-host of Match Game-Hollywood Squares Hour." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/60907/17> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/60907/17/3> ;
        ont:subject       <http://dbpedia.org/resource/Gene_Rayburn> .

<http://w3id.org/polifonia/pilot/meetups/60907/12/5>
        rdf:type          ont:Sentence ;
        ont:excerpt       "The show was canceled in 1969 to make room for the topical, short-lived game show Letters to Laugh-In." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/60907/12> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/60907/12/4> ;
        ont:subject       <http://dbpedia.org/resource/Gene_Rayburn> .

<http://w3id.org/polifonia/pilot/meetups/60907/3/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He enlisted in the United States Army Air Forces and served in World War II." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/60907/3> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/60907/3/3> ;
        ont:subject       <http://dbpedia.org/resource/Gene_Rayburn> .

<http://w3id.org/polifonia/pilot/meetups/60907/7>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/60907/7/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/60907/7/1> .

<http://w3id.org/polifonia/pilot/meetups/60907/16/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Three years after the original Match Game was canceled, Rayburn hosted the short-lived Heatter-Quigley Productions show The Amateur's Guide to Love." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/60907/16> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/60907/16/3> ;
        ont:subject       <http://dbpedia.org/resource/Gene_Rayburn> .

<http://w3id.org/polifonia/pilot/meetups/60907/20>
        rdf:type  ont:Paragraph ;
