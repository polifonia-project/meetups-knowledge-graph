        ont:subject         <http://dbpedia.org/resource/Gene_Rayburn> ;
        ont:timeExpression  "TV variety show as one" .

<http://w3id.org/polifonia/pilot/meetups/60907/19/5>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Rayburn was also concerned that human overpopulation would become an issue in the 21st century and that it would become more difficult to supply resources such as food if the population grew too large." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/60907/19> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/60907/19/4> ;
        ont:subject       <http://dbpedia.org/resource/Gene_Rayburn> .

<http://w3id.org/polifonia/pilot/meetups/60907/2/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Rayburn was born in Christopher, Illinois, the younger of two children of Croatian immigrants." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/60907/2> ;
        ont:place         <http://dbpedia.org/resource/Illinois> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/60907/2/0> ;
        ont:subject       <http://dbpedia.org/resource/Gene_Rayburn> .

