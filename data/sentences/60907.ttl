<http://w3id.org/polifonia/pilot/meetups/60907/10/5>
        rdf:type            ont:Sentence ;
        ont:excerpt         "In an uncredited role (he reportedly did not want his name to appear), Rayburn played a TV interviewer in the movie It Happened to Jane (1959) starring Doris Day." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/60907/10> ;
        ont:person          <http://dbpedia.org/resource/Doris_Day> , <http://dbpedia.org/resource/Jane_Fonda> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/60907/10/4> ;
        ont:subject         <http://dbpedia.org/resource/Gene_Rayburn> ;
        ont:timeExpression  "1959" .

<http://w3id.org/polifonia/pilot/meetups/60907/1>
        rdf:type  ont:Paragraph ;
