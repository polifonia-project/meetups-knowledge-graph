        rdf:_7    <http://w3id.org/polifonia/pilot/meetups/60907/6/6> .

<http://w3id.org/polifonia/pilot/meetups/60907/16/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Rayburn also hosted some episodes of Tattletales." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/60907/16> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/60907/16/2> ;
        ont:subject       <http://dbpedia.org/resource/Gene_Rayburn> .

<http://w3id.org/polifonia/pilot/meetups/60907/2/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He had an elder brother, Alfred, who was killed when Rayburn was a child, and a younger half-brother, Milan Rubessa Jr." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/60907/2> ;
        ont:place         <http://dbpedia.org/resource/Milan> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/60907/2/2> ;
        ont:subject       <http://dbpedia.org/resource/Gene_Rayburn> .

<http://w3id.org/polifonia/pilot/meetups/60907/10/4>
        rdf:type            ont:Sentence ;
        ont:excerpt         "On radio, Rayburn became one of the many hosts of the NBC program Monitor in 1961 and remained with the show until 1973." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/60907/10> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/60907/10/3> ;
        ont:subject         <http://dbpedia.org/resource/Gene_Rayburn> ;
        ont:timeExpression  "one of" , "show until 1973" .

