        rdf:_4    <http://w3id.org/polifonia/pilot/meetups/60907/11/3> .

<http://w3id.org/polifonia/pilot/meetups/60907/9/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Television career ===" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/60907/9> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/60907/9/0> ;
        ont:subject       <http://dbpedia.org/resource/Gene_Rayburn> .

<http://w3id.org/polifonia/pilot/meetups/60907/24>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/60907/24/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/60907/24/1> .

<http://w3id.org/polifonia/pilot/meetups/60907/17/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He appeared on Fantasy Island as a game show host—he and another host played by Jan Murray were game show rivals who vied to win the woman they both loved by creating the ultimate game show, with life-or-death consequences." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/60907/17> ;
        ont:person        <http://dbpedia.org/resource/Jan_Murray> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/60907/17/1> ;
        ont:subject       <http://dbpedia.org/resource/Gene_Rayburn> .

<http://w3id.org/polifonia/pilot/meetups/60907/12/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Rayburn usually played it straight, though he would make jokes as the situation warranted." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/60907/12> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/60907/12/2> ;
        ont:subject       <http://dbpedia.org/resource/Gene_Rayburn> .

<http://w3id.org/polifonia/pilot/meetups/60907/8/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Bye Bye Birdie when Dick Van Dyke left the production to star in The Dick Van Dyke Show." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/60907/8> ;
        ont:person        <http://dbpedia.org/resource/Dick_Van_Dyke> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/60907/8/0> ;
        ont:subject       <http://dbpedia.org/resource/Gene_Rayburn> .

<http://w3id.org/polifonia/pilot/meetups/60907/3/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "An aspiring actor and opera singer, Rayburn moved to New York City but unable to find stage work, found a job as a page and tour guide at NBC studios at 30 Rockefeller Plaza." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/60907/3> ;
        ont:place         <http://dbpedia.org/resource/New_York_City> , <http://dbpedia.org/resource/30_Rockefeller_Plaza> , <http://dbpedia.org/resource/The_Burbank_Studios> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/60907/3/1> ;
        ont:subject       <http://dbpedia.org/resource/Gene_Rayburn> .

<http://w3id.org/polifonia/pilot/meetups/60907/5>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/60907/5/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/60907/5/1> .

<http://w3id.org/polifonia/pilot/meetups/60907/16/2>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Also during the run of the 1970s Match Game, Rayburn and his wife Helen appeared on the game show Tattletales, hosted by Bert Convy." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/60907/16> ;
        ont:person          <http://dbpedia.org/resource/Bert_Convy> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/60907/16/1> ;
        ont:subject         <http://dbpedia.org/resource/Gene_Rayburn> ;
        ont:timeExpression  "of the 1970s" .

<http://w3id.org/polifonia/pilot/meetups/60907/11/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "==" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/60907/11> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/60907/11/2> ;
        ont:subject       <http://dbpedia.org/resource/Gene_Rayburn> .

<http://w3id.org/polifonia/pilot/meetups/60907/7/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Stage career ===" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/60907/7> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/60907/7/0> ;
        ont:subject       <http://dbpedia.org/resource/Gene_Rayburn> .

<http://w3id.org/polifonia/pilot/meetups/60907/2/2>
        rdf:type            ont:Sentence ;
        ont:excerpt         "After she married Rubessa on November 10, 1919, Rayburn took the name Eugene Rubessa ()." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/60907/2> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/60907/2/1> ;
        ont:subject         <http://dbpedia.org/resource/Gene_Rayburn> ;
        ont:timeExpression  "November 10 , 1919" .

<http://w3id.org/polifonia/pilot/meetups/60907/10/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "From there he hosted shows such as Choose Up Sides, Dough Re Mi, and the daytime version of Tic Tac Dough." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/60907/10> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/60907/10/2> ;
        ont:subject       <http://dbpedia.org/resource/Gene_Rayburn> .

<http://w3id.org/polifonia/pilot/meetups/60907/13/8>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Rayburn was nominated for three Daytime Emmy Award for Outstanding Game Show Host." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/60907/13> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/60907/13/7> ;
        ont:subject       <http://dbpedia.org/resource/Gene_Rayburn> .

<http://w3id.org/polifonia/pilot/meetups/60907/16>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/60907/16/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/60907/16/1> ;
        rdf:_3    <http://w3id.org/polifonia/pilot/meetups/60907/16/2> ;
        rdf:_4    <http://w3id.org/polifonia/pilot/meetups/60907/16/3> ;
        rdf:_5    <http://w3id.org/polifonia/pilot/meetups/60907/16/4> ;
        rdf:_6    <http://w3id.org/polifonia/pilot/meetups/60907/16/5> .

<http://w3id.org/polifonia/pilot/meetups/60907/6/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He had a morning drive time radio show in New York City, first with Jack Lescoulie (Anything Goes) and later with Dee Finch (Rayburn & Finch) on WNEW (now WBBR)." ;
