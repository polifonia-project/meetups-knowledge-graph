        ont:subject       <http://dbpedia.org/resource/Leon_Theremin> .

<http://w3id.org/polifonia/pilot/meetups/18571/2/8>
        rdf:type            ont:Sentence ;
        ont:excerpt         "On 9 May 1913 Theremin and his cousin attended Ioffe's dissertation defense." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/18571/2> ;
        ont:person          <http://dbpedia.org/resource/Abram_Ioffe> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/18571/2/7> ;
        ont:subject         <http://dbpedia.org/resource/Leon_Theremin> ;
        ont:timeExpression  "9 May 1913" .

<http://w3id.org/polifonia/pilot/meetups/18571/41>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/18571/41/0> .

<http://w3id.org/polifonia/pilot/meetups/18571/20>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/18571/20/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/18571/20/1> ;
        rdf:_3    <http://w3id.org/polifonia/pilot/meetups/18571/20/2> .

<http://w3id.org/polifonia/pilot/meetups/18571/5/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Although only in his second academic year, the deanery of the Faculty of Physics and Astronomy recommended that Theremin go to the Nikolayevska Military Engineering School in Petrograd (previously Saint Petersburg), which usually only accepted students in their fourth year." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/18571/5> ;
        ont:place         <http://dbpedia.org/resource/Saint_Petersburg> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/18571/5/0> ;
        ont:subject       <http://dbpedia.org/resource/Leon_Theremin> .

<http://w3id.org/polifonia/pilot/meetups/18571/19/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Lavrentiy Beria, the head of the secret police organization NKVD (the predecessor of the KGB), used the Buran device to spy on the British, French and US embassies in Moscow." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/18571/19> ;
        ont:person        <http://dbpedia.org/resource/Lavrentiy_Beria> ;
        ont:place         <http://dbpedia.org/resource/United_States> , <http://dbpedia.org/resource/United_Kingdom> , <http://dbpedia.org/resource/Moscow> , <http://dbpedia.org/resource/France> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/18571/19/1> ;
        ont:subject       <http://dbpedia.org/resource/Leon_Theremin> .

<http://w3id.org/polifonia/pilot/meetups/18571/40/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Burglar alarm, or \"Signalling Apparatus\" which used the Theremin effect (1920s)." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/18571/40> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/18571/40/0> ;
        ont:subject         <http://dbpedia.org/resource/Leon_Theremin> ;
        ont:timeExpression  "1920s" .

<http://w3id.org/polifonia/pilot/meetups/18571/6/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "==" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/18571/6> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/18571/6/0> ;
        ont:subject       <http://dbpedia.org/resource/Leon_Theremin> .

<http://w3id.org/polifonia/pilot/meetups/18571/22/6>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Further electronic music projects were banned, and Theremin was summarily dismissed." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/18571/22> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/18571/22/5> ;
        ont:subject       <http://dbpedia.org/resource/Leon_Theremin> .

<http://w3id.org/polifonia/pilot/meetups/18571/41/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Electromechanical television – Nipkow disk with mirrors instead of slots (ca. 1925)." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/18571/41> ;
        ont:person          <http://dbpedia.org/resource/Paul_Gottlieb_Nipkow> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/18571/41/0> ;
        ont:subject         <http://dbpedia.org/resource/Leon_Theremin> ;
        ont:timeExpression  "1925" .

<http://w3id.org/polifonia/pilot/meetups/18571/7/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "The day after Ioffe's invitation, Theremin started at the institute." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/18571/7> ;
        ont:person        <http://dbpedia.org/resource/Abram_Ioffe> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/18571/7/0> ;
        ont:subject       <http://dbpedia.org/resource/Leon_Theremin> .

<http://w3id.org/polifonia/pilot/meetups/18571/28>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/18571/28/0> .

<http://w3id.org/polifonia/pilot/meetups/18571/5/8>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Theremin recalled that on an evening when his hopes of overcoming these obstructing experts reached a low ebb, Abram Fedorovich Ioffe telephoned him." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/18571/5> ;
        ont:person        <http://dbpedia.org/resource/Abram_Ioffe> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/18571/5/7> ;
        ont:subject       <http://dbpedia.org/resource/Leon_Theremin> .

<http://w3id.org/polifonia/pilot/meetups/18571/10/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "After being sent on a lengthy tour of Europe starting 1927 – including London, Paris and towns in Germany – during which he demonstrated his invention to full audiences, Theremin found his way to the United States, arriving on 30 December 1927 with his first wife Katia." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/18571/10> ;
        ont:person          <http://dbpedia.org/resource/Katia_Mann> ;
        ont:place           <http://dbpedia.org/resource/Paris> , <http://dbpedia.org/resource/Europe> , <http://dbpedia.org/resource/Nazi_Germany> , <http://dbpedia.org/resource/London> , <http://dbpedia.org/resource/United_States> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/18571/10/0> ;
        ont:subject         <http://dbpedia.org/resource/Leon_Theremin> ;
        ont:timeExpression  "30 December 1927" .

<http://w3id.org/polifonia/pilot/meetups/18571/42/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Terpsitone – platform that converts dance movements into tones (1932)." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/18571/42> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/18571/42/0> ;
        ont:subject         <http://dbpedia.org/resource/Leon_Theremin> ;
        ont:timeExpression  "1932" .

<http://w3id.org/polifonia/pilot/meetups/18571/8/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "On 24 May 1924 Theremin married 20-year-old Katia (Ekaterina Pavlovna) Konstantinova, and they lived together in his parents' apartment on Marat street." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/18571/8> ;
        ont:person          <http://dbpedia.org/resource/Katia_Mann> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/18571/8/0> ;
        ont:subject         <http://dbpedia.org/resource/Leon_Theremin> ;
        ont:timeExpression  "24 May 1924" .

<http://w3id.org/polifonia/pilot/meetups/18571/2/12>
        rdf:type          ont:Sentence ;
        ont:excerpt       "From then on Theremin endeavoured to study the microcosm, in the same way he had studied the macrocosm with his hand-built telescope." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/18571/2> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/18571/2/11> ;
        ont:subject       <http://dbpedia.org/resource/Leon_Theremin> .

<http://w3id.org/polifonia/pilot/meetups/18571/2/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He had a sister named Helena." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/18571/2> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/18571/2/2> ;
        ont:subject       <http://dbpedia.org/resource/Leon_Theremin> .

<http://w3id.org/polifonia/pilot/meetups/18571/43/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Theremin cello – an electronic cello with no strings and no bow, using a plastic fingerboard, a handle for volume and two knobs for sound shaping (ca. 1930)." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/18571/43> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/18571/43/0> ;
        ont:subject         <http://dbpedia.org/resource/Leon_Theremin> ;
        ont:timeExpression  "1930" .

<http://w3id.org/polifonia/pilot/meetups/18571/11/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Theremin set up a laboratory in New York in the 1930s, where he further refined the theremin and experimented with other inventions and new electronic musical instruments." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/18571/11> ;
        ont:place           <http://dbpedia.org/resource/New_York_City> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/18571/11/0> ;
        ont:subject         <http://dbpedia.org/resource/Leon_Theremin> ;
        ont:timeExpression  "in the 1930s" .

