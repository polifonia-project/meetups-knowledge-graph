        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/18571/15/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/18571/15/1> ;
        rdf:_3    <http://w3id.org/polifonia/pilot/meetups/18571/15/2> .

<http://w3id.org/polifonia/pilot/meetups/18571/3/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Karl then reserved a room and equipment for Theremin's experiments." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/18571/3> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/18571/3/2> ;
        ont:subject       <http://dbpedia.org/resource/Leon_Theremin> .

<http://w3id.org/polifonia/pilot/meetups/18571/12/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1930, ten thereminists performed on stage at Carnegie Hall." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/18571/12> ;
        ont:place         <http://dbpedia.org/resource/Carnegie_Hall> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/18571/12/0> ;
        ont:subject       <http://dbpedia.org/resource/Leon_Theremin> .

<http://w3id.org/polifonia/pilot/meetups/18571/44/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Keyboard theremin (ca. 1930), a small keyboard \"with hornlike tones\"." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/18571/44> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/18571/44/0> ;
        ont:subject         <http://dbpedia.org/resource/Leon_Theremin> ;
        ont:timeExpression  "1930" .

<http://w3id.org/polifonia/pilot/meetups/18571/17/5>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Shortly after he returned he was imprisoned in the Butyrka prison and later sent to work in the Kolyma gold mines." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/18571/17> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/18571/17/4> ;
        ont:subject       <http://dbpedia.org/resource/Leon_Theremin> .

<http://w3id.org/polifonia/pilot/meetups/18571/21/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Later life ==" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/18571/21> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/18571/21/0> ;
        ont:subject       <http://dbpedia.org/resource/Leon_Theremin> .

<http://w3id.org/polifonia/pilot/meetups/18571/13/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1931, he worked with composer Joseph Schillinger to build an instrument called the rhythmicon." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/18571/13> ;
        ont:person        <http://dbpedia.org/resource/Composer> , <http://dbpedia.org/resource/Joseph_Schillinger> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/18571/13/0> ;
        ont:subject       <http://dbpedia.org/resource/Leon_Theremin> .

<http://w3id.org/polifonia/pilot/meetups/18571/45/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Rhythmicon – world's first drum machine (1931)." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/18571/45> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/18571/45/0> ;
        ont:subject         <http://dbpedia.org/resource/Leon_Theremin> ;
        ont:timeExpression  "1931" .

<http://w3id.org/polifonia/pilot/meetups/18571/44>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/18571/44/0> .

<http://w3id.org/polifonia/pilot/meetups/18571/22/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "By 1947 Theremin had remarried, to Maria Guschina, his third wife, and they had two children:" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/18571/22> ;
        ont:person        <http://dbpedia.org/resource/Maria_II_of_Portugal> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/18571/22/0> ;
        ont:subject       <http://dbpedia.org/resource/Leon_Theremin> .

<http://w3id.org/polifonia/pilot/meetups/18571/23>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/18571/23/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/18571/23/1> ;
        rdf:_3    <http://w3id.org/polifonia/pilot/meetups/18571/23/2> .

<http://w3id.org/polifonia/pilot/meetups/18571/5/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In the course of the next three and a half years he oversaw the construction of a radio station in Saratov to connect the Volga area with Moscow, graduated from Petrograd University, became deputy leader of the new Military Radiotechnical Laboratory in Moscow, and finished as the broadcast supervisor of the radio transmitter at Tsarskoye Selo near Petrograd (then renamed Detskoye Selo)." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/18571/5> ;
