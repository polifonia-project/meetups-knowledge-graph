        rdf:type          ont:Sentence ;
        ont:excerpt       "Alderman, who was raised in Colorado Springs, operated a restaurant there for a time." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1495782/19> ;
        ont:person        <http://dbpedia.org/resource/Alderman> ;
        ont:place         <http://dbpedia.org/resource/Colorado> ;
