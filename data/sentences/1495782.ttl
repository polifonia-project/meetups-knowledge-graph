<http://w3id.org/polifonia/pilot/meetups/1495782/1/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Etting, who enjoyed singing in school and church, never took voice lessons." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1495782/1> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1495782/1/2> ;
        ont:subject       <http://dbpedia.org/resource/Ruth_Etting> .

<http://w3id.org/polifonia/pilot/meetups/1495782/7/18>
        rdf:type            ont:Sentence ;
        ont:excerpt         "While she received a marquee billing for Roman Scandals, Etting had only two lines in the film and sang just one song." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1495782/7> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1495782/7/17> ;
        ont:subject         <http://dbpedia.org/resource/Ruth_Etting> ;
        ont:timeExpression  "one song" .

