        rdf:type          ont:Sentence ;
        ont:excerpt       "She replied, \"No, I hope I never do.\"" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1495782/17> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1495782/17/13> ;
