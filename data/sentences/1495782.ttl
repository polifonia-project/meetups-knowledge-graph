<http://w3id.org/polifonia/pilot/meetups/1495782/44/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Video games ===" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1495782/44> ;
