<http://w3id.org/polifonia/pilot/meetups/1495782/9/2>
        rdf:type            ont:Sentence ;
        ont:excerpt         "She signed with Brunswick and remained there until May 1934, when she re-signed with Columbia through July 1935." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1495782/9> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1495782/9/1> ;
        ont:subject         <http://dbpedia.org/resource/Ruth_Etting> ;
        ont:timeExpression  "May 1934" , "July 1935" .

<http://w3id.org/polifonia/pilot/meetups/1495782/11/12>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Snyder's arguing and fighting at venues where Etting was employed caused her to be passed by for jobs in the United States." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1495782/11> ;
        ont:place         <http://dbpedia.org/resource/United_States> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1495782/11/11> ;
        ont:subject       <http://dbpedia.org/resource/Ruth_Etting> .

<http://w3id.org/polifonia/pilot/meetups/1495782/13/6>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Etting requested police protection after the telephone call and arranged for private protection." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1495782/13> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1495782/13/5> ;
        ont:subject       <http://dbpedia.org/resource/Ruth_Etting> .

<http://w3id.org/polifonia/pilot/meetups/1495782/32/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Whoopee! (1928) – in which she introduced \"Love Me or Leave Me\"" ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1495782/32> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1495782/32/0> ;
        ont:subject         <http://dbpedia.org/resource/Ruth_Etting> ;
        ont:timeExpression  "1928" .

<http://w3id.org/polifonia/pilot/meetups/1495782/5/13>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Her big moment came when a featured vocalist suddenly became ill and was unable to perform." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1495782/5> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1495782/5/12> ;
