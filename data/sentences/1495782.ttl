        rdf:type            ont:Sentence ;
        ont:excerpt         "Etting, who made many of her own clothes, did her own housekeeping and lived frugally, initially announced her retirement in 1935." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1495782/11> ;
