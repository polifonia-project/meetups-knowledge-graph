        rdf:type          ont:Sentence ;
        ont:excerpt       "Threats and the shooting ===" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1495782/12> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1495782/12/0> ;
