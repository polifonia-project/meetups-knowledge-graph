        ont:subject         <http://dbpedia.org/resource/Ruth_Etting> ;
        ont:timeExpression  "February 12 , 2005" .

<http://w3id.org/polifonia/pilot/meetups/1495782/5/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Etting was interested in drawing at an early age; she drew and sketched anywhere she was able." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1495782/5> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1495782/5/3> ;
