        ont:person        <http://dbpedia.org/resource/John_Otterbein_Snyder> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1495782/13/13> ;
        ont:subject       <http://dbpedia.org/resource/Ruth_Etting> .

<http://w3id.org/polifonia/pilot/meetups/1495782/7/15>
        rdf:type            ont:Sentence ;
        ont:excerpt         "In Hollywood, Etting made a long series of movie shorts between 1929 and 1936, and three feature movies in 1933 and 1934." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1495782/7> ;
        ont:place           <http://dbpedia.org/resource/Hollywood> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1495782/7/14> ;
        ont:subject         <http://dbpedia.org/resource/Ruth_Etting> ;
        ont:timeExpression  "three feature" , "between 1929 and 1936" , "in 1933 and 1934" .

