<http://w3id.org/polifonia/pilot/meetups/1495782/13/19>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Snyder's daughter, Edith, picked it up and held it on her father, shooting at him but hitting the floor instead." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1495782/13> ;
