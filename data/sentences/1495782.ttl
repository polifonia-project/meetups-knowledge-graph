<http://w3id.org/polifonia/pilot/meetups/1495782/9/4>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Etting returned to the US and signed with Decca in December 1936 and recorded until April 1937, when she basically retired from recording." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1495782/9> ;
        ont:place           <http://dbpedia.org/resource/United_States> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1495782/9/3> ;
        ont:subject         <http://dbpedia.org/resource/Ruth_Etting> ;
        ont:timeExpression  "December 1936" , "April 1937" .

<http://w3id.org/polifonia/pilot/meetups/1495782/15/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Three days after the shooting of Myrl Alderman, the pianist's second wife, Alma, sued Etting for alienation of her husband's affections." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1495782/15> ;
        ont:person        <http://dbpedia.org/resource/Alma_Mahler> , <http://dbpedia.org/resource/Alderman> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1495782/15/0> ;
        ont:subject       <http://dbpedia.org/resource/Ruth_Etting> .

<http://w3id.org/polifonia/pilot/meetups/1495782/11/14>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Soon after the couple arrived in England, Snyder became involved in a street fight which created adverse publicity for Etting." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1495782/11> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1495782/11/13> ;
        ont:subject       <http://dbpedia.org/resource/Ruth_Etting> .

<http://w3id.org/polifonia/pilot/meetups/1495782/5/15>
        rdf:type          ont:Sentence ;
        ont:excerpt       "She quickly changed into the costume and scanned the music arrangements; the performer was male, so Etting tried to adjust by singing in a lower register." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1495782/5> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1495782/5/14> ;
        ont:subject       <http://dbpedia.org/resource/Ruth_Etting> .

<http://w3id.org/polifonia/pilot/meetups/1495782/13/8>
        rdf:type            ont:Sentence ;
        ont:excerpt         "On October 15, 1938, Snyder detained Myrl Alderman at a local radio station and forced the pianist to take him to his former wife at gunpoint." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1495782/13> ;
        ont:person          <http://dbpedia.org/resource/John_Otterbein_Snyder> , <http://dbpedia.org/resource/Alderman> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1495782/13/7> ;
        ont:subject         <http://dbpedia.org/resource/Ruth_Etting> ;
        ont:timeExpression  "October 15 , 1938" .

