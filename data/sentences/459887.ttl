<http://w3id.org/polifonia/pilot/meetups/459887/19/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Prince's song \"Avalanche\" mentions Hammond in the lyric \"Mr. John Hammond with his pen in hand..." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/19> ;
        ont:person        <http://dbpedia.org/resource/John_P._Hammond> , <http://dbpedia.org/resource/Prince> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/19/3> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

<http://w3id.org/polifonia/pilot/meetups/459887/16/18>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Hammond was much older than the majority of the other men, and he had a rough time adjusting to the military life." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/16> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/16/17> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

<http://w3id.org/polifonia/pilot/meetups/459887/23/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "John Hammond on American Masters (PBS)" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/23> ;
        ont:person        <http://dbpedia.org/resource/John_P._Hammond> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/23/0> ;
