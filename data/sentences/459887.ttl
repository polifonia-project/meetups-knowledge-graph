<http://w3id.org/polifonia/pilot/meetups/459887/17/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "==" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/17> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/17/0> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

<http://w3id.org/polifonia/pilot/meetups/459887/0/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He is the father of blues musician John P. Hammond." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/0> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/0/1> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

<http://w3id.org/polifonia/pilot/meetups/459887/11/3>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Hammond remembers that before the 1920s, black musicians could always find jobs, even if they were low paying." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/459887/11> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/459887/11/2> ;
        ont:subject         <http://dbpedia.org/resource/John_Hammond_(record_producer)> ;
        ont:timeExpression  "before the 1920s" .

<http://w3id.org/polifonia/pilot/meetups/459887/11/12>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Hammond proposed that Goodman produce a multiracial record; however, Goodman believed this route would hurt his musical reputation." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/11> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/11/11> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

<http://w3id.org/polifonia/pilot/meetups/459887/8/10>
        rdf:type            ont:Sentence ;
        ont:excerpt         "He coordinated a second From Spirituals to Swing concert in 1939." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/459887/8> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/459887/8/9> ;
        ont:subject         <http://dbpedia.org/resource/John_Hammond_(record_producer)> ;
        ont:timeExpression  "concert in 1939" .

<http://w3id.org/polifonia/pilot/meetups/459887/4/30>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Much to the disappointment of his father, a Yale alumnus, in 1931 he dropped out of school for a career in the music industry, first becoming the U.S. correspondent for Melody Maker." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/4> ;
        ont:place         <http://dbpedia.org/resource/United_States> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/4/29> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

<http://w3id.org/polifonia/pilot/meetups/459887/18/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "\"John's Idea\", originally titled \"I May Be Wrong It's John's Idea\", is a tribute to John Hammond written by Count Basie." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/18> ;
        ont:person        <http://dbpedia.org/resource/Count_Basie> , <http://dbpedia.org/resource/John_P._Hammond> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/18/0> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

<http://w3id.org/polifonia/pilot/meetups/459887/16/8>
        rdf:type            ont:Sentence ;
        ont:excerpt         "In November 1942, Jemy gave birth to their first son, John P. Hammond." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/459887/16> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/459887/16/7> ;
        ont:subject         <http://dbpedia.org/resource/John_Hammond_(record_producer)> ;
        ont:timeExpression  "November 1942" .

<http://w3id.org/polifonia/pilot/meetups/459887/25>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/459887/25/0> .

<http://w3id.org/polifonia/pilot/meetups/459887/16/35>
        rdf:type          ont:Sentence ;
        ont:excerpt       "His family moved to Greenwich Village, where Jemy gave birth to their third son, Jason." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/16> ;
        ont:place         <http://dbpedia.org/resource/Greenwich_Village> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/16/34> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

<http://w3id.org/polifonia/pilot/meetups/459887/2/2>
        rdf:type            ont:Sentence ;
        ont:excerpt         "His mother was one of three daughters of William Douglas Sloane and Emily Thorn Vanderbilt, and a granddaughter of William Henry Vanderbilt." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/459887/2> ;
        ont:person          <http://dbpedia.org/resource/William_Douglas_Sloane> , <http://dbpedia.org/resource/Emily_Thorn_Vanderbilt> , <http://dbpedia.org/resource/William_Henry_Vanderbilt> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/459887/2/1> ;
        ont:subject         <http://dbpedia.org/resource/John_Hammond_(record_producer)> ;
        ont:timeExpression  "one of" .

<http://w3id.org/polifonia/pilot/meetups/459887/19/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Hammond's son, John P. Hammond, became an American blues musician." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/19> ;
        ont:place         <http://dbpedia.org/resource/United_States> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/19/0> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

<http://w3id.org/polifonia/pilot/meetups/459887/16/14>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Goodman eventually grew weary of Hammond's contentious nature." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/16> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/16/13> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

<http://w3id.org/polifonia/pilot/meetups/459887/7/7>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Hammond recorded Fletcher Henderson, Benny Carter, Joe Venuti, Benny Goodman, and other jazz performers during a time when the economy was bad enough during the Great Depression that many of them would not have otherwise had the opportunity to enter a studio and play real jazz (a handful of these in this special series were issued in the US)." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/7> ;
        ont:person        <http://dbpedia.org/resource/Fletcher_Henderson> , <http://dbpedia.org/resource/Benny_Carter> , <http://dbpedia.org/resource/Benny_Goodman> , <http://dbpedia.org/resource/Joe_Venuti> ;
        ont:place         <http://dbpedia.org/resource/United_States> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/7/6> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

<http://w3id.org/polifonia/pilot/meetups/459887/13/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "However, Hammond was not a communist." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/13> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/13/2> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

<http://w3id.org/polifonia/pilot/meetups/459887/11/20>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Hammond's work with civil rights came from multiple angles." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/11> ;
        ont:person        <http://dbpedia.org/resource/Civil_and_political_rights> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/11/19> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

<http://w3id.org/polifonia/pilot/meetups/459887/3/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "His father was a brother of Ogden H. Hammond, ambassador to Spain, and uncle to politician Millicent Fenwick." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/3> ;
        ont:person        <http://dbpedia.org/resource/Millicent_Fenwick> , <http://dbpedia.org/resource/Ambassador> ;
        ont:place         <http://dbpedia.org/resource/Spain> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/3/1> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

<http://w3id.org/polifonia/pilot/meetups/459887/4/17>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Hammond's love for music flourished." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/4> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/4/16> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

<http://w3id.org/polifonia/pilot/meetups/459887/8/7>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1933 he heard the seventeen-year-old Billie Holiday perform in Harlem and arranged for her recording debut, on a Benny Goodman session." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/8> ;
        ont:person        <http://dbpedia.org/resource/Billie_Holiday> , <http://dbpedia.org/resource/Benny_Goodman> ;
        ont:place         <http://dbpedia.org/resource/Harlem> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/8/6> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

<http://w3id.org/polifonia/pilot/meetups/459887/12>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/459887/12/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/459887/12/1> .

<http://w3id.org/polifonia/pilot/meetups/459887/16/43>
        rdf:type            ont:Sentence ;
        ont:excerpt         "By this marriage Hammond had one stepdaughter, (Esme) Rosita Sarnoff (born 1943)." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/459887/16> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/459887/16/42> ;
        ont:subject         <http://dbpedia.org/resource/John_Hammond_(record_producer)> ;
        ont:timeExpression  "one stepdaughter" .

<http://w3id.org/polifonia/pilot/meetups/459887/4/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He was steered toward classical music by his mother but was more interested in the music sung and played by the servants, many of whom were black." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/4> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/4/1> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

<http://w3id.org/polifonia/pilot/meetups/459887/16/22>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Jemy speculated that Hammond was in a concert and disregarded the letter; however, that claim has been proven unlikely due to Hammond's strict schedule in basic training." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/16> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/16/21> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

<http://w3id.org/polifonia/pilot/meetups/459887/11/28>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1936, the group appeared in a live concert at the Chicago Hot Jazz Society." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/11> ;
        ont:place         <http://dbpedia.org/resource/Chicago> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/11/27> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

<http://w3id.org/polifonia/pilot/meetups/459887/4/25>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He studied the violin and, later, viola." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/4> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/4/24> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

<http://w3id.org/polifonia/pilot/meetups/459887/16/3>
        rdf:type            ont:Sentence ;
        ont:excerpt         "On March 8, 1941, Hammond married Jemy in New Haven, Connecticut." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/459887/16> ;
        ont:place           <http://dbpedia.org/resource/New_Haven_Colony> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/459887/16/2> ;
        ont:subject         <http://dbpedia.org/resource/John_Hammond_(record_producer)> ;
        ont:timeExpression  "March 8 , 1941" .

<http://w3id.org/polifonia/pilot/meetups/459887/6/2>
        rdf:type            ont:Sentence ;
        ont:excerpt         "He set up one of the first regular live jazz programs, and wrote regularly about the racial divide." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/459887/6> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/459887/6/1> ;
        ont:subject         <http://dbpedia.org/resource/John_Hammond_(record_producer)> ;
        ont:timeExpression  "one of" .

<http://w3id.org/polifonia/pilot/meetups/459887/16/51>
        rdf:type          ont:Sentence ;
        ont:excerpt       "It is said that he died listening to the music of Billie Holiday." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/16> ;
        ont:person        <http://dbpedia.org/resource/Billie_Holiday> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/16/50> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

<http://w3id.org/polifonia/pilot/meetups/459887/20>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/459887/20/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/459887/20/1> .

<http://w3id.org/polifonia/pilot/meetups/459887/16/30>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Toward the end of World War II, Hammond was transferred to Fort Benning, Georgia, known for its intense racism." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/16> ;
        ont:place         <http://dbpedia.org/resource/Fort_Benning> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/16/29> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

<http://w3id.org/polifonia/pilot/meetups/459887/7>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/459887/7/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/459887/7/1> ;
        rdf:_3    <http://w3id.org/polifonia/pilot/meetups/459887/7/2> ;
        rdf:_4    <http://w3id.org/polifonia/pilot/meetups/459887/7/3> ;
        rdf:_5    <http://w3id.org/polifonia/pilot/meetups/459887/7/4> ;
        rdf:_6    <http://w3id.org/polifonia/pilot/meetups/459887/7/5> ;
        rdf:_7    <http://w3id.org/polifonia/pilot/meetups/459887/7/6> ;
        rdf:_8    <http://w3id.org/polifonia/pilot/meetups/459887/7/7> .

<http://w3id.org/polifonia/pilot/meetups/459887/7/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Through this position, Hammond gained a reputation as a well-educated jazz fan." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/7> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/7/1> ;
