<http://w3id.org/polifonia/pilot/meetups/459887/4/6>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Hammond became interested in social reform at a young age." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/4> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/4/5> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

<http://w3id.org/polifonia/pilot/meetups/459887/16/47>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Although this impaired him physically, his wife's death left him in despair." ;
