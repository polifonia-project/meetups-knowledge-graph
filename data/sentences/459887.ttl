<http://w3id.org/polifonia/pilot/meetups/459887/11/16>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Hammond attributes fate to his finding of Holiday." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/11> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/11/15> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

<http://w3id.org/polifonia/pilot/meetups/459887/11/7>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He remembers this year being extraordinary due to his establishment of relationships with British record companies." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/11> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/11/6> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

<http://w3id.org/polifonia/pilot/meetups/459887/8/14>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He produced Dylan's early recordings, \"Blowin' in the Wind\" and \"A Hard Rain's a-Gonna Fall\"." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/8> ;
        ont:person        <http://dbpedia.org/resource/Bob_Dylan> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/8/13> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

<http://w3id.org/polifonia/pilot/meetups/459887/4/13>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Upon his return to the states, Hammond searched for records by black musicians but could not find them in the greater Manhattan area." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/4> ;
        ont:place         <http://dbpedia.org/resource/Manhattan> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/4/12> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

