<http://w3id.org/polifonia/pilot/meetups/459887/0/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "John Henry Hammond II (December 15, 1910 – July 10, 1987) was an American record producer, civil rights activist, and music critic from the 1930s to the early 1980s." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/459887/0> ;
        ont:person          <http://dbpedia.org/resource/Henry_Hammond> , <http://dbpedia.org/resource/Civil_and_political_rights> , <http://dbpedia.org/resource/John_W._Henry> ;
        ont:place           <http://dbpedia.org/resource/United_States> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/459887/0/0> ;
        ont:subject         <http://dbpedia.org/resource/John_Hammond_(record_producer)> ;
        ont:timeExpression  "July 10 , 1987" , "December 15 , 1910" , "from the 1930s" .

<http://w3id.org/polifonia/pilot/meetups/459887/11/10>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1933, he helped Benny Goodman receive a record deal with Columbia Records, which at the time was only known as English Columbia." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/11> ;
        ont:person        <http://dbpedia.org/resource/Benny_Goodman> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/11/9> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

<http://w3id.org/polifonia/pilot/meetups/459887/11/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "When Hammond entered the jazz community, integration had not yet begun." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/11> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/11/0> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

<http://w3id.org/polifonia/pilot/meetups/459887/4/28>
        rdf:type            ont:Sentence ;
        ont:excerpt         "In the fall semester of 1930, Hammond had to withdraw due to a recurring case of jaundice." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/459887/4> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/459887/4/27> ;
        ont:subject         <http://dbpedia.org/resource/John_Hammond_(record_producer)> ;
        ont:timeExpression  "fall semester of 1930" .

<http://w3id.org/polifonia/pilot/meetups/459887/1/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "==" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/1> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/1/0> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

<http://w3id.org/polifonia/pilot/meetups/459887/16/6>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1942, Hammond took his wife on a road trip to Los Angeles." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/16> ;
        ont:place         <http://dbpedia.org/resource/Los_Angeles> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/16/5> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

<http://w3id.org/polifonia/pilot/meetups/459887/6/5>
        rdf:type            ont:Sentence ;
        ont:excerpt         "This pre-occupation with social issues was to continue, and in 1941 he was one of the founders of the Council on African Affairs." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/459887/6> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/459887/6/4> ;
        ont:subject         <http://dbpedia.org/resource/John_Hammond_(record_producer)> ;
        ont:timeExpression  "one of" .

<http://w3id.org/polifonia/pilot/meetups/459887/23>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/459887/23/0> .

<http://w3id.org/polifonia/pilot/meetups/459887/12/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "FBI investigation ==" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/12> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/12/0> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

<http://w3id.org/polifonia/pilot/meetups/459887/16/33>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Hammond was not much a part of the bebop movement, but he shared the sentiment against racism." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/16> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/16/32> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

<http://w3id.org/polifonia/pilot/meetups/459887/2/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Hammond was born in New York, christened John Henry Hammond Jr., although both his father and grandfather shared the same name." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/2> ;
        ont:person        <http://dbpedia.org/resource/John_W._Henry> , <http://dbpedia.org/resource/Henry_Hammond> ;
        ont:place         <http://dbpedia.org/resource/New_York_City> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/2/0> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

<http://w3id.org/polifonia/pilot/meetups/459887/16/12>
        rdf:type          ont:Sentence ;
        ont:excerpt       "After years of arguments and disagreements about the musical directions of Goodman's band, Hammond   and Goodman had a complete falling out, some of which has been attributed to their differing class and educational backgrounds (Goodman had been born into poverty and had quit high school in order to help support his family as a musician)." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/16> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/16/11> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

<http://w3id.org/polifonia/pilot/meetups/459887/7/5>
        rdf:type          ont:Sentence ;
        ont:excerpt       "For this reason, Hammond quit his work with WEVD." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/7> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/7/4> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

<http://w3id.org/polifonia/pilot/meetups/459887/13/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Due to the various benefits and fund-raisers that Hammond hosted for the popular front, his name was often listed in The Daily Worker, a communist newspaper." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/13> ;
        ont:person        <http://dbpedia.org/resource/Listed_building> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/13/0> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

<http://w3id.org/polifonia/pilot/meetups/459887/11/9>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He was an attractive producer to these companies because he did not desire a profit for himself." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/11> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/11/8> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

<http://w3id.org/polifonia/pilot/meetups/459887/11/18>
        rdf:type          ont:Sentence ;
        ont:excerpt       "\"Later in 1933, he heard Teddy Wilson, a jazz pianist, on the Chicago radio." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/11> ;
        ont:person        <http://dbpedia.org/resource/Teddy_Wilson> ;
        ont:place         <http://dbpedia.org/resource/Chicago> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/11/17> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

<http://w3id.org/polifonia/pilot/meetups/459887/3/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "His father attended Yale University, graduating with a law degree from Columbia Law School." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/3> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/3/0> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

<http://w3id.org/polifonia/pilot/meetups/459887/4/15>
        rdf:type            ont:Sentence ;
        ont:excerpt         "In 1925 Hammond graduated from the elementary institution St. Bernard's School at the age of 14." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/459887/4> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/459887/4/14> ;
        ont:subject         <http://dbpedia.org/resource/John_Hammond_(record_producer)> ;
        ont:timeExpression  "age of 14" .

<http://w3id.org/polifonia/pilot/meetups/459887/8/5>
        rdf:type          ont:Sentence ;
        ont:excerpt       "His friendship with Benny Carter gave him a status that allowed him to enter this musical community." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/8> ;
        ont:person        <http://dbpedia.org/resource/Benny_Carter> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/8/4> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

<http://w3id.org/polifonia/pilot/meetups/459887/14/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Personal life ==" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/14> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/14/0> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

<http://w3id.org/polifonia/pilot/meetups/459887/4/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Hammond showed interest in music from an early age." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/4> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/4/0> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

<http://w3id.org/polifonia/pilot/meetups/459887/16/41>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Esme shared Hammond's musical passion and was planning to divorce her husband." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/16> ;
        ont:person        <http://dbpedia.org/resource/Divorce> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/16/40> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

<http://w3id.org/polifonia/pilot/meetups/459887/10>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/459887/10/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/459887/10/1> .

<http://w3id.org/polifonia/pilot/meetups/459887/16/20>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Douglas came down with a serious illness." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/16> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/16/19> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

<http://w3id.org/polifonia/pilot/meetups/459887/5/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "==" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/5> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/5/0> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

<http://w3id.org/polifonia/pilot/meetups/459887/11/26>
        rdf:type          ont:Sentence ;
        ont:excerpt       "While some of this integration had already taken place, Hammond remembers it as being hidden." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/11> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/11/25> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

<http://w3id.org/polifonia/pilot/meetups/459887/4/23>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Its editor Ernest Gruening was also a Hotchkiss alumnus, class of 1903, who was interested in social issues and social justice." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/459887/4> ;
        ont:person          <http://dbpedia.org/resource/Ernest_Gruening> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/459887/4/22> ;
        ont:subject         <http://dbpedia.org/resource/John_Hammond_(record_producer)> ;
        ont:timeExpression  "class of 1903" .

<http://w3id.org/polifonia/pilot/meetups/459887/16/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "While he was seen publicly with various women, the relationships were never substantial." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/16> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/16/0> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

<http://w3id.org/polifonia/pilot/meetups/459887/6/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1931, Hammond funded the recording of pianist Garland Wilson, marking the beginning of a long string of artistic successes as record producer." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/6> ;
        ont:person        <http://dbpedia.org/resource/Garland_Wilson> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/6/0> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

<http://w3id.org/polifonia/pilot/meetups/459887/4/8>
        rdf:type          ont:Sentence ;
        ont:excerpt       "She often found solace in religion." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/4> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/4/7> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

<http://w3id.org/polifonia/pilot/meetups/459887/16/49>
        rdf:type            ont:Sentence ;
        ont:excerpt         "While treatments worked for some time, she died May 19, 1986, of complications of AIDS, which she had contracted from a blood transfusion." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/459887/16> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/459887/16/48> ;
        ont:subject         <http://dbpedia.org/resource/John_Hammond_(record_producer)> ;
        ont:timeExpression  "May 19 , 1986" .

<http://w3id.org/polifonia/pilot/meetups/459887/18>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/459887/18/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/459887/18/1> .

<http://w3id.org/polifonia/pilot/meetups/459887/16/28>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Hammond began his efforts by organizing concerts for the soldiers featuring African-American musicians." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/16> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/16/27> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

<http://w3id.org/polifonia/pilot/meetups/459887/5>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/459887/5/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/459887/5/1> .

<http://w3id.org/polifonia/pilot/meetups/459887/17/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Legacy ==" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/17> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/17/0> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

<http://w3id.org/polifonia/pilot/meetups/459887/0/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Hammond was instrumental in sparking or furthering numerous musical careers, including those of Bob Dylan, Bruce Springsteen, Benny Goodman, Harry James, Charlie Christian, Billie Holiday, Count Basie, Teddy Wilson, Big Joe Turner, Pete Seeger, Babatunde Olatunji, Aretha Franklin, George Benson, Freddie Green, Leonard Cohen, Arthur Russell, Jim Copp, Asha Puthli, Stevie Ray Vaughan and Mike Bloomfield." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/0> ;
        ont:person        <http://dbpedia.org/resource/Leonard_Cohen> , <http://dbpedia.org/resource/Mike_Bloomfield> , <http://dbpedia.org/resource/George_Gershwin> , <http://dbpedia.org/resource/Asha_Puthli> , <http://dbpedia.org/resource/Freddie_Green> , <http://dbpedia.org/resource/Aretha_Franklin> , <http://dbpedia.org/resource/Lord_Arthur_Russell> , <http://dbpedia.org/resource/Charlie_Christian> , <http://dbpedia.org/resource/Bob_Dylan> , <http://dbpedia.org/resource/Harry_James> , <http://dbpedia.org/resource/Teddy_Wilson> , <http://dbpedia.org/resource/Babatunde_Olatunji> , <http://dbpedia.org/resource/Pete_Seeger> , <http://dbpedia.org/resource/Stevie_Ray_Vaughan> , <http://dbpedia.org/resource/Count_Basie> , <http://dbpedia.org/resource/Billie_Holiday> , <http://dbpedia.org/resource/Benny_Goodman> , <http://dbpedia.org/resource/Big_Joe_Turner> , <http://dbpedia.org/resource/Bruce_Springsteen> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/0/2> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

<http://w3id.org/polifonia/pilot/meetups/459887/7/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1932, Hammond acquired a nonpaying job on the WEVD radio station as a disc jockey." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/7> ;
        ont:person        <http://dbpedia.org/resource/Disc_jockey> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/7/0> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

<http://w3id.org/polifonia/pilot/meetups/459887/11/13>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In this year, Hammond broke out of the traditional role of a producer and became a talent scout, after hearing Billie Holiday." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/11> ;
        ont:person        <http://dbpedia.org/resource/Billie_Holiday> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/11/12> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

<http://w3id.org/polifonia/pilot/meetups/459887/11/4>
        rdf:type            ont:Sentence ;
        ont:excerpt         "After the instatement of Local 802, a union of professional musicians within New York City, Hammond saw more white people receiving jobs than black people." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/459887/11> ;
        ont:place           <http://dbpedia.org/resource/New_York_City> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/459887/11/3> ;
        ont:subject         <http://dbpedia.org/resource/John_Hammond_(record_producer)> ;
        ont:timeExpression  "Local 802" .

<http://w3id.org/polifonia/pilot/meetups/459887/8/11>
        rdf:type          ont:Sentence ;
        ont:excerpt       "After serving in the military during World War II, Hammond felt unmoved by the bebop jazz scene of the mid-1940s." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/8> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/8/10> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

<http://w3id.org/polifonia/pilot/meetups/459887/18/1>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Hammond received a Grammy Trustees Award for being credited with co-producing a Bessie Smith reissue in 1971, and in 1986 was inducted into the Rock and Roll Hall of Fame." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/459887/18> ;
        ont:person          <http://dbpedia.org/resource/Bessie_Smith> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/459887/18/0> ;
        ont:subject         <http://dbpedia.org/resource/John_Hammond_(record_producer)> ;
        ont:timeExpression  "reissue in 1971" .

<http://w3id.org/polifonia/pilot/meetups/459887/4/10>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Hammond notes that the first jazz music that he heard was in London in 1923 on a trip with his family." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/459887/4> ;
        ont:place           <http://dbpedia.org/resource/London> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/459887/4/9> ;
        ont:subject         <http://dbpedia.org/resource/John_Hammond_(record_producer)> ;
        ont:timeExpression  "1923 on" .

<http://w3id.org/polifonia/pilot/meetups/459887/16/9>
        rdf:type            ont:Sentence ;
        ont:excerpt         "On March 21, 1942, Hammond's sister, Alice married Benny Goodman." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/459887/16> ;
        ont:person          <http://dbpedia.org/resource/Benny_Goodman> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/459887/16/8> ;
        ont:subject         <http://dbpedia.org/resource/John_Hammond_(record_producer)> ;
        ont:timeExpression  "March 21 , 1942" .

<http://w3id.org/polifonia/pilot/meetups/459887/8/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1934, Hammond is known to have introduced Benny Goodman and Fletcher Henderson." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/8> ;
        ont:person        <http://dbpedia.org/resource/Fletcher_Henderson> , <http://dbpedia.org/resource/Benny_Goodman> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/8/0> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

<http://w3id.org/polifonia/pilot/meetups/459887/26>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/459887/26/0> .

<http://w3id.org/polifonia/pilot/meetups/459887/16/36>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Hammond threw himself back into his work, which greatly upset his wife." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/16> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/16/35> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

<http://w3id.org/polifonia/pilot/meetups/459887/19/1>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Hammond was one of the first men to racially integrate the American music industry." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/459887/19> ;
        ont:place           <http://dbpedia.org/resource/United_States> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/459887/19/0> ;
        ont:subject         <http://dbpedia.org/resource/John_Hammond_(record_producer)> ;
        ont:timeExpression  "one of" .

<http://w3id.org/polifonia/pilot/meetups/459887/16/15>
        rdf:type            ont:Sentence ;
        ont:excerpt         "However, out of a sense of shared grief following Alice's death in 1978, John and Benny reconciled." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/459887/16> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/459887/16/14> ;
        ont:subject         <http://dbpedia.org/resource/John_Hammond_(record_producer)> ;
        ont:timeExpression  "death in 1978" .

<http://w3id.org/polifonia/pilot/meetups/459887/9/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Hammond oversaw the highly influential posthumous reissues of Robert Johnson's recorded work (produced by Frank Driggs), convincing Columbia Records to issue the album King of the Delta Blues Singers in 1961." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/9> ;
        ont:person        <http://dbpedia.org/resource/Robert_Johnson> , <http://dbpedia.org/resource/Frank_Driggs> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/9/0> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

<http://w3id.org/polifonia/pilot/meetups/459887/11/21>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1933, he traveled South to attend a trial regarding the Scottsboro case, a case in which two white girls accused nine black boys of raping them." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/11> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/11/20> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

<http://w3id.org/polifonia/pilot/meetups/459887/3/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Despite the family fortune from his mother's side of the family, which included wealth from the W. & J. Sloane chain, his father worked to provide for his family and maintain the family fortune." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/3> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/3/2> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

<http://w3id.org/polifonia/pilot/meetups/459887/4/18>
        rdf:type          ont:Sentence ;
        ont:excerpt       "However, he felt limited within the confines of a boarding school." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/4> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/4/17> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

<http://w3id.org/polifonia/pilot/meetups/459887/8/8>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Four years later, he heard the Count Basie orchestra broadcasting from Kansas City and brought it to New York, where it began to receive national attention." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/8> ;
        ont:person        <http://dbpedia.org/resource/Count_Basie> ;
        ont:place         <http://dbpedia.org/resource/Kansas_City_metropolitan_area> , <http://dbpedia.org/resource/New_York_City> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/8/7> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .
