<http://w3id.org/polifonia/pilot/meetups/459887/16/25>
        rdf:type          ont:Sentence ;
        ont:excerpt       "After basic training, Hammond reported to Camp Plauche, where he was placed to organize activities for the black soldiers." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/16> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/16/24> ;
