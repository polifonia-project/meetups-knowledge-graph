<http://w3id.org/polifonia/pilot/meetups/459887/16/21>
        rdf:type          ont:Sentence ;
        ont:excerpt       "While Jemy sent Hammond a telegram to alert him of his newborn's condition, Hammond said that he never received it." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/16> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/16/20> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

<http://w3id.org/polifonia/pilot/meetups/459887/5/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Career ==" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/5> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/5/0> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

<http://w3id.org/polifonia/pilot/meetups/459887/11/27>
        rdf:type          ont:Sentence ;
        ont:excerpt       "However, in 1935, the Goodman Trio began recording." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/11> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/11/26> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

<http://w3id.org/polifonia/pilot/meetups/459887/4/24>
        rdf:type            ont:Sentence ;
        ont:excerpt         "In the fall of 1929, Hammond entered Yale University as a member of the class of 1933." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/459887/4> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/459887/4/23> ;
        ont:subject         <http://dbpedia.org/resource/John_Hammond_(record_producer)> ;
        ont:timeExpression  "fall of 1929" , "class of 1933" .

<http://w3id.org/polifonia/pilot/meetups/459887/16/2>
        rdf:type            ont:Sentence ;
        ont:excerpt         "However, in 1940 at a Manhattan party, Hammond met Jemison \"Jemy\" McBride." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/459887/16> ;
        ont:person          <http://dbpedia.org/resource/Kayla_McBride> , <http://dbpedia.org/resource/Mae_Jemison> ;
        ont:place           <http://dbpedia.org/resource/Manhattan> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/459887/16/1> ;
        ont:subject         <http://dbpedia.org/resource/John_Hammond_(record_producer)> ;
        ont:timeExpression  "1940 at" .

<http://w3id.org/polifonia/pilot/meetups/459887/6/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He moved to Greenwich Village, where he claimed to have engaged in bohemian life and worked for an integrated music world." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/6> ;
        ont:place         <http://dbpedia.org/resource/Greenwich_Village> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/6/0> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

<http://w3id.org/polifonia/pilot/meetups/459887/16/50>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Hammond was distraught and died on July 10, 1987, after a series of strokes." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/459887/16> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/459887/16/49> ;
        ont:subject         <http://dbpedia.org/resource/John_Hammond_(record_producer)> ;
        ont:timeExpression  "July 10 , 1987" .

<http://w3id.org/polifonia/pilot/meetups/459887/4/9>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Hammond shared her desire to help the community with his privilege." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/4> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/4/8> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

<http://w3id.org/polifonia/pilot/meetups/459887/19>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/459887/19/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/459887/19/1> ;
        rdf:_3    <http://w3id.org/polifonia/pilot/meetups/459887/19/2> ;
        rdf:_4    <http://w3id.org/polifonia/pilot/meetups/459887/19/3> ;
        rdf:_5    <http://w3id.org/polifonia/pilot/meetups/459887/19/4> ;
        rdf:_6    <http://w3id.org/polifonia/pilot/meetups/459887/19/5> .

<http://w3id.org/polifonia/pilot/meetups/459887/16/29>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He noted that shortly after these concerts began, an integrated sports team formed." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/16> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/16/28> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

<http://w3id.org/polifonia/pilot/meetups/459887/6>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/459887/6/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/459887/6/1> ;
        rdf:_3    <http://w3id.org/polifonia/pilot/meetups/459887/6/2> ;
        rdf:_4    <http://w3id.org/polifonia/pilot/meetups/459887/6/3> ;
        rdf:_5    <http://w3id.org/polifonia/pilot/meetups/459887/6/4> ;
        rdf:_6    <http://w3id.org/polifonia/pilot/meetups/459887/6/5> .

<http://w3id.org/polifonia/pilot/meetups/459887/0/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He is also largely responsible for the revival of delta blues artist Robert Johnson's music." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/0> ;
        ont:person        <http://dbpedia.org/resource/Robert_Johnson> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/0/3> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

<http://w3id.org/polifonia/pilot/meetups/459887/7/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He did not discriminate when choosing which musicians to air; the station allowed Hammond complete freedom on the station as long as he paid for his time slot." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/7> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/7/0> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

<http://w3id.org/polifonia/pilot/meetups/459887/11/14>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He remarks that he was astounded to discover that she was the daughter of Clarence Holiday from Fletcher Henderson's band." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/11> ;
        ont:person        <http://dbpedia.org/resource/Fletcher_Henderson> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/11/13> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

<http://w3id.org/polifonia/pilot/meetups/459887/11/5>
        rdf:type          ont:Sentence ;
        ont:excerpt       "However, this did not stop the African-American musicians." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/11> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/11/4> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

<http://w3id.org/polifonia/pilot/meetups/459887/8/12>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Rejoining Columbia Records in the late 1950s, he signed Pete Seeger and Babatunde Olatunji to the label, and discovered Aretha Franklin, then an eighteen-year-old gospel singer." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/8> ;
        ont:person        <http://dbpedia.org/resource/Babatunde_Olatunji> , <http://dbpedia.org/resource/Pete_Seeger> , <http://dbpedia.org/resource/Aretha_Franklin> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/8/11> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

<http://w3id.org/polifonia/pilot/meetups/459887/4/11>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He heard a band called The Georgians, a white Dixieland jazz group, and saw an African American show called From Dixie to Broadway, that featured Sidney Bechet." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/4> ;
        ont:person        <http://dbpedia.org/resource/Dixie> , <http://dbpedia.org/resource/Sidney_Bechet> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/4/10> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

<http://w3id.org/polifonia/pilot/meetups/459887/8/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "It is said that Hammond convinced the musicians to 'swing' the current jazz hits, so that they could play in a free manner like the original New Orleans Jazz." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/8> ;
        ont:place         <http://dbpedia.org/resource/New_Orleans> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/8/0> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

<http://w3id.org/polifonia/pilot/meetups/459887/16/37>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1948, Jemy asked Hammond for a divorce." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/16> ;
        ont:person        <http://dbpedia.org/resource/Divorce> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/16/36> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

<http://w3id.org/polifonia/pilot/meetups/459887/19/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Before the Civil Rights Act passed, Tom Wilson, an African American, replaced Hammond as Bob Dylan's record producer." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/19> ;
        ont:person        <http://dbpedia.org/resource/Bob_Dylan> , <http://dbpedia.org/resource/Thomas_F._Wilson> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/19/1> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

<http://w3id.org/polifonia/pilot/meetups/459887/16/16>
        rdf:type            ont:Sentence ;
        ont:excerpt         "In November 1943, after the United States entered World War II, Hammond began military training." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/459887/16> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/459887/16/15> ;
        ont:subject         <http://dbpedia.org/resource/John_Hammond_(record_producer)> ;
        ont:timeExpression  "November 1943" .

<http://w3id.org/polifonia/pilot/meetups/459887/9/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Musicians Hammond signed to the label included Leonard Cohen and Bruce Springsteen." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/9> ;
        ont:person        <http://dbpedia.org/resource/Leonard_Cohen> , <http://dbpedia.org/resource/Bruce_Springsteen> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/9/0> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

<http://w3id.org/polifonia/pilot/meetups/459887/11/22>
        rdf:type            ont:Sentence ;
        ont:excerpt         "The testimonies of the two girls did not align with the story." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/459887/11> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/459887/11/21> ;
        ont:subject         <http://dbpedia.org/resource/John_Hammond_(record_producer)> ;
        ont:timeExpression  "of the two" .

<http://w3id.org/polifonia/pilot/meetups/459887/3/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He worked \"as a banker, lawyer, and railroad executive\"." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/3> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/3/3> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

<http://w3id.org/polifonia/pilot/meetups/459887/4/19>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Hammond succeeded in convincing the headmaster to allow him to go into the city every other weekend, a rare privilege, so that he could take lessons from Ronald Murat." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/4> ;
        ont:person        <http://dbpedia.org/resource/Joachim_Murat> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/4/18> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

<http://w3id.org/polifonia/pilot/meetups/459887/8/9>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1938, Hammond organized the first From Spirituals to Swing concert at Carnegie Hall, presenting a broad program of blues, jazz and gospel artists, including Ida Cox, Big Joe Turner, Albert Ammons, Pete Johnson, Meade \"Lux\" Lewis, Sister Rosetta Tharpe, the Count Basie orchestra, Sidney Bechet, Sonny Terry, James P. Johnson, and Big Bill Broonzy (who took the place of the murdered Robert Johnson)." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/8> ;
        ont:person        <http://dbpedia.org/resource/Pete_Johnson> , <http://dbpedia.org/resource/C._S._Lewis> , <http://dbpedia.org/resource/Big_Joe_Turner> , <http://dbpedia.org/resource/Big_Bill_Broonzy> , <http://dbpedia.org/resource/Sonny_Terry> , <http://dbpedia.org/resource/Sidney_Bechet> , <http://dbpedia.org/resource/Lyndon_B._Johnson> , <http://dbpedia.org/resource/Sister_Rosetta_Tharpe> , <http://dbpedia.org/resource/Albert_Ammons> , <http://dbpedia.org/resource/Ida_Cox> , <http://dbpedia.org/resource/Robert_Johnson> , <http://dbpedia.org/resource/Count_Basie> ;
        ont:place         <http://dbpedia.org/resource/Carnegie_Hall> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/8/8> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

<http://w3id.org/polifonia/pilot/meetups/459887/4/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He loved Sir Harry Lauder's \"Roamin' in the Gloamin'\"." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/4> ;
        ont:person        <http://dbpedia.org/resource/Harry_Lauder> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/4/3> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

<http://w3id.org/polifonia/pilot/meetups/459887/16/45>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Left a widow, Emily Hammond became infatuated with Frank Buchman." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/16> ;
        ont:person        <http://dbpedia.org/resource/Emily_Dickinson> , <http://dbpedia.org/resource/Frank_Buchman> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/16/44> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

<http://w3id.org/polifonia/pilot/meetups/459887/14>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/459887/14/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/459887/14/1> .

<http://w3id.org/polifonia/pilot/meetups/459887/10/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "==" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/10> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/10/0> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

<http://w3id.org/polifonia/pilot/meetups/459887/16/24>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Hammond returned after basic training on a three-day pass, but he and his wife were distant." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/16> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/16/23> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

<http://w3id.org/polifonia/pilot/meetups/459887/1>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/459887/1/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/459887/1/1> .

<http://w3id.org/polifonia/pilot/meetups/459887/11/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Hammond recognized jazz music to have originated as an African-American musical genre." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/11> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/11/0> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

<http://w3id.org/polifonia/pilot/meetups/459887/4/27>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He made frequent trips into New York city and wrote regularly for trade magazines." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/4> ;
        ont:place         <http://dbpedia.org/resource/New_York_City> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/4/26> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

<http://w3id.org/polifonia/pilot/meetups/459887/16/5>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Although both sets of parents approved of the couple, neither set attended the wedding." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/16> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/16/4> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

<http://w3id.org/polifonia/pilot/meetups/459887/20/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "References ==" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/20> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/20/0> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

<http://w3id.org/polifonia/pilot/meetups/459887/6/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "To bring recognition to the Negro's supremacy in jazz was the most effective and constructive form of social protest I could think of.\"" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/6> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/6/3> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

<http://w3id.org/polifonia/pilot/meetups/459887/22>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/459887/22/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/459887/22/1> .

<http://w3id.org/polifonia/pilot/meetups/459887/12/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "==" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/12> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/12/0> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

<http://w3id.org/polifonia/pilot/meetups/459887/16/32>
        rdf:type          ont:Sentence ;
        ont:excerpt       "During this time period, bebop music grew out of late night jam sessions of black musicians." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/16> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/16/31> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

<http://w3id.org/polifonia/pilot/meetups/459887/9>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/459887/9/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/459887/9/1> ;
        rdf:_3    <http://w3id.org/polifonia/pilot/meetups/459887/9/2> ;
        rdf:_4    <http://w3id.org/polifonia/pilot/meetups/459887/9/3> .

<http://w3id.org/polifonia/pilot/meetups/459887/16/11>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Hammond did not look kindly upon her marriage to Goodman." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/16> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/16/10> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

<http://w3id.org/polifonia/pilot/meetups/459887/21/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Bibliography ==" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/21> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/21/0> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

<http://w3id.org/polifonia/pilot/meetups/459887/7/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "When the station transferred from the Broadway Central Hotel to the Claridge Hotel, the new venue would not allow the black musicians to use the main elevator." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/7> ;
        ont:place         <http://dbpedia.org/resource/Hotel_Claridge> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/7/3> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

<http://w3id.org/polifonia/pilot/meetups/459887/13/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "J. Edgar Hoover, FBI director, investigated Hammond's link to the Communist Party." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/13> ;
        ont:person        <http://dbpedia.org/resource/J._Edgar_Hoover> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/13/0> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

<http://w3id.org/polifonia/pilot/meetups/459887/11/17>
        rdf:type          ont:Sentence ;
        ont:excerpt       "After hearing her sing for the first time, he wrote, \"She weighs over 200 pounds, is incredibly beautiful, and sings as well as anybody I have ever heard." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/11> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/11/16> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

<http://w3id.org/polifonia/pilot/meetups/459887/11/8>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Hammond was able to secure contracts for various musicians." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/11> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/11/7> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

<http://w3id.org/polifonia/pilot/meetups/459887/4/14>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He learned that African American music was sold in different stores, so he began to search for this music in Harlem." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/4> ;
        ont:place         <http://dbpedia.org/resource/Harlem> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/4/13> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

<http://w3id.org/polifonia/pilot/meetups/459887/22/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "External links ==" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/22> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/22/0> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

<http://w3id.org/polifonia/pilot/meetups/459887/8/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "While initially his race proved a problem in connecting with this community, he formed relationships with various musicians that allowed him to surpass this barrier." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/8> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/8/3> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

<http://w3id.org/polifonia/pilot/meetups/459887/14/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "==" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/14> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/14/0> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

<http://w3id.org/polifonia/pilot/meetups/459887/16/40>
        rdf:type          ont:Sentence ;
        ont:excerpt       "A year later, in 1949, Hammond met Esme Sarnoff, originally Esme O'Brien, the former wife of NBC chairman Robert Sarnoff, and a daughter of Mary and Esmond O'Brien." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/16> ;
        ont:person        <http://dbpedia.org/resource/Mary_I_of_England> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/16/39> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

<http://w3id.org/polifonia/pilot/meetups/459887/19/5>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Sayin' 'Sign your kingdom over to me and be known throughout the land!'\"" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/19> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/19/4> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

<http://w3id.org/polifonia/pilot/meetups/459887/16/19>
        rdf:type          ont:Sentence ;
        ont:excerpt       "While he was still in basic training, Jemy gave birth to their second child, Douglas, early in 1944." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/16> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/16/18> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

<http://w3id.org/polifonia/pilot/meetups/459887/15/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "John had four sisters: Alice, Rachel, Adele, and Emily." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/15> ;
        ont:person        <http://dbpedia.org/resource/Emily_Dickinson> , <http://dbpedia.org/resource/Adele> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/15/0> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

<http://w3id.org/polifonia/pilot/meetups/459887/11/25>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Starting in 1935, musicians began to record in mixed-race groups." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/11> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/11/24> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

<http://w3id.org/polifonia/pilot/meetups/459887/3/7>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Well-known clergyman and peace activist William Sloane Coffin Jr. was a cousin." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/3> ;
        ont:person        <http://dbpedia.org/resource/William_Sloane_Coffin> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/3/6> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

<http://w3id.org/polifonia/pilot/meetups/459887/4/22>
        rdf:type          ont:Sentence ;
        ont:excerpt       "The summer after graduating from Hotchkiss in 1929, Hammond went to work for a newspaper in Maine, the Portland Evening News." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/4> ;
        ont:place         <http://dbpedia.org/resource/Isle_of_Portland> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/4/21> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

<http://w3id.org/polifonia/pilot/meetups/459887/16/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Early in his career, Hammond focused more on his work than his love life." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/16> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/16/0> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

<http://w3id.org/polifonia/pilot/meetups/459887/4/7>
        rdf:type          ont:Sentence ;
        ont:excerpt       "His mother also promoted social reform as a means to give back some of her fortune to the community." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/4> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/4/6> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

<http://w3id.org/polifonia/pilot/meetups/459887/16/48>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Esme Hammond was diagnosed with breast cancer." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/16> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/16/47> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

<http://w3id.org/polifonia/pilot/meetups/459887/17>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/459887/17/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/459887/17/1> .

<http://w3id.org/polifonia/pilot/meetups/459887/16/27>
        rdf:type          ont:Sentence ;
        ont:excerpt       "The armed forces were still segregated and discriminatory." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/459887/16> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/459887/16/26> ;
        ont:subject       <http://dbpedia.org/resource/John_Hammond_(record_producer)> .

