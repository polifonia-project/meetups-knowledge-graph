        ont:timeExpression  "In the 1970s" .

<http://w3id.org/polifonia/pilot/meetups/64598/26/17>
        rdf:type            ont:Sentence ;
        ont:excerpt         "He died in Watford General hospital on 31 October 1995, after a short illness, at the age of 94." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/64598/26> ;
        ont:place           <http://dbpedia.org/resource/Watford_General_Hospital> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/64598/26/16> ;
