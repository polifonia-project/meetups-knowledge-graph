        rdf:_9    <http://w3id.org/polifonia/pilot/meetups/64598/11/8> .

<http://w3id.org/polifonia/pilot/meetups/64598/33/11>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Musical Opinion's critic praised the composer's \"wonderful control and splendid craftsmanship\" in this piece, and predicted that it could become the most popular of all Bush's orchestral works." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/64598/33> ;
        ont:person        <http://dbpedia.org/resource/Composer> , <http://dbpedia.org/resource/George_W._Bush> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/64598/33/10> ;
        ont:subject       <http://dbpedia.org/resource/Alan_Bush> .

<http://w3id.org/polifonia/pilot/meetups/64598/17/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "This action drew strong protests from, among others, E. M. Forster and Ralph Vaughan Williams." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/64598/17> ;
        ont:person        <http://dbpedia.org/resource/E._M._Forster> , <http://dbpedia.org/resource/Ralph_Vaughan_Williams> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/64598/17/1> ;
        ont:subject       <http://dbpedia.org/resource/Alan_Bush> .

<http://w3id.org/polifonia/pilot/meetups/64598/21/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "====" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/64598/21> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/64598/21/0> ;
        ont:subject       <http://dbpedia.org/resource/Alan_Bush> .

<http://w3id.org/polifonia/pilot/meetups/64598/14/6>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1935 Bush began work on a piano concerto which, completed in 1937, included the unusual feature of a mixed chorus and baritone soloist in the finale, singing a radical text by Randall Swingler." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/64598/14> ;
        ont:person        <http://dbpedia.org/resource/Baritone> , <http://dbpedia.org/resource/Randall_Swingler> , <http://dbpedia.org/resource/George_W._Bush> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/64598/14/5> ;
        ont:subject       <http://dbpedia.org/resource/Alan_Bush> .

<http://w3id.org/polifonia/pilot/meetups/64598/24/7>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Colin Mason, writing in The Guardian, thought the work had a stronger socialist programme than the Nottingman symphony of 1949." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/64598/24> ;
        ont:person          <http://dbpedia.org/resource/Colin_Mason> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/64598/24/6> ;
        ont:subject         <http://dbpedia.org/resource/Alan_Bush> ;
        ont:timeExpression  "symphony of 1949" .

<http://w3id.org/polifonia/pilot/meetups/64598/4/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Family background and early life" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/64598/4> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/64598/4/0> ;
        ont:subject       <http://dbpedia.org/resource/Alan_Bush> .

<http://w3id.org/polifonia/pilot/meetups/64598/14/16>
        rdf:type            ont:Sentence ;
        ont:excerpt         "He also began to write a major orchestral work, his Symphony No. 1 in C. Amid this busy life Bush was elected a Fellow of the Royal Academy of Music." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/64598/14> ;
        ont:person          <http://dbpedia.org/resource/George_W._Bush> , <http://dbpedia.org/resource/Fellow> ;
        ont:place           <http://dbpedia.org/resource/Royal_Academy_of_Arts> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/64598/14/15> ;
        ont:subject         <http://dbpedia.org/resource/Alan_Bush> ;
        ont:timeExpression  "1 in" .

<http://w3id.org/polifonia/pilot/meetups/64598/7/8>
