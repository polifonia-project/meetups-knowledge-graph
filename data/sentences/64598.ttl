        ont:subject         <http://dbpedia.org/resource/Alan_Bush> ;
        ont:timeExpression  "for some 15" .

<http://w3id.org/polifonia/pilot/meetups/64598/0/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "A committed communist, his uncompromising political beliefs were often reflected in his music." ;
