        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/64598/26> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/64598/26/12> ;
        ont:subject       <http://dbpedia.org/resource/Alan_Bush> .

<http://w3id.org/polifonia/pilot/meetups/64598/20/18>
        rdf:type          ont:Sentence ;
        ont:excerpt       "The Daily Telegraph's critic observed that \"it was the orchestra, i.e. society, that after a strenuous opening gave up the struggle\"." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/64598/20> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/64598/20/17> ;
        ont:subject       <http://dbpedia.org/resource/Alan_Bush> .

<http://w3id.org/polifonia/pilot/meetups/64598/19/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Persona non grata =" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/64598/19> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/64598/19/0> ;
        ont:subject       <http://dbpedia.org/resource/Alan_Bush> .

<http://w3id.org/polifonia/pilot/meetups/64598/29/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "John Ireland, Bush's early mentor, instilled \"the sophisticated and restrained craftsmanship which marked Bush's music from the beginning\", introducing him to folksong and Palestrina, both important building blocks in the development of Bush's mature style." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/64598/29> ;
        ont:person        <http://dbpedia.org/resource/John_Ireland_(composer)> , <http://dbpedia.org/resource/George_W._Bush> , <http://dbpedia.org/resource/Giovanni_Pierluigi_da_Palestrina> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/64598/29/1> ;
        ont:subject       <http://dbpedia.org/resource/Alan_Bush> .

<http://w3id.org/polifonia/pilot/meetups/64598/26/6>
        rdf:type            ont:Sentence ;
        ont:excerpt         "His 80th birthday in December 1980 was celebrated at concerts in London, Birmingham and East Germany, and the BBC broadcast a special birthday musical tribute." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/64598/26> ;
        ont:place           <http://dbpedia.org/resource/Birmingham> , <http://dbpedia.org/resource/East_Germany> , <http://dbpedia.org/resource/London> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/64598/26/5> ;
        ont:subject         <http://dbpedia.org/resource/Alan_Bush> ;
        ont:timeExpression  "December 1980" .

<http://w3id.org/polifonia/pilot/meetups/64598/6/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "===" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/64598/6> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/64598/6/0> ;
        ont:subject       <http://dbpedia.org/resource/Alan_Bush> .

<http://w3id.org/polifonia/pilot/meetups/64598/10/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "The following year he joined the London Labour Choral Union (LLCU), a group of largely London-based choirs that had been organised by the socialist composer Rutland Boughton, with Labour Party support, to \"develop the musical instincts of the people and to render service to the Labour movement\"." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/64598/10> ;
        ont:person        <http://dbpedia.org/resource/Rutland_Boughton> , <http://dbpedia.org/resource/Composer> ;
        ont:place         <http://dbpedia.org/resource/London> , <http://dbpedia.org/resource/United_States> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/64598/10/1> ;
        ont:subject       <http://dbpedia.org/resource/Alan_Bush> .

<http://w3id.org/polifonia/pilot/meetups/64598/34/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "His first venture, Wat Tyler, was  written in a form which Bush thought acceptable to the general British public; it was not his choice, he wrote, that the opera and its successors all found their initial audiences in East Germany." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/64598/34> ;
        ont:person        <http://dbpedia.org/resource/Wat_Tyler> , <http://dbpedia.org/resource/George_W._Bush> ;
        ont:place         <http://dbpedia.org/resource/United_Kingdom> , <http://dbpedia.org/resource/East_Germany> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/64598/34/0> ;
        ont:subject       <http://dbpedia.org/resource/Alan_Bush> .

<http://w3id.org/polifonia/pilot/meetups/64598/20/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "But the composer's refusal to modify his pro-Soviet stance following the onset of the Cold War alienated both the public and the music establishment, a factor which Bush acknowledged 20 years later: \"People who were in a position to promote my works were afraid to do so." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/64598/20> ;
        ont:person        <http://dbpedia.org/resource/George_W._Bush> , <http://dbpedia.org/resource/Composer> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/64598/20/2> ;
        ont:subject       <http://dbpedia.org/resource/Alan_Bush> .

<http://w3id.org/polifonia/pilot/meetups/64598/41>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/64598/41/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/64598/41/1> ;
        rdf:_3    <http://w3id.org/polifonia/pilot/meetups/64598/41/2> .

<http://w3id.org/polifonia/pilot/meetups/64598/22/21>
        rdf:type            ont:Sentence ;
        ont:excerpt         "The opera, to a libretto by Barrie Stavis, was premiered at the German Berlin State Opera on 29 September 1970, and in 1979 was broadcast by the BBC." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/64598/22> ;
        ont:person          <http://dbpedia.org/resource/Barrie_Stavis> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/64598/22/20> ;
        ont:subject         <http://dbpedia.org/resource/Alan_Bush> ;
        ont:timeExpression  "29 September 1970" .

<http://w3id.org/polifonia/pilot/meetups/64598/17/12>
        rdf:type            ont:Sentence ;
        ont:excerpt         "The posting was withdrawn; and Bush remained in London until his discharge in December 1945." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/64598/17> ;
        ont:person          <http://dbpedia.org/resource/George_W._Bush> ;
        ont:place           <http://dbpedia.org/resource/London> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/64598/17/11> ;
        ont:subject         <http://dbpedia.org/resource/Alan_Bush> ;
        ont:timeExpression  "December 1945" .

<http://w3id.org/polifonia/pilot/meetups/64598/31/5>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Nancy Bush describes the Piano Concerto as Bush's first attempt to fuse his musical and political ideas." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/64598/31> ;
        ont:person        <http://dbpedia.org/resource/Nancy_Reagan> , <http://dbpedia.org/resource/George_W._Bush> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/64598/31/4> ;
        ont:subject       <http://dbpedia.org/resource/Alan_Bush> .

<http://w3id.org/polifonia/pilot/meetups/64598/11>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/64598/11/0> ;
