<http://w3id.org/polifonia/pilot/meetups/64598/7/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "1, and Piano Sonata in B minor, Op." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/64598/7> ;
