        ont:subject         <http://dbpedia.org/resource/Alan_Bush> ;
        ont:timeExpression  "1945 ===" .

<http://w3id.org/polifonia/pilot/meetups/64598/22/13>
        rdf:type          ont:Sentence ;
        ont:excerpt       "\"For his third opera, Bush chose a contemporary theme – the struggle against colonial rule." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/64598/22> ;
        ont:person        <http://dbpedia.org/resource/George_W._Bush> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/64598/22/12> ;
        ont:subject       <http://dbpedia.org/resource/Alan_Bush> .

<http://w3id.org/polifonia/pilot/meetups/64598/20/11>
        rdf:type          ont:Sentence ;
        ont:excerpt       "The orchestra had suspended its operations in 1941; after the war, Bush's failure to secure funding for it led to its closure, despite considerable artistic success." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/64598/20> ;
        ont:person        <http://dbpedia.org/resource/George_W._Bush> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/64598/20/10> ;
        ont:subject       <http://dbpedia.org/resource/Alan_Bush> .

