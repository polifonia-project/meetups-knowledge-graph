<http://w3id.org/polifonia/pilot/meetups/64598/10/6>
        rdf:type          ont:Sentence ;
        ont:excerpt       "His music at the time seemed so adventurous and vigorous\"." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/64598/10> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/64598/10/5> ;
