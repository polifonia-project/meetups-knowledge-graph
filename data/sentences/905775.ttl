        rdf:type          ont:Sentence ;
        ont:excerpt       "The final Moebius & Plank collaboration, En Route was recorded in Conny's Studio in 1986 but left incomplete as Plank's health deteriorated." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/905775/15> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/905775/15/6> ;
