<http://w3id.org/polifonia/pilot/meetups/905775/21/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "His studio, at his home on the southern outskirts of Cologne, continued to be run by his widow Christa Fast and their son until her failing health and the general change in the music business forced them to offer its contents for sale in May 2006." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/905775/21> ;
        ont:place           <http://dbpedia.org/resource/Cologne> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/905775/21/0> ;
        ont:subject         <http://dbpedia.org/resource/Conny_Plank> ;
        ont:timeExpression  "May 2006" .

<http://w3id.org/polifonia/pilot/meetups/905775/15/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1983, Moebius & Plank also recorded the album Ludwig's Law using an Emulator, an early form of sampling keyboard that enabled them to duplicate other instruments without having musicians to play them." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/905775/15> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/905775/15/3> ;
        ont:subject       <http://dbpedia.org/resource/Conny_Plank> .

<http://w3id.org/polifonia/pilot/meetups/905775/34>
        rdf:type  ont:Paragraph ;
