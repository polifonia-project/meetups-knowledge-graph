<http://w3id.org/polifonia/pilot/meetups/905775/21/5>
        rdf:type          ont:Sentence ;
        ont:excerpt       "It is also reputedly laminated in wood taken from a single cherry tree from Plank's own garden." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/905775/21> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/905775/21/4> ;
        ont:subject       <http://dbpedia.org/resource/Conny_Plank> .

<http://w3id.org/polifonia/pilot/meetups/905775/6/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Plank favored a very 'live' production sound, especially on drums." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/905775/6> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/905775/6/0> ;
        ont:subject       <http://dbpedia.org/resource/Conny_Plank> .

<http://w3id.org/polifonia/pilot/meetups/905775/1/1>
        rdf:type            ont:Sentence ;
        ont:excerpt         "He collaborated with Dieter Moebius on five Moebius & Plank studio albums recorded between 1979 and 1986." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/905775/1> ;
        ont:person          <http://dbpedia.org/resource/Dieter_Moebius> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/905775/1/0> ;
        ont:subject         <http://dbpedia.org/resource/Conny_Plank> ;
        ont:timeExpression  "between 1979 and 1986" .

<http://w3id.org/polifonia/pilot/meetups/905775/0/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "His innovative work as a sound engineer and producer in Germany's krautrock and kosmische music scenes helped to shape postwar European popular music." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/905775/0> ;
        ont:place         <http://dbpedia.org/resource/Germany> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/905775/0/0> ;
        ont:subject       <http://dbpedia.org/resource/Conny_Plank> .

<http://w3id.org/polifonia/pilot/meetups/905775/5/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Plank used multi-track recording facilities." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/905775/5> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/905775/5/0> ;
        ont:subject       <http://dbpedia.org/resource/Conny_Plank> .

