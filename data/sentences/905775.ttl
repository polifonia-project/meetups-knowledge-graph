        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/905775/33/0> .

<http://w3id.org/polifonia/pilot/meetups/905775/4/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Plank (who began his career as soundman for Marlene Dietrich) was an ardent believer in the possibilities of electronic music and electronic soundscapes." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/905775/4> ;
        ont:person        <http://dbpedia.org/resource/Marlene_Dietrich> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/905775/4/0> ;
        ont:subject       <http://dbpedia.org/resource/Conny_Plank> .

<http://w3id.org/polifonia/pilot/meetups/905775/18>
