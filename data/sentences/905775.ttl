<http://w3id.org/polifonia/pilot/meetups/905775/15/3>
        rdf:type            ont:Sentence ;
        ont:excerpt         "These two albums are early examples of the predecessors of techno and electronica." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/905775/15> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/905775/15/2> ;
        ont:subject         <http://dbpedia.org/resource/Conny_Plank> ;
        ont:timeExpression  "two" .

