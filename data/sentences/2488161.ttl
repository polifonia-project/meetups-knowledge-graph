<http://w3id.org/polifonia/pilot/meetups/2488161/6/8>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He went up to the 52nd floor of the CBS building where they had to wrestle him to the ground." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2488161/6> ;
        ont:place         <http://dbpedia.org/resource/CBS_Building> ;
