        ont:person        <http://dbpedia.org/resource/Oswald_of_Northumbria> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2488161/9/12> ;
        ont:subject       <http://dbpedia.org/resource/Skip_Spence> .

<http://w3id.org/polifonia/pilot/meetups/2488161/5/12>
        rdf:type          ont:Sentence ;
        ont:excerpt       "\"As described by bandmate Peter Lewis, it appears that both Jerry Miller and bandmate Don Stevenson were targets of Spence:" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2488161/5> ;
        ont:person        <http://dbpedia.org/resource/Peter_B._Lewis> , <http://dbpedia.org/resource/Jerry_Miller> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2488161/5/11> ;
        ont:subject       <http://dbpedia.org/resource/Skip_Spence> .

<http://w3id.org/polifonia/pilot/meetups/2488161/9/5>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Voluminous consumption of heroin and cocaine resulted in a further involuntary committal for Spence." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2488161/9> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2488161/9/4> ;
        ont:subject       <http://dbpedia.org/resource/Skip_Spence> .

<http://w3id.org/polifonia/pilot/meetups/2488161/13/5>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Don Stevenson (an original Moby Grape member) also performed." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2488161/13> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2488161/13/4> ;
        ont:subject       <http://dbpedia.org/resource/Skip_Spence> .

<http://w3id.org/polifonia/pilot/meetups/2488161/5/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Spence was a guitarist in the band the Other Side before Marty Balin recruited him to be the drummer for Jefferson Airplane (apparently because he looked the part)." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2488161/5> ;
        ont:person        <http://dbpedia.org/resource/Marty_Balin> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2488161/5/0> ;
        ont:subject       <http://dbpedia.org/resource/Skip_Spence> .

<http://w3id.org/polifonia/pilot/meetups/2488161/41/0>
