<http://w3id.org/polifonia/pilot/meetups/2488161/6/13>
        rdf:type          ont:Sentence ;
        ont:excerpt       "They shot him full of Thorazine for six months." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2488161/6> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2488161/6/12> ;
