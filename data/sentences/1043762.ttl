        ont:subject         <http://dbpedia.org/resource/Jack_Levine> ;
        ont:timeExpression  "1956" , "1958" , "1954" .

<http://w3id.org/polifonia/pilot/meetups/1043762/2/2>
        rdf:type            ont:Sentence ;
        ont:excerpt         "At Harvard University from 1929 to 1933, Levine and classmate Hyman Bloom studied with Denman Ross." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1043762/2> ;
        ont:person          <http://dbpedia.org/resource/Denman_Ross> , <http://dbpedia.org/resource/Hyman_Bloom> ;
