<http://w3id.org/polifonia/pilot/meetups/1043762/4/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1978 a retrospective of Levine's work was held at the Jewish Museum (New York)." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1043762/4> ;
        ont:place         <http://dbpedia.org/resource/New_York_City> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1043762/4/3> ;
