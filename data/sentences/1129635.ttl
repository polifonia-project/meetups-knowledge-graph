<http://w3id.org/polifonia/pilot/meetups/1129635/20/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He appeared in the audiobook of Jimmy Buffett's A Salty Piece of Land." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1129635/20> ;
        ont:person        <http://dbpedia.org/resource/Jimmy_Buffett> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1129635/20/0> ;
        ont:subject       <http://dbpedia.org/resource/J._D._Souther> .

<http://w3id.org/polifonia/pilot/meetups/1129635/13/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "On October 14, 2008, Souther released If the World Was You, his first new studio release in nearly 25 years." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1129635/13> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1129635/13/0> ;
        ont:subject         <http://dbpedia.org/resource/J._D._Souther> ;
        ont:timeExpression  "October 14 , 2008" .

<http://w3id.org/polifonia/pilot/meetups/1129635/12/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "The Don Henley hit song \"The Heart of the Matter\" (1989) was also co-written by Souther." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1129635/12> ;
        ont:person          <http://dbpedia.org/resource/Don_Henley> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1129635/12/0> ;
        ont:subject         <http://dbpedia.org/resource/J._D._Souther> ;
        ont:timeExpression  "1989" .

<http://w3id.org/polifonia/pilot/meetups/1129635/1>
