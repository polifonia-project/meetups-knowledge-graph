        ont:subject         <http://dbpedia.org/resource/J._D._Souther> ;
        ont:timeExpression  "December 2002" .

<http://w3id.org/polifonia/pilot/meetups/1129635/3/2>
        rdf:type            ont:Sentence ;
        ont:excerpt         "After moving to Los Angeles in the late 1960s, Souther met musician and songwriter Glenn Frey, and eventually the two became roommates and musical collaborators, and for a time performed as a folk duo using the name Longbranch Pennywhistle." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1129635/3> ;
        ont:person          <http://dbpedia.org/resource/Glenn_Frey> ;
        ont:place           <http://dbpedia.org/resource/Los_Angeles> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1129635/3/1> ;
        ont:subject         <http://dbpedia.org/resource/J._D._Souther> ;
        ont:timeExpression  "two" .

