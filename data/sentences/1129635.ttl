        rdf:type            ont:Sentence ;
        ont:excerpt         "The Eagles recording \"Heartache Tonight,\" written by Souther, Bob Seger, Glenn Frey, and Don Henley, was released in 1979 and became the band's final chart-topping song on the Hot 100." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1129635/8> ;
        ont:person          <http://dbpedia.org/resource/Don_Henley> , <http://dbpedia.org/resource/Glenn_Frey> , <http://dbpedia.org/resource/Bob_Seger> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1129635/8/0> ;
