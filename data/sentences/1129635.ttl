<http://w3id.org/polifonia/pilot/meetups/1129635/14/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "On May 31, 2011, Souther released Natural History, featuring new versions of his songs recorded by other artists." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1129635/14> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1129635/14/0> ;
