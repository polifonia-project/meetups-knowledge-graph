        rdf:type            ont:Sentence ;
        ont:excerpt         "On October 9, 2012, he released Midnight in Tokyo, an EP that was recorded live." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1129635/15> ;
        ont:place           <http://dbpedia.org/resource/Tokyo> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1129635/15/0> ;
