        rdf:type            ont:Sentence ;
        ont:excerpt         "In December 2002, Souther moved from the Hollywood Hills to Nashville, Tennessee." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1129635/23> ;
        ont:place           <http://dbpedia.org/resource/Hollywood_Hills> , <http://dbpedia.org/resource/Tennessee> ;
