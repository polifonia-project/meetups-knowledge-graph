<http://w3id.org/polifonia/pilot/meetups/898417/2/6>
        rdf:type          ont:Sentence ;
        ont:excerpt       "\"My mother got to Panama with directions to the home of a family called Kopp." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/898417/2> ;
        ont:place         <http://dbpedia.org/resource/Panama> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/898417/2/5> ;
