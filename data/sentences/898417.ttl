        rdf:type          ont:Sentence ;
        ont:excerpt       "He was discharged from the Army in 1947, at the rank of Staff Sergeant." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/898417/6> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/898417/6/14> ;
