<http://w3id.org/polifonia/pilot/meetups/898417/21/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "They played the judges of the \"Drag Queen of the Year Contest\" that opened the film." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/898417/21> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/898417/21/1> ;
        ont:subject       <http://dbpedia.org/resource/José_Sarria> .

<http://w3id.org/polifonia/pilot/meetups/898417/9/18>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Sarria encouraged patrons to be as open and honest as possible." ;
