        rdf:type          ont:Sentence ;
        ont:excerpt       "Brendel certainly takes his place among the greatest Beethoven interpreters of any time, and this disc finds him at his most inspiring." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/140417/3> ;
        ont:person        <http://dbpedia.org/resource/Ludwig_van_Beethoven> , <http://dbpedia.org/resource/Alfred_Brendel> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/140417/3/23> ;
        ont:subject       <http://dbpedia.org/resource/Alfred_Brendel> .

<http://w3id.org/polifonia/pilot/meetups/140417/38/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He has received Lifetime Achievement Awards by Edison, Midem Classical Awards, Deutscher Schallplattenpreis, Gramophone, and ECHO Klassik." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/140417/38> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/140417/38/1> ;
