        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/166331/25/25> ;
        ont:subject         <http://dbpedia.org/resource/Celia_Cruz> ;
        ont:timeExpression  "June 2 , 2021" , "East 195th" .

