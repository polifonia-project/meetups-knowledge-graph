        ont:timeExpression  "26 September 2007" , "25 May 2008" .

<http://w3id.org/polifonia/pilot/meetups/166331/17/7>
        rdf:type          ont:Sentence ;
        ont:excerpt       "For this record she won her third Latin Grammy and her second American Grammy." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/166331/17> ;
        ont:place         <http://dbpedia.org/resource/United_States> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/166331/17/6> ;
        ont:subject       <http://dbpedia.org/resource/Celia_Cruz> .

<http://w3id.org/polifonia/pilot/meetups/166331/2/2>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Celia was one of the eldest among fourteen children, including her three siblings, Dolores, Gladys and Bárbaro, and she used to sing cradle songs to put them to sleep." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/166331/2> ;
        ont:person          <http://dbpedia.org/resource/Mary_Pickford> ;
        ont:place           <http://dbpedia.org/resource/Dolores_Hidalgo> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/166331/2/1> ;
        ont:subject         <http://dbpedia.org/resource/Celia_Cruz> ;
        ont:timeExpression  "one of" .

<http://w3id.org/polifonia/pilot/meetups/166331/25/23>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 2018, a monument to Celia Cruz was unveiled in the Cuban Heritage Park in Hialeah, Florida." ;
