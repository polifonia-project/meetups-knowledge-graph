        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/166331/25/17> ;
        ont:subject         <http://dbpedia.org/resource/Celia_Cruz> ;
        ont:timeExpression  "October 2015" .

<http://w3id.org/polifonia/pilot/meetups/166331/0/5>
        rdf:type            ont:Sentence ;
        ont:excerpt         "In 1960, after the Cuban Revolution caused the nationalization of the music industry, Cruz left her native country, becoming one of the symbols and spokespersons of the Cuban community in exile." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/166331/0> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/166331/0/4> ;
        ont:subject         <http://dbpedia.org/resource/Celia_Cruz> ;
        ont:timeExpression  "one of" .

