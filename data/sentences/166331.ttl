        ont:place         <http://dbpedia.org/resource/Mexico> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/166331/16/6> ;
        ont:subject       <http://dbpedia.org/resource/Celia_Cruz> .

<http://w3id.org/polifonia/pilot/meetups/166331/20/3>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Confident, Cruz said she did not shed one tear and that she was aiming to resume her artistic career." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/166331/20> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/166331/20/2> ;
        ont:subject         <http://dbpedia.org/resource/Celia_Cruz> ;
        ont:timeExpression  "one tear" .

