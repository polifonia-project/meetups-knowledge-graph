        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1990, Cruz won her first Grammy Award (Best Tropical Latin Performance) for her album Ritmo en el corazón, recorded with Ray Barretto." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/166331/14> ;
