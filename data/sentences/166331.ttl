<http://w3id.org/polifonia/pilot/meetups/166331/10/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "On 15 July 1960, following the Cuban Revolution, a contract for Sonora Matancera in Mexico arose." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/166331/10> ;
        ont:place           <http://dbpedia.org/resource/Mexico> ;
