        rdf:type            ont:Sentence ;
        ont:excerpt         "A live recording of this song was subsequently made available in 2005 on a commemorative CD honoring the festival's then 20-year history entitled, \"Central Park SummerStage: Live from the Heart of the City\"." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/166331/18> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/166331/18/1> ;
