        rdf:type          ont:Sentence ;
        ont:excerpt       "Through a formidable work ethic, Cruz rose to the very top in her genre." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/166331/23> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/166331/23/5> ;
