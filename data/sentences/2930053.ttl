<http://w3id.org/polifonia/pilot/meetups/2930053/4/2>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Wilson was signed to the Soul City imprint and Rivers produced the sessions that yielded the 1968 U.S. R&B hit single \"The Snake\" (U.S. Pop #27), which became popular on the Northern Soul circuit in the United Kingdom." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/2930053/4> ;
        ont:person          <http://dbpedia.org/resource/Woodrow_Wilson> ;
        ont:place           <http://dbpedia.org/resource/Soul_City,_North_Carolina> , <http://dbpedia.org/resource/United_Kingdom> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/2930053/4/1> ;
        ont:subject         <http://dbpedia.org/resource/Al_Wilson_(singer)> ;
        ont:timeExpression  "1968" , "27" .

