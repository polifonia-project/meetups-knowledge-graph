        ont:subject         <http://dbpedia.org/resource/Al_Wilson_(singer)> ;
        ont:timeExpression  "41 in" .

<http://w3id.org/polifonia/pilot/meetups/2930053/3/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He also developed his stand-up comedy routine in case he didn't succeed as a singer." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2930053/3> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2930053/3/2> ;
