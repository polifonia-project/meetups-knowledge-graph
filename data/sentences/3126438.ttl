<http://w3id.org/polifonia/pilot/meetups/3126438/2/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Born Marguerite Upton in 1893 in Berkley, Virginia (now part of Norfolk), she was known as Peggy." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/3126438/2> ;
        ont:person          <http://dbpedia.org/resource/Margaret_of_Valois> ;
        ont:place           <http://dbpedia.org/resource/Virginia> , <http://dbpedia.org/resource/Norfolk> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/3126438/2/0> ;
        ont:subject         <http://dbpedia.org/resource/Peggy_Hopkins_Joyce> ;
        ont:timeExpression  "1893 in" .

<http://w3id.org/polifonia/pilot/meetups/3126438/11/52>
        rdf:type            ont:Sentence ;
        ont:excerpt         "\"On December 3, 1945, Joyce married for the fifth time to consulting engineer Anthony Easton." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/3126438/11> ;
        ont:person          <http://dbpedia.org/resource/James_Joyce> , <http://dbpedia.org/resource/Engineer> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/3126438/11/51> ;
        ont:subject         <http://dbpedia.org/resource/Peggy_Hopkins_Joyce> ;
        ont:timeExpression  "December 3 , 1945" .

<http://w3id.org/polifonia/pilot/meetups/3126438/11/31>
        rdf:type          ont:Sentence ;
        ont:excerpt       "While she was recuperating, she gave an interview to a reporter claiming that she was \"...through with men.\"" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/3126438/11> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/3126438/11/30> ;
        ont:subject       <http://dbpedia.org/resource/Peggy_Hopkins_Joyce> .

<http://w3id.org/polifonia/pilot/meetups/3126438/3/0>
        rdf:type          ont:Sentence ;
