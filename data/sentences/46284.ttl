        ont:excerpt       "==" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/46284/54> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/46284/54/0> ;
        ont:subject       <http://dbpedia.org/resource/Milton_Berle> .

<http://w3id.org/polifonia/pilot/meetups/46284/30/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "At the time, Berle was discussing the emotional fallout from an experience he had with impregnating a woman with whom he was not married, having to then decide whether or not they would keep the child." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/46284/30> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/46284/30/0> ;
        ont:subject       <http://dbpedia.org/resource/Milton_Berle> .

<http://w3id.org/polifonia/pilot/meetups/46284/13/6>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Others in the cast were Pert Kelton, Mary Schipp, Jack Albertson, Arthur Q. Bryan, Ed Begley, Brazilian singer Dick Farney, and announcer Frank Gallop." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/46284/13> ;
        ont:person        <http://dbpedia.org/resource/Jack_Albertson> , <http://dbpedia.org/resource/Pert_Kelton> , <http://dbpedia.org/resource/Frank_Gallop> , <http://dbpedia.org/resource/Mike_Bryan> , <http://dbpedia.org/resource/King_Arthur> , <http://dbpedia.org/resource/Dick_Farney> , <http://dbpedia.org/resource/Mary_I_of_England> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/46284/13/5> ;
        ont:subject       <http://dbpedia.org/resource/Milton_Berle> .

<http://w3id.org/polifonia/pilot/meetups/46284/37/5>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Berle had two stepdaughters from his marriage to Adams, Leslie and Susan Brown." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/46284/37> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/46284/37/4> ;
        ont:subject       <http://dbpedia.org/resource/Milton_Berle> .

<http://w3id.org/polifonia/pilot/meetups/46284/22>
