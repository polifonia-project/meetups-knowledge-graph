<http://w3id.org/polifonia/pilot/meetups/46284/4/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He also worked as a child model and was \"Buster Brown\" for \"Buster Brown\" shoes." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/46284/4> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/46284/4/0> ;
