        rdf:type          ont:Sentence ;
        ont:excerpt       "A few years later, the men started working on Kook's Tour, a new TV series." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/589925/24> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/589925/24/0> ;
        ont:subject       <http://dbpedia.org/resource/Larry_Fine> .

