<http://w3id.org/polifonia/pilot/meetups/589925/30/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He’s the reactor, the most vulnerable." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/589925/30> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/589925/30/0> ;
        ont:subject       <http://dbpedia.org/resource/Larry_Fine> .

