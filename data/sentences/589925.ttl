        ont:subject         <http://dbpedia.org/resource/Larry_Fine> ;
        ont:timeExpression  "October 5 , 1902" , "family at 3rd" .

<http://w3id.org/polifonia/pilot/meetups/589925/19/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "They married in 1926." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/589925/19> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/589925/19/0> ;
        ont:subject       <http://dbpedia.org/resource/Larry_Fine> .

<http://w3id.org/polifonia/pilot/meetups/589925/17/9>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He was a yapper.\"" ;
