<http://w3id.org/polifonia/pilot/meetups/589925/4/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "His parents later gave Fine violin lessons to help strengthen the damaged muscles in his forearm." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/589925/4> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/589925/4/0> ;
