<http://w3id.org/polifonia/pilot/meetups/589925/20/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In an interview, Fine admitted that he often gave money to actors who needed help and never asked to be repaid." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/589925/20> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/589925/20/3> ;
        ont:subject       <http://dbpedia.org/resource/Larry_Fine> .

<http://w3id.org/polifonia/pilot/meetups/589925/27/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Legacy ==" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/589925/27> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/589925/27/0> ;
        ont:subject       <http://dbpedia.org/resource/Larry_Fine> .

<http://w3id.org/polifonia/pilot/meetups/589925/12/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Healy encouraged Fine to keep the hairstyle." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/589925/12> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/589925/12/2> ;
