<http://w3id.org/polifonia/pilot/meetups/589925/33/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Young Larry is portrayed by Lance Chantiles-Wertz." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/589925/33> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/589925/33/0> ;
        ont:subject       <http://dbpedia.org/resource/Larry_Fine> .

