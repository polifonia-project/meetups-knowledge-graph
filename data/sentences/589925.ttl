        rdf:type          ont:Sentence ;
        ont:excerpt       "The effort to create a mural on that site began when a local weekly newspaper suggested that the city should somehow honor him." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/589925/31> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/589925/31/0> ;
