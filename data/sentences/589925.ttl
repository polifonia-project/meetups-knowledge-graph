<http://w3id.org/polifonia/pilot/meetups/589925/19/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Fine met his wife, Mabel Haney, in 1922, when both were working in vaudeville." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/589925/19> ;
        ont:person        <http://dbpedia.org/resource/Mabel_Gardiner_Hubbard> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/589925/19/0> ;
        ont:subject       <http://dbpedia.org/resource/Larry_Fine> .

<http://w3id.org/polifonia/pilot/meetups/589925/17/8>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He was the kind of guy who always said anything." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/589925/17> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/589925/17/7> ;
