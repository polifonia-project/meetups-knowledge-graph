<http://w3id.org/polifonia/pilot/meetups/44309/2/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Younger American playwrights, such as Paula Vogel, credit Albee's mix of theatricality and biting dialogue with helping to reinvent postwar American theatre in the early 1960s." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/44309/2> ;
        ont:person        <http://dbpedia.org/resource/Paula_Vogel> ;
        ont:place         <http://dbpedia.org/resource/United_States> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/44309/2/0> ;
        ont:subject       <http://dbpedia.org/resource/Edward_Albee> .

<http://w3id.org/polifonia/pilot/meetups/44309/14/10>
        rdf:type            ont:Sentence ;
        ont:excerpt         "His longtime partner, Jonathan Richard Thomas, a sculptor, died on May 2, 2005 from bladder cancer." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/44309/14> ;
        ont:person          <http://dbpedia.org/resource/Richard_Thomas_(actor)> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/44309/14/9> ;
        ont:subject         <http://dbpedia.org/resource/Edward_Albee> ;
        ont:timeExpression  "May 2 , 2005" .

