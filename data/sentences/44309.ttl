        rdf:_3    <http://w3id.org/polifonia/pilot/meetups/44309/19/2> .

<http://w3id.org/polifonia/pilot/meetups/44309/1/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "His works are often considered frank examinations of the modern condition." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/44309/1> ;
