        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/44309/6/4> ;
        ont:subject       <http://dbpedia.org/resource/Edward_Albee> .

<http://w3id.org/polifonia/pilot/meetups/44309/14/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Albee dated Muir's sister, Delphine, and escorted her to her coming-out party." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/44309/14> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/44309/14/3> ;
        ont:subject       <http://dbpedia.org/resource/Edward_Albee> .

<http://w3id.org/polifonia/pilot/meetups/44309/2/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "His middle period comprised plays that explored the psychology of maturing, marriage, and sexual relationships." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/44309/2> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/44309/2/0> ;
        ont:subject       <http://dbpedia.org/resource/Edward_Albee> .

<http://w3id.org/polifonia/pilot/meetups/44309/19>
