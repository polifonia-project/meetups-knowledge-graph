<http://w3id.org/polifonia/pilot/meetups/7672/13/1>
        rdf:type            ont:Sentence ;
        ont:excerpt         "In 1963, Metro-Goldwyn-Mayer contracted with Sib Tower 12 to have Jones and his staff produce new Tom and Jerry cartoons as well as a television adaptation of all Tom and Jerry theatricals produced to that date." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/7672/13> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/7672/13/0> ;
        ont:subject         <http://dbpedia.org/resource/Chuck_Jones> ;
        ont:timeExpression  "Tower 12" .

<http://w3id.org/polifonia/pilot/meetups/7672/8/8>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Almost all animators joined, in reaction to salary cuts imposed by Leon Schlesinger." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/7672/8> ;
        ont:person        <http://dbpedia.org/resource/Leon_Schlesinger> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/7672/8/7> ;
        ont:subject       <http://dbpedia.org/resource/Chuck_Jones> .

