<http://w3id.org/polifonia/pilot/meetups/7672/8/16>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Because of his role as a supervisor in the studio, he could not himself join the union." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/7672/8> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/7672/8/15> ;
        ont:subject       <http://dbpedia.org/resource/Chuck_Jones> .

<http://w3id.org/polifonia/pilot/meetups/7672/10/5>
        rdf:type          ont:Sentence ;
        ont:excerpt       "The feature was produced by UPA and directed by his former Warner Bros. collaborator, Abe Levitow." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/7672/10> ;
        ont:person        <http://dbpedia.org/resource/Abe_Levitow> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/7672/10/4> ;
        ont:subject       <http://dbpedia.org/resource/Chuck_Jones> .

<http://w3id.org/polifonia/pilot/meetups/7672/17/2>
        rdf:type            ont:Sentence ;
        ont:excerpt         "New shorts were made for Bugs Bunny's Bustin' Out All Over (1980)." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/7672/17> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/7672/17/1> ;
        ont:subject         <http://dbpedia.org/resource/Chuck_Jones> ;
        ont:timeExpression  "1980" .

<http://w3id.org/polifonia/pilot/meetups/7672/8/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Jones became a director (or \"supervisor\", the original title for an animation director in the studio) himself in 1938 when Frank Tashlin left the studio." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/7672/8> ;
        ont:person        <http://dbpedia.org/resource/Frank_Tashlin> , <http://dbpedia.org/resource/Animation_director> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/7672/8/3> ;
        ont:subject       <http://dbpedia.org/resource/Chuck_Jones> .

<http://w3id.org/polifonia/pilot/meetups/7672/20>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/7672/20/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/7672/20/1> .

<http://w3id.org/polifonia/pilot/meetups/7672/9/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "During the same year, he directed Hell-Bent for Election, a campaign film for Franklin D. Roosevelt." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/7672/9> ;
        ont:person        <http://dbpedia.org/resource/Franklin_D._Roosevelt> , <http://dbpedia.org/resource/Benjamin_Franklin> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/7672/9/3> ;
        ont:subject       <http://dbpedia.org/resource/Chuck_Jones> .

<http://w3id.org/polifonia/pilot/meetups/7672/27/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "I stand guilty before the world of directing over three hundred cartoons in the last fifty or sixty years." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/7672/27> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/7672/27/2> ;
        ont:subject       <http://dbpedia.org/resource/Chuck_Jones> .

<http://w3id.org/polifonia/pilot/meetups/7672/19/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "It was forwarded to Michael Barrier, who conducted the interview with Clampett and was distributed by Jones to multiple people concerned with animation over the years." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/7672/19> ;
        ont:person        <http://dbpedia.org/resource/Michael_Barrier> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/7672/19/1> ;
        ont:subject       <http://dbpedia.org/resource/Chuck_Jones> .

<http://w3id.org/polifonia/pilot/meetups/7672/5/12>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Then I went on to take animator's drawings and traced them onto the celluloid." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/7672/5> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/7672/5/11> ;
        ont:subject       <http://dbpedia.org/resource/Chuck_Jones> .

<http://w3id.org/polifonia/pilot/meetups/7672/13/5>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Jones directed the classic animated short" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/7672/13> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/7672/13/4> ;
        ont:subject       <http://dbpedia.org/resource/Chuck_Jones> .

<http://w3id.org/polifonia/pilot/meetups/7672/4>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/7672/4/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/7672/4/1> .

<http://w3id.org/polifonia/pilot/meetups/7672/22/6>
        rdf:type          ont:Sentence ;
        ont:excerpt       "The cartoon was dedicated to Friz Freleng, who had died in 1995." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/7672/22> ;
        ont:person        <http://dbpedia.org/resource/Friz_Freleng> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/7672/22/5> ;
        ont:subject       <http://dbpedia.org/resource/Chuck_Jones> .

<http://w3id.org/polifonia/pilot/meetups/7672/28>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/7672/28/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/7672/28/1> .

<http://w3id.org/polifonia/pilot/meetups/7672/29/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "For his contribution to the motion picture industry, Jones has a star on the Hollywood Walk of Fame at 7011 Hollywood Blvd." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/7672/29> ;
        ont:place         <http://dbpedia.org/resource/Hollywood> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/7672/29/2> ;
        ont:subject       <http://dbpedia.org/resource/Chuck_Jones> .

<http://w3id.org/polifonia/pilot/meetups/7672/8/11>
        rdf:type          ont:Sentence ;
        ont:excerpt       "His insulting manner had a unifying effect on the staff." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/7672/8> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/7672/8/10> ;
        ont:subject       <http://dbpedia.org/resource/Chuck_Jones> .

<http://w3id.org/polifonia/pilot/meetups/7672/10/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Jones remained at Warner Bros. throughout the 1950s, except for a brief period in 1953 when Warner closed the animation studio." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/7672/10> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/7672/10/0> ;
        ont:subject         <http://dbpedia.org/resource/Chuck_Jones> ;
        ont:timeExpression  "period in 1953" , "throughout the 1950s" .

