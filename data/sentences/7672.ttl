        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/7672/34> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/7672/34/0> ;
        ont:subject       <http://dbpedia.org/resource/Chuck_Jones> .

<http://w3id.org/polifonia/pilot/meetups/7672/1/4>
        rdf:type            ont:Sentence ;
