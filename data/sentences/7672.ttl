        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1997, Jones was awarded the Edward MacDowell Medal." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/7672/22> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/7672/22/10> ;
