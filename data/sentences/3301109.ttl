<http://w3id.org/polifonia/pilot/meetups/3301109/12/13>
        rdf:type          ont:Sentence ;
        ont:excerpt       "She died seven years later, it was like the song came true..." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/3301109/12> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/3301109/12/12> ;
        ont:subject       <http://dbpedia.org/resource/Toma_Zdravković> .

<http://w3id.org/polifonia/pilot/meetups/3301109/11/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1969, he and Silvana Armenulić sang in the same group, and Zdravković, to express his gratitude to her for helping him out, wrote the song “Šta će mi život\"." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/3301109/11> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/3301109/11/0> ;
        ont:subject       <http://dbpedia.org/resource/Toma_Zdravković> .

