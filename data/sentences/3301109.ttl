<http://w3id.org/polifonia/pilot/meetups/3301109/12/10>
        rdf:type          ont:Sentence ;
        ont:excerpt       "It was her song, inspired by her life, and her problems." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/3301109/12> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/3301109/12/9> ;
