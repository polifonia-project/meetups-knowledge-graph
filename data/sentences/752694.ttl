        ont:timeExpression  "one of" .

<http://w3id.org/polifonia/pilot/meetups/752694/4/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Vassilis Tsitsanis was a close friend with Andreas Papandreou, Prime Minister of the country." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/752694/4> ;
        ont:person        <http://dbpedia.org/resource/Andreas_Papandreou> , <http://dbpedia.org/resource/Vassilis_Andrianopoulos> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/752694/4/0> ;
        ont:subject       <http://dbpedia.org/resource/Vassilis_Tsitsanis> .

<http://w3id.org/polifonia/pilot/meetups/752694/2/6>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Tsitsanis was also nicknamed \"Tsilias\"." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/752694/2> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/752694/2/5> ;
