<http://w3id.org/polifonia/pilot/meetups/752694/0/1>
        rdf:type            ont:Sentence ;
        ont:excerpt         "He became one of the leading Greek composers of his time and is widely regarded as one of the founders of modern Rebetiko and Laiko music." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/752694/0> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/752694/0/0> ;
