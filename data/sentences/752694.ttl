        ont:place         <http://dbpedia.org/resource/Royal_Brompton_Hospital> , <http://dbpedia.org/resource/London> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/752694/4/0> ;
        ont:subject       <http://dbpedia.org/resource/Vassilis_Tsitsanis> .

<http://w3id.org/polifonia/pilot/meetups/752694/2/7>
        rdf:type          ont:Sentence ;
        ont:excerpt       "From a young age, Tsitsanis was interested in music and learned to play violin, mandola and mandolin which were mainstays of so many of his songs." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/752694/2> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/752694/2/6> ;
