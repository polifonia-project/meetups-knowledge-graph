<http://w3id.org/polifonia/pilot/meetups/1517721/3/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In fact, when Brun wrote that, 24 claimants were involved in a lawsuit establishing the legitimate heirs." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1517721/3> ;
        ont:person        <http://dbpedia.org/resource/Brun_Motorsport> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1517721/3/3> ;
        ont:subject       <http://dbpedia.org/resource/Euday_L._Bowman> .

<http://w3id.org/polifonia/pilot/meetups/1517721/4/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He lived together with his sister, Miss Mary M. Bowman, who wrote a part of Twelfth Street Rag." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1517721/4> ;
        ont:person        <http://dbpedia.org/resource/Mary_I_of_England> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1517721/4/2> ;
        ont:subject       <http://dbpedia.org/resource/Euday_L._Bowman> .

<http://w3id.org/polifonia/pilot/meetups/1517721/6/1>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Upon her death one year later, Ed G. Max was appointed temporary administrator for her estate." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1517721/6> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1517721/6/0> ;
        ont:subject         <http://dbpedia.org/resource/Euday_L._Bowman> ;
        ont:timeExpression  "one year" .

<http://w3id.org/polifonia/pilot/meetups/1517721/0/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Euday Louis Bowman (November 9, 1887 – May 26, 1949) was an American pianist and composer of ragtime and blues who represented the style of Texas Ragtime." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1517721/0> ;
        ont:person          <http://dbpedia.org/resource/Composer> ;
        ont:place           <http://dbpedia.org/resource/United_States> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1517721/0/0> ;
        ont:subject         <http://dbpedia.org/resource/Euday_L._Bowman> ;
        ont:timeExpression  "November 9 , 1887" , "May 26 , 1949" .

<http://w3id.org/polifonia/pilot/meetups/1517721/7/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Euday Bowman's work is not public domain." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1517721/7> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1517721/7/0> ;
        ont:subject       <http://dbpedia.org/resource/Euday_L._Bowman> .

