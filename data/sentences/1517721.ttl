<http://w3id.org/polifonia/pilot/meetups/1517721/3/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He did not lose a leg and he did not die without heirs." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1517721/3> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1517721/3/2> ;
        ont:subject       <http://dbpedia.org/resource/Euday_L._Bowman> .

<http://w3id.org/polifonia/pilot/meetups/1517721/4/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Bowman was also an arranger for popular orchestras." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1517721/4> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1517721/4/1> ;
        ont:subject       <http://dbpedia.org/resource/Euday_L._Bowman> .

<http://w3id.org/polifonia/pilot/meetups/1517721/5/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Claim of Royalties ==" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1517721/5> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1517721/5/0> ;
