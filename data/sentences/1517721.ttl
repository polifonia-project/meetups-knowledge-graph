        ont:excerpt       "Euday had no children, so the royalties went to his sister." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1517721/6> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1517721/6/0> ;
