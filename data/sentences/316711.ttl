        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/316711/56/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/316711/56/1> .

<http://w3id.org/polifonia/pilot/meetups/316711/20/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He was found dead in his hotel room two days after his last performance, at the Cahors Blues Festival in France." ;
