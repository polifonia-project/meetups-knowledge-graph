        rdf:type          ont:Sentence ;
        ont:excerpt       "[The Lazer is] the closest thing I've found to sounding like a Strat and feeling like a Gibson ..." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/316711/32> ;
        ont:person        <http://dbpedia.org/resource/Fender_Stratocaster> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/316711/32/0> ;
        ont:subject       <http://dbpedia.org/resource/Johnny_Winter> .

