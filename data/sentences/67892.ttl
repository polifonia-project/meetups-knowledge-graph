        ont:place         <http://dbpedia.org/resource/San_Francisco> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/67892/16/1> ;
        ont:subject       <http://dbpedia.org/resource/Enrico_Caruso> .

<http://w3id.org/polifonia/pilot/meetups/67892/26/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "A few days before a performance of Pagliacci at the Met (Pierre Key says it was 4 December, the day after the Samson and Delilah injury) he suffered a chill and developed a cough and a \"dull pain in his side\"." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/67892/26> ;
        ont:person        <http://dbpedia.org/resource/Samson> , <http://dbpedia.org/resource/Pierre_Curie> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/67892/26/2> ;
        ont:subject       <http://dbpedia.org/resource/Enrico_Caruso> .

<http://w3id.org/polifonia/pilot/meetups/67892/22/12>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Giachetti's subsequent attempts to sue him for damages were dismissed by the courts." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/67892/22> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/67892/22/11> ;
