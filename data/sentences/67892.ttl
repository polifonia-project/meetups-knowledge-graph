<http://w3id.org/polifonia/pilot/meetups/67892/3/6>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Caruso himself and his brother Giovanni may have been the source of the exaggerated number." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/67892/3> ;
