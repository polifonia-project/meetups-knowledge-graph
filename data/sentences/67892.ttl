        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/67892/32/0> ;
        ont:subject         <http://dbpedia.org/resource/Enrico_Caruso> ;
        ont:timeExpression  "tenor for 18" .

<http://w3id.org/polifonia/pilot/meetups/67892/101/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "in Camden, New Jersey, Caruso later recorded in Camden's Trinity Church, which Victor acquired as a recording studio in 1917 for its acoustical properties and which could accommodate a large band of musicians." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/67892/101> ;
        ont:person          <http://dbpedia.org/resource/Enrico_Caruso> ;
        ont:place           <http://dbpedia.org/resource/New_Jersey> , <http://dbpedia.org/resource/Trinity_Church_on_the_Green> , <http://dbpedia.org/resource/Camden_Town> ;
