        ont:place         <http://dbpedia.org/resource/Naples> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/67892/10/3> ;
        ont:subject       <http://dbpedia.org/resource/Enrico_Caruso> .

<http://w3id.org/polifonia/pilot/meetups/67892/30/6>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Beverly Sills noted in an interview: \"I was able to do it with television and radio and media and all kinds of assists." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/67892/30> ;
        ont:person        <http://dbpedia.org/resource/Beverly_Sills> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/67892/30/5> ;
