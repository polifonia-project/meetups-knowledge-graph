        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1948, Katz produced the English-Yiddish stage revue Borscht Capades, co-starring with his son Joel Grey." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2416191/28> ;
        ont:person        <http://dbpedia.org/resource/Joel_Grey> ;
