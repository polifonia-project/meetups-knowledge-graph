        rdf:type          ont:Sentence ;
        ont:excerpt       "After leaving Columbia Records in 1973, Ackles concentrated on fulfilling his publishing contract with Warner Bros., writing songs to order for the company's artists." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2579694/14> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2579694/14/0> ;
