        rdf:type            ont:Sentence ;
        ont:excerpt         "Beginning in 1960, Curtin also composed many of the stock tunes used as incidental music in the various Hanna-Barbera series, along with the jingle heard underneath Hanna-Barbera's closing logo in 1979." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1470994/4> ;
        ont:person          <http://dbpedia.org/resource/Production_logo> , <http://dbpedia.org/resource/John_Curtin> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1470994/4/9> ;
        ont:subject         <http://dbpedia.org/resource/Hoyt_Curtin> ;
        ont:timeExpression  "closing logo in 1979" .

