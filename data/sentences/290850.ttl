        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/290850/56/0> ;
        ont:subject         <http://dbpedia.org/resource/Alejo_Carpentier> ;
        ont:timeExpression  "1943 trip" .

<http://w3id.org/polifonia/pilot/meetups/290850/11/20>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Although abroad, Carpentier still maintained contact with Cuba by sending articles and poems to contribute to Havana publications such as Ensayos Convergentes." ;
