        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/290850/23> ;
        ont:place         <http://dbpedia.org/resource/Latin_America> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/290850/23/0> ;
