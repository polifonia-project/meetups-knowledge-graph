        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/290850/11/6> ;
        ont:subject       <http://dbpedia.org/resource/Alejo_Carpentier> .

<http://w3id.org/polifonia/pilot/meetups/290850/22/0>
        rdf:type          ont:Sentence ;
