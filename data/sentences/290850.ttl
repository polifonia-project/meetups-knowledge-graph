        ont:subject       <http://dbpedia.org/resource/Alejo_Carpentier> .

<http://w3id.org/polifonia/pilot/meetups/290850/56/1>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Carpentier's El reino de este mundo was inspired by his 1943 trip to Haiti, and Los pasos perdidos drew on his visit to Venezuela in 1949." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/290850/56> ;
        ont:person          <http://dbpedia.org/resource/Georges_Carpentier> ;
