<http://w3id.org/polifonia/pilot/meetups/192024/9/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Live at Monterey was released in 1971 by Epic Records." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/192024/9> ;
        ont:place           <http://dbpedia.org/resource/Monterey,_California> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/192024/9/0> ;
        ont:subject         <http://dbpedia.org/resource/Johnny_Otis> ;
        ont:timeExpression  "1971 by" .

<http://w3id.org/polifonia/pilot/meetups/192024/4/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Soon after, he dropped out of Berkeley High School, in his junior year." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/192024/4> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/192024/4/0> ;
        ont:subject       <http://dbpedia.org/resource/Johnny_Otis> .

<http://w3id.org/polifonia/pilot/meetups/192024/2/7>
        rdf:type          ont:Sentence ;
        ont:excerpt       "They had four children: two sons, Shuggie Otis and Nicholas Otis—both of whom became musicians—and two daughters, Janice and Laura." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/192024/2> ;
        ont:person        <http://dbpedia.org/resource/Laura_Bush> , <http://dbpedia.org/resource/Nicholas_II_of_Russia> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/192024/2/6> ;
        ont:subject       <http://dbpedia.org/resource/Johnny_Otis> .

<http://w3id.org/polifonia/pilot/meetups/192024/12/8>
        rdf:type          ont:Sentence ;
        ont:excerpt       "After Otis' retirement in late 2004, his grandson Lucky hosted the show at KPFA for two years, until its final airing in late 2006, when Otis and his wife moved back to Los Angeles." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/192024/12> ;
        ont:place         <http://dbpedia.org/resource/Los_Angeles> , <http://dbpedia.org/resource/Otis_Air_National_Guard_Base> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/192024/12/7> ;
        ont:subject       <http://dbpedia.org/resource/Johnny_Otis> .

<http://w3id.org/polifonia/pilot/meetups/192024/8/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "One of Otis's most famous compositions is the ballad \"Every Beat of My Heart\", first recorded by the Royals in 1952 for Federal Records." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/192024/8> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/192024/8/0> ;
        ont:subject         <http://dbpedia.org/resource/Johnny_Otis> ;
        ont:timeExpression  "1952 for" , "One of" .

