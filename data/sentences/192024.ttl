        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/192024/7/3> ;
        ont:subject         <http://dbpedia.org/resource/Johnny_Otis> ;
        ont:timeExpression  "one stating" , "that the two" , "17" .

