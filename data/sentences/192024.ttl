        ont:subject         <http://dbpedia.org/resource/Johnny_Otis> ;
        ont:timeExpression  "1978–1981" , "1984–1986" .

<http://w3id.org/polifonia/pilot/meetups/192024/7/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1952, while in Houston, Texas, Otis auditioned the singer Willie Mae \"Big Mama\" Thornton." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/192024/7> ;
        ont:person        <http://dbpedia.org/resource/Big_Mama_Thornton> ;
        ont:place         <http://dbpedia.org/resource/Houston> , <http://dbpedia.org/resource/Texas> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/192024/7/0> ;
        ont:subject       <http://dbpedia.org/resource/Johnny_Otis> .

<http://w3id.org/polifonia/pilot/meetups/192024/12/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He then became deputy chief of staff to state Assemblyman, later, Democratic Congressman, Mervyn M. Dymally." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/192024/12> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/192024/12/1> ;
        ont:subject       <http://dbpedia.org/resource/Johnny_Otis> .

<http://w3id.org/polifonia/pilot/meetups/192024/10/8>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Otis performed across the United States and Europe well through the 1990s, headlining the San Francisco Blues Festival in 1990 and 2000." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/192024/10> ;
        ont:place           <http://dbpedia.org/resource/Europe> , <http://dbpedia.org/resource/United_States> ;
