        ont:subject         <http://dbpedia.org/resource/Johnny_Otis> ;
        ont:timeExpression  "January 17 , 2012" .

<http://w3id.org/polifonia/pilot/meetups/192024/6/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Otis discovered the tenor saxophonist Big Jay McNeely, who played on his up-tempo \"Barrelhouse Stomp\"." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/192024/6> ;
        ont:person        <http://dbpedia.org/resource/Big_Jay_McNeely> , <http://dbpedia.org/resource/Tenor_saxophone> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/192024/6/1> ;
        ont:subject       <http://dbpedia.org/resource/Johnny_Otis> .

<http://w3id.org/polifonia/pilot/meetups/192024/8/12>
        rdf:type            ont:Sentence ;
        ont:excerpt         "It was a hit in the summer of 1958, peaking at number 9 on the U.S. Pop chart, and was Otis's only Top 10 single." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/192024/8> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/192024/8/11> ;
        ont:subject         <http://dbpedia.org/resource/Johnny_Otis> ;
        ont:timeExpression  "9 on" , "summer of 1958" .

<http://w3id.org/polifonia/pilot/meetups/192024/0/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Otis has been called the \"Godfather of Rhythm and Blues\"." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/192024/0> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/192024/0/2> ;
        ont:subject       <http://dbpedia.org/resource/Johnny_Otis> .

<http://w3id.org/polifonia/pilot/meetups/192024/10/11>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He was inducted into the National Rhythm & Blues Hall of Fame in 2017." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/192024/10> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/192024/10/10> ;
        ont:subject       <http://dbpedia.org/resource/Johnny_Otis> .

<http://w3id.org/polifonia/pilot/meetups/192024/10/4>
        rdf:type            ont:Sentence ;
        ont:excerpt         "In the summer of 1987, Otis hosted his own Red Beans & Rice R&B Music Festival in Los Angeles, which featured top-name acts and hosted a Southern-style red beans and rice cook-off." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/192024/10> ;
        ont:place           <http://dbpedia.org/resource/Los_Angeles> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/192024/10/3> ;
        ont:subject         <http://dbpedia.org/resource/Johnny_Otis> ;
        ont:timeExpression  "summer of 1987" .

<http://w3id.org/polifonia/pilot/meetups/192024/5/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "His band included Wynonie Harris, Charles Brown, and Illinois Jacquet, among others." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/192024/5> ;
        ont:person        <http://dbpedia.org/resource/Charles_I_of_England> , <http://dbpedia.org/resource/Illinois_Jacquet> , <http://dbpedia.org/resource/Wynonie_Harris> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/192024/5/1> ;
        ont:subject       <http://dbpedia.org/resource/Johnny_Otis> .

