<http://w3id.org/polifonia/pilot/meetups/192024/8/7>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He continued to perform and appeared on TV shows in Los Angeles from 1957." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/192024/8> ;
        ont:place         <http://dbpedia.org/resource/Los_Angeles> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/192024/8/6> ;
        ont:subject       <http://dbpedia.org/resource/Johnny_Otis> .

<http://w3id.org/polifonia/pilot/meetups/192024/7>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/192024/7/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/192024/7/1> ;
