        rdf:type          ont:Sentence ;
        ont:excerpt       "He attended the University of Pittsburgh's Graduate School of Child Development, where he began his 30-year collaboration with child psychologist Margaret McFarland." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/137869/1> ;
