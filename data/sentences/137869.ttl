        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/137869/17/12> ;
        ont:subject         <http://dbpedia.org/resource/Fred_Rogers> ;
        ont:timeExpression  "March 1 , 2003" .

<http://w3id.org/polifonia/pilot/meetups/137869/19/15>
        rdf:type            ont:Sentence ;
        ont:excerpt         "The 2019 drama film A Beautiful Day in the Neighborhood tells the story of Rogers and his television series, with Tom Hanks portraying Rogers." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/137869/19> ;
        ont:person          <http://dbpedia.org/resource/Tom_Hanks> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/137869/19/14> ;
        ont:subject         <http://dbpedia.org/resource/Fred_Rogers> ;
        ont:timeExpression  "2019 drama film" .

