        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/137869/13/7> ;
        ont:subject       <http://dbpedia.org/resource/Fred_Rogers> .

<http://w3id.org/polifonia/pilot/meetups/137869/13/23>
        rdf:type          ont:Sentence ;
