        rdf:_3    <http://w3id.org/polifonia/pilot/meetups/137869/8/2> .

<http://w3id.org/polifonia/pilot/meetups/137869/1/2>
        rdf:type            ont:Sentence ;
        ont:excerpt         "He graduated from Pittsburgh Theological Seminary with a bachelor's degree in divinity in 1962 and became a Presbyterian minister in 1963." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/137869/1> ;
        ont:place           <http://dbpedia.org/resource/Pittsburgh> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/137869/1/1> ;
        ont:subject         <http://dbpedia.org/resource/Fred_Rogers> ;
        ont:timeExpression  "minister in 1963" , "divinity in 1962" .

<http://w3id.org/polifonia/pilot/meetups/137869/19/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Marc Brown, creator of another PBS children's show, Arthur, considered Rogers both a friend and \"a terrific role model for how to use television and the media to be helpful to kids and families\"." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/137869/19> ;
        ont:person        <http://dbpedia.org/resource/Marc_Brown_(author)> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/137869/19/0> ;
        ont:subject       <http://dbpedia.org/resource/Fred_Rogers> .

<http://w3id.org/polifonia/pilot/meetups/137869/29/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In the Oakland neighborhood, his portrait is included in the Martin Luther King Jr. and \"Interpretations of Oakland\" murals." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/137869/29> ;
        ont:person        <http://dbpedia.org/resource/Martin_Luther> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/137869/29/0> ;
        ont:subject       <http://dbpedia.org/resource/Fred_Rogers> .

