<http://w3id.org/polifonia/pilot/meetups/137869/4/10>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Rogers had a difficult childhood." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/137869/4> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/137869/4/9> ;
        ont:subject       <http://dbpedia.org/resource/Fred_Rogers> .

<http://w3id.org/polifonia/pilot/meetups/137869/5/11>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He regularly appeared before church officials to keep up his ordination." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/137869/5> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/137869/5/10> ;
        ont:subject       <http://dbpedia.org/resource/Fred_Rogers> .

<http://w3id.org/polifonia/pilot/meetups/137869/15/21>
        rdf:type          ont:Sentence ;
        ont:excerpt       "For example, since hosting Misterogers in Canada, he answered every letter sent to him by hand." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/137869/15> ;
        ont:place         <http://dbpedia.org/resource/Canada> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/137869/15/20> ;
        ont:subject       <http://dbpedia.org/resource/Fred_Rogers> .

<http://w3id.org/polifonia/pilot/meetups/137869/17/23>
        rdf:type          ont:Sentence ;
        ont:excerpt       "The memorial was broadcast several times on Pittsburgh television stations and websites throughout the day." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/137869/17> ;
        ont:place         <http://dbpedia.org/resource/Pittsburgh> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/137869/17/22> ;
        ont:subject       <http://dbpedia.org/resource/Fred_Rogers> .

<http://w3id.org/polifonia/pilot/meetups/137869/9/15>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Rogers also worked with Coombs on the children's show Butternut Square from 1964 to 1967." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/137869/9> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/137869/9/14> ;
        ont:subject         <http://dbpedia.org/resource/Fred_Rogers> ;
        ont:timeExpression  "from 1964 to 1967" .

<http://w3id.org/polifonia/pilot/meetups/137869/11/28>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Rogers wrote and edited all the episodes, played the piano and sang for most of the songs, wrote 200 songs and 13 operas, created all the characters (both puppet and human), played most of the major puppet roles, hosted every episode, and produced and approved every detail of the program." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/137869/11> ;
