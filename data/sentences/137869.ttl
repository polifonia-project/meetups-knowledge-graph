        ont:subject         <http://dbpedia.org/resource/Fred_Rogers> ;
        ont:timeExpression  "May 3 , 2003" .

<http://w3id.org/polifonia/pilot/meetups/137869/4/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Rogers was born on March 20, 1928, in Latrobe, Pennsylvania, about 40 miles (64 km) outside of Pittsburgh, at 705 Main Street to James and Nancy Rogers." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/137869/4> ;
        ont:place           <http://dbpedia.org/resource/James_River> , <http://dbpedia.org/resource/Pittsburgh> , <http://dbpedia.org/resource/Latrobe,_Pennsylvania> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/137869/4/0> ;
        ont:subject         <http://dbpedia.org/resource/Fred_Rogers> ;
        ont:timeExpression  "64 km" , "March 20 , 1928" .

