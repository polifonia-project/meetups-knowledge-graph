<http://w3id.org/polifonia/pilot/meetups/137869/13/21>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Rogers was \"one of the country's most sought-after commencement speakers\", making over 150 speeches." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/137869/13> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/137869/13/20> ;
