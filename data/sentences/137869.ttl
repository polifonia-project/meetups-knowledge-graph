<http://w3id.org/polifonia/pilot/meetups/137869/15/23>
        rdf:type          ont:Sentence ;
        ont:excerpt       "King wrote that Rogers saw responding to his viewers' letters as \"a pastoral duty of sorts\"." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/137869/15> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/137869/15/22> ;
        ont:subject       <http://dbpedia.org/resource/Fred_Rogers> .

<http://w3id.org/polifonia/pilot/meetups/137869/9/17>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Coombs remained in Toronto, became Rogers' Canadian equivalent of an iconic television personality, creating the long-running children's program Mr. Dressup, which ran from 1967 to 1996." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/137869/9> ;
        ont:place           <http://dbpedia.org/resource/Toronto> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/137869/9/16> ;
        ont:subject         <http://dbpedia.org/resource/Fred_Rogers> ;
        ont:timeExpression  "from 1967 to 1996" .

