        ont:person          <http://dbpedia.org/resource/Composer> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1322881/4/1> ;
        ont:subject         <http://dbpedia.org/resource/Pannalal_Ghosh> ;
        ont:timeExpression  "1940" .

<http://w3id.org/polifonia/pilot/meetups/1322881/2/8>
