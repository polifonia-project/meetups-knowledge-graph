        ont:subject       <http://dbpedia.org/resource/Pannalal_Ghosh> .

<http://w3id.org/polifonia/pilot/meetups/1322881/2/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Ghosh received his initial training in music from his father, learning to play the sitar." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1322881/2> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1322881/2/2> ;
        ont:subject       <http://dbpedia.org/resource/Pannalal_Ghosh> .

