        rdf:type          ont:Sentence ;
        ont:excerpt       "He was a stage and television performer in the late 1950s and has been a UK recording star since the early 1960s." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/208956/0> ;
        ont:place         <http://dbpedia.org/resource/United_Kingdom> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/208956/0/1> ;
