<http://w3id.org/polifonia/pilot/meetups/208956/4/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Brown was born in Swarby, Lincolnshire." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/208956/4> ;
        ont:place         <http://dbpedia.org/resource/Swarby> , <http://dbpedia.org/resource/Lincolnshire> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/208956/4/0> ;
        ont:subject       <http://dbpedia.org/resource/Joe_Brown_(singer)> .

<http://w3id.org/polifonia/pilot/meetups/208956/16/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "A triple DVD and CD set of the latter tour was released of his show at the Liverpool Philharmonic." ;
