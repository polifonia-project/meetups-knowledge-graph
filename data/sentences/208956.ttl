        ont:place         <http://dbpedia.org/resource/Liverpool> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/208956/16/1> ;
        ont:subject       <http://dbpedia.org/resource/Joe_Brown_(singer)> .

<http://w3id.org/polifonia/pilot/meetups/208956/11/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Brown still performs and makes occasional television appearances." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/208956/11> ;
