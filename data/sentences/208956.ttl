<http://w3id.org/polifonia/pilot/meetups/208956/8/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "This eclectic collection of musical styles, together with his hits, became the basis of his live sets ever since." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/208956/8> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/208956/8/0> ;
