<http://w3id.org/polifonia/pilot/meetups/2800969/3/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Growing up, Mayreder lived in a large household and received an education that was typical of the well-off." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2800969/3> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2800969/3/3> ;
        ont:subject       <http://dbpedia.org/resource/Rosa_Mayreder> .

<http://w3id.org/polifonia/pilot/meetups/2800969/5/9>
        rdf:type          ont:Sentence ;
        ont:excerpt       "The latter work, which criticized the double standard and discrimination against women, was translated into English." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2800969/5> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2800969/5/8> ;
        ont:subject       <http://dbpedia.org/resource/Rosa_Mayreder> .

