<http://w3id.org/polifonia/pilot/meetups/2800969/0/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Rosa Mayreder (née Obermeyer; 30 November 1858, in Vienna – 19 January 1938, in Vienna) was an Austrian freethinker, author, painter, musician and feminist." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/2800969/0> ;
        ont:person          <http://dbpedia.org/resource/Rosa_Mayreder> ;
        ont:place           <http://dbpedia.org/resource/Vienna> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/2800969/0/0> ;
        ont:subject         <http://dbpedia.org/resource/Rosa_Mayreder> ;
        ont:timeExpression  "19 January 1938" , "30 November 1858" .

<http://w3id.org/polifonia/pilot/meetups/2800969/3/5>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Mayreder was taught to play the piano, sing, speak French, and draw by private tutors." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2800969/3> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2800969/3/4> ;
        ont:subject       <http://dbpedia.org/resource/Rosa_Mayreder> .

