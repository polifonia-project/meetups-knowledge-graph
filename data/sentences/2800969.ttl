<http://w3id.org/polifonia/pilot/meetups/2800969/8/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Mayreder was an influence on Swedish literary critic Klara Johanson." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2800969/8> ;
        ont:place         <http://dbpedia.org/resource/Sweden> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2800969/8/0> ;
        ont:subject       <http://dbpedia.org/resource/Rosa_Mayreder> .

<http://w3id.org/polifonia/pilot/meetups/2800969/3/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Her father was Franz Arnold Obermayer, who was the owner of a prosperous tavern." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2800969/3> ;
        ont:person        <http://dbpedia.org/resource/Benedict_Arnold> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2800969/3/0> ;
        ont:subject       <http://dbpedia.org/resource/Rosa_Mayreder> .

<http://w3id.org/polifonia/pilot/meetups/2800969/6/6>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Rosa formed a warm friendship with Wolf and developed one of her stories as the libretto for his opera Der Corregidor, which was first performed in Mannheim in 1896." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/2800969/6> ;
        ont:place           <http://dbpedia.org/resource/Mannheim> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/2800969/6/5> ;
        ont:subject         <http://dbpedia.org/resource/Rosa_Mayreder> ;
        ont:timeExpression  "one of" .
