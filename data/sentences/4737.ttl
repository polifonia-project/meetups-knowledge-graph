        rdf:type            ont:Sentence ;
        ont:excerpt         "At about the age of four he contracted polio, which incapacitated his right leg." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/4737/2> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/4737/2/0> ;
