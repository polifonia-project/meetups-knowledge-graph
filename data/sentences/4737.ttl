<http://w3id.org/polifonia/pilot/meetups/4737/8>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/4737/8/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/4737/8/1> .

<http://w3id.org/polifonia/pilot/meetups/4737/6/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "McGhee and Terry were both recipients of a 1982 National Heritage Fellowship awarded by the National Endowment for the Arts, which is the United States government's highest honor in the folk and traditional arts." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/4737/6> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/4737/6/0> ;
        ont:subject         <http://dbpedia.org/resource/Brownie_McGhee> ;
        ont:timeExpression  "of a 1982" .

<http://w3id.org/polifonia/pilot/meetups/4737/1/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Life and career ==" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/4737/1> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/4737/1/0> ;
        ont:subject       <http://dbpedia.org/resource/Brownie_McGhee> .

<http://w3id.org/polifonia/pilot/meetups/4737/4/6>
        rdf:type            ont:Sentence ;
        ont:excerpt         "He also appeared in the television series Matlock, in a 1989 episode entitled \"The Blues Singer\", playing a friend of an old blues musician (Joe Seneca) who is accused of murder." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/4737/4> ;
        ont:person          <http://dbpedia.org/resource/Joe_Seneca> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/4737/4/5> ;
        ont:subject         <http://dbpedia.org/resource/Brownie_McGhee> ;
        ont:timeExpression  "1989 episode" .

<http://w3id.org/polifonia/pilot/meetups/4737/5/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Happy Traum, a former guitar student of McGhee's, edited a blues guitar instruction guide and songbook, Guitar Styles of Brownie McGhee, published in 1971, in which McGhee, between lessons, talked about his life and the blues." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/4737/5> ;
        ont:person        <http://dbpedia.org/resource/Brownie_McGhee> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/4737/5/0> ;
        ont:subject       <http://dbpedia.org/resource/Brownie_McGhee> .

