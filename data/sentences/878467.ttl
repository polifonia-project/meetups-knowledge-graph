        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/878467/7/1> ;
        ont:subject       <http://dbpedia.org/resource/Wavy_Gravy> .

<http://w3id.org/polifonia/pilot/meetups/878467/11/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Romney said he considered this a mystical event, and assumed Wavy Gravy as his legal name." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/878467/11> ;
        ont:person        <http://dbpedia.org/resource/Wavy_Gravy> , <http://dbpedia.org/resource/Mitt_Romney> ;
