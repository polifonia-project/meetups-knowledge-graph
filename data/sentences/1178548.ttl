<http://w3id.org/polifonia/pilot/meetups/1178548/0/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Nathaniel Shilkret (December 25, 1889 – February 18, 1982) was an American composer, conductor, clarinetist, pianist, business executive, and music director." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1178548/0> ;
        ont:person          <http://dbpedia.org/resource/Composer> , <http://dbpedia.org/resource/Nathaniel_Shilkret> , <http://dbpedia.org/resource/Conducting> ;
        ont:place           <http://dbpedia.org/resource/United_States> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1178548/0/0> ;
        ont:subject         <http://dbpedia.org/resource/Nathaniel_Shilkret> ;
        ont:timeExpression  "December 25 , 1889" , "February 18 , 1982" .

