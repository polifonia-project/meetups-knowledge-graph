        ont:timeExpression  "October 1946" .

<http://w3id.org/polifonia/pilot/meetups/1178548/9/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He also composed the theme song \"Some Sweet Day\" for the film Children of the Ritz in the same year." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1178548/9> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1178548/9/2> ;
        ont:subject       <http://dbpedia.org/resource/Nathaniel_Shilkret> .

<http://w3id.org/polifonia/pilot/meetups/1178548/16/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "He worked at RKO-Pathe, making short films from 1946 through the mid-1950s." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1178548/16> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1178548/16/0> ;
