<http://w3id.org/polifonia/pilot/meetups/1178548/12/1>
        rdf:type            ont:Sentence ;
        ont:excerpt         "His last recording released on the Victor label was the American Banjo Album (P-218) recorded in October 1946." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1178548/12> ;
        ont:place           <http://dbpedia.org/resource/United_States> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1178548/12/0> ;
