        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1968 Danny appeared in Spede Pasanen's film comedy Noin seitsemän veljestä as a medieval troubadour and performed his song \"Seitsemän kertaa seitsemän\" which became another hit." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/3606266/6> ;
        ont:person        <http://dbpedia.org/resource/Troubadour> , <http://dbpedia.org/resource/Spede_Pasanen> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/3606266/6/0> ;
