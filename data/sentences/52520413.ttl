        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/52520413/14/0> ;
        ont:subject         <http://dbpedia.org/resource/Elvira_Ríos> ;
        ont:timeExpression  "18 November 1961" .

<http://w3id.org/polifonia/pilot/meetups/52520413/6/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Manuel Riachi, an assistant to Paramount Pictures producer Arthur Hornblow Jr., discovered her singing over XEW and took her to Hollywood to appear in the musical Tropic Holiday, where she sang songs by Lara and played Tito Guízar's sister." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/52520413/6> ;
        ont:person        <http://dbpedia.org/resource/King_Arthur> , <http://dbpedia.org/resource/Manuel_I_of_Portugal> ;
        ont:place         <http://dbpedia.org/resource/Hollywood> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/52520413/6/1> ;
        ont:subject       <http://dbpedia.org/resource/Elvira_Ríos> .

