        ont:timeExpression  "1942" .

<http://w3id.org/polifonia/pilot/meetups/52520413/3/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "She was the firstborn child of José María Gallegos Villalobos and María Guadalupe Ríos Rodríguez." ;
