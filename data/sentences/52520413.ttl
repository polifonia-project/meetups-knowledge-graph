        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1960, she recorded her second album for RCA Victor, Ausencia, accompanied by Gonzalo \"Chalo\" Cervera's orchestra and pianist Pepe Agüeros." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/52520413/13> ;
        ont:person        <http://dbpedia.org/resource/Pepe_Romero> , <http://dbpedia.org/resource/Gonzalo_Pizarro> ;
