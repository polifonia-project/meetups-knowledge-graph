        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/2330834/5> ;
        ont:place           <http://dbpedia.org/resource/Pepper_Pot,_Brighton> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/2330834/5/0> ;
        ont:subject         <http://dbpedia.org/resource/Don_McNeill_(radio_presenter)> ;
        ont:timeExpression  "8" .

