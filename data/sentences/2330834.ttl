<http://w3id.org/polifonia/pilot/meetups/2330834/2/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He was a first cousin of United States Secretary of Defense Caspar Weinberger; McNeill's mother was the elder sister of Weinberger's father." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2330834/2> ;
        ont:person        <http://dbpedia.org/resource/Caspar_Weinberger> ;
        ont:place         <http://dbpedia.org/resource/United_States> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2330834/2/1> ;
        ont:subject       <http://dbpedia.org/resource/Don_McNeill_(radio_presenter)> .

<http://w3id.org/polifonia/pilot/meetups/2330834/5/7>
        rdf:type          ont:Sentence ;
        ont:excerpt       "(Countless local shows even now refer to themselves as The Breakfast Club)." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2330834/5> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2330834/5/6> ;
