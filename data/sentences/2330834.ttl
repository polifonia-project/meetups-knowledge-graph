        rdf:type          ont:Sentence ;
        ont:excerpt       "That year, Bleyer traveled to Chicago to record some patriotic spoken word recordings by McNeill." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2330834/5> ;
        ont:person        <http://dbpedia.org/resource/Archie_Bleyer> , <http://dbpedia.org/resource/Spoken_word> ;
