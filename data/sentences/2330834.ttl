        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/2330834/5/12> ;
        ont:subject         <http://dbpedia.org/resource/Don_McNeill_(radio_presenter)> ;
        ont:timeExpression  "television in 1954â€“1955" .

