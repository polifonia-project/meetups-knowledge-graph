<http://w3id.org/polifonia/pilot/meetups/2330834/5/15>
        rdf:type          ont:Sentence ;
        ont:excerpt       "McNeill's radio series finally ended in 1968, when McNeill retired from entertainment and public life." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2330834/5> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2330834/5/14> ;
        ont:subject       <http://dbpedia.org/resource/Don_McNeill_(radio_presenter)> .

<http://w3id.org/polifonia/pilot/meetups/2330834/5/3>
        rdf:type            ont:Sentence ;
        ont:excerpt         "The show premiered on June 23, 1933, with informal talk and jokes based on topical events, and often included audience interviews." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/2330834/5> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/2330834/5/2> ;
        ont:subject         <http://dbpedia.org/resource/Don_McNeill_(radio_presenter)> ;
        ont:timeExpression  "June 23 , 1933" .

