        ont:subject       <http://dbpedia.org/resource/Albert_Baez> .

<http://w3id.org/polifonia/pilot/meetups/2891853/4/1>
        rdf:type            ont:Sentence ;
        ont:excerpt         "From 1950 to 1956, he held a professorship at the University of Redlands, where he continued his X-ray research." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/2891853/4> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/2891853/4/0> ;
