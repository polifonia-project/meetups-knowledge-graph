        ont:timeExpression  "From 1950 to 1956" .

<http://w3id.org/polifonia/pilot/meetups/2891853/2/7>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He married Joan Chandos Bridge, the daughter of an Episcopalian priest, in 1936." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2891853/2> ;
        ont:person        <http://dbpedia.org/resource/Priest> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2891853/2/6> ;
        ont:subject       <http://dbpedia.org/resource/Albert_Baez> .

<http://w3id.org/polifonia/pilot/meetups/2891853/8/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "After his retirement, Baez occasionally delivered physics lectures and was president of Vivamos Mejor/USA, an organization founded in 1988 to help impoverished villages in Mexico." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2891853/8> ;
        ont:place         <http://dbpedia.org/resource/Mexico> , <http://dbpedia.org/resource/United_States> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2891853/8/0> ;
        ont:subject       <http://dbpedia.org/resource/Albert_Baez> .

