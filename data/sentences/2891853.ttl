        ont:place         <http://dbpedia.org/resource/Boston> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2891853/4/2> ;
        ont:subject       <http://dbpedia.org/resource/Albert_Baez> .

<http://w3id.org/polifonia/pilot/meetups/2891853/2/11>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1948, Baez co-invented, with his doctoral program advisor, Paul Kirkpatrick, the X-ray reflection microscope for examination of living cells." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2891853/2> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2891853/2/10> ;
        ont:subject       <http://dbpedia.org/resource/Albert_Baez> .

<http://w3id.org/polifonia/pilot/meetups/2891853/2/9>
        rdf:type            ont:Sentence ;
        ont:excerpt         "The two had three daughters (Pauline, Joan and Mimi), then moved to California:" ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/2891853/2> ;
        ont:person          <http://dbpedia.org/resource/Mimi_Smith> ;
        ont:place           <http://dbpedia.org/resource/California> ;
