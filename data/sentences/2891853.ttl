<http://w3id.org/polifonia/pilot/meetups/2891853/9/5>
        rdf:type          ont:Sentence ;
        ont:excerpt       "His obituary in the New York Times states that \"his survivors include his wife, Joan Bridge Baez of Woodside, Calif.\"" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2891853/9> ;
        ont:person        <http://dbpedia.org/resource/Joan_Bridge> ;
        ont:place         <http://dbpedia.org/resource/New_York_City> , <http://dbpedia.org/resource/Woodside_tram_stop> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2891853/9/4> ;
        ont:subject       <http://dbpedia.org/resource/Albert_Baez> .

<http://w3id.org/polifonia/pilot/meetups/2891853/4/6>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Later that year he moved his family to Claremont, California, where he joined the faculty at Harvey Mudd College." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2891853/4> ;
        ont:place         <http://dbpedia.org/resource/Claremont,_California> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2891853/4/5> ;
        ont:subject       <http://dbpedia.org/resource/Albert_Baez> .

<http://w3id.org/polifonia/pilot/meetups/2891853/2/14>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1948, while still a graduate student at Stanford, he developed concentric circles of alternating opaque and transparent materials to use diffraction instead of refraction to focus X-rays." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2891853/2> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2891853/2/13> ;
        ont:subject       <http://dbpedia.org/resource/Albert_Baez> .

<http://w3id.org/polifonia/pilot/meetups/2891853/14/3>
        rdf:type            ont:Sentence ;
