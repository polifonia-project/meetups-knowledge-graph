        ont:subject         <http://dbpedia.org/resource/Albert_Baez> ;
        ont:timeExpression  "November 15 , 1912" , "March 20 , 2007" .

<http://w3id.org/polifonia/pilot/meetups/2891853/8/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Baez himself was inducted into the HENAAC Hall of Fame in 1998." ;
