        rdf:type            ont:Sentence ;
        ont:excerpt         "Albert Vinicio Báez (; November 15, 1912 – March 20, 2007) was a Mexican-American physicist and the father of singers Joan Baez and Mimi Fariña, and an uncle of John C. Baez." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/2891853/0> ;
