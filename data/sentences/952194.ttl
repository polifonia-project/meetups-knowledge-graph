<http://w3id.org/polifonia/pilot/meetups/952194/9/5>
        rdf:type            ont:Sentence ;
        ont:excerpt         "On August 24, 1959, Time magazine, in reviewing the history of the European edition of The Herald Tribune, reported that Buchwald's column had achieved an \"institutional quality\"." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/952194/9> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/952194/9/4> ;
        ont:subject         <http://dbpedia.org/resource/Art_Buchwald> ;
        ont:timeExpression  "August 24 , 1959" .

<http://w3id.org/polifonia/pilot/meetups/952194/31/0>
