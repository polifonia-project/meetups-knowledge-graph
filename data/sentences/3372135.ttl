        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/3372135/6/0> ;
        ont:subject         <http://dbpedia.org/resource/David_Maslanka> ;
        ont:timeExpression  "1974" , "1978" , "five residency" .

<http://w3id.org/polifonia/pilot/meetups/3372135/1/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "His compositional style is rhythmically intense and complex, highly tonal and melodically-oriented." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/3372135/1> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/3372135/1/0> ;
