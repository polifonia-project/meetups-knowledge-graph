<http://w3id.org/polifonia/pilot/meetups/909222/6/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1995, Cleopatra Records released his Land of Mist CD, a collection of instrumental ambience." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/909222/6> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/909222/6/1> ;
        ont:subject       <http://dbpedia.org/resource/Ron_Geesin> .

<http://w3id.org/polifonia/pilot/meetups/909222/4/8>
        rdf:type            ont:Sentence ;
        ont:excerpt         "In 1990, he designed and produced the 'Tune Tube', a giant interactive sound and light installation at the MacLellan Galleries, as part of ‘Glasgow 1990’." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/909222/4> ;
        ont:place           <http://dbpedia.org/resource/Glasgow> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/909222/4/7> ;
        ont:subject         <http://dbpedia.org/resource/Ron_Geesin> ;
        ont:timeExpression  "Glasgow 1990" .

