<http://w3id.org/polifonia/pilot/meetups/909222/3/3>
        rdf:type            ont:Sentence ;
        ont:excerpt         "After scoring The Body (1970), his other film scores include John Schlesinger's film Sunday Bloody Sunday (1971), Ghost Story (1974), Sword of the Valiant (1984) and The Girl in the Picture (1985)." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/909222/3> ;
        ont:person          <http://dbpedia.org/resource/John_Schlesinger> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/909222/3/2> ;
        ont:subject         <http://dbpedia.org/resource/Ron_Geesin> ;
        ont:timeExpression  "1971" , "1970" , "1985" , "1984" , "1974" .

<http://w3id.org/polifonia/pilot/meetups/909222/10/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Geesin is married to the artist Frances Geesin." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/909222/10> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/909222/10/0> ;
