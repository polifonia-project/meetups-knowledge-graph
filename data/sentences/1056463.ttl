        rdf:type            ont:Sentence ;
        ont:excerpt         "Eugène Joseph Bozza (4 April 1905 – 28 September 1991) was a French composer and violinist." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1056463/0> ;
        ont:person          <http://dbpedia.org/resource/Saint_Joseph> ;
