        ont:timeExpression  "28 September 1991" , "4 April 1905" .

<http://w3id.org/polifonia/pilot/meetups/1056463/60/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "\"The French Three : a Comparison (performed) of Recital Music by Darius Milhaud, Henri Tomasi, and Eugène Bozza.\"" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1056463/60> ;
        ont:person        <http://dbpedia.org/resource/Henri_Tomasi> , <http://dbpedia.org/resource/Darius_Milhaud> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1056463/60/0> ;
        ont:subject       <http://dbpedia.org/resource/Eugène_Bozza> .

<http://w3id.org/polifonia/pilot/meetups/1056463/16/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Nuages for saxophone quartet is indicative of the Impressionistic side of Bozza." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1056463/16> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1056463/16/3> ;
        ont:subject       <http://dbpedia.org/resource/Eugène_Bozza> .

<http://w3id.org/polifonia/pilot/meetups/1056463/20/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Though he spent much of his life in the central and southern large population centers of Paris and Valenciennes, many of Bozza's stage works were premiered in Lille, located in northeastern France." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1056463/20> ;
        ont:place         <http://dbpedia.org/resource/France> , <http://dbpedia.org/resource/Valenciennes> , <http://dbpedia.org/resource/Lille> , <http://dbpedia.org/resource/Paris> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1056463/20/0> ;
        ont:subject       <http://dbpedia.org/resource/Eugène_Bozza> .

<http://w3id.org/polifonia/pilot/meetups/1056463/5/5>
        rdf:type          ont:Sentence ;
        ont:excerpt       "After five years of touring Europe with the orchestra, Bozza resigned and returned to the Conservatory to study conducting with Henri Rabaud." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1056463/5> ;
        ont:person        <http://dbpedia.org/resource/Henri_Rabaud> ;
        ont:place         <http://dbpedia.org/resource/Europe> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1056463/5/4> ;
        ont:subject       <http://dbpedia.org/resource/Eugène_Bozza> .

