        rdf:_9    <http://w3id.org/polifonia/pilot/meetups/2667661/7/8> .

<http://w3id.org/polifonia/pilot/meetups/2667661/14/3>
        rdf:type            ont:Sentence ;
        ont:excerpt         "In April 2010 Goldsmith was honoured as the International Music Person of the Year for his outstanding contributions to the Music Industry at the Musexpo by the City of West Hollywood." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/2667661/14> ;
        ont:place           <http://dbpedia.org/resource/Hollywood> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/2667661/14/2> ;
        ont:subject         <http://dbpedia.org/resource/Harvey_Goldsmith> ;
        ont:timeExpression  "April 2010" .

<http://w3id.org/polifonia/pilot/meetups/2667661/9/7>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Goldsmith was appointed a Commander of the Order of the British Empire for his work in the entertainment industry." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2667661/9> ;
        ont:place         <http://dbpedia.org/resource/British_Empire> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2667661/9/6> ;
        ont:subject       <http://dbpedia.org/resource/Harvey_Goldsmith> .

<http://w3id.org/polifonia/pilot/meetups/2667661/11/14>
        rdf:type            ont:Sentence ;
        ont:excerpt         "In December 2008 Goldsmith took over the management of Jeff Beck after meeting at the Classic Rock Awards where they were both collecting awards." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/2667661/11> ;
        ont:person          <http://dbpedia.org/resource/Jeff_Beck> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/2667661/11/13> ;
        ont:subject         <http://dbpedia.org/resource/Harvey_Goldsmith> ;
        ont:timeExpression  "December 2008" .

<http://w3id.org/polifonia/pilot/meetups/2667661/1>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/2667661/1/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/2667661/1/1> ;
        rdf:_3    <http://w3id.org/polifonia/pilot/meetups/2667661/1/2> .

<http://w3id.org/polifonia/pilot/meetups/2667661/21>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/2667661/21/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/2667661/21/1> .

<http://w3id.org/polifonia/pilot/meetups/2667661/13/3>
        rdf:type            ont:Sentence ;
        ont:excerpt         "In September 2009 Goldsmith produced the Tower Festival in the Tower of London which ran for 10 days." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/2667661/13> ;
        ont:place           <http://dbpedia.org/resource/London> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/2667661/13/2> ;
        ont:subject         <http://dbpedia.org/resource/Harvey_Goldsmith> ;
        ont:timeExpression  "September 2009" .

<http://w3id.org/polifonia/pilot/meetups/2667661/11/9>
        rdf:type          ont:Sentence ;
        ont:excerpt       "This was the first concert ever held at the Emirates Football Stadium." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2667661/11> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2667661/11/8> ;
        ont:subject       <http://dbpedia.org/resource/Harvey_Goldsmith> .

<http://w3id.org/polifonia/pilot/meetups/2667661/9/1>
        rdf:type            ont:Sentence ;
        ont:excerpt         "This has been one of the most successful shows to perform in the UK, returning to London's Royal Albert Hall on a regular basis." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/2667661/9> ;
        ont:place           <http://dbpedia.org/resource/London> , <http://dbpedia.org/resource/United_Kingdom> , <http://dbpedia.org/resource/Royal_Albert_Hall> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/2667661/9/0> ;
        ont:subject         <http://dbpedia.org/resource/Harvey_Goldsmith> ;
        ont:timeExpression  "one of" .

<http://w3id.org/polifonia/pilot/meetups/2667661/4/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "A decade later the scale of many popular concerts had increased and moved to larger stadiums and had established Wembley Stadium as a major venue for the top artists of the world." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2667661/4> ;
        ont:place         <http://dbpedia.org/resource/Wembley_Stadium> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2667661/4/1> ;
        ont:subject       <http://dbpedia.org/resource/Harvey_Goldsmith> .

<http://w3id.org/polifonia/pilot/meetups/2667661/19>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/2667661/19/0> .

<http://w3id.org/polifonia/pilot/meetups/2667661/7/7>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1992 Goldsmith organised The Freddie Mercury Tribute Concert to increase AIDS awareness and this was a live TV success." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2667661/7> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2667661/7/6> ;
        ont:subject       <http://dbpedia.org/resource/Harvey_Goldsmith> .

<http://w3id.org/polifonia/pilot/meetups/2667661/16/2>
        rdf:type            ont:Sentence ;
        ont:excerpt         "In August 2014, Goldsmith was one of 200 public figures who were signatories to a letter to The Guardian opposing Scottish independence in the run-up to September's referendum on that issue." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/2667661/16> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/2667661/16/1> ;
        ont:subject         <http://dbpedia.org/resource/Harvey_Goldsmith> ;
        ont:timeExpression  "August 2014" , "one of" .

<http://w3id.org/polifonia/pilot/meetups/2667661/11/3>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Goldsmith was honoured with the 15th Music Industry Trust's Award in October 2006, one of the highest accolades to be awarded in the music industry, in recognition of his contribution to the music industry." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/2667661/11> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/2667661/11/2> ;
        ont:subject         <http://dbpedia.org/resource/Harvey_Goldsmith> ;
        ont:timeExpression  "October 2006 , one" .

<http://w3id.org/polifonia/pilot/meetups/2667661/7/10>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Goldsmith persuaded Pavarotti to return and perform again in UK at the Leeds Castle in Kent during 1993." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2667661/7> ;
        ont:place         <http://dbpedia.org/resource/Kent> , <http://dbpedia.org/resource/Leeds_Castle> , <http://dbpedia.org/resource/United_Kingdom> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2667661/7/9> ;
        ont:subject       <http://dbpedia.org/resource/Harvey_Goldsmith> .

