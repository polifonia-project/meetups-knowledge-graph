        rdf:_8    <http://w3id.org/polifonia/pilot/meetups/2667661/15/7> .

<http://w3id.org/polifonia/pilot/meetups/2667661/7/12>
        rdf:type          ont:Sentence ;
        ont:excerpt       "This concert featured Sir Georg Solti, Dame Kiri Te Kanawa, Murray Perahia, and Gabriela Beňačková, and was televised worldwide." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2667661/7> ;
        ont:person        <http://dbpedia.org/resource/Murray_Perahia> , <http://dbpedia.org/resource/Georg_Solti> , <http://dbpedia.org/resource/Kiri_Te_Kanawa> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2667661/7/11> ;
        ont:subject       <http://dbpedia.org/resource/Harvey_Goldsmith> .

<http://w3id.org/polifonia/pilot/meetups/2667661/15/4>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Goldsmith chaired the judges' panel for the Editorial Intelligence Awards in October 2011 and became a board member." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/2667661/15> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/2667661/15/3> ;
        ont:subject         <http://dbpedia.org/resource/Harvey_Goldsmith> ;
        ont:timeExpression  "October 2011" .

<http://w3id.org/polifonia/pilot/meetups/2667661/7/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1986, he promoted Luciano Pavarotti at Wembley Arena." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2667661/7> ;
        ont:person        <http://dbpedia.org/resource/Luciano_Pavarotti> ;
        ont:place         <http://dbpedia.org/resource/Wembley_Arena> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2667661/7/2> ;
        ont:subject       <http://dbpedia.org/resource/Harvey_Goldsmith> .

<http://w3id.org/polifonia/pilot/meetups/2667661/8>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/2667661/8/0> .

<http://w3id.org/polifonia/pilot/meetups/2667661/9/8>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Between 1996 and 1998 Goldsmith oversaw spectacular productions of the Lord of the Dance musical, which featured Michael Flatley in the leading role." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2667661/9> ;
        ont:person        <http://dbpedia.org/resource/Michael_Flatley> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2667661/9/7> ;
        ont:subject       <http://dbpedia.org/resource/Harvey_Goldsmith> .

<http://w3id.org/polifonia/pilot/meetups/2667661/2>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/2667661/2/0> .

<http://w3id.org/polifonia/pilot/meetups/2667661/22>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/2667661/22/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/2667661/22/1> .

<http://w3id.org/polifonia/pilot/meetups/2667661/13/4>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Weeks later, Last Night of the Poms starring Dame Edna and Sir Les Patterson went on tour around the UK reviving the hit show from 28 years previous." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/2667661/13> ;
        ont:place           <http://dbpedia.org/resource/United_Kingdom> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/2667661/13/3> ;
        ont:subject         <http://dbpedia.org/resource/Harvey_Goldsmith> ;
        ont:timeExpression  "hit show from 28" .

<http://w3id.org/polifonia/pilot/meetups/2667661/9/2>
        rdf:type            ont:Sentence ;
        ont:excerpt         "The show has been seen by around 183,000 people and sells in the region of 98% of its tickets." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/2667661/9> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/2667661/9/1> ;
        ont:subject         <http://dbpedia.org/resource/Harvey_Goldsmith> ;
        ont:timeExpression  "98 %" .

<http://w3id.org/polifonia/pilot/meetups/2667661/4/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1975, he was the promoter behind the Loftus Road Concert at Queens Park Rangers' Stadium for" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2667661/4> ;
        ont:place         <http://dbpedia.org/resource/Loftus_Road> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2667661/4/2> ;
        ont:subject       <http://dbpedia.org/resource/Harvey_Goldsmith> .

<http://w3id.org/polifonia/pilot/meetups/2667661/7/8>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In the same year he became chairman of the first National Music Day (UK), which was instigated to promote the importance of music in UK." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2667661/7> ;
        ont:place         <http://dbpedia.org/resource/United_Kingdom> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2667661/7/7> ;
        ont:subject       <http://dbpedia.org/resource/Harvey_Goldsmith> .

<http://w3id.org/polifonia/pilot/meetups/2667661/16/3>
        rdf:type            ont:Sentence ;
        ont:excerpt         "In 2017, Goldsmith staged a hugely successful world tour for Hans Zimmer, one of the most successful film music composers working today." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/2667661/16> ;
        ont:person          <http://dbpedia.org/resource/Hans_Zimmer> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/2667661/16/2> ;
        ont:subject         <http://dbpedia.org/resource/Harvey_Goldsmith> ;
        ont:timeExpression  "one of" .

<http://w3id.org/polifonia/pilot/meetups/2667661/11/4>
        rdf:type            ont:Sentence ;
        ont:excerpt         "In the same year, Midem honoured The Live Aid founders and Live 8 producers Goldsmith, Bob Geldof and John Kennedy OBE, with the Midem Personality of the Year Award, which was awarded in recognition of their outstanding work in bringing the music industry together to help alleviate poverty and famine in Africa." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/2667661/11> ;
        ont:person          <http://dbpedia.org/resource/John_F._Kennedy> , <http://dbpedia.org/resource/Bob_Geldof> ;
        ont:place           <http://dbpedia.org/resource/Africa> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/2667661/11/3> ;
        ont:subject         <http://dbpedia.org/resource/Harvey_Goldsmith> ;
        ont:timeExpression  "Live 8" .

<http://w3id.org/polifonia/pilot/meetups/2667661/14>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/2667661/14/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/2667661/14/1> ;
        rdf:_3    <http://w3id.org/polifonia/pilot/meetups/2667661/14/2> ;
        rdf:_4    <http://w3id.org/polifonia/pilot/meetups/2667661/14/3> .

<http://w3id.org/polifonia/pilot/meetups/2667661/7/11>
        rdf:type            ont:Sentence ;
        ont:excerpt         "In 1994 under the auspices of The Prince of Wales and President Havel of the Czech Republic, Goldsmith was appointed Executive Producer of The Prague Heritage Fund Gala Classical Concert." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/2667661/7> ;
        ont:place           <http://dbpedia.org/resource/Prague> , <http://dbpedia.org/resource/Havel> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/2667661/7/10> ;
        ont:subject         <http://dbpedia.org/resource/Harvey_Goldsmith> ;
        ont:timeExpression  "1994 under" .

<http://w3id.org/polifonia/pilot/meetups/2667661/15/3>
        rdf:type            ont:Sentence ;
        ont:excerpt         "In recognition of his outstanding contribution to the institution's music education, Goldsmith was awarded an honorary fellowship from Ravensbourne, the university sector college innovating in digital media and design, in September 2011." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/2667661/15> ;
        ont:person          <http://dbpedia.org/resource/Fellow> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/2667661/15/2> ;
        ont:subject         <http://dbpedia.org/resource/Harvey_Goldsmith> ;
        ont:timeExpression  "September 2011" .

<http://w3id.org/polifonia/pilot/meetups/2667661/7/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Goldsmith then became a member of The Prince's Trust Board and Vice Chairman of Prince's Trust Trading Ltd." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2667661/7> ;
        ont:person        <http://dbpedia.org/resource/Prince> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2667661/7/1> ;
        ont:subject       <http://dbpedia.org/resource/Harvey_Goldsmith> .

<http://w3id.org/polifonia/pilot/meetups/2667661/7>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/2667661/7/0> ;
