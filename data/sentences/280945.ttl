<http://w3id.org/polifonia/pilot/meetups/280945/6/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Pepper continued as a film and television character actor into the 1960s." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/280945/6> ;
        ont:person        <http://dbpedia.org/resource/Character_actor> ;
