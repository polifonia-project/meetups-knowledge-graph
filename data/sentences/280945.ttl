<http://w3id.org/polifonia/pilot/meetups/280945/1/1>
        rdf:type            ont:Sentence ;
        ont:excerpt         "He first came to national prominence in the 1920s as part of the duo Salt and Pepper with Frank Salt." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/280945/1> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/280945/1/0> ;
        ont:subject         <http://dbpedia.org/resource/Jack_Pepper> ;
        ont:timeExpression  "in the 1920s" .

<http://w3id.org/polifonia/pilot/meetups/280945/5/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1940, he appeared in the Bing Crosby film Rhythm on the River and Road to Singapore, the first Bob Hope–Bing Crosby \"Road\" picture." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/280945/5> ;
        ont:person        <http://dbpedia.org/resource/Bing_Crosby> ;
        ont:place         <http://dbpedia.org/resource/Singapore> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/280945/5/0> ;
        ont:subject       <http://dbpedia.org/resource/Jack_Pepper> .

