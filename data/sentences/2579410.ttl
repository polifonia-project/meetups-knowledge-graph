        rdf:_9    <http://w3id.org/polifonia/pilot/meetups/2579410/15/8> .

<http://w3id.org/polifonia/pilot/meetups/2579410/17/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "==" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2579410/17> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2579410/17/0> ;
        ont:subject       <http://dbpedia.org/resource/Erwin_Schulhoff> .

<http://w3id.org/polifonia/pilot/meetups/2579410/12/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Musical style ==" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2579410/12> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2579410/12/0> ;
        ont:subject       <http://dbpedia.org/resource/Erwin_Schulhoff> .

<http://w3id.org/polifonia/pilot/meetups/2579410/15/6>
        rdf:type            ont:Sentence ;
        ont:excerpt         "A New York Times critic in 1932 called the Duo for violin and cello (1925) \"long-winded and even insincere\", while a performance in 2012 noted it was dedicated to Janáček, evokes Ravel's Sonata for Violin and Cello and \"blends folk and contemporary elements\" while employing \"a range of sonorities and effects like dramatic pizzicatos\" while \"vivacious Hungarian fiddle playing enlivens the Zingaresca movement\"." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/2579410/15> ;
        ont:person          <http://dbpedia.org/resource/Maurice_Ravel> ;
        ont:place           <http://dbpedia.org/resource/New_York_City> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/2579410/15/5> ;
        ont:subject         <http://dbpedia.org/resource/Erwin_Schulhoff> ;
        ont:timeExpression  "performance in 2012" , "1925" .

<http://w3id.org/polifonia/pilot/meetups/2579410/16/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In general, Schulhoff's music remains connected to Western tonality, though—like Prokofiev, among others—the fundamentally triadic conception of his music is often embellished by passages of intense dissonance." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2579410/16> ;
        ont:person        <http://dbpedia.org/resource/Sergei_Prokofiev> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2579410/16/0> ;
        ont:subject       <http://dbpedia.org/resource/Erwin_Schulhoff> .

<http://w3id.org/polifonia/pilot/meetups/2579410/11/1>
        rdf:type            ont:Sentence ;
        ont:excerpt         "He died there on 18 August 1942 from tuberculosis." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/2579410/11> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/2579410/11/0> ;
        ont:subject         <http://dbpedia.org/resource/Erwin_Schulhoff> ;
        ont:timeExpression  "18 August 1942" .

<http://w3id.org/polifonia/pilot/meetups/2579410/15/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Schulhoff's third period dates from approximately 1923 to 1932." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2579410/15> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2579410/15/0> ;
        ont:subject       <http://dbpedia.org/resource/Erwin_Schulhoff> .

<http://w3id.org/polifonia/pilot/meetups/2579410/10/1>
        rdf:type            ont:Sentence ;
        ont:excerpt         "In 1932 he composed a musical version of The Communist Manifesto (Op. 82)." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/2579410/10> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/2579410/10/0> ;
        ont:subject         <http://dbpedia.org/resource/Erwin_Schulhoff> ;
        ont:timeExpression  "82" .

<http://w3id.org/polifonia/pilot/meetups/2579410/5>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/2579410/5/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/2579410/5/1> ;
        rdf:_3    <http://w3id.org/polifonia/pilot/meetups/2579410/5/2> ;
        rdf:_4    <http://w3id.org/polifonia/pilot/meetups/2579410/5/3> .

<http://w3id.org/polifonia/pilot/meetups/2579410/5/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He wrote his friend Alban Berg in 1921:" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2579410/5> ;
        ont:person        <http://dbpedia.org/resource/Alban_Berg> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2579410/5/2> ;
        ont:subject       <http://dbpedia.org/resource/Erwin_Schulhoff> .

<http://w3id.org/polifonia/pilot/meetups/2579410/13/6>
        rdf:type            ont:Sentence ;
        ont:excerpt         "A 1923 report of a Bochum performance puts Schulhoff in the context of his contemporaries:" ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/2579410/13> ;
        ont:place           <http://dbpedia.org/resource/Bochum> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/2579410/13/5> ;
        ont:subject         <http://dbpedia.org/resource/Erwin_Schulhoff> ;
        ont:timeExpression  "1923 report" .

<http://w3id.org/polifonia/pilot/meetups/2579410/14/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "The patience of the town of Bochum was sorely tried by four composers of the most modern tendency in one concert." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/2579410/14> ;
        ont:place           <http://dbpedia.org/resource/Bochum> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/2579410/14/0> ;
