<http://w3id.org/polifonia/pilot/meetups/2579410/15/5>
        rdf:type            ont:Sentence ;
        ont:excerpt         "He thought that innovations like an entire movement of the Suite for Chamber Orchestra (1921) for percussion alone and the use of the siren in another \"would have seemed outlandish enough in 1921, even if it all sounds a bit tame now [1995].\"" ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/2579410/15> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/2579410/15/4> ;
        ont:subject         <http://dbpedia.org/resource/Erwin_Schulhoff> ;
        ont:timeExpression  "1995 ]" , "1921" .

<http://w3id.org/polifonia/pilot/meetups/2579410/11/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "In June 1941, Schulhoff was deported to the Wülzburg prison near Weißenburg, Bavaria." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/2579410/11> ;
        ont:place           <http://dbpedia.org/resource/Bavaria> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/2579410/11/0> ;
        ont:subject         <http://dbpedia.org/resource/Erwin_Schulhoff> ;
        ont:timeExpression  "June 1941" .

<http://w3id.org/polifonia/pilot/meetups/2579410/14/5>
        rdf:type          ont:Sentence ;
        ont:excerpt       "The composer made a speedy escape." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2579410/14> ;
