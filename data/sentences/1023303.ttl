        rdf:type          ont:Sentence ;
        ont:excerpt       "I'd say yes, with tongue only slightly in cheek.\"" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1023303/2> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1023303/2/9> ;
        ont:subject       <http://dbpedia.org/resource/Pat_Hare> .

<http://w3id.org/polifonia/pilot/meetups/1023303/0/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "His guitar work with Little Junior's Blue Flames had a major influence on the rockabilly style, and his guitar playing on blues records by artists such as Muddy Waters was influential among 1960s British Invasion blues rock bands such as the Rolling Stones and the Yardbirds." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1023303/0> ;
        ont:person        <http://dbpedia.org/resource/Muddy_Waters> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1023303/0/1> ;
        ont:subject       <http://dbpedia.org/resource/Pat_Hare> .

<http://w3id.org/polifonia/pilot/meetups/1023303/2/23>
        rdf:type            ont:Sentence ;
        ont:excerpt         "He died in 1980 in Saint Paul, Minnesota." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1023303/2> ;
        ont:place           <http://dbpedia.org/resource/Minnesota> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1023303/2/22> ;
