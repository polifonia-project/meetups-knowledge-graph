<http://w3id.org/polifonia/pilot/meetups/1023303/2/4>
        rdf:type            ont:Sentence ;
        ont:excerpt         "He played the electric guitar solo on \"Love My Baby\" (1953), which later inspired the rockabilly style." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1023303/2> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1023303/2/3> ;
        ont:subject         <http://dbpedia.org/resource/Pat_Hare> ;
        ont:timeExpression  "1953" .

<http://w3id.org/polifonia/pilot/meetups/1023303/2/18>
        rdf:type            ont:Sentence ;
        ont:excerpt         "In December 1963, Hare shot his girlfriend dead and also shot a policeman who came to investigate." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1023303/2> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1023303/2/17> ;
        ont:subject         <http://dbpedia.org/resource/Pat_Hare> ;
        ont:timeExpression  "December 1963" .

