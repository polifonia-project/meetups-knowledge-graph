        rdf:type            ont:Sentence ;
        ont:excerpt         "Auburn \"Pat\" Hare (December 20, 1930 â€“ September 26, 1980) was a Memphis electric blues guitarist and singer." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1023303/0> ;
