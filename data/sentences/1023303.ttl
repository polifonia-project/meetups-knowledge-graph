<http://w3id.org/polifonia/pilot/meetups/1023303/2/21>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Hare spent the last 16 years of his life in prison, where he formed a band named Sounds Incarcerated." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1023303/2> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1023303/2/20> ;
        ont:subject       <http://dbpedia.org/resource/Pat_Hare> .

<http://w3id.org/polifonia/pilot/meetups/1023303/2/5>
        rdf:type            ont:Sentence ;
        ont:excerpt         "One of their biggest hits was \"Next Time You See Me\", which in 1957 reached number 5 on the Billboard R&B chart and number 74 on the Billboard Hot 100 pop chart." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1023303/2> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1023303/2/4> ;
        ont:subject         <http://dbpedia.org/resource/Pat_Hare> ;
        ont:timeExpression  "One of" , "74 on" , "5 on" , "100 pop chart" .

<http://w3id.org/polifonia/pilot/meetups/1023303/2/19>
        rdf:type          ont:Sentence ;
        ont:excerpt       "At the time of his arrest, he was playing in the blues band of Muddy Waters." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1023303/2> ;
        ont:person        <http://dbpedia.org/resource/Muddy_Waters> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1023303/2/18> ;
        ont:subject       <http://dbpedia.org/resource/Pat_Hare> .

