        ont:timeExpression  "one of" .

<http://w3id.org/polifonia/pilot/meetups/1023303/2/16>
        rdf:type          ont:Sentence ;
        ont:excerpt       "According to Robert Palmer, the song is \"as heavy metal as it gets.\"" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1023303/2> ;
        ont:person        <http://dbpedia.org/resource/Robert_Palmer_(singer)> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1023303/2/15> ;
        ont:subject       <http://dbpedia.org/resource/Pat_Hare> .

