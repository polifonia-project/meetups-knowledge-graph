        rdf:type            ont:Sentence ;
        ont:excerpt         "Hare was one of the first guitarists to purposely use the effects of distortion in his playing." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1023303/2> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1023303/2/1> ;
