        ont:person          <http://dbpedia.org/resource/Poet> , <http://dbpedia.org/resource/Lord_Byron> , <http://dbpedia.org/resource/Don_Juan> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/295286/40/0> ;
        ont:subject         <http://dbpedia.org/resource/Dionysios_Solomos> ;
        ont:timeExpression  "Isles 15" .

<http://w3id.org/polifonia/pilot/meetups/295286/50/2>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Valaoritis wrote to Constantinos Asopius in 1859 after the poet's death: \"the nation's hopes were deceived\" and in 1877 in a letter to Emmanouil Roidis he wrote that Solomos left us \"only a hymn and some incoherent verses\"." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/295286/50> ;
        ont:person          <http://dbpedia.org/resource/Poet> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/295286/50/1> ;
        ont:subject         <http://dbpedia.org/resource/Dionysios_Solomos> ;
        ont:timeExpression  "1877 in" , "1859 after" .

