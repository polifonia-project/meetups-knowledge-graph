<http://w3id.org/polifonia/pilot/meetups/1491559/1/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Career ==" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1491559/1> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1491559/1/0> ;
        ont:subject       <http://dbpedia.org/resource/Jerry_Keller> .

<http://w3id.org/polifonia/pilot/meetups/1491559/2/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Born in Fort Smith, Arkansas, Keller moved with his family to Tulsa, Oklahoma, when he was aged six, and Keller attended Tulsa's Will Rogers High School, graduating in 1955." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1491559/2> ;
        ont:place         <http://dbpedia.org/resource/Harrison,_Arkansas> , <http://dbpedia.org/resource/Oklahoma> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1491559/2/0> ;
        ont:subject       <http://dbpedia.org/resource/Jerry_Keller> .

<http://w3id.org/polifonia/pilot/meetups/1491559/2/6>
        rdf:type            ont:Sentence ;
        ont:excerpt         "In 1959 it climbed to No. 14 in the Billboard Hot 100." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1491559/2> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1491559/2/5> ;
        ont:subject         <http://dbpedia.org/resource/Jerry_Keller> ;
        ont:timeExpression  "Hot 100" , "14 in" .

<http://w3id.org/polifonia/pilot/meetups/1491559/0/1>
        rdf:type            ont:Sentence ;
        ont:excerpt         "He is best known for his 1959 hit song, \"Here Comes Summer.\"" ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1491559/0> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1491559/0/0> ;
        ont:subject         <http://dbpedia.org/resource/Jerry_Keller> ;
        ont:timeExpression  "1959 hit song" .

<http://w3id.org/polifonia/pilot/meetups/1491559/1/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "==" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1491559/1> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1491559/1/0> ;
        ont:subject       <http://dbpedia.org/resource/Jerry_Keller> .

<http://w3id.org/polifonia/pilot/meetups/1491559/2/5>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Keller's biggest self-penned hit was 1959's \"Here Comes Summer.\"" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1491559/2> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1491559/2/4> ;
        ont:subject       <http://dbpedia.org/resource/Jerry_Keller> .

<http://w3id.org/polifonia/pilot/meetups/1491559/0/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Jerry Paul Keller (born June 20, 1937) is an American pop singer and songwriter." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1491559/0> ;
        ont:place           <http://dbpedia.org/resource/United_States> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1491559/0/0> ;
        ont:subject         <http://dbpedia.org/resource/Jerry_Keller> ;
        ont:timeExpression  "June 20 , 1937" .

<http://w3id.org/polifonia/pilot/meetups/1491559/2/13>
        rdf:type            ont:Sentence ;
        ont:excerpt         "He also wrote \"The Legend of Shenandoah\", recited by James Stewart in the 1965 film Shenandoah." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1491559/2> ;
        ont:person          <http://dbpedia.org/resource/James_Stewart> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1491559/2/12> ;
        ont:subject         <http://dbpedia.org/resource/Jerry_Keller> ;
        ont:timeExpression  "1965 film" .

<http://w3id.org/polifonia/pilot/meetups/1491559/2/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Pat Boone, a friend from church, recommended Marty Mills as Keller's manager." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1491559/2> ;
        ont:person        <http://dbpedia.org/resource/Pat_Boone> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1491559/2/3> ;
        ont:subject       <http://dbpedia.org/resource/Jerry_Keller> .
