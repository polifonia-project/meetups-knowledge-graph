<http://w3id.org/polifonia/pilot/meetups/1491559/2/12>
        rdf:type            ont:Sentence ;
        ont:excerpt         "In the film arena, Keller wrote soundtrack music for I Saw What You Did (1965) and Angel in My Pocket (1969)." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1491559/2> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1491559/2/11> ;
        ont:subject         <http://dbpedia.org/resource/Jerry_Keller> ;
        ont:timeExpression  "1969" , "1965" .

<http://w3id.org/polifonia/pilot/meetups/1491559/2/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Keller attended the University of Tulsa and moved to New York in 1956." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1491559/2> ;
        ont:place         <http://dbpedia.org/resource/New_York_City> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1491559/2/2> ;
        ont:subject       <http://dbpedia.org/resource/Jerry_Keller> .

<http://w3id.org/polifonia/pilot/meetups/1491559/2/9>
        rdf:type          ont:Sentence ;
        ont:excerpt       "\"A Man and a Woman\" was recorded by such artists as Matt Monro, Ella Fitzgerald, Engelbert Humperdinck, Johnny Mathis and Jos√© Feliciano." ;
