<http://w3id.org/polifonia/pilot/meetups/152097/16/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Guyler was cremated at Mount Gravatt Cemetery and his ashes were placed at Mount Thompson Memorial Gardens in the Brisbane suburb of Holland Park in a family \"garden seat\" estate." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/152097/16> ;
        ont:place         <http://dbpedia.org/resource/Holland_Park> , <http://dbpedia.org/resource/Suburb> , <http://dbpedia.org/resource/Brisbane> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/152097/16/2> ;
        ont:subject       <http://dbpedia.org/resource/Deryck_Guyler> .

<http://w3id.org/polifonia/pilot/meetups/152097/5/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "For a Children's Hour documentary about life in the coal mines, which Guyler was narrating, the producer had visited a mine and recorded most-realistic sound effects." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/152097/5> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/152097/5/1> ;
        ont:subject       <http://dbpedia.org/resource/Deryck_Guyler> .

<http://w3id.org/polifonia/pilot/meetups/152097/4/2>
        rdf:type            ont:Sentence ;
        ont:excerpt         "He took part in a Royal Command Performance of ITMA for King George VI and Queen Elizabeth in December 1947." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/152097/4> ;
        ont:person          <http://dbpedia.org/resource/George_V> , <http://dbpedia.org/resource/Queen_Elizabeth_The_Queen_Mother> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/152097/4/1> ;
        ont:subject         <http://dbpedia.org/resource/Deryck_Guyler> ;
        ont:timeExpression  "December 1947" .

