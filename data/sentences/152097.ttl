        ont:timeExpression  "area at 113" , "briefly at 12" .

<http://w3id.org/polifonia/pilot/meetups/152097/6/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In the same period, he was on the radio series Just Fancy for 9 years which starred Eric Barker." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/152097/6> ;
        ont:person        <http://dbpedia.org/resource/Eric_Barker> ;
