        ont:excerpt       "He was known for his often amusing asides in rehearsals." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/152097/5> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/152097/5/0> ;
