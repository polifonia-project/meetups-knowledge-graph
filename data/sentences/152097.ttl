        ont:person          <http://dbpedia.org/resource/Rupert_Murdoch> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/152097/6/8> ;
        ont:subject         <http://dbpedia.org/resource/Deryck_Guyler> ;
        ont:timeExpression  "Number Two" , "Number One" .

