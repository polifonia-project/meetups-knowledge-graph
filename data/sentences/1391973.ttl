<http://w3id.org/polifonia/pilot/meetups/1391973/15/7>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He also featured in the title role of Rupert of Hentzau in 1964." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1391973/15> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1391973/15/6> ;
