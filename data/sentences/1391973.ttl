<http://w3id.org/polifonia/pilot/meetups/1391973/31/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Peter Wyngarde had an active fan club from the mid-1950s to 1985." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1391973/31> ;
        ont:person        <http://dbpedia.org/resource/Peter_Wyngarde> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1391973/31/0> ;
