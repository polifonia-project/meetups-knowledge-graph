        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1391973/25/7> ;
        ont:subject       <http://dbpedia.org/resource/Peter_Wyngarde> .

<http://w3id.org/polifonia/pilot/meetups/1391973/36/1>
        rdf:type          ont:Sentence ;
