        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1391973/15/2> ;
        ont:subject       <http://dbpedia.org/resource/Peter_Wyngarde> .

<http://w3id.org/polifonia/pilot/meetups/1391973/27/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Peter Wyngarde married the actress Dorinda Stevens on 6 March 1951 when he was 23." ;
