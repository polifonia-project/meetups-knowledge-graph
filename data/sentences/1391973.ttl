        rdf:type          ont:Sentence ;
        ont:excerpt       "He began acting during his internment when he played all the characters in a version of Doctor Jekyll and Mr Hyde." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1391973/10> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1391973/10/1> ;
