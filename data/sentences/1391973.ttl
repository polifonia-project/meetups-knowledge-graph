        ont:timeExpression  "November 1955" .

<http://w3id.org/polifonia/pilot/meetups/1391973/11/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Peter Wyngarde was in the camp, under his real name of Cyril Goldbert." ;
