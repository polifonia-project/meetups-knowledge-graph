<http://w3id.org/polifonia/pilot/meetups/1391973/11/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "The last time, when he had begun to be successful, he cut me dead in St James's Park." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1391973/11> ;
        ont:person        <http://dbpedia.org/resource/James_the_Great> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1391973/11/1> ;
        ont:subject       <http://dbpedia.org/resource/Peter_Wyngarde> .

<http://w3id.org/polifonia/pilot/meetups/1391973/15/10>
        rdf:type            ont:Sentence ;
        ont:excerpt         "In Jack Clayton's The Innocents (1961), he had brief unspeaking scenes as the leering Peter Quint with Deborah Kerr and Pamela Franklin." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1391973/15> ;
        ont:person          <http://dbpedia.org/resource/Jack_Clayton> , <http://dbpedia.org/resource/Pamela_Franklin> , <http://dbpedia.org/resource/Deborah_Kerr> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1391973/15/9> ;
        ont:subject         <http://dbpedia.org/resource/Peter_Wyngarde> ;
        ont:timeExpression  "1961" .

<http://w3id.org/polifonia/pilot/meetups/1391973/4/6>
        rdf:type          ont:Sentence ;
        ont:excerpt       "However, during a subsequent visit to Singapore in 1972 he denied having previously been there." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1391973/4> ;
        ont:place         <http://dbpedia.org/resource/Singapore> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1391973/4/5> ;
        ont:subject       <http://dbpedia.org/resource/Peter_Wyngarde> .

