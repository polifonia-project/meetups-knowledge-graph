        ont:subject         <http://dbpedia.org/resource/Peter_Wyngarde> ;
        ont:timeExpression  "1960" , "1956" .

<http://w3id.org/polifonia/pilot/meetups/1391973/31/2>
        rdf:type            ont:Sentence ;
        ont:excerpt         "It went online in 2000, and maintains a regularly updated blog." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1391973/31> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1391973/31/1> ;
