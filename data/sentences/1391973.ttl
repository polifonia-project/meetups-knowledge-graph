        ont:subject       <http://dbpedia.org/resource/Peter_Wyngarde> .

<http://w3id.org/polifonia/pilot/meetups/1391973/29/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "His voice is still of great clarity and sound, his eyes unchanged since that period known as his prime." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1391973/29> ;
