<http://w3id.org/polifonia/pilot/meetups/492026/3/10>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Bikel did not return to live in Israel, nor did he take part in the 1948 Arab–Israeli War." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/492026/3> ;
        ont:place           <http://dbpedia.org/resource/Israel> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/492026/3/9> ;
        ont:subject         <http://dbpedia.org/resource/Theodore_Bikel> ;
        ont:timeExpression  "in the 1948" .

