        rdf:type            ont:Sentence ;
        ont:excerpt         "Theodore Meir Bikel ( bih-KEL; May 2, 1924 – July 21, 2015) was an Austrian-American actor, folk singer, musician, composer, unionist, and political activist." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/492026/0> ;
        ont:person          <http://dbpedia.org/resource/Composer> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/492026/0/0> ;
        ont:subject         <http://dbpedia.org/resource/Theodore_Bikel> ;
        ont:timeExpression  "May 2 , 1924" , "July 21 , 2015" .

<http://w3id.org/polifonia/pilot/meetups/492026/11/10>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Following the festival, Bikel, Seeger, and Dylan traveled to a planned rally in Greenwood, Mississippi, to perform Dylan's newly written song, \"Only a Pawn in Their Game\", about the man who murdered Medgar Evers." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/492026/11> ;
        ont:person        <http://dbpedia.org/resource/Medgar_Evers> , <http://dbpedia.org/resource/Bob_Dylan> ;
        ont:place         <http://dbpedia.org/resource/Greenwood,_Mississippi> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/492026/11/9> ;
        ont:subject       <http://dbpedia.org/resource/Theodore_Bikel> .

<http://w3id.org/polifonia/pilot/meetups/492026/12/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Personal life ==" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/492026/12> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/492026/12/0> ;
        ont:subject       <http://dbpedia.org/resource/Theodore_Bikel> .

