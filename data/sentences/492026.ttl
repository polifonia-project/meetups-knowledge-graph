        rdf:type          ont:Sentence ;
        ont:excerpt       "Bikel was president of the Associated Actors and Artistes of America until 2014, and was president of Actors' Equity in the late 1970s and early 1980s." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/492026/1> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/492026/1/0> ;
        ont:subject       <http://dbpedia.org/resource/Theodore_Bikel> .

