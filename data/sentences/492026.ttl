<http://w3id.org/polifonia/pilot/meetups/492026/15/11>
        rdf:type            ont:Sentence ;
        ont:excerpt         "The producers stopped complaining, however, when after one show he was picked up backstage by a limousine carrying Eleanor Roosevelt, and he accompanied her to a Democratic rally as her special guest." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/492026/15> ;
        ont:person          <http://dbpedia.org/resource/Eleanor_Roosevelt> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/492026/15/10> ;
        ont:subject         <http://dbpedia.org/resource/Theodore_Bikel> ;
        ont:timeExpression  "one show" .

<http://w3id.org/polifonia/pilot/meetups/492026/15/2>
        rdf:type            ont:Sentence ;
        ont:excerpt         "He was president of Actors' Equity from 1977 to 1982, in which office he supported human-rights causes." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/492026/15> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/492026/15/1> ;
        ont:subject         <http://dbpedia.org/resource/Theodore_Bikel> ;
        ont:timeExpression  "from 1977 to 1982" .

