        ont:subject         <http://dbpedia.org/resource/Theodore_Bikel> ;
        ont:timeExpression  "1952" , "1957" , "1953" , "1951" .

<http://w3id.org/polifonia/pilot/meetups/492026/11/11>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Originally, only Bikel and Seeger were scheduled to perform, but Bikel wanted Dylan to go with them." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/492026/11> ;
        ont:person        <http://dbpedia.org/resource/Bob_Dylan> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/492026/11/10> ;
        ont:subject       <http://dbpedia.org/resource/Theodore_Bikel> .

