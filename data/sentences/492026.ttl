        rdf:type          ont:Sentence ;
        ont:excerpt       "He served as the chairman of the board of directors of Partners for Progressive Israel, where he also lectured." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/492026/1> ;
        ont:place         <http://dbpedia.org/resource/Israel> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/492026/1/0> ;
        ont:subject       <http://dbpedia.org/resource/Theodore_Bikel> .

<http://w3id.org/polifonia/pilot/meetups/492026/6/23>
