<http://w3id.org/polifonia/pilot/meetups/492026/15/6>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He has left an indelible mark on generations of members past and generations of members to come." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/492026/15> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/492026/15/5> ;
