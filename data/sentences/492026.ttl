<http://w3id.org/polifonia/pilot/meetups/492026/3/9>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Bikel moved to the United States in 1954 and became a naturalized citizen in 1961." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/492026/3> ;
        ont:place           <http://dbpedia.org/resource/United_States> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/492026/3/8> ;
