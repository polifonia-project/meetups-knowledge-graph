        rdf:type            ont:Sentence ;
        ont:excerpt         "Myroslav Skoryk (Ukrainian: Мирослав Михайлович Скорик, 13 July 1938 – 1 June 2020) was a Ukrainian composer and teacher." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/3143897/0> ;
        ont:person          <http://dbpedia.org/resource/Myroslav_Skoryk> , <http://dbpedia.org/resource/Composer> ;
