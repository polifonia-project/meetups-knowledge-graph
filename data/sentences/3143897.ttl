        ont:subject       <http://dbpedia.org/resource/Myroslav_Skoryk> .

<http://w3id.org/polifonia/pilot/meetups/3143897/12/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Many works by Skoryk have been performed by leading ensembles and soloists including Leontovych Quartet, Oleh Krysa, Volodymyr Vynnytsky, Oleg Chmyr, Mykola Suk, Victor Markiw, and Alexander Slobodyanik." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/3143897/12> ;
        ont:person        <http://dbpedia.org/resource/Mykola_Zerov> , <http://dbpedia.org/resource/Josef_Suk_(composer)> , <http://dbpedia.org/resource/Alexander_Slobodyanik> , <http://dbpedia.org/resource/Oleh_Krysa> ;
