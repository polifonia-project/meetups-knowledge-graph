<http://w3id.org/polifonia/pilot/meetups/3143897/0/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "His music is contemporary in style and contains idioms from diverse sources including German, Welsh, English, and Eastern European artistic traditions." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/3143897/0> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/3143897/0/0> ;
        ont:subject       <http://dbpedia.org/resource/Myroslav_Skoryk> .

<http://w3id.org/polifonia/pilot/meetups/3143897/5/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Between 1955 and 1960 Skoryk studied at the Lviv Conservatory, and there he received training in composition and theory." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/3143897/5> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/3143897/5/0> ;
        ont:subject       <http://dbpedia.org/resource/Myroslav_Skoryk> .

