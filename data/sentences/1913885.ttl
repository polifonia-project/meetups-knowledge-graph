<http://w3id.org/polifonia/pilot/meetups/1913885/1/1>
        rdf:type            ont:Sentence ;
        ont:excerpt         "When he was eight years old, he moved to Lisbon to become the protégé of José Vianna da Motta who was one of the last pupils of Franz Liszt." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1913885/1> ;
        ont:person          <http://dbpedia.org/resource/Franz_Liszt> ;
        ont:place           <http://dbpedia.org/resource/Lisbon> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1913885/1/0> ;
        ont:subject         <http://dbpedia.org/resource/Sequeira_Costa> ;
        ont:timeExpression  "one of" .

<http://w3id.org/polifonia/pilot/meetups/1913885/2/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "At the age of 22, Sequeira Costa won the second Grand Prix at the Marguerite Long International Piano Competition in 1951." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1913885/2> ;
        ont:person          <http://dbpedia.org/resource/Marguerite_Long> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1913885/2/0> ;
        ont:subject         <http://dbpedia.org/resource/Sequeira_Costa> ;
        ont:timeExpression  "age of 22" .

<http://w3id.org/polifonia/pilot/meetups/1913885/2/6>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Sequeira Costa recorded the complete cycle of piano concertos by Sergei Rachmaninoff." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1913885/2> ;
        ont:person        <http://dbpedia.org/resource/Sergei_Rachmaninoff> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1913885/2/5> ;
