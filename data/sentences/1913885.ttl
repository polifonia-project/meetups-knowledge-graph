        rdf:type          ont:Sentence ;
        ont:excerpt       "As a child, Sequeira Costa showed exceptional musical talent." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1913885/1> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1913885/1/0> ;
