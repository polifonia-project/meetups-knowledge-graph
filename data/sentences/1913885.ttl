<http://w3id.org/polifonia/pilot/meetups/1913885/1/2>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Following Vianna da Motta's death in 1948, Sequeira Costa continued his studies in London under another eminent pianist, Mark Hambourg." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1913885/1> ;
        ont:person          <http://dbpedia.org/resource/Mark_Hambourg> ;
        ont:place           <http://dbpedia.org/resource/London> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1913885/1/1> ;
        ont:subject         <http://dbpedia.org/resource/Sequeira_Costa> ;
        ont:timeExpression  "death in 1948" .

<http://w3id.org/polifonia/pilot/meetups/1913885/2/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Five years later, he founded the Vianna da Motta International Music Competition in Lisbon." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1913885/2> ;
        ont:place         <http://dbpedia.org/resource/Lisbon> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1913885/2/0> ;
        ont:subject       <http://dbpedia.org/resource/Sequeira_Costa> .

<http://w3id.org/polifonia/pilot/meetups/1913885/2/7>
        rdf:type          ont:Sentence ;
        ont:excerpt       "His final recording was of the complete sonatas of Ludwig van Beethoven." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1913885/2> ;
        ont:person        <http://dbpedia.org/resource/Ludwig_van_Beethoven> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1913885/2/6> ;
        ont:subject       <http://dbpedia.org/resource/Sequeira_Costa> .

<http://w3id.org/polifonia/pilot/meetups/1913885/3/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "From 1976, Sequeira Costa served as the Cordelia Brown Murphy Distinguished Professor of Piano at the University of Kansas." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1913885/3> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1913885/3/0> ;
        ont:subject       <http://dbpedia.org/resource/Sequeira_Costa> .

