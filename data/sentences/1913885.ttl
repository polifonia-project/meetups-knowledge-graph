<http://w3id.org/polifonia/pilot/meetups/1913885/1/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Under these teachers, Sequeira Costa was immersed in both the German and French schools of pianism." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1913885/1> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1913885/1/3> ;
        ont:subject       <http://dbpedia.org/resource/Sequeira_Costa> .

<http://w3id.org/polifonia/pilot/meetups/1913885/2/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Both in the years 1960 and 2000 he was part of the jurors of the Chopin International Piano Competition." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1913885/2> ;
        ont:person        <http://dbpedia.org/resource/Frédéric_Chopin> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1913885/2/2> ;
        ont:subject       <http://dbpedia.org/resource/Sequeira_Costa> .

<http://w3id.org/polifonia/pilot/meetups/1913885/3/2>
        rdf:type            ont:Sentence ;
        ont:excerpt         "They recorded two cd's with works for two pianos by Rachmaninoff and Spanish composers and frequently gave two piano recitals and performed together with orchestra." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1913885/3> ;
        ont:person          <http://dbpedia.org/resource/Sergei_Rachmaninoff> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1913885/3/1> ;
        ont:subject         <http://dbpedia.org/resource/Sequeira_Costa> ;
        ont:timeExpression  "two piano" , "two cd" .

