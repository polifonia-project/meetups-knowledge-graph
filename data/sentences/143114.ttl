        rdf:type            ont:Sentence ;
        ont:excerpt         "(In his 1985 appearance on The Tonight Show, Gleason told Johnny Carson that he had played pool frequently since childhood, and drew from those experiences in The Hustler.)" ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/143114/51> ;
        ont:person          <http://dbpedia.org/resource/Johnny_Carson> , <http://dbpedia.org/resource/Jackie_Gleason> ;
