        rdf:type            ont:Sentence ;
        ont:excerpt         "As of May 2010, the theater was scheduled to be razed as part of a convention-center remodeling project and replaced by a hotel." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/143114/68> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/143114/68/0> ;
