        rdf:type          ont:Sentence ;
        ont:excerpt       "As early as 1952, when The Jackie Gleason Show captured Saturday night for CBS, Gleason regularly smoked six packs of cigarettes a day, but he never smoked on The Honeymooners." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/143114/65> ;
        ont:person        <http://dbpedia.org/resource/Jackie_Gleason> ;
