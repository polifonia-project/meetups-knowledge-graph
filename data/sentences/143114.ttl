        ont:person          <http://dbpedia.org/resource/Jackie_Gleason> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/143114/4/9> ;
        ont:subject         <http://dbpedia.org/resource/Jackie_Gleason> ;
        ont:timeExpression  "4 per night" .

<http://w3id.org/polifonia/pilot/meetups/143114/74/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "A city park in Lauderhill, Florida was named the \"Jackie Gleason Park\" in his honor; it is located near his former home and features racquetball and basketball courts and a children's playground." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/143114/74> ;
        ont:person        <http://dbpedia.org/resource/Jackie_Gleason> ;
        ont:place         <http://dbpedia.org/resource/Central_Broward_Regional_Park> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/143114/74/0> ;
        ont:subject       <http://dbpedia.org/resource/Jackie_Gleason> .

