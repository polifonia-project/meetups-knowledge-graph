        rdf:type            ont:Sentence ;
        ont:excerpt         "He went into downtown Tulsa, walked into a hardware store, and asked its owner to lend him $200 for the train trip to New York." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/143114/59> ;
