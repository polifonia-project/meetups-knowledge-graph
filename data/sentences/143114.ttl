        rdf:type            ont:Sentence ;
        ont:excerpt         "Throughout the 1950s and 1960s Gleason enjoyed a prominent secondary music career, producing a series of best-selling \"mood music\" albums with jazz overtones for Capitol Records." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/143114/31> ;
        ont:person          <http://dbpedia.org/resource/Jackie_Gleason> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/143114/31/0> ;
        ont:subject         <http://dbpedia.org/resource/Jackie_Gleason> ;
        ont:timeExpression  "Throughout the 1950s" .

<http://w3id.org/polifonia/pilot/meetups/143114/41/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "By 1964 Gleason had moved the production from New York to Miami Beach, Florida, reportedly because he liked year-round access to the golf course at the nearby Inverrary Country Club in Lauderhill (where he built his final home)." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/143114/41> ;
        ont:person        <http://dbpedia.org/resource/Jackie_Gleason> ;
        ont:place         <http://dbpedia.org/resource/New_York_City> , <http://dbpedia.org/resource/Central_Broward_Regional_Park> , <http://dbpedia.org/resource/Miami_Beach,_Florida> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/143114/41/0> ;
        ont:subject       <http://dbpedia.org/resource/Jackie_Gleason> .

<http://w3id.org/polifonia/pilot/meetups/143114/61/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "A complete listing of the holdings of Gleason's library has been issued by the online cataloging service LibraryThing." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/143114/61> ;
        ont:person        <http://dbpedia.org/resource/Jackie_Gleason> ;
