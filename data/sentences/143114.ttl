        rdf:type            ont:Sentence ;
        ont:excerpt         "Gleason's first significant recognition as an entertainer came on Broadway when he appeared in the hit musical Follow the Girls (1944)." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/143114/7> ;
        ont:person          <http://dbpedia.org/resource/Jackie_Gleason> , <http://dbpedia.org/resource/Girl> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/143114/7/9> ;
        ont:subject         <http://dbpedia.org/resource/Jackie_Gleason> ;
        ont:timeExpression  "1944" .

<http://w3id.org/polifonia/pilot/meetups/143114/24/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Titles for the sketch were tossed around until someone came up with The Honeymooners." ;
