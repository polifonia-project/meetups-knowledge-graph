<http://w3id.org/polifonia/pilot/meetups/143114/22/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Gleason's most popular character by far was blustery bus driver Ralph Kramden." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/143114/22> ;
        ont:person        <http://dbpedia.org/resource/Jackie_Gleason> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/143114/22/0> ;
        ont:subject       <http://dbpedia.org/resource/Jackie_Gleason> .

<http://w3id.org/polifonia/pilot/meetups/143114/42/2>
        rdf:type            ont:Sentence ;
        ont:excerpt         "The sketches were remakes of the 1957 world-tour episodes, in which Kramden and Norton win a slogan contest and take their wives to international destinations." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/143114/42> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/143114/42/1> ;
        ont:subject         <http://dbpedia.org/resource/Jackie_Gleason> ;
        ont:timeExpression  "of the 1957" .

<http://w3id.org/polifonia/pilot/meetups/143114/9/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "(William Bendix originated the role on radio but was initially unable to accept the television role because of film commitments.)" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/143114/9> ;
        ont:person        <http://dbpedia.org/resource/William_Bendix> ;
