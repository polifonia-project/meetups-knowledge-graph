        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/143114/15/0> .

<http://w3id.org/polifonia/pilot/meetups/143114/70/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Remarkably, while Gleason was nominated three times for an Emmy Award, he never won." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/143114/70> ;
        ont:person        <http://dbpedia.org/resource/Jackie_Gleason> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/143114/70/0> ;
        ont:subject       <http://dbpedia.org/resource/Jackie_Gleason> .

<http://w3id.org/polifonia/pilot/meetups/143114/33/3>
        rdf:type            ont:Sentence ;
        ont:excerpt         "It always amazed the professional musicians how a guy who technically did not know one note from another could do that." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/143114/33> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/143114/33/2> ;
        ont:subject         <http://dbpedia.org/resource/Jackie_Gleason> ;
        ont:timeExpression  "one note" .

<http://w3id.org/polifonia/pilot/meetups/143114/63/6>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Halford wanted a quiet home life but Gleason fell back into spending his nights out." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/143114/63> ;
        ont:person        <http://dbpedia.org/resource/Jackie_Gleason> , <http://dbpedia.org/resource/Rob_Halford> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/143114/63/5> ;
        ont:subject       <http://dbpedia.org/resource/Jackie_Gleason> .

<http://w3id.org/polifonia/pilot/meetups/143114/10/5>
        rdf:type            ont:Sentence ;
        ont:excerpt         "He framed the acts with splashy dance numbers, developed sketch characters he would refine over the next decade, and became enough of a presence that CBS wooed him to its network in 1952." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/143114/10> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/143114/10/4> ;
        ont:subject         <http://dbpedia.org/resource/Jackie_Gleason> ;
        ont:timeExpression  "network in 1952" .

<http://w3id.org/polifonia/pilot/meetups/143114/20/6>
        rdf:type          ont:Sentence ;
        ont:excerpt       "When he made mistakes, he often blamed the cue cards." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/143114/20> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/143114/20/5> ;
        ont:subject       <http://dbpedia.org/resource/Jackie_Gleason> .

