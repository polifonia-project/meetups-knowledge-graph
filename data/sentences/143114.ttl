<http://w3id.org/polifonia/pilot/meetups/143114/34/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Williams was not given credit for his work until the early 1960s, albeit only in small print on the backs of album covers." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/143114/34> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/143114/34/0> ;
        ont:subject       <http://dbpedia.org/resource/Jackie_Gleason> .

<http://w3id.org/polifonia/pilot/meetups/143114/28>
        rdf:type  ont:Paragraph ;
