        rdf:type          ont:Sentence ;
        ont:excerpt       "Biography ==" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/699151/1> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/699151/1/0> ;
