<http://w3id.org/polifonia/pilot/meetups/699151/2/2>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Carey was one of the station's original employees, beginning work there in October 1953 — six months before WQED actually started broadcasting." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/699151/2> ;
        ont:person          <http://dbpedia.org/resource/Henry_Charles_Carey> ;
