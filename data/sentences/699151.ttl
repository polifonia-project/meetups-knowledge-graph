<http://w3id.org/polifonia/pilot/meetups/699151/0/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Josephine Vicari Massucci Franz (August 20, 1930 – May 28, 2004), known by the stage name Josie Carey, was a lyricist and a host of several children's television shows." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/699151/0> ;
        ont:person          <http://dbpedia.org/resource/Josie_Carey> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/699151/0/0> ;
        ont:subject         <http://dbpedia.org/resource/Josie_Carey> ;
        ont:timeExpression  "May 28 , 2004" , "August 20 , 1930" .

<http://w3id.org/polifonia/pilot/meetups/699151/3/5>
        rdf:type            ont:Sentence ;
        ont:excerpt         "The Children's Corner was syndicated on NBC for 39 weeks and won a Sylvania Award in 1955 for the best locally produced children's program in the country." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/699151/3> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/699151/3/4> ;
        ont:subject         <http://dbpedia.org/resource/Josie_Carey> ;
        ont:timeExpression  "1955 for" .

