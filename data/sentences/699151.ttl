<http://w3id.org/polifonia/pilot/meetups/699151/2/1>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Carey was the host of Pittsburgh's children show, The Children's Corner on WQED from 1953 to 1961." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/699151/2> ;
        ont:person          <http://dbpedia.org/resource/Drew_Carey> ;
        ont:place           <http://dbpedia.org/resource/Pittsburgh> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/699151/2/0> ;
        ont:subject         <http://dbpedia.org/resource/Josie_Carey> ;
        ont:timeExpression  "from 1953 to 1961" .

