        ont:person          <http://dbpedia.org/resource/President_of_the_United_States> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/648294/20/1> ;
        ont:subject         <http://dbpedia.org/resource/Dennis_DeYoung> ;
        ont:timeExpression  "group as one" .

