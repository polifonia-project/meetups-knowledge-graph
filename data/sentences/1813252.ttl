<http://w3id.org/polifonia/pilot/meetups/1813252/10/4>
        rdf:type            ont:Sentence ;
        ont:excerpt         "The memorial, in the shape of a broken pillar, was unveiled in December 1913." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1813252/10> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1813252/10/3> ;
        ont:subject         <http://dbpedia.org/resource/Wallace_Hartley> ;
        ont:timeExpression  "December 1913" .

<http://w3id.org/polifonia/pilot/meetups/1813252/9/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Erected by voluntary contributions to commemorate the heroism of a native of this town." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1813252/9> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1813252/9/0> ;
        ont:subject       <http://dbpedia.org/resource/Wallace_Hartley> .

<http://w3id.org/polifonia/pilot/meetups/1813252/4/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "None of the band members survived the sinking, and the story of them playing to the end became a popular legend." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1813252/4> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1813252/4/1> ;
        ont:subject       <http://dbpedia.org/resource/Wallace_Hartley> .

<http://w3id.org/polifonia/pilot/meetups/1813252/13/9>
        rdf:type          ont:Sentence ;
        ont:excerpt       "It has two large cracks on either side of the upper bout due to moisture damage, and is no longer playable." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1813252/13> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1813252/13/8> ;
        ont:subject       <http://dbpedia.org/resource/Wallace_Hartley> .

<http://w3id.org/polifonia/pilot/meetups/1813252/2/8>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Whilst serving on the Mauretania, the employment of Cunard musicians was transferred to the music agency C.W. & F.N. Black, which supplied musicians for Cunard and the White Star Line." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1813252/2> ;
        ont:place         <http://dbpedia.org/resource/Mauretania> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1813252/2/7> ;
        ont:subject       <http://dbpedia.org/resource/Wallace_Hartley> .

<http://w3id.org/polifonia/pilot/meetups/1813252/8/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "The memorial is inscribed: Wallace Hartley" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1813252/8> ;
        ont:person        <http://dbpedia.org/resource/Wallace_Hartley> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1813252/8/0> ;
        ont:subject       <http://dbpedia.org/resource/Wallace_Hartley> .

<http://w3id.org/polifonia/pilot/meetups/1813252/13/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "The violin was later passed on to a violin teacher who gave it to the current owner's mother." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1813252/13> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1813252/13/2> ;
        ont:subject       <http://dbpedia.org/resource/Wallace_Hartley> .

<http://w3id.org/polifonia/pilot/meetups/1813252/21>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/1813252/21/0> .

<http://w3id.org/polifonia/pilot/meetups/1813252/7/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Several press reports confirmed that Wallace was found \"fully dressed with his music case strapped to his body\"." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1813252/7> ;
        ont:person        <http://dbpedia.org/resource/Alfred_Russel_Wallace> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1813252/7/0> ;
        ont:subject       <http://dbpedia.org/resource/Wallace_Hartley> .

<http://w3id.org/polifonia/pilot/meetups/1813252/2/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Albion himself introduced the hymn \"Nearer, My God, to Thee\" to the congregation." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1813252/2> ;
        ont:person        <http://dbpedia.org/resource/God> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1813252/2/1> ;
        ont:subject       <http://dbpedia.org/resource/Wallace_Hartley> .

