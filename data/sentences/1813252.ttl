        rdf:type          ont:Sentence ;
        ont:excerpt       "This was later moved slightly to make way for the World War One memorial." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1813252/10> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1813252/10/0> ;
