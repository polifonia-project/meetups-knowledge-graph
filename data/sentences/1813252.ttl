        ont:timeExpression  "April 15th" .

<http://w3id.org/polifonia/pilot/meetups/1813252/4/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Many of the survivors said that he and the band continued to play until the very end." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1813252/4> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1813252/4/0> ;
        ont:subject       <http://dbpedia.org/resource/Wallace_Hartley> .

<http://w3id.org/polifonia/pilot/meetups/1813252/13/8>
        rdf:type          ont:Sentence ;
        ont:excerpt       "The violin now resides in Tennessee at a museum dedicated to the victims of the sinking, and is open to public viewing." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1813252/13> ;
        ont:place         <http://dbpedia.org/resource/Tennessee> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1813252/13/7> ;
        ont:subject       <http://dbpedia.org/resource/Wallace_Hartley> .

