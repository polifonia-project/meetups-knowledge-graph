<http://w3id.org/polifonia/pilot/meetups/1813252/10/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "The people of Broken Hill were so moved by the bravery of the ship's bandsmen that within a few weeks they had launched a public appeal to create a memorial to them." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1813252/10> ;
        ont:place         <http://dbpedia.org/resource/Broken_Hill> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1813252/10/2> ;
        ont:subject       <http://dbpedia.org/resource/Wallace_Hartley> .

<http://w3id.org/polifonia/pilot/meetups/1813252/9/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Bandmaster of the RMS Titanic who perished in the foundering of that vessel, April 15th 1912." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1813252/9> ;
        ont:person          <http://dbpedia.org/resource/Bandmaster> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1813252/9/0> ;
