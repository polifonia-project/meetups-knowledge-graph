        ont:place         <http://dbpedia.org/resource/Free_France> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/49370/13/9> ;
        ont:subject       <http://dbpedia.org/resource/Anthony_Burgess> .

<http://w3id.org/polifonia/pilot/meetups/49370/13/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1941, Burgess was pursued by the Royal Military Police for desertion after overstaying his leave from Morpeth military base with his future bride Lynne." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/49370/13> ;
        ont:person        <http://dbpedia.org/resource/Bride> ;
        ont:place         <http://dbpedia.org/resource/Morpeth_railway_station> ;
