        ont:subject       <http://dbpedia.org/resource/Anthony_Burgess> .

<http://w3id.org/polifonia/pilot/meetups/49370/60/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1991 he was awarded the title of Companion of Literature by the Royal Society of Literature." ;
