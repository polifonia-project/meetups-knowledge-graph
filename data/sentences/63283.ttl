<http://w3id.org/polifonia/pilot/meetups/63283/4/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Van Vliet was born Don Glen Vliet in Glendale, California, on January 15, 1941, to Glen Alonzo Vliet, a service station owner of Dutch ancestry from Kansas, and Willie Sue Vliet (née Warfield), who was from Arkansas." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/63283/4> ;
        ont:person          <http://dbpedia.org/resource/Captain_Beefheart> ;
        ont:place           <http://dbpedia.org/resource/Warfield> , <http://dbpedia.org/resource/Kansas> , <http://dbpedia.org/resource/Arkansas> , <http://dbpedia.org/resource/Glendale,_California> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/63283/4/0> ;
        ont:subject         <http://dbpedia.org/resource/Captain_Beefheart> ;
        ont:timeExpression  "January 15 , 1941" .

<http://w3id.org/polifonia/pilot/meetups/63283/17/6>
        rdf:type          ont:Sentence ;
