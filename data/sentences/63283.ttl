        ont:person          <http://dbpedia.org/resource/Wayne_Handley> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/63283/19/0> ;
        ont:subject         <http://dbpedia.org/resource/Captain_Beefheart> ;
        ont:timeExpression  "November 1967" .

<http://w3id.org/polifonia/pilot/meetups/63283/14/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Later, the Beatles planned to sign Beefheart to their experimental Zapple label (plans that were scrapped after Allen Klein took over the group's management)." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/63283/14> ;
        ont:person        <http://dbpedia.org/resource/Allen_Klein> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/63283/14/2> ;
        ont:subject       <http://dbpedia.org/resource/Captain_Beefheart> .

<http://w3id.org/polifonia/pilot/meetups/63283/28/6>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Bassist Jerry Handley had also departed, with Gary Marker stepping in." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/63283/28> ;
        ont:person        <http://dbpedia.org/resource/Wayne_Handley> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/63283/28/5> ;
        ont:subject       <http://dbpedia.org/resource/Captain_Beefheart> .

<http://w3id.org/polifonia/pilot/meetups/63283/73/12>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Particularly notable is their song \"Muffin Man\", included on the Zappa/Beefheart Bongo Fury album, as well as Zappa's compilation album Strictly Commercial (1995)." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/63283/73> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/63283/73/11> ;
