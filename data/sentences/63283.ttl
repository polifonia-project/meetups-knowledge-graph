        ont:subject         <http://dbpedia.org/resource/Captain_Beefheart> ;
        ont:timeExpression  "September 1967" .

<http://w3id.org/polifonia/pilot/meetups/63283/29/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "The Magic Band began recordings for Trout Mask Replica with bassist Gary \"Magic\" Marker at T.T.G. (on \"Moonlight on Vermont\" and \"Veteran's Day Poppy\"), but later enlisted bassist Mark Boston after his departure." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/63283/29> ;
        ont:person        <http://dbpedia.org/resource/Rockette_Morton> ;
        ont:place         <http://dbpedia.org/resource/Vermont> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/63283/29/0> ;
        ont:subject       <http://dbpedia.org/resource/Captain_Beefheart> .

