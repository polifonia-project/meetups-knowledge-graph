        ont:subject         <http://dbpedia.org/resource/Captain_Beefheart> ;
        ont:timeExpression  "between 1964 and 1982" , "13 studio" .

<http://w3id.org/polifonia/pilot/meetups/63283/77/31>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Harvey's first experience of Beefheart's music was as a child." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/63283/77> ;
