        rdf:_3    <http://w3id.org/polifonia/pilot/meetups/671985/10/2> .

<http://w3id.org/polifonia/pilot/meetups/671985/4/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "The family moved to Jamaica, Queens when Mal was four years old." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/671985/4> ;
        ont:place         <http://dbpedia.org/resource/Jamaica,_Queens> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/671985/4/1> ;
        ont:subject       <http://dbpedia.org/resource/Mal_Waldron> .

<http://w3id.org/polifonia/pilot/meetups/671985/13/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Waldron's first wife, Elaine, occasionally sang on his recordings." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/671985/13> ;
        ont:person        <http://dbpedia.org/resource/Mal_Waldron> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/671985/13/1> ;
        ont:subject       <http://dbpedia.org/resource/Mal_Waldron> .

<http://w3id.org/polifonia/pilot/meetups/671985/9/7>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Eventually, I overdosed." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/671985/9> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/671985/9/6> ;
        ont:subject       <http://dbpedia.org/resource/Mal_Waldron> .

<http://w3id.org/polifonia/pilot/meetups/671985/11/23>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Waldron moved from Munich to Brussels in the 1990s, stating that, in Belgium, \"nobody stands on the corner waiting for the lights to change." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/671985/11> ;
        ont:person          <http://dbpedia.org/resource/Mal_Waldron> ;
        ont:place           <http://dbpedia.org/resource/Belgium> , <http://dbpedia.org/resource/Brussels> , <http://dbpedia.org/resource/Munich> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/671985/11/22> ;
        ont:subject         <http://dbpedia.org/resource/Mal_Waldron> ;
        ont:timeExpression  "in the 1990s" .

