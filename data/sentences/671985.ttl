<http://w3id.org/polifonia/pilot/meetups/671985/4/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Waldron's parents discouraged his initial interest in jazz, but he was able to maintain it by listening to swing on the radio." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/671985/4> ;
        ont:person        <http://dbpedia.org/resource/Mal_Waldron> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/671985/4/2> ;
        ont:subject       <http://dbpedia.org/resource/Mal_Waldron> .

<http://w3id.org/polifonia/pilot/meetups/671985/13/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "His second wife was Japanese, and they owned and let several apartments in Japan." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/671985/13> ;
        ont:place         <http://dbpedia.org/resource/Japan> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/671985/13/2> ;
        ont:subject       <http://dbpedia.org/resource/Mal_Waldron> .

<http://w3id.org/polifonia/pilot/meetups/671985/9/8>
        rdf:type          ont:Sentence ;
        ont:excerpt       "I couldn't remember my own name." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/671985/9> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/671985/9/7> ;
        ont:subject       <http://dbpedia.org/resource/Mal_Waldron> .

<http://w3id.org/polifonia/pilot/meetups/671985/11/24>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In Germany they watch the lights instead of the cars." ;
