<http://w3id.org/polifonia/pilot/meetups/671985/4/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Mal Waldron was born in New York City on August 16, 1925, to West Indian immigrants." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/671985/4> ;
        ont:person          <http://dbpedia.org/resource/Mal_Waldron> ;
        ont:place           <http://dbpedia.org/resource/New_York_City> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/671985/4/0> ;
        ont:subject         <http://dbpedia.org/resource/Mal_Waldron> ;
        ont:timeExpression  "August 16 , 1925" .

<http://w3id.org/polifonia/pilot/meetups/671985/13/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Waldron married twice and had seven children – two with his first wife and five with the second." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/671985/13> ;
        ont:place           <http://dbpedia.org/resource/Waldron_Island> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/671985/13/0> ;
        ont:subject         <http://dbpedia.org/resource/Mal_Waldron> ;
        ont:timeExpression  "two with" , "five with" .

<http://w3id.org/polifonia/pilot/meetups/671985/11/8>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Two years later, another Waldron recording session was the first for another label that became firmly established – Enja Records." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/671985/11> ;
        ont:person        <http://dbpedia.org/resource/Mal_Waldron> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/671985/11/7> ;
        ont:subject       <http://dbpedia.org/resource/Mal_Waldron> .

<http://w3id.org/polifonia/pilot/meetups/671985/9/5>
        rdf:type          ont:Sentence ;
        ont:excerpt       "After a while, the musicians thought ..." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/671985/9> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/671985/9/4> ;
        ont:subject       <http://dbpedia.org/resource/Mal_Waldron> .

<http://w3id.org/polifonia/pilot/meetups/671985/11/21>
        rdf:type          ont:Sentence ;
        ont:excerpt       "[...] So to do this face to face, it is more direct, stronger and more accurate\"." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/671985/11> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/671985/11/20> ;
        ont:subject       <http://dbpedia.org/resource/Mal_Waldron> .

