<http://w3id.org/polifonia/pilot/meetups/671985/4/8>
        rdf:type          ont:Sentence ;
        ont:excerpt       "This allowed him to listen to the greats of jazz in clubs on 52nd Street and elsewhere in the city." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/671985/4> ;
        ont:place         <http://dbpedia.org/resource/52nd_Street_(Manhattan)> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/671985/4/7> ;
        ont:subject       <http://dbpedia.org/resource/Mal_Waldron> .

