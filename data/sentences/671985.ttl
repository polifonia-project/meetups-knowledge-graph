        ont:person          <http://dbpedia.org/resource/Mal_Waldron> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/671985/11/8> ;
        ont:subject         <http://dbpedia.org/resource/Mal_Waldron> ;
        ont:timeExpression  "1971 album" .

<http://w3id.org/polifonia/pilot/meetups/671985/9/6>
        rdf:type          ont:Sentence ;
        ont:excerpt       "well, if you have the name you might as well have the game." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/671985/9> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/671985/9/5> ;
