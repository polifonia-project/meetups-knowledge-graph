        ont:timeExpression  "one for" , "1965 score" .

<http://w3id.org/polifonia/pilot/meetups/671985/9/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1963 Waldron had a major breakdown caused by a heroin overdose." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/671985/9> ;
        ont:person        <http://dbpedia.org/resource/Mal_Waldron> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/671985/9/0> ;
