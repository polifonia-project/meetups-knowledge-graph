        rdf:type          ont:Sentence ;
        ont:excerpt       "Waldron also composed for theater (Amiri Baraka's The Slave and Dutchman), television, and short films." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/671985/11> ;
        ont:person        <http://dbpedia.org/resource/Amiri_Baraka> , <http://dbpedia.org/resource/Mal_Waldron> ;
