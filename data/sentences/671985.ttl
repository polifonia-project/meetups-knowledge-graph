<http://w3id.org/polifonia/pilot/meetups/671985/4/4>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Waldron had classical piano lessons from the age of around seven until he was about 16." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/671985/4> ;
        ont:person          <http://dbpedia.org/resource/Mal_Waldron> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/671985/4/3> ;
