<http://w3id.org/polifonia/pilot/meetups/671985/13/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Combining birthday celebrations with a tour, Waldron took both families – ex-wife, wife, seven children and two grandchildren – on his three-week tour of Japan that coincided with his seventieth birthday." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/671985/13> ;
        ont:person        <http://dbpedia.org/resource/Mal_Waldron> ;
        ont:place         <http://dbpedia.org/resource/Empire_of_Japan> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/671985/13/3> ;
        ont:subject       <http://dbpedia.org/resource/Mal_Waldron> .

<http://w3id.org/polifonia/pilot/meetups/671985/9/9>
        rdf:type          ont:Sentence ;
        ont:excerpt       "My hands were trembling, I couldn't play the piano." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/671985/9> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/671985/9/8> ;
        ont:subject       <http://dbpedia.org/resource/Mal_Waldron> .

<http://w3id.org/polifonia/pilot/meetups/671985/11/25>
        rdf:type          ont:Sentence ;
