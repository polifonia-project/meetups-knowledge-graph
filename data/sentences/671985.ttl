        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/671985/20> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/671985/20/0> ;
        ont:subject       <http://dbpedia.org/resource/Mal_Waldron> .

<http://w3id.org/polifonia/pilot/meetups/671985/7/13>
        rdf:type          ont:Sentence ;
        ont:excerpt       "71–72  Waldron estimated that he composed more than 400 pieces of music during his time with Prestige." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/671985/7> ;
        ont:person        <http://dbpedia.org/resource/Mal_Waldron> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/671985/7/12> ;
        ont:subject       <http://dbpedia.org/resource/Mal_Waldron> .

<http://w3id.org/polifonia/pilot/meetups/671985/15/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "When he first played with Mingus, Waldron was a follower of Horace Silver's style, which used added chords and passing notes, as well as Bud Powell's, which contained many runs." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/671985/15> ;
        ont:person        <http://dbpedia.org/resource/Horace> , <http://dbpedia.org/resource/Bud_Powell> , <http://dbpedia.org/resource/Mal_Waldron> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/671985/15/0> ;
        ont:subject       <http://dbpedia.org/resource/Mal_Waldron> .

<http://w3id.org/polifonia/pilot/meetups/671985/0/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "During Waldron's period as house pianist for Prestige Records in the late 1950s, he appeared on dozens of albums and composed for many of them, including writing his most famous song, \"Soul Eyes\", for Coltrane." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/671985/0> ;
        ont:person        <http://dbpedia.org/resource/Mal_Waldron> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/671985/0/2> ;
        ont:subject       <http://dbpedia.org/resource/Mal_Waldron> .

<http://w3id.org/polifonia/pilot/meetups/671985/11/29>
        rdf:type            ont:Sentence ;
        ont:excerpt         "After some years of indifferent health, Waldron, a heavy smoker, was diagnosed with cancer in 2002." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/671985/11> ;
        ont:person          <http://dbpedia.org/resource/Mal_Waldron> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/671985/11/28> ;
        ont:subject         <http://dbpedia.org/resource/Mal_Waldron> ;
        ont:timeExpression  "cancer in 2002" .

<http://w3id.org/polifonia/pilot/meetups/671985/6>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/671985/6/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/671985/6/1> ;
        rdf:_3    <http://w3id.org/polifonia/pilot/meetups/671985/6/2> .

<http://w3id.org/polifonia/pilot/meetups/671985/7/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Waldron went on to work with Ike Quebec in New York in 1950 and made his recording debut with the saxophonist in 1952." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/671985/7> ;
        ont:person          <http://dbpedia.org/resource/Mal_Waldron> , <http://dbpedia.org/resource/Ike_Quebec> ;
        ont:place           <http://dbpedia.org/resource/New_York_City> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/671985/7/0> ;
        ont:subject         <http://dbpedia.org/resource/Mal_Waldron> ;
        ont:timeExpression  "saxophonist in 1952" .

