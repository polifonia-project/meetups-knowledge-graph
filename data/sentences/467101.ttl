        ont:subject       <http://dbpedia.org/resource/Rufus_Thomas> .

<http://w3id.org/polifonia/pilot/meetups/467101/6/5>
        rdf:type            ont:Sentence ;
        ont:excerpt         "The 1963 follow-up, \"Walking the Dog\", engineered by Tom Dowd of Atlantic, became one of his most successful records, reaching #10 on the Billboard pop chart." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/467101/6> ;
        ont:person          <http://dbpedia.org/resource/Tom_Dowd> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/467101/6/4> ;
        ont:subject         <http://dbpedia.org/resource/Rufus_Thomas> ;
        ont:timeExpression  "1963 follow-up" , "one of" , "10 on" .

<http://w3id.org/polifonia/pilot/meetups/467101/4/17>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He began working as a DJ at radio station WDIA in 1951, and hosted an afternoon R&B show called Hoot and Holler." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/467101/4> ;
        ont:person        <http://dbpedia.org/resource/Disc_jockey> ;
        ont:place         <http://dbpedia.org/resource/Valley> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/467101/4/16> ;
        ont:subject       <http://dbpedia.org/resource/Rufus_Thomas> .

<http://w3id.org/polifonia/pilot/meetups/467101/2/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Thomas was born in the rural community of Cayce, Mississippi, the son of a sharecropper." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/467101/2> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/467101/2/0> ;
        ont:subject       <http://dbpedia.org/resource/Rufus_Thomas> .

<http://w3id.org/polifonia/pilot/meetups/467101/15/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "A character named Rufus in “Kill Bill: Volume 2” played with Rufus Thomas." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/467101/15> ;
