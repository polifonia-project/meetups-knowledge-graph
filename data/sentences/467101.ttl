        rdf:type          ont:Sentence ;
        ont:excerpt       "occupied many important roles in the local scene." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/467101/0> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/467101/0/3> ;
