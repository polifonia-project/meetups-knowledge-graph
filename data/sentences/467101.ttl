        ont:timeExpression  "in the 2008" , "June 25 , 2019" .

<http://w3id.org/polifonia/pilot/meetups/467101/2/6>
        rdf:type            ont:Sentence ;
        ont:excerpt         "After graduating from high school, Thomas attended Tennessee A&I University for one semester, but economic constraints led him to leave to pursue a career as a full-time entertainer." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/467101/2> ;
        ont:place           <http://dbpedia.org/resource/Tennessee> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/467101/2/5> ;
        ont:subject         <http://dbpedia.org/resource/Rufus_Thomas> ;
        ont:timeExpression  "one semester" .

