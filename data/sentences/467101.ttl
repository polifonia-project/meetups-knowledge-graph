        rdf:type            ont:Sentence ;
        ont:excerpt         "A further dance-oriented release in 1971, \"The Breakdown\", climbed to number 2 R&B and number 31 Pop." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/467101/6> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/467101/6/15> ;
        ont:subject         <http://dbpedia.org/resource/Rufus_Thomas> ;
        ont:timeExpression  "release in 1971" .

<http://w3id.org/polifonia/pilot/meetups/467101/23/0>
