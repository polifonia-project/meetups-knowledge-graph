        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/472272/8/0> .

<http://w3id.org/polifonia/pilot/meetups/472272/16/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "LaFontaine was referenced, with opening clips of his work and several subsequent verbal homages, in the film In a World..., written and directed by Lake Bell." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/472272/16> ;
        ont:person        <http://dbpedia.org/resource/Lake_Bell> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/472272/16/0> ;
        ont:subject       <http://dbpedia.org/resource/Don_LaFontaine> .

<http://w3id.org/polifonia/pilot/meetups/472272/11/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Health and death ==" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/472272/11> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/472272/11/0> ;
        ont:subject       <http://dbpedia.org/resource/Don_LaFontaine> .

<http://w3id.org/polifonia/pilot/meetups/472272/0/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Donald Leroy LaFontaine (August 26, 1940 – September 1, 2008) was an American voice actor who recorded more than 5,000 film trailers and hundreds of thousands of television advertisements, network promotions, and video game trailers." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/472272/0> ;
        ont:person          <http://dbpedia.org/resource/Voice_acting> ;
        ont:place           <http://dbpedia.org/resource/United_States> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/472272/0/0> ;
        ont:subject         <http://dbpedia.org/resource/Don_LaFontaine> ;
        ont:timeExpression  "August 26 , 1940" , "5,000 film" , "September 1 , 2008" .

<http://w3id.org/polifonia/pilot/meetups/472272/15/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "\"The Apprentice Scout\", an episode of Chowder, is dedicated to LaFontaine." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/472272/15> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/472272/15/0> ;
        ont:subject       <http://dbpedia.org/resource/Don_LaFontaine> .

<http://w3id.org/polifonia/pilot/meetups/472272/10/10>
        rdf:type            ont:Sentence ;
        ont:excerpt         "LaFontaine was featured as the celebrity in one of these ads which began airing in August 2006." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/472272/10> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/472272/10/9> ;
        ont:subject         <http://dbpedia.org/resource/Don_LaFontaine> ;
        ont:timeExpression  "August 2006" , "celebrity in one" .

<http://w3id.org/polifonia/pilot/meetups/472272/11>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/472272/11/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/472272/11/1> .

<http://w3id.org/polifonia/pilot/meetups/472272/10/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "LaFontaine was a recurring guest narrator for clues on the game show Jeopardy!" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/472272/10> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/472272/10/0> ;
        ont:subject       <http://dbpedia.org/resource/Don_LaFontaine> .

<http://w3id.org/polifonia/pilot/meetups/472272/2>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/472272/2/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/472272/2/1> .

<http://w3id.org/polifonia/pilot/meetups/472272/7/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Lafontaine also did announcing for a few WWE Pay Per View events, as well as the \"Don't Try This at Home\" bumper and the opening narration for the television shows Team Knight Rider, Renegade and Rambo: The Force of Freedom." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/472272/7> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/472272/7/3> ;
        ont:subject       <http://dbpedia.org/resource/Don_LaFontaine> .

<http://w3id.org/polifonia/pilot/meetups/472272/14/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "On September 6, 2008, America's Most Wanted showed a visual with a picture of him with words below that said \"In Memoriam: Don LaFontaine August 26, 1940 – September 1, 2008.\"" ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/472272/14> ;
        ont:person          <http://dbpedia.org/resource/Don_LaFontaine> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/472272/14/0> ;
        ont:subject         <http://dbpedia.org/resource/Don_LaFontaine> ;
        ont:timeExpression  "September 1 , 2008" , "August 26 , 1940" , "September 6 , 2008" .

<http://w3id.org/polifonia/pilot/meetups/472272/12/6>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Happy?\"" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/472272/12> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/472272/12/5> ;
        ont:subject       <http://dbpedia.org/resource/Don_LaFontaine> .

<http://w3id.org/polifonia/pilot/meetups/472272/6/4>
        rdf:type            ont:Sentence ;
        ont:excerpt         "At his peak, he voiced about 60 promotions a week, and sometimes as many as 35 in a single day." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/472272/6> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/472272/6/3> ;
        ont:subject         <http://dbpedia.org/resource/Don_LaFontaine> ;
        ont:timeExpression  "35 in" .

<http://w3id.org/polifonia/pilot/meetups/472272/13/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "==" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/472272/13> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/472272/13/0> ;
        ont:subject       <http://dbpedia.org/resource/Don_LaFontaine> .

<http://w3id.org/polifonia/pilot/meetups/472272/5/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "After MGM bought the spots, LaFontaine began a career as a voiceover artist." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/472272/5> ;
        ont:person        <http://dbpedia.org/resource/Voice_acting> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/472272/5/3> ;
        ont:subject       <http://dbpedia.org/resource/Don_LaFontaine> .
