<http://w3id.org/polifonia/pilot/meetups/472272/14/1>
        rdf:type            ont:Sentence ;
        ont:excerpt         "John Walsh had announced, prior to the dedication sign, that LaFontaine—who had been the show's announcer since 1988—had died at the age of 68." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/472272/14> ;
        ont:person          <http://dbpedia.org/resource/John_Walsh_(television_host)> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/472272/14/0> ;
        ont:subject         <http://dbpedia.org/resource/Don_LaFontaine> ;
        ont:timeExpression  "age of 68" , "announcer since 1988—had" .

<http://w3id.org/polifonia/pilot/meetups/472272/3/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "LaFontaine was born on August 26, 1940, in Duluth, Minnesota, to Alfred and Ruby LaFontaine." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/472272/3> ;
        ont:place           <http://dbpedia.org/resource/Minnesota> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/472272/3/0> ;
        ont:subject         <http://dbpedia.org/resource/Don_LaFontaine> ;
        ont:timeExpression  "August 26 , 1940" .

<http://w3id.org/polifonia/pilot/meetups/472272/6/5>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Once he established himself, most studios were willing to pay a high fee for his service." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/472272/6> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/472272/6/4> ;
        ont:subject       <http://dbpedia.org/resource/Don_LaFontaine> .

<http://w3id.org/polifonia/pilot/meetups/472272/12/7>
        rdf:type          ont:Sentence ;
        ont:excerpt       "The episode also ended with a short tribute to him, although the iTunes, UK, Spanish, and Disney+ versions of the episode omitted the dedication." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/472272/12> ;
        ont:place         <http://dbpedia.org/resource/United_Kingdom> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/472272/12/6> ;
        ont:subject       <http://dbpedia.org/resource/Don_LaFontaine> .

<http://w3id.org/polifonia/pilot/meetups/472272/13/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Legacy ==" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/472272/13> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/472272/13/0> ;
        ont:subject       <http://dbpedia.org/resource/Don_LaFontaine> .

<http://w3id.org/polifonia/pilot/meetups/472272/2/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "==" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/472272/2> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/472272/2/0> ;
        ont:subject       <http://dbpedia.org/resource/Don_LaFontaine> .

<http://w3id.org/polifonia/pilot/meetups/472272/17/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "After his death, voice-overs in film trailers became rare, with most trailers now relying on music, sound effects, and dialogue taken from the film." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/472272/17> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/472272/17/0> ;
        ont:subject       <http://dbpedia.org/resource/Don_LaFontaine> .

<http://w3id.org/polifonia/pilot/meetups/472272/12/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "His family made a public appeal for prayers on Mediabistro." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/472272/12> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/472272/12/0> ;
        ont:subject       <http://dbpedia.org/resource/Don_LaFontaine> .

<http://w3id.org/polifonia/pilot/meetups/472272/1/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He became identified with the phrase \"In a world...\", used in so many movie trailers that it became a cliché." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/472272/1> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/472272/1/0> ;
        ont:subject       <http://dbpedia.org/resource/Don_LaFontaine> .

<http://w3id.org/polifonia/pilot/meetups/472272/10/7>
        rdf:type          ont:Sentence ;
        ont:excerpt       "LaFontaine once claimed that he enjoyed recording messages like these because it allowed him to be creative in writing unique messages, and said that he would do so for anyone who contacted him if he had the time." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/472272/10> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/472272/10/6> ;
        ont:subject       <http://dbpedia.org/resource/Don_LaFontaine> .

<http://w3id.org/polifonia/pilot/meetups/472272/17>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/472272/17/0> .

