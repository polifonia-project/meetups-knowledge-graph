        ont:place           <http://dbpedia.org/resource/Hawaii> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/198362/12/0> ;
        ont:subject         <http://dbpedia.org/resource/The_Boswell_Sisters> ;
        ont:timeExpression  "50 broadcast transcription" .

