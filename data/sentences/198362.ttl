        rdf:_3    <http://w3id.org/polifonia/pilot/meetups/198362/20/2> ;
        rdf:_4    <http://w3id.org/polifonia/pilot/meetups/198362/20/3> .

<http://w3id.org/polifonia/pilot/meetups/198362/24/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "===" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/198362/24> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/198362/24/0> ;
        ont:subject       <http://dbpedia.org/resource/The_Boswell_Sisters> .

