        rdf:type          ont:Sentence ;
        ont:excerpt       "Hit singles ==" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/198362/38> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/198362/38/0> ;
