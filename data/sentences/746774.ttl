<http://w3id.org/polifonia/pilot/meetups/746774/0/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Harold Jacob \"Hecky\" Rome (May 27, 1908 – October 26, 1993) was an American composer, lyricist, and writer for musical theater." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/746774/0> ;
        ont:person          <http://dbpedia.org/resource/Composer> ;
        ont:place           <http://dbpedia.org/resource/United_States> , <http://dbpedia.org/resource/Ancient_Rome> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/746774/0/0> ;
        ont:subject         <http://dbpedia.org/resource/Harold_Rome> ;
        ont:timeExpression  "May 27 , 1908" , "October 26 , 1993" .

<http://w3id.org/polifonia/pilot/meetups/746774/3/5>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1949, he wrote the English lyrics of the French song \"Mais qu’est-ce que j’ai?\"." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/746774/3> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/746774/3/4> ;
        ont:subject       <http://dbpedia.org/resource/Harold_Rome> .

<http://w3id.org/polifonia/pilot/meetups/746774/20/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Versions by Johnnie Johnston and by Martha Stewart were hits in 1945 reaching numbers 7 and 12 respectively in the Billboard chart." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/746774/20> ;
        ont:person        <http://dbpedia.org/resource/Martha_Stewart> , <http://dbpedia.org/resource/Johnnie_Johnston> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/746774/20/1> ;
        ont:subject       <http://dbpedia.org/resource/Harold_Rome> .

