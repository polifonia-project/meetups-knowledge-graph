        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/141330/6/0> ;
        ont:subject       <http://dbpedia.org/resource/Peter_Max> .

<http://w3id.org/polifonia/pilot/meetups/141330/1/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Early life and education" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/141330/1> ;
