        ont:subject       <http://dbpedia.org/resource/Peter_Max> .

<http://w3id.org/polifonia/pilot/meetups/141330/15/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He was also romantically connected with Tina Louise." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/141330/15> ;
        ont:person        <http://dbpedia.org/resource/Tina_Louise> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/141330/15/1> ;
        ont:subject       <http://dbpedia.org/resource/Peter_Max> .

<http://w3id.org/polifonia/pilot/meetups/141330/10/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1990, Max purchased a collection of Chevrolet Corvettes for an intended art project, but never used them." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/141330/10> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/141330/10/2> ;
        ont:subject       <http://dbpedia.org/resource/Peter_Max> .

<http://w3id.org/polifonia/pilot/meetups/141330/7>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/141330/7/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/141330/7/1> .

<http://w3id.org/polifonia/pilot/meetups/141330/0/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Works by Max are associated with the visual arts and culture of the 1960s, particularly psychedelic art and pop art." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/141330/0> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/141330/0/0> ;
        ont:subject       <http://dbpedia.org/resource/Peter_Max> .

<http://w3id.org/polifonia/pilot/meetups/141330/5/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "=== 1960s" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/141330/5> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/141330/5/0> ;
        ont:subject       <http://dbpedia.org/resource/Peter_Max> .

<http://w3id.org/polifonia/pilot/meetups/141330/10/14>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 2017, Max did the cover art for the Aug/Sept issue of AARP magazine." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/141330/10> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/141330/10/13> ;
        ont:subject       <http://dbpedia.org/resource/Peter_Max> .

<http://w3id.org/polifonia/pilot/meetups/141330/8/5>
        rdf:type            ont:Sentence ;
        ont:excerpt         "The book project commemorated the United States Bicentennial and included the following foreword: \"Peter Max Paints America is based on works of art commissioned by ASEA of Sweden on the 200th anniversary of the founding of the United States of America, in sincere recognition of the historic bonds of friendship between the people of Sweden and the people of the United States, recalling that Sweden was one of the first countries to extend its hand in friendship to the new nation.\"" ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/141330/8> ;
        ont:person          <http://dbpedia.org/resource/Peter_Max> ;
        ont:place           <http://dbpedia.org/resource/Sweden> , <http://dbpedia.org/resource/United_States> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/141330/8/4> ;
        ont:subject         <http://dbpedia.org/resource/Peter_Max> ;
        ont:timeExpression  "one of" .

<http://w3id.org/polifonia/pilot/meetups/141330/12/8>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He has collaborated with companies to create clothing, homewares, clocks, and other material goods." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/141330/12> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/141330/12/7> ;
        ont:subject       <http://dbpedia.org/resource/Peter_Max> .

<http://w3id.org/polifonia/pilot/meetups/141330/4/0>
        rdf:type          ont:Sentence ;
