<http://w3id.org/polifonia/pilot/meetups/754282/19>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/754282/19/0> .

<http://w3id.org/polifonia/pilot/meetups/754282/8/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "The early graph piece was followed by the Rothko orchestra work, the pieces for Source Magazine, the Feldman-derived chamber works, the pieces typed out or written in longhand, the out-and-out conceptual works among other things, and the model drone works (which include the sax and organ Coeur d'Orr and The Oak of the Golden Dreams, the latter based on the Balinese 'Slendro' scale which scale I used again 18 years later on 'The Real Dream of Sails')." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/754282/8> ;
        ont:person        <http://dbpedia.org/resource/Mark_Rothko> ;
        ont:place         <http://dbpedia.org/resource/Golden_Dreams> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/754282/8/0> ;
        ont:subject       <http://dbpedia.org/resource/Harold_Budd> .

<http://w3id.org/polifonia/pilot/meetups/754282/3/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Budd joined him in gigs around the Monterey area." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/754282/3> ;
        ont:place         <http://dbpedia.org/resource/Monterey,_California> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/754282/3/1> ;
        ont:subject       <http://dbpedia.org/resource/Harold_Budd> .

<http://w3id.org/polifonia/pilot/meetups/754282/11/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Budd resigned from the institute in 1976 and began recording his new compositions, produced by Eno." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/754282/11> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/754282/11/0> ;
        ont:subject         <http://dbpedia.org/resource/Harold_Budd> ;
        ont:timeExpression  "institute in 1976" .

