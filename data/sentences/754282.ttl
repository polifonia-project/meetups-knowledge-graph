        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/754282/26/0> .

<http://w3id.org/polifonia/pilot/meetups/754282/6/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In the following years, he gained a notable reputation in the local avant-garde community." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/754282/6> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/754282/6/0> ;
        ont:subject       <http://dbpedia.org/resource/Harold_Budd> .

<http://w3id.org/polifonia/pilot/meetups/754282/11/16>
        rdf:type            ont:Sentence ;
        ont:excerpt         "He composed music for the score of the 2020 miniseries I Know" ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/754282/11> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/754282/11/15> ;
        ont:subject         <http://dbpedia.org/resource/Harold_Budd> ;
        ont:timeExpression  "of the 2020" .

<http://w3id.org/polifonia/pilot/meetups/754282/5/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He switched to a course in harmony and his musical talent was spotted by a teacher who encouraged him to compose." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/754282/5> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/754282/5/0> ;
        ont:subject       <http://dbpedia.org/resource/Harold_Budd> .

<http://w3id.org/polifonia/pilot/meetups/754282/0/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Budd developed what he called a \"soft pedal\" technique for playing piano." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/754282/0> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/754282/0/1> ;
        ont:subject       <http://dbpedia.org/resource/Harold_Budd> .

<http://w3id.org/polifonia/pilot/meetups/754282/20>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/754282/20/0> .

<http://w3id.org/polifonia/pilot/meetups/754282/11/5>
        rdf:type          ont:Sentence ;
        ont:excerpt       "While he is often placed in the Ambient category, he emphatically declared that he was not an Ambient artist, and felt that he got \"kidnapped\" into the category." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/754282/11> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/754282/11/4> ;
        ont:subject       <http://dbpedia.org/resource/Harold_Budd> .

<http://w3id.org/polifonia/pilot/meetups/754282/9/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1972, while still retaining his teaching career at the California Institute for the Arts, he resurfaced as a composer." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/754282/9> ;
        ont:person        <http://dbpedia.org/resource/Composer> ;
        ont:place         <http://dbpedia.org/resource/California> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/754282/9/0> ;
        ont:subject       <http://dbpedia.org/resource/Harold_Budd> .

<http://w3id.org/polifonia/pilot/meetups/754282/4/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Education, academic career and early works ==" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/754282/4> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/754282/4/0> ;
        ont:subject       <http://dbpedia.org/resource/Harold_Budd> .

<http://w3id.org/polifonia/pilot/meetups/754282/8>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/754282/8/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/754282/8/1> .

<http://w3id.org/polifonia/pilot/meetups/754282/11/10>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He had a long-running collaboration with guitarist Robin Guthrie." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/754282/11> ;
        ont:person        <http://dbpedia.org/resource/Robin_Guthrie> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/754282/11/9> ;
        ont:subject       <http://dbpedia.org/resource/Harold_Budd> .

<http://w3id.org/polifonia/pilot/meetups/754282/18>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/754282/18/0> .

<http://w3id.org/polifonia/pilot/meetups/754282/10/5>
        rdf:type          ont:Sentence ;
        ont:excerpt       "I didn't know anything." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/754282/10> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/754282/10/4> ;
        ont:subject       <http://dbpedia.org/resource/Harold_Budd> .

<http://w3id.org/polifonia/pilot/meetups/754282/8/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "The road from my first colored graph piece in 1962 to my renunciation of composing in 1970 to my resurfacing as a composer in 1972 was a process of trying out an idea and when it was obviously successful abandoning it." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/754282/8> ;
        ont:person          <http://dbpedia.org/resource/Composer> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/754282/8/0> ;
        ont:subject         <http://dbpedia.org/resource/Harold_Budd> ;
        ont:timeExpression  "graph piece in 1962" , "composer in 1972" .

<http://w3id.org/polifonia/pilot/meetups/754282/3/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Jazz saxophonist Albert Ayler was drafted at the same time and was also a member of the band." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/754282/3> ;
        ont:person        <http://dbpedia.org/resource/Albert_Ayler> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/754282/3/0> ;
        ont:subject       <http://dbpedia.org/resource/Harold_Budd> .

<http://w3id.org/polifonia/pilot/meetups/754282/6/6>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Soon afterwards, Budd gave up composition, disgusted by the \"academic pyrotechnics\" of the avant-garde community." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/754282/6> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/754282/6/5> ;
        ont:subject       <http://dbpedia.org/resource/Harold_Budd> .

