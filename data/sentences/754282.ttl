<http://w3id.org/polifonia/pilot/meetups/754282/7/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Composer and recording artist ==" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/754282/7> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/754282/7/0> ;
        ont:subject       <http://dbpedia.org/resource/Harold_Budd> .

<http://w3id.org/polifonia/pilot/meetups/754282/2/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He was sent him up to the desert to live with friends and relatives as often as possible, but the reality in Los Angeles was growing up in a tough neighborhood, and as the oldest son, being the man of the house." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/754282/2> ;
        ont:place         <http://dbpedia.org/resource/Los_Angeles> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/754282/2/1> ;
        ont:subject       <http://dbpedia.org/resource/Harold_Budd> .

<http://w3id.org/polifonia/pilot/meetups/754282/10/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "I owe Eno everything, OK?" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/754282/10> ;
        ont:place         <http://dbpedia.org/resource/Oklahoma> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/754282/10/0> ;
        ont:subject       <http://dbpedia.org/resource/Harold_Budd> .

