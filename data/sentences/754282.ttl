        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/754282/7> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/754282/7/0> ;
        ont:subject       <http://dbpedia.org/resource/Harold_Budd> .

<http://w3id.org/polifonia/pilot/meetups/754282/2/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Harold was only 13 when his father passed away, and soon his family fell out of their comfortable middle class existence." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/754282/2> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/754282/2/0> ;
        ont:subject       <http://dbpedia.org/resource/Harold_Budd> .

<http://w3id.org/polifonia/pilot/meetups/754282/12>
