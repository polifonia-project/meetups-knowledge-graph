<http://w3id.org/polifonia/pilot/meetups/2657736/6/6>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Schuman and Mackay reunited in 1983 for the BBC one-off TV drama Video Stars, with Mackay again providing music." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/2657736/6> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/2657736/6/5> ;
