        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2657736/8/0> ;
        ont:subject       <http://dbpedia.org/resource/Andy_Mackay> .

<http://w3id.org/polifonia/pilot/meetups/2657736/3/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "A classically trained woodwind player, he studied music and English literature at Reading University." ;
