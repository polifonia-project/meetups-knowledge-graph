        rdf:type          ont:Sentence ;
        ont:excerpt       "Haynes received the Variety Club's award as ITV Personality of 1961 and appeared on the Royal Variety Performance in the same year." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2811507/7> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2811507/7/0> ;
