        rdf:type          ont:Sentence ;
        ont:excerpt       "Music ==" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2811507/9> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2811507/9/0> ;
        ont:subject       <http://dbpedia.org/resource/Arthur_Haynes> .

<http://w3id.org/polifonia/pilot/meetups/2811507/14/2>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Volumes 3 and 4 contain episodes from 1962, Volumes 5 and 6 both containing episodes based on scripts by Johnny Speight." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/2811507/14> ;
        ont:person          <http://dbpedia.org/resource/Johnny_Speight> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/2811507/14/1> ;
        ont:subject         <http://dbpedia.org/resource/Arthur_Haynes> ;
        ont:timeExpression  "4 contain" .

<http://w3id.org/polifonia/pilot/meetups/2811507/10>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/2811507/10/0> .

<http://w3id.org/polifonia/pilot/meetups/2811507/8/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "While in Hollywood, Cary Grant turned up with an entourage at a place where Haynes was staying and lavished great praise on him, calling him the greatest comedy star in the world." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2811507/8> ;
        ont:person        <http://dbpedia.org/resource/Cary_Grant> ;
        ont:place         <http://dbpedia.org/resource/Hollywood> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2811507/8/0> ;
        ont:subject       <http://dbpedia.org/resource/Arthur_Haynes> .

<http://w3id.org/polifonia/pilot/meetups/2811507/12/2>
        rdf:type            ont:Sentence ;
        ont:excerpt         "His wife Queenie died on 7 November 2010 aged 95." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/2811507/12> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/2811507/12/1> ;
        ont:subject         <http://dbpedia.org/resource/Arthur_Haynes> ;
        ont:timeExpression  "7 November 2010" .

<http://w3id.org/polifonia/pilot/meetups/2811507/7/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "The shows also made a star of Nicholas Parsons, who had first appeared with Haynes in Strike a New Note and Get Happy and who tended to play supercilious neighbours and authority figures in the comedy sketches." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2811507/7> ;
        ont:person        <http://dbpedia.org/resource/Nicholas_Parsons> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2811507/7/0> ;
        ont:subject       <http://dbpedia.org/resource/Arthur_Haynes> .

<http://w3id.org/polifonia/pilot/meetups/2811507/2/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He then became an entertainer while serving with the Royal Engineers during the war." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2811507/2> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2811507/2/1> ;
        ont:subject       <http://dbpedia.org/resource/Arthur_Haynes> .

<http://w3id.org/polifonia/pilot/meetups/2811507/5/7>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Later episodes had improved sets." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2811507/5> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2811507/5/6> ;
        ont:subject       <http://dbpedia.org/resource/Arthur_Haynes> .

<http://w3id.org/polifonia/pilot/meetups/2811507/6/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "A number of the shows started and ended with Arthur Haynes driving a horse and cart along a narrow country lane, whistling and (unconvincingly) playing a harmonica." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2811507/6> ;
        ont:person        <http://dbpedia.org/resource/King_Arthur> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2811507/6/0> ;
        ont:subject       <http://dbpedia.org/resource/Arthur_Haynes> .

<http://w3id.org/polifonia/pilot/meetups/2811507/4/7>
        rdf:type            ont:Sentence ;
        ont:excerpt         "On 21 February 1956, Haynes appeared in the first edition of ATV variety series Strike a New Note, which was supposed to air talent for the new independent television station." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/2811507/4> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/2811507/4/6> ;
        ont:subject         <http://dbpedia.org/resource/Arthur_Haynes> ;
        ont:timeExpression  "21 February 1956" .

<http://w3id.org/polifonia/pilot/meetups/2811507/5/1>
        rdf:type            ont:Sentence ;
        ont:excerpt         "There were 95 thirty-minute shows, 62 thirty-five-minute shows and one fifty-minute show, spread over fifteen series." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/2811507/5> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/2811507/5/0> ;
        ont:subject         <http://dbpedia.org/resource/Arthur_Haynes> ;
        ont:timeExpression  "95 thirty-minute" , "one fifty-minute show" , "62 thirty-five-minute" .

