        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/2811507/17/1> .

<http://w3id.org/polifonia/pilot/meetups/2811507/4/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "They became a double act in the show where Chester wrote the scripts." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2811507/4> ;
        ont:place         <http://dbpedia.org/resource/Chester> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2811507/4/2> ;
        ont:subject       <http://dbpedia.org/resource/Arthur_Haynes> .

<http://w3id.org/polifonia/pilot/meetups/2811507/14/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Volume 7 completes the sequence." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2811507/14> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2811507/14/2> ;
        ont:subject       <http://dbpedia.org/resource/Arthur_Haynes> .

