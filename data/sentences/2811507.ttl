<http://w3id.org/polifonia/pilot/meetups/2811507/14/1>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Volume 1 contains thirteen of the earliest surviving episodes from 1960, while Volume 2 contains twelve episodes dating from 1961." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/2811507/14> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/2811507/14/0> ;
        ont:subject         <http://dbpedia.org/resource/Arthur_Haynes> ;
        ont:timeExpression  "while Volume 2" .

<http://w3id.org/polifonia/pilot/meetups/2811507/4/1>
        rdf:type            ont:Sentence ;
        ont:excerpt         "He continued to work with Chester after the war in the BBC Radio series Stand Easy (1946–49)." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/2811507/4> ;
        ont:place           <http://dbpedia.org/resource/Chester> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/2811507/4/0> ;
        ont:subject         <http://dbpedia.org/resource/Arthur_Haynes> ;
        ont:timeExpression  "1946–49" .

<http://w3id.org/polifonia/pilot/meetups/2811507/4/12>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Haynes who had been a stand-up comedian was given his own series later in the year and Parsons who had been an all-rounder found himself cast as his straight man." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2811507/4> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2811507/4/11> ;
        ont:subject       <http://dbpedia.org/resource/Arthur_Haynes> .

<http://w3id.org/polifonia/pilot/meetups/2811507/8/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1965, Haynes appeared in the Rock Hudson/Gina Lollobrigida film Strange Bedfellows." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2811507/8> ;
        ont:person        <http://dbpedia.org/resource/Gina_Lollobrigida> ;
        ont:place         <http://dbpedia.org/resource/Hudson_River> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2811507/8/0> ;
        ont:subject       <http://dbpedia.org/resource/Arthur_Haynes> .

