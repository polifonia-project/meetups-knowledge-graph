        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/2811507/4> ;
        ont:person          <http://dbpedia.org/resource/Comedian> , <http://dbpedia.org/resource/King_Arthur> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/2811507/4/3> ;
        ont:subject         <http://dbpedia.org/resource/Arthur_Haynes> ;
        ont:timeExpression  "four series" , "1962–65" .

