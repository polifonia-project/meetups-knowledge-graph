        rdf:type          ont:Sentence ;
        ont:excerpt       "Bell spent the last 15 years of her life in Culver City, where she was involved with local light opera and community activities." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/52266861/13> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/52266861/13/8> ;
