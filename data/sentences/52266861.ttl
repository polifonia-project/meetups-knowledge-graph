<http://w3id.org/polifonia/pilot/meetups/52266861/11/7>
        rdf:type          ont:Sentence ;
        ont:excerpt       "She sang in more than 200 concerts during her career." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/52266861/11> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/52266861/11/6> ;
        ont:subject       <http://dbpedia.org/resource/Marion_Bell> .

<http://w3id.org/polifonia/pilot/meetups/52266861/6/3>
        rdf:type            ont:Sentence ;
        ont:excerpt         "After signing with MGM at age 15, Bell toured with the Marx Brothers in their vaudeville show, and had a small uncredited role in their 1935  movie A Night at the Opera." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/52266861/6> ;
        ont:person          <http://dbpedia.org/resource/Marx_Brothers> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/52266861/6/2> ;
        ont:subject         <http://dbpedia.org/resource/Marion_Bell> ;
        ont:timeExpression  "at age 15" , "1935 movie" .

<http://w3id.org/polifonia/pilot/meetups/52266861/9/8>
        rdf:type          ont:Sentence ;
        ont:excerpt       "While Brigadoon was enjoying its successful run, she gave a recital featuring the European art music she preferred." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/52266861/9> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/52266861/9/7> ;
