        rdf:_4    <http://w3id.org/polifonia/pilot/meetups/52266861/4/3> .

<http://w3id.org/polifonia/pilot/meetups/52266861/11/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Bell sang in the world premiere of the folk opera Down in the Valley by Weill and Arnold Sundgaard for the Indiana University Opera Theatre in 1948." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/52266861/11> ;
        ont:person        <http://dbpedia.org/resource/Arnold_Sundgaard> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/52266861/11/0> ;
        ont:subject       <http://dbpedia.org/resource/Marion_Bell> .

<http://w3id.org/polifonia/pilot/meetups/52266861/9/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "It was suggested that she audition." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/52266861/9> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/52266861/9/0> ;
        ont:subject       <http://dbpedia.org/resource/Marion_Bell> .

<http://w3id.org/polifonia/pilot/meetups/52266861/4/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Back in California, she studied with Nina Koshetz and sang leading roles with the San Francisco Opera Company, including the Shepherd in Wagner’s Tannhäuser and the young girl in Love of Three Kings by Montemezzi." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/52266861/4> ;
        ont:person        <http://dbpedia.org/resource/Nina_Koshetz> ;
        ont:place         <http://dbpedia.org/resource/California> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/52266861/4/1> ;
        ont:subject       <http://dbpedia.org/resource/Marion_Bell> .

<http://w3id.org/polifonia/pilot/meetups/52266861/2/8>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Her third husband was Tom Charlesworth, an actor and singer she met during a production of The Chocolate Soldier." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/52266861/2> ;
        ont:person        <http://dbpedia.org/resource/James_H._Charlesworth> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/52266861/2/7> ;
        ont:subject       <http://dbpedia.org/resource/Marion_Bell> .

<http://w3id.org/polifonia/pilot/meetups/52266861/10/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "==" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/52266861/10> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/52266861/10/0> ;
        ont:subject       <http://dbpedia.org/resource/Marion_Bell> .

<http://w3id.org/polifonia/pilot/meetups/52266861/8/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Brigadoon and New York recital ==" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/52266861/8> ;
        ont:place         <http://dbpedia.org/resource/New_York_City> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/52266861/8/0> ;
        ont:subject       <http://dbpedia.org/resource/Marion_Bell> .

<http://w3id.org/polifonia/pilot/meetups/52266861/9/17>
        rdf:type          ont:Sentence ;
        ont:excerpt       "There she contracted infectious hepatitis, but continued working contrary to her doctor's orders." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/52266861/9> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/52266861/9/16> ;
        ont:subject       <http://dbpedia.org/resource/Marion_Bell> .

<http://w3id.org/polifonia/pilot/meetups/52266861/13/5>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In the late 1990s, Bell provided archival film of her Town Hall recital for \"Classic Arts Showcase\" television, of which she was said to be a regular viewer." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/52266861/13> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/52266861/13/4> ;
        ont:subject       <http://dbpedia.org/resource/Marion_Bell> .

<http://w3id.org/polifonia/pilot/meetups/52266861/3/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "==" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/52266861/3> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/52266861/3/1> ;
        ont:subject       <http://dbpedia.org/resource/Marion_Bell> .

<http://w3id.org/polifonia/pilot/meetups/52266861/2/10>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Bell was known for her \"striking\" appearance, and was described as \"raven\" haired, \"big-bosomed and sexy,\" and \"handsome to the eye.\"" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/52266861/2> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/52266861/2/9> ;
        ont:subject       <http://dbpedia.org/resource/Marion_Bell> .

<http://w3id.org/polifonia/pilot/meetups/52266861/7/1>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Her MGM contract delayed her Broadway debut by preventing her from accepting a role in the Lerner-Loewe musical The Day Before Spring in 1945." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/52266861/7> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/52266861/7/0> ;
        ont:subject         <http://dbpedia.org/resource/Marion_Bell> ;
        ont:timeExpression  "Spring in 1945" .

<http://w3id.org/polifonia/pilot/meetups/52266861/2/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "She had three sisters, Evelyn, Ruth, and Veronica." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/52266861/2> ;
