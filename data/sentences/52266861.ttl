<http://w3id.org/polifonia/pilot/meetups/52266861/11/1>
        rdf:type            ont:Sentence ;
        ont:excerpt         "She appeared in the telecast production that aired January 1950 as the first show of the NBC Opera Theatre series." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/52266861/11> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/52266861/11/0> ;
        ont:subject         <http://dbpedia.org/resource/Marion_Bell> ;
        ont:timeExpression  "January 1950" .

<http://w3id.org/polifonia/pilot/meetups/52266861/9/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "She traveled to New York City to do so, and there met Lerner for the first time." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/52266861/9> ;
        ont:person        <http://dbpedia.org/resource/Alan_Jay_Lerner> ;
        ont:place         <http://dbpedia.org/resource/New_York_City> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/52266861/9/1> ;
        ont:subject       <http://dbpedia.org/resource/Marion_Bell> .

<http://w3id.org/polifonia/pilot/meetups/52266861/4/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "She also sang at the now-defunct St. Louis Opera Company and the Opera Nacional in Mexico City." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/52266861/4> ;
        ont:place         <http://dbpedia.org/resource/Mexico_City> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/52266861/4/2> ;
        ont:subject       <http://dbpedia.org/resource/Marion_Bell> .

