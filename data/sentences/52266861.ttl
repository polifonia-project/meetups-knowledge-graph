        rdf:type            ont:Sentence ;
        ont:excerpt         "Bell began her career on local radio by the age of eight, singing with Ted Straeter's orchestra." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/52266861/4> ;
        ont:person          <http://dbpedia.org/resource/Ted_Straeter> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/52266861/4/0> ;
        ont:subject         <http://dbpedia.org/resource/Marion_Bell> ;
        ont:timeExpression  "age of eight" .

<http://w3id.org/polifonia/pilot/meetups/52266861/2/6>
        rdf:type          ont:Sentence ;
        ont:excerpt       "The marriage lasted two years." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/52266861/2> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/52266861/2/5> ;
        ont:subject       <http://dbpedia.org/resource/Marion_Bell> .

<http://w3id.org/polifonia/pilot/meetups/52266861/17>
        rdf:type  ont:Paragraph ;
