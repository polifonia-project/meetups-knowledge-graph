<http://w3id.org/polifonia/pilot/meetups/52266861/2/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1947, she married the librettist Alan Jay Lerner." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/52266861/2> ;
        ont:person        <http://dbpedia.org/resource/Alan_Jay_Lerner> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/52266861/2/3> ;
