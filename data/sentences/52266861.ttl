<http://w3id.org/polifonia/pilot/meetups/52266861/9/13>
        rdf:type          ont:Sentence ;
        ont:excerpt       "The reviewer found her \"most at home\" with the song \"Somehow I Could Never Believe\" from Street Scene by Kurt Weill." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/52266861/9> ;
        ont:person        <http://dbpedia.org/resource/Kurt_Weill> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/52266861/9/12> ;
        ont:subject       <http://dbpedia.org/resource/Marion_Bell> .

<http://w3id.org/polifonia/pilot/meetups/52266861/13/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "She retreated from the relationship, and her fame declined." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/52266861/13> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/52266861/13/0> ;
