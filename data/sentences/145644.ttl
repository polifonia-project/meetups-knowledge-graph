        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/145644/6/6> ;
        ont:subject         <http://dbpedia.org/resource/Walter_Pidgeon> ;
        ont:timeExpression  "in the 1920s" , "1925" .

