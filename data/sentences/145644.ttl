        ont:subject         <http://dbpedia.org/resource/Walter_Pidgeon> ;
        ont:timeExpression  "1942" , "1950" , "1941" .

<http://w3id.org/polifonia/pilot/meetups/145644/3/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Pidgeon received his formal education in local schools and the University of New Brunswick, where he studied law and drama." ;
