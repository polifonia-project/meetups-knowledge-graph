        ont:timeExpression  "1930" , "1931" .

<http://w3id.org/polifonia/pilot/meetups/145644/8/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "It was not until he starred in the Academy Award-winning Best Picture How Green Was My Valley (1941) that his popularity reached its height." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/145644/8> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/145644/8/0> ;
        ont:subject         <http://dbpedia.org/resource/Walter_Pidgeon> ;
        ont:timeExpression  "1941" .

<http://w3id.org/polifonia/pilot/meetups/145644/3/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "His brother, Larry, was an editorial writer for the Santa Barbara News-Press." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/145644/3> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/145644/3/0> ;
        ont:subject       <http://dbpedia.org/resource/Walter_Pidgeon> .

<http://w3id.org/polifonia/pilot/meetups/145644/10/8>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1965, he played the king in Rodgers and Hammerstein's CBS television production of Cinderella, starring Lesley Ann Warren." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/145644/10> ;
        ont:person        <http://dbpedia.org/resource/Lesley_Ann_Warren> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/145644/10/7> ;
        ont:subject       <http://dbpedia.org/resource/Walter_Pidgeon> .

<http://w3id.org/polifonia/pilot/meetups/145644/6/6>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Pidgeon's success in Elsie Janis' shows created a rift between them, leading to Pidgeon's eventual dismissal—and his decision to head to Hollywood." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/145644/6> ;
        ont:person        <http://dbpedia.org/resource/Elsie_Janis> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/145644/6/5> ;
        ont:subject       <http://dbpedia.org/resource/Walter_Pidgeon> .

