        rdf:_3    <http://w3id.org/polifonia/pilot/meetups/140632/4/2> ;
        rdf:_4    <http://w3id.org/polifonia/pilot/meetups/140632/4/3> ;
        rdf:_5    <http://w3id.org/polifonia/pilot/meetups/140632/4/4> ;
        rdf:_6    <http://w3id.org/polifonia/pilot/meetups/140632/4/5> ;
        rdf:_7    <http://w3id.org/polifonia/pilot/meetups/140632/4/6> ;
        rdf:_8    <http://w3id.org/polifonia/pilot/meetups/140632/4/7> ;
        rdf:_9    <http://w3id.org/polifonia/pilot/meetups/140632/4/8> .

<http://w3id.org/polifonia/pilot/meetups/140632/1/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Biography ==" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/140632/1> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/140632/1/0> ;
        ont:subject       <http://dbpedia.org/resource/Carl_W._Stalling> .

<http://w3id.org/polifonia/pilot/meetups/140632/29/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Works by or about Carl W. Stalling at Internet Archive" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/140632/29> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/140632/29/0> ;
        ont:subject       <http://dbpedia.org/resource/Carl_W._Stalling> .

<http://w3id.org/polifonia/pilot/meetups/140632/19>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/140632/19/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/140632/19/1> .

<http://w3id.org/polifonia/pilot/meetups/140632/2/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "His parents were from Germany; his father arrived in the United States in 1883." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/140632/2> ;
        ont:place         <http://dbpedia.org/resource/Germany> , <http://dbpedia.org/resource/United_States> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/140632/2/0> ;
        ont:subject       <http://dbpedia.org/resource/Carl_W._Stalling> .

<http://w3id.org/polifonia/pilot/meetups/140632/9/18>
        rdf:type            ont:Sentence ;
        ont:excerpt         "The theme of the Looney Tunes series was \"The Merry-Go-Round Broke Down\" (1937), a minor hit from the team of Dave Franklin and Cliff Friend." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/140632/9> ;
        ont:person          <http://dbpedia.org/resource/Dave_Franklin> , <http://dbpedia.org/resource/Cliff_Friend> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/140632/9/17> ;
        ont:subject         <http://dbpedia.org/resource/Carl_W._Stalling> ;
        ont:timeExpression  "1937" .

<http://w3id.org/polifonia/pilot/meetups/140632/15/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "If there was a lady dressed in red, he'd always play \"The Lady in Red\"." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/140632/15> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/140632/15/1> ;
        ont:subject       <http://dbpedia.org/resource/Carl_W._Stalling> .

<http://w3id.org/polifonia/pilot/meetups/140632/5/38>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Franklyn died suddenly in 1962 and was replaced by William Lava for the remainder of Looney Tunes and Merrie Melodies' theatrical runs." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/140632/5> ;
        ont:person        <http://dbpedia.org/resource/William_Lava> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/140632/5/37> ;
        ont:subject       <http://dbpedia.org/resource/Carl_W._Stalling> .

<http://w3id.org/polifonia/pilot/meetups/140632/5/17>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Stalling already had a reputation as a very talented musician and composer." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/140632/5> ;
        ont:person        <http://dbpedia.org/resource/Composer> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/140632/5/16> ;
        ont:subject       <http://dbpedia.org/resource/Carl_W._Stalling> .

<http://w3id.org/polifonia/pilot/meetups/140632/3/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "His actual job at the time was to play \"organ accompaniment\" for silent films." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/140632/3> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/140632/3/0> ;
        ont:subject       <http://dbpedia.org/resource/Carl_W._Stalling> .

<http://w3id.org/polifonia/pilot/meetups/140632/16/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He exaggerated that one had to be 108-years-old to even remember the existence of the song." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/140632/16> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/140632/16/1> ;
        ont:subject       <http://dbpedia.org/resource/Carl_W._Stalling> .

<http://w3id.org/polifonia/pilot/meetups/140632/27>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/140632/27/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/140632/27/1> .

<http://w3id.org/polifonia/pilot/meetups/140632/3/10>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1928, Disney was on a journey from California to New York City to record the sound and make the preview of Steamboat Willie, Disney's first released sound short." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/140632/3> ;
        ont:place         <http://dbpedia.org/resource/California> , <http://dbpedia.org/resource/New_York_City> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/140632/3/9> ;
        ont:subject       <http://dbpedia.org/resource/Carl_W._Stalling> .

<http://w3id.org/polifonia/pilot/meetups/140632/4/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Animation historian Allan Neuwirth credits Stalling for basically inventing the process of creating a film score for cartoons." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/140632/4> ;
        ont:person        <http://dbpedia.org/resource/Allan_Neuwirth> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/140632/4/0> ;
        ont:subject       <http://dbpedia.org/resource/Carl_W._Stalling> .

<http://w3id.org/polifonia/pilot/meetups/140632/10/5>
        rdf:type          ont:Sentence ;
        ont:excerpt       "But Scott did not actually compose his works with the intention of using them as film scores." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/140632/10> ;
        ont:person        <http://dbpedia.org/resource/Walter_Scott> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/140632/10/4> ;
        ont:subject       <http://dbpedia.org/resource/Carl_W._Stalling> .

<http://w3id.org/polifonia/pilot/meetups/140632/9/6>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He chose popular songs whose titles fit the on-screen gags." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/140632/9> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/140632/9/5> ;
        ont:subject       <http://dbpedia.org/resource/Carl_W._Stalling> .

<http://w3id.org/polifonia/pilot/meetups/140632/5/25>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Prior to 1936, most of the animated films of these two series included film scores by either Frank Marsales, Bernard B. Brown, or Norman Spencer." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/140632/5> ;
        ont:person          <http://dbpedia.org/resource/Frank_Marsales> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/140632/5/24> ;
        ont:subject         <http://dbpedia.org/resource/Carl_W._Stalling> ;
        ont:timeExpression  "two series" .

<http://w3id.org/polifonia/pilot/meetups/140632/5/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Stalling helped Disney streamline and update the sound process used in creating early animated sound films, following the long and laborious synchronization process used in Steamboat Willie." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/140632/5> ;
        ont:person        <http://dbpedia.org/resource/Walt_Disney> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/140632/5/0> ;
        ont:subject       <http://dbpedia.org/resource/Carl_W._Stalling> .

<http://w3id.org/polifonia/pilot/meetups/140632/3/9>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Stalling and Disney kept in touch through correspondence, and considered each other friends." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/140632/3> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/140632/3/8> ;
        ont:subject       <http://dbpedia.org/resource/Carl_W._Stalling> .

<http://w3id.org/polifonia/pilot/meetups/140632/35>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/140632/35/0> .

<http://w3id.org/polifonia/pilot/meetups/140632/14>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/140632/14/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/140632/14/1> .

<http://w3id.org/polifonia/pilot/meetups/140632/3/18>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He probably realized that his career as an organist for a silent movie theatre was coming to an end, because the silent film era was also at its end." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/140632/3> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/140632/3/17> ;
        ont:subject       <http://dbpedia.org/resource/Carl_W._Stalling> .

<http://w3id.org/polifonia/pilot/meetups/140632/6/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Composing style ==" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/140632/6> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/140632/6/0> ;
        ont:subject       <http://dbpedia.org/resource/Carl_W._Stalling> .

<http://w3id.org/polifonia/pilot/meetups/140632/12/5>
        rdf:type          ont:Sentence ;
        ont:excerpt       "This was what made Stalling's work so effective." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/140632/12> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/140632/12/4> ;
        ont:subject       <http://dbpedia.org/resource/Carl_W._Stalling> .

<http://w3id.org/polifonia/pilot/meetups/140632/9/13>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Most of his film scores involved 500 measures in ten sections." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/140632/9> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/140632/9/12> ;
        ont:subject       <http://dbpedia.org/resource/Carl_W._Stalling> .

<http://w3id.org/polifonia/pilot/meetups/140632/5/33>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Stalling remained with Warner Bros. until he retired in 1958." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/140632/5> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/140632/5/32> ;
        ont:subject       <http://dbpedia.org/resource/Carl_W._Stalling> .

<http://w3id.org/polifonia/pilot/meetups/140632/10/13>
        rdf:type          ont:Sentence ;
        ont:excerpt       "His melodies are heard through most of the classic Warner Brothers cartoons, and imitated in new Looney Tunes compilations and features such as Looney Tunes: Back in Action." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/140632/10> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/140632/10/12> ;
        ont:subject       <http://dbpedia.org/resource/Carl_W._Stalling> .

<http://w3id.org/polifonia/pilot/meetups/140632/7/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Stalling is among the first music directors to extensively use the metronome to time film scores." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/140632/7> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/140632/7/0> ;
        ont:subject       <http://dbpedia.org/resource/Carl_W._Stalling> .

<http://w3id.org/polifonia/pilot/meetups/140632/5/12>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Finding few outlets in New York, Stalling rejoined Iwerks at his studio in California, while freelancing for Disney and others." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/140632/5> ;
        ont:place         <http://dbpedia.org/resource/New_York_City> , <http://dbpedia.org/resource/California> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/140632/5/11> ;
        ont:subject       <http://dbpedia.org/resource/Carl_W._Stalling> .

<http://w3id.org/polifonia/pilot/meetups/140632/5/9>
        rdf:type          ont:Sentence ;
        ont:excerpt       "According to Strauss, this version of the click track is credited to sound effects artist Jimmy MacDonald." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/140632/5> ;
        ont:person        <http://dbpedia.org/resource/Richard_Strauss> , <http://dbpedia.org/resource/Jimmy_MacDonald_(sound_effects_artist)> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/140632/5/8> ;
        ont:subject       <http://dbpedia.org/resource/Carl_W._Stalling> .

<http://w3id.org/polifonia/pilot/meetups/140632/22>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/140632/22/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/140632/22/1> .

<http://w3id.org/polifonia/pilot/meetups/140632/7>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/140632/7/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/140632/7/1> ;
        rdf:_3    <http://w3id.org/polifonia/pilot/meetups/140632/7/2> .

<http://w3id.org/polifonia/pilot/meetups/140632/8/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Working with directors Tex Avery, Bob Clampett, Friz Freleng, Robert McKimson and Chuck Jones, he developed the Looney Tunes style of very rapid and tightly coordinated musical cues, punctuated with both instrumental and recorded sound effects, and occasionally reaching into full blown musical fantasies such as The Rabbit of Seville and A Corny Concerto." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/140632/8> ;
        ont:person        <http://dbpedia.org/resource/Tex_Avery> , <http://dbpedia.org/resource/Robert_McKimson> , <http://dbpedia.org/resource/Chuck_Jones> , <http://dbpedia.org/resource/Bob_Clampett> , <http://dbpedia.org/resource/Friz_Freleng> ;
        ont:place         <http://dbpedia.org/resource/Seville> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/140632/8/0> ;
        ont:subject       <http://dbpedia.org/resource/Carl_W._Stalling> .

<http://w3id.org/polifonia/pilot/meetups/140632/10/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Stalling made extensive use of the many works of Raymond Scott, whose music was licensed by Warner Bros. in the early 1940s." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/140632/10> ;
        ont:person        <http://dbpedia.org/resource/Raymond_Scott> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/140632/10/0> ;
        ont:subject       <http://dbpedia.org/resource/Carl_W._Stalling> .

<http://w3id.org/polifonia/pilot/meetups/140632/2/4>
        rdf:type            ont:Sentence ;
        ont:excerpt         "By the age of 12, he was the principal piano accompanist in his hometown's silent movie house." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/140632/2> ;
        ont:person          <http://dbpedia.org/resource/Accompaniment> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/140632/2/3> ;
        ont:subject         <http://dbpedia.org/resource/Carl_W._Stalling> ;
        ont:timeExpression  "age of 12" .

<http://w3id.org/polifonia/pilot/meetups/140632/9/21>
        rdf:type          ont:Sentence ;
        ont:excerpt       "An electric guitar provided the song's initial sound effect." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/140632/9> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/140632/9/20> ;
        ont:subject       <http://dbpedia.org/resource/Carl_W._Stalling> .

<http://w3id.org/polifonia/pilot/meetups/140632/9/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Together they set the time signatures to which the short was to be drawn." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/140632/9> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/140632/9/0> ;
        ont:subject       <http://dbpedia.org/resource/Carl_W._Stalling> .

<http://w3id.org/polifonia/pilot/meetups/140632/15/5>
        rdf:type            ont:Sentence ;
        ont:excerpt         "I had a bee one time, and my God, if he didn't go and find a piece of music written in 1906 or something called \"I'm a Busy Little Bumble Bee\"." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/140632/15> ;
        ont:person          <http://dbpedia.org/resource/God> , <http://dbpedia.org/resource/Bumblebee> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/140632/15/4> ;
        ont:subject         <http://dbpedia.org/resource/Carl_W._Stalling> ;
        ont:timeExpression  "one time" .

<http://w3id.org/polifonia/pilot/meetups/140632/5/20>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Stalling had been recommended to Schlesinger by storyman Ben Hardaway." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/140632/5> ;
        ont:person        <http://dbpedia.org/resource/Ben_Hardaway> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/140632/5/19> ;
        ont:subject       <http://dbpedia.org/resource/Carl_W._Stalling> .

<http://w3id.org/polifonia/pilot/meetups/140632/11/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Film critic Leonard Maltin pointed out that listening to the soundtracks of the Warner cartoons was an important part of his musical education; the use of the full Warner Bros. Orchestra resulted in a richness of sound that is often lacking in more modern cartoons." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/140632/11> ;
        ont:person        <http://dbpedia.org/resource/Leonard_Maltin> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/140632/11/0> ;
        ont:subject       <http://dbpedia.org/resource/Carl_W._Stalling> .

<http://w3id.org/polifonia/pilot/meetups/140632/3/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Stalling was at his job at the Isis Movie Theatre, demonstrating his ability to combine well-known music by other creators with his own, improvised compositions." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/140632/3> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/140632/3/3> ;
        ont:subject       <http://dbpedia.org/resource/Carl_W._Stalling> .

<http://w3id.org/polifonia/pilot/meetups/140632/30>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/140632/30/0> .

<http://w3id.org/polifonia/pilot/meetups/140632/16/5>
        rdf:type          ont:Sentence ;
        ont:excerpt       "It was a hit song from the musical A Winsome Widow, produced by Florenz Ziegfeld Jr.." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/140632/16> ;
        ont:person        <http://dbpedia.org/resource/Florenz_Ziegfeld_Jr.> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/140632/16/4> ;
        ont:subject       <http://dbpedia.org/resource/Carl_W._Stalling> .

<http://w3id.org/polifonia/pilot/meetups/140632/20/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Warner Bros., 1990" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/140632/20> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/140632/20/0> ;
        ont:subject       <http://dbpedia.org/resource/Carl_W._Stalling> .

<http://w3id.org/polifonia/pilot/meetups/140632/3/13>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Plane Crazy and The Gallopin' Gaucho were originally silent films and were the first two Mickey Mouse animated short films in production." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/140632/3> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/140632/3/12> ;
        ont:subject       <http://dbpedia.org/resource/Carl_W._Stalling> .

<http://w3id.org/polifonia/pilot/meetups/140632/12/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Allan Neuwirth considers Stalling's work style in the Warner Bros. films to be highly recognizable." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/140632/12> ;
        ont:person        <http://dbpedia.org/resource/Allan_Neuwirth> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/140632/12/0> ;
        ont:subject       <http://dbpedia.org/resource/Carl_W._Stalling> .

<http://w3id.org/polifonia/pilot/meetups/140632/4/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Stalling encouraged Disney to create a new series of animated short films, in which the animation and its action would be created to match the music." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/140632/4> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/140632/4/3> ;
        ont:subject       <http://dbpedia.org/resource/Carl_W._Stalling> .

<http://w3id.org/polifonia/pilot/meetups/140632/10/8>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Scenes involving automobiles were often accompanied by \"In My Merry Oldsmobile\"." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/140632/10> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/140632/10/7> ;
        ont:subject       <http://dbpedia.org/resource/Carl_W._Stalling> .

<http://w3id.org/polifonia/pilot/meetups/140632/21/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Warner Bros., 1995" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/140632/21> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/140632/21/0> ;
        ont:subject       <http://dbpedia.org/resource/Carl_W._Stalling> .

<http://w3id.org/polifonia/pilot/meetups/140632/9/9>
        rdf:type          ont:Sentence ;
        ont:excerpt       "His musical cues, the unedited periods between the commencement and end of a single musical take, had varying lengths." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/140632/9> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/140632/9/8> ;
        ont:subject       <http://dbpedia.org/resource/Carl_W._Stalling> .

<http://w3id.org/polifonia/pilot/meetups/140632/5/28>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Like his predecessors as music director for the studio, Stalling had full access to the expansive Warner Bros. catalog and musicians." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/140632/5> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/140632/5/27> ;
        ont:subject       <http://dbpedia.org/resource/Carl_W._Stalling> .

<http://w3id.org/polifonia/pilot/meetups/140632/13/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "==" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/140632/13> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/140632/13/0> ;
        ont:subject       <http://dbpedia.org/resource/Carl_W._Stalling> .

<http://w3id.org/polifonia/pilot/meetups/140632/5/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Initially, Wilfred Jackson utilised a Metronome to set a definitive tempo of the cartoon sections, that then got further developed by the years." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/140632/5> ;
        ont:person        <http://dbpedia.org/resource/Wilfred_Jackson> , <http://dbpedia.org/resource/Tempo> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/140632/5/3> ;
        ont:subject       <http://dbpedia.org/resource/Carl_W._Stalling> .

