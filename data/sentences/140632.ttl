<http://w3id.org/polifonia/pilot/meetups/140632/10/1>
        rdf:type            ont:Sentence ;
        ont:excerpt         "According to Strauss, Stalling relied heavily on the music Scott composed during the 1930s." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/140632/10> ;
        ont:person          <http://dbpedia.org/resource/Walter_Scott> , <http://dbpedia.org/resource/Richard_Strauss> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/140632/10/0> ;
        ont:subject         <http://dbpedia.org/resource/Carl_W._Stalling> ;
        ont:timeExpression  "during the 1930s" .

<http://w3id.org/polifonia/pilot/meetups/140632/2/5>
        rdf:type          ont:Sentence ;
        ont:excerpt       "For a short period, he was also the theatre organist at the St. Louis Theatre, which eventually became Powell Symphony Hall." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/140632/2> ;
        ont:place         <http://dbpedia.org/resource/Powell_Hall> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/140632/2/4> ;
        ont:subject       <http://dbpedia.org/resource/Carl_W._Stalling> .

<http://w3id.org/polifonia/pilot/meetups/140632/9/22>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Stalling was a master at quickly changing musical styles based on the action in the cartoon." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/140632/9> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/140632/9/21> ;
        ont:subject       <http://dbpedia.org/resource/Carl_W._Stalling> .

<http://w3id.org/polifonia/pilot/meetups/140632/9/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "The animators of the film were measuring animation frames per beat." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/140632/9> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/140632/9/1> ;
        ont:subject       <http://dbpedia.org/resource/Carl_W._Stalling> .

