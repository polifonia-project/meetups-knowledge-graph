<http://w3id.org/polifonia/pilot/meetups/140632/23/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Bugs Bunny at the Symphony." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/140632/23> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/140632/23/0> ;
        ont:subject       <http://dbpedia.org/resource/Carl_W._Stalling> .

<http://w3id.org/polifonia/pilot/meetups/140632/9/15>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Neil Strauss notes that this orchestra was often employed for relatively undemanding film scores for live-action feature films." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/140632/9> ;
        ont:person        <http://dbpedia.org/resource/Neil_Strauss> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/140632/9/14> ;
        ont:subject       <http://dbpedia.org/resource/Carl_W._Stalling> .

<http://w3id.org/polifonia/pilot/meetups/140632/5/35>
        rdf:type          ont:Sentence ;
        ont:excerpt       "After Stalling retired in 1958, he was succeeded by Milt Franklyn, who had assisted Stalling as an arranger since the mid 1930s and was promoted to musical director in the early 1950s." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/140632/5> ;
        ont:person        <http://dbpedia.org/resource/Milt_Franklyn> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/140632/5/34> ;
        ont:subject       <http://dbpedia.org/resource/Carl_W._Stalling> .

<http://w3id.org/polifonia/pilot/meetups/140632/5/14>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1936, when Leon Schlesinger—under contract to produce animated shorts for Warner Bros.—hired Iwerks, Stalling went with him to become a full-time cartoon music composer." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/140632/5> ;
        ont:person        <http://dbpedia.org/resource/Composer> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/140632/5/13> ;
        ont:subject       <http://dbpedia.org/resource/Carl_W._Stalling> .

<http://w3id.org/polifonia/pilot/meetups/140632/24/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "==" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/140632/24> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/140632/24/0> ;
        ont:subject       <http://dbpedia.org/resource/Carl_W._Stalling> .

<http://w3id.org/polifonia/pilot/meetups/140632/16/10>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Stalling did use the melody composed by Felix Mendelssohn in several animated shorts, but never in combination with an actual cave scene." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/140632/16> ;
        ont:person        <http://dbpedia.org/resource/Felix_Mendelssohn> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/140632/16/9> ;
        ont:subject       <http://dbpedia.org/resource/Carl_W._Stalling> .

