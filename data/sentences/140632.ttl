<http://w3id.org/polifonia/pilot/meetups/140632/14/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Chuck Jones and the other Looney Tunes directors sometimes complained about Stalling's proclivity for musical quotation and punning." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/140632/14> ;
        ont:person        <http://dbpedia.org/resource/Chuck_Jones> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/140632/14/0> ;
        ont:subject       <http://dbpedia.org/resource/Carl_W._Stalling> .

<http://w3id.org/polifonia/pilot/meetups/140632/23/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "(Live Concert Recording from the Sydney Opera House with the Sydney Symphony conducted by George Daugherty.)" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/140632/23> ;
        ont:person        <http://dbpedia.org/resource/George_Gershwin> , <http://dbpedia.org/resource/Michael_Daugherty> ;
        ont:place         <http://dbpedia.org/resource/Sydney_Opera_House> , <http://dbpedia.org/resource/Sydney> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/140632/23/0> ;
        ont:subject       <http://dbpedia.org/resource/Carl_W._Stalling> .

<http://w3id.org/polifonia/pilot/meetups/140632/9/16>
        rdf:type          ont:Sentence ;
        ont:excerpt       "When working for Stalling, the orchestra would find itself burdened with more challenging and taxing work." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/140632/9> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/140632/9/15> ;
        ont:subject       <http://dbpedia.org/resource/Carl_W._Stalling> .

<http://w3id.org/polifonia/pilot/meetups/140632/5/36>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Stalling and Franklyn had shared credits for musical direction during the last years of Stalling's tenure." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/140632/5> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/140632/5/35> ;
        ont:subject       <http://dbpedia.org/resource/Carl_W._Stalling> .

<http://w3id.org/polifonia/pilot/meetups/140632/15/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He was a brilliant musician." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/140632/15> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/140632/15/0> ;
        ont:subject       <http://dbpedia.org/resource/Carl_W._Stalling> .

<http://w3id.org/polifonia/pilot/meetups/140632/5/15>
        rdf:type            ont:Sentence ;
        ont:excerpt         "According to Sigall, Stalling was hired by the Leon Schlesinger studio in July, 1936." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/140632/5> ;
        ont:person          <http://dbpedia.org/resource/Leon_Schlesinger> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/140632/5/14> ;
        ont:subject         <http://dbpedia.org/resource/Carl_W._Stalling> ;
        ont:timeExpression  "1936" .

