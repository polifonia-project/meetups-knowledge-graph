<http://w3id.org/polifonia/pilot/meetups/140632/25/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Original music scores and other documents relating to Carl W. Stalling (1900-1978) can be found at the University of Wyoming - American Heritage Center." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/140632/25> ;
        ont:place         <http://dbpedia.org/resource/Wyoming> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/140632/25/0> ;
        ont:subject       <http://dbpedia.org/resource/Carl_W._Stalling> .

<http://w3id.org/polifonia/pilot/meetups/140632/10/2>
        rdf:type            ont:Sentence ;
        ont:excerpt         "For example, the reportedly \"fast and wacky\" \"Powerhouse\" (1937) by Scott was frequently used to accompany animated scenes involving conveyor-belts or chases." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/140632/10> ;
        ont:person          <http://dbpedia.org/resource/Walter_Scott> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/140632/10/1> ;
        ont:subject         <http://dbpedia.org/resource/Carl_W._Stalling> ;
        ont:timeExpression  "1937" .

<http://w3id.org/polifonia/pilot/meetups/140632/9/23>
        rdf:type          ont:Sentence ;
        ont:excerpt       "His arrangements were complicated and technically demanding." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/140632/9> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/140632/9/22> ;
        ont:subject       <http://dbpedia.org/resource/Carl_W._Stalling> .

<http://w3id.org/polifonia/pilot/meetups/140632/9/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "After the animation process was completed, Stalling would receive the animators' exposure sheets or bar sheets." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/140632/9> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/140632/9/2> ;
        ont:subject       <http://dbpedia.org/resource/Carl_W._Stalling> .

<http://w3id.org/polifonia/pilot/meetups/140632/5/22>
        rdf:type          ont:Sentence ;
        ont:excerpt       "According to Sigall, the hiring of Stalling turned out to be a smart move for Schlesinger." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/140632/5> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/140632/5/21> ;
        ont:subject       <http://dbpedia.org/resource/Carl_W._Stalling> .

<http://w3id.org/polifonia/pilot/meetups/140632/26/0>
        rdf:type          ont:Sentence ;
