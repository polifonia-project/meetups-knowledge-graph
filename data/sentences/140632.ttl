<http://w3id.org/polifonia/pilot/meetups/140632/9/14>
        rdf:type          ont:Sentence ;
        ont:excerpt       "His compositions were performed by Warner Brothers' fifty-piece orchestra." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/140632/9> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/140632/9/13> ;
        ont:subject       <http://dbpedia.org/resource/Carl_W._Stalling> .

<http://w3id.org/polifonia/pilot/meetups/140632/5/34>
        rdf:type          ont:Sentence ;
        ont:excerpt       "His last cartoon was To Itch His Own, directed by Chuck Jones." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/140632/5> ;
        ont:person        <http://dbpedia.org/resource/Chuck_Jones> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/140632/5/33> ;
        ont:subject       <http://dbpedia.org/resource/Carl_W._Stalling> .

<http://w3id.org/polifonia/pilot/meetups/140632/7/2>
        rdf:type            ont:Sentence ;
        ont:excerpt         "He was one of three composers, along with Max Steiner and Scott Bradley, credited with the invention of the click track." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/140632/7> ;
        ont:person          <http://dbpedia.org/resource/Max_Steiner> , <http://dbpedia.org/resource/Scott_Bradley_(composer)> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/140632/7/1> ;
        ont:subject         <http://dbpedia.org/resource/Carl_W._Stalling> ;
        ont:timeExpression  "one of" .

<http://w3id.org/polifonia/pilot/meetups/140632/32/0>
