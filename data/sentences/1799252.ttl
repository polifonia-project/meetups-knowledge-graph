<http://w3id.org/polifonia/pilot/meetups/1799252/17/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Playing video cues and sync recording mono audio for quick playback in the studio was also a very popular time saver." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1799252/17> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1799252/17/0> ;
