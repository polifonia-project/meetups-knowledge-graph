<http://w3id.org/polifonia/pilot/meetups/1799252/14/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "All facilities were cross connected electronically at a central location so that all facilities were available to each other as needed." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1799252/14> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1799252/14/2> ;
