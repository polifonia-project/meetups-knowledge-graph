<http://w3id.org/polifonia/pilot/meetups/1799252/18/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "In 1962, sensing a business opportunity in the Bay Area's commercial jingle industry, Putnam purchased a majority interest in Coast Recorders and moved the studio to a location at 960 Bush Street." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1799252/18> ;
        ont:person          <http://dbpedia.org/resource/George_W._Bush> ;
        ont:place           <http://dbpedia.org/resource/San_Francisco_Bay_Area> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1799252/18/0> ;
        ont:subject         <http://dbpedia.org/resource/Bill_Putnam> ;
        ont:timeExpression  "location at 960" .

