<http://w3id.org/polifonia/pilot/meetups/1799252/8/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Putnam also worked for the Armed Forces Network to record big bands." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1799252/8> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1799252/8/1> ;
        ont:subject       <http://dbpedia.org/resource/Bill_Putnam> .

