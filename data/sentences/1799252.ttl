<http://w3id.org/polifonia/pilot/meetups/1799252/25/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Putnam was married four times." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1799252/25> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1799252/25/0> ;
