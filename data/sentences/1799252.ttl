        rdf:type            ont:Sentence ;
        ont:excerpt         "Putnam died in Riverside, California, at the age of 69, and was laid to rest in Valley Oaks Memorial Park in Westlake Village, California." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1799252/26> ;
        ont:place           <http://dbpedia.org/resource/Westlake_Village,_California> ;
