        rdf:type            ont:Sentence ;
        ont:excerpt         "Illinois (1946-1957)" ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1799252/9> ;
        ont:place           <http://dbpedia.org/resource/Illinois> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1799252/9/0> ;
        ont:subject         <http://dbpedia.org/resource/Bill_Putnam> ;
        ont:timeExpression  "1946-1957" .

<http://w3id.org/polifonia/pilot/meetups/1799252/4/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "While in the Boy Scouts, working toward a 'wireless' merit badge, Putnam built a crystal radio and a one-tube radio with his father's help, sparking his love of electronics." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1799252/4> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1799252/4/1> ;
        ont:subject       <http://dbpedia.org/resource/Bill_Putnam> .

<http://w3id.org/polifonia/pilot/meetups/1799252/20/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1985, Putnam sold the original United Recording studio to Allen Sides, who renamed it Ocean Way Recording." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1799252/20> ;
        ont:person        <http://dbpedia.org/resource/Gerald_R._Allen> , <http://dbpedia.org/resource/Universal_Audio> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1799252/20/0> ;
        ont:subject       <http://dbpedia.org/resource/Bill_Putnam> .

