        rdf:type            ont:Sentence ;
        ont:excerpt         "United's facilities included three acoustically isolated studios of varying sizes, three lacquer mastering studios (one stereo) and a stereo re-mixing room." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1799252/14> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1799252/14/0> ;
