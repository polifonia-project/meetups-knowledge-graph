        ont:excerpt       "He enjoyed his work on the riverboat, moving on only after he realized that showboat entertainment was coming to an end." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/248708/7> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/248708/7/16> ;
