        rdf:_4    <http://w3id.org/polifonia/pilot/meetups/248708/4/3> .

<http://w3id.org/polifonia/pilot/meetups/248708/15/15>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Skelton was willing to negotiate with MGM to extend the agreement provided he would receive the right to pursue television." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/248708/15> ;
        ont:place         <http://dbpedia.org/resource/Skelton-in-Cleveland> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/248708/15/14> ;
        ont:subject       <http://dbpedia.org/resource/Red_Skelton> .

<http://w3id.org/polifonia/pilot/meetups/248708/29/7>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Sometimes during live telecasts and taped programs, Skelton would break up or cause his guest stars to laugh." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/248708/29> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/248708/29/6> ;
        ont:subject       <http://dbpedia.org/resource/Red_Skelton> .

<http://w3id.org/polifonia/pilot/meetups/248708/15/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "His MGM contract was rigid enough to require the studio's written consent for his weekly radio shows, as well as any benefit or similar appearances he made; radio offered fewer restrictions, more creative control, and a higher salary." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/248708/15> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/248708/15/3> ;
        ont:subject       <http://dbpedia.org/resource/Red_Skelton> .

<http://w3id.org/polifonia/pilot/meetups/248708/4/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Skelton believed that his life's work was to make people laugh; he wanted to be known as a clown because he defined it as being able to do everything." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/248708/4> ;
        ont:place         <http://dbpedia.org/resource/Skelton-in-Cleveland> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/248708/4/0> ;
        ont:subject       <http://dbpedia.org/resource/Red_Skelton> .

<http://w3id.org/polifonia/pilot/meetups/248708/27/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Skelton was unable to work in television until the end of his 1951 MGM movie contract; a renegotiation to extend the pact provided permission after that point." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/248708/27> ;
        ont:place         <http://dbpedia.org/resource/Skelton-in-Cleveland> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/248708/27/0> ;
        ont:subject       <http://dbpedia.org/resource/Red_Skelton> .

<http://w3id.org/polifonia/pilot/meetups/248708/22/2>
        rdf:type            ont:Sentence ;
        ont:excerpt         "His last Raleigh radio show was on June 6, 1944, the day before he was formally inducted as a private; he was not assigned to Special Services at that time." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/248708/22> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/248708/22/1> ;
        ont:subject         <http://dbpedia.org/resource/Red_Skelton> ;
        ont:timeExpression  "June 6 , 1944" .

<http://w3id.org/polifonia/pilot/meetups/248708/36/5>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Its cancellation after one season ended his television career, and he returned to live performances." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/248708/36> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/248708/36/4> ;
        ont:subject         <http://dbpedia.org/resource/Red_Skelton> ;
        ont:timeExpression  "one season" .

<http://w3id.org/polifonia/pilot/meetups/248708/31/7>
        rdf:type            ont:Sentence ;
        ont:excerpt         "According to an International News Service article that appeared in the August 1, 1957,  issue of the St. Joseph, Missouri News Press, Richard said that the audience with the Pope was the high point of the trip so far." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/248708/31> ;
        ont:person          <http://dbpedia.org/resource/Saint_Joseph> ;
        ont:place           <http://dbpedia.org/resource/Missouri> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/248708/31/6> ;
        ont:subject         <http://dbpedia.org/resource/Red_Skelton> ;
        ont:timeExpression  "August 1 , 1957" .

<http://w3id.org/polifonia/pilot/meetups/248708/48/1>
        rdf:type            ont:Sentence ;
        ont:excerpt         "No. 1, in Indiana." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/248708/48> ;
        ont:place           <http://dbpedia.org/resource/Indiana> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/248708/48/0> ;
