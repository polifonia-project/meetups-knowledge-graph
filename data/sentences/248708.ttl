        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/248708/68> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/248708/68/0> ;
        ont:subject       <http://dbpedia.org/resource/Red_Skelton> .

<http://w3id.org/polifonia/pilot/meetups/248708/4>
        rdf:type  ont:Paragraph ;
