        ont:place           <http://dbpedia.org/resource/Mont_Blanc> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/248708/39/8> ;
        ont:subject         <http://dbpedia.org/resource/Red_Skelton> ;
        ont:timeExpression  "use of two" .

