        ont:person          <http://dbpedia.org/resource/Dalida> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1876457/10/4> ;
        ont:subject         <http://dbpedia.org/resource/Luigi_Tenco> ;
        ont:timeExpression  "in vain five" .

<http://w3id.org/polifonia/pilot/meetups/1876457/17/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "The first witnesses who entered the room did not even see the suicide note." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1876457/17> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1876457/17/1> ;
        ont:subject       <http://dbpedia.org/resource/Luigi_Tenco> .

