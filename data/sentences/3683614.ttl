        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/3683614/39/0> ;
        ont:subject       <http://dbpedia.org/resource/Suzanne_Ciani> .

<http://w3id.org/polifonia/pilot/meetups/3683614/2/4>
        rdf:type            ont:Sentence ;
        ont:excerpt         "From 1964 to 1968, Ciani studied traditional liberal arts at Wellesley College in nearby Wellesley where she received classical music training." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/3683614/2> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/3683614/2/3> ;
        ont:subject         <http://dbpedia.org/resource/Suzanne_Ciani> ;
        ont:timeExpression  "From 1964 to 1968" .

<http://w3id.org/polifonia/pilot/meetups/3683614/9/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "The producers agreed for her to perform her own music with the show's house band, but they cut to a commercial break when they started to play." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/3683614/9> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/3683614/9/0> ;
        ont:subject       <http://dbpedia.org/resource/Suzanne_Ciani> .

<http://w3id.org/polifonia/pilot/meetups/3683614/7/10>
        rdf:type          ont:Sentence ;
        ont:excerpt       "The US federal government approached her to design sounds for flight simulators." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/3683614/7> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/3683614/7/9> ;
        ont:subject       <http://dbpedia.org/resource/Suzanne_Ciani> .

<http://w3id.org/polifonia/pilot/meetups/3683614/7/9>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Ciani then sampled her voice onto a sound chip with the aim of selling it for use in other applications, including elevator announcements." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/3683614/7> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/3683614/7/8> ;
        ont:subject       <http://dbpedia.org/resource/Suzanne_Ciani> .

