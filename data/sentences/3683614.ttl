<http://w3id.org/polifonia/pilot/meetups/3683614/10/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1985, Ciani received a Bronze Lion award at the Cannes Lions International Festival of Creativity." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/3683614/10> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/3683614/10/0> ;
