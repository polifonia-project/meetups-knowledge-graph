<http://w3id.org/polifonia/pilot/meetups/3683614/16/12>
        rdf:type            ont:Sentence ;
        ont:excerpt         "In June 2018, Ciani released LIVE Quadraphonic, a live album documenting her first solo performance on a Buchla synthesizer in 40 years." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/3683614/16> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/3683614/16/11> ;
        ont:subject         <http://dbpedia.org/resource/Suzanne_Ciani> ;
        ont:timeExpression  "synthesizer in 40" , "June 2018" .

