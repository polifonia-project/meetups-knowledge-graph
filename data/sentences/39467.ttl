<http://w3id.org/polifonia/pilot/meetups/39467/3/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "After Oehlenschläger returned home, he wrote at a sitting his  poem Guldhornene, in a manner totally new to Danish literature." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/39467/3> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/39467/3/3> ;
        ont:subject       <http://dbpedia.org/resource/Adam_Oehlenschläger> .

<http://w3id.org/polifonia/pilot/meetups/39467/3/19>
        rdf:type            ont:Sentence ;
        ont:excerpt         "In the spring of 1809, Oehlenschläger went to Rome to visit Bertel Thorvaldsen, and in his house wrote the tragedy of Correggio." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/39467/3> ;
        ont:person          <http://dbpedia.org/resource/Bertel_Thorvaldsen> , <http://dbpedia.org/resource/Antonio_da_Correggio> ;
        ont:place           <http://dbpedia.org/resource/Rome> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/39467/3/18> ;
        ont:subject         <http://dbpedia.org/resource/Adam_Oehlenschläger> ;
        ont:timeExpression  "spring of 1809" .

<http://w3id.org/polifonia/pilot/meetups/39467/2/4>
        rdf:type            ont:Sentence ;
        ont:excerpt         "At the age of nine, Oehlenschläger began to write fluent verses." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/39467/2> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/39467/2/3> ;
        ont:subject         <http://dbpedia.org/resource/Adam_Oehlenschläger> ;
        ont:timeExpression  "age of nine" .

