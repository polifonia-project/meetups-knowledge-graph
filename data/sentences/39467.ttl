<http://w3id.org/polifonia/pilot/meetups/39467/0/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Adam Gottlob Oehlenschläger  (14 November 1779 – 20 January 1850) was a Danish poet and playwright." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/39467/0> ;
        ont:person          <http://dbpedia.org/resource/Poet> , <http://dbpedia.org/resource/Playwright> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/39467/0/0> ;
        ont:subject         <http://dbpedia.org/resource/Adam_Oehlenschläger> ;
        ont:timeExpression  "20 January 1850" , "14 November 1779" .

<http://w3id.org/polifonia/pilot/meetups/39467/3/5>
        rdf:type          ont:Sentence ;
        ont:excerpt       "The result of his new enthusiasm speedily showed itself in a somewhat hasty volume of poems, published in 1803, now chiefly remembered as containing the  piece called Sanct Hansaften-Spil." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/39467/3> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/39467/3/4> ;
        ont:subject       <http://dbpedia.org/resource/Adam_Oehlenschläger> .

<http://w3id.org/polifonia/pilot/meetups/39467/2/5>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Three years later, he attracted the notice of the poet Edvard Storm (1749–1794) and as a result Öhlenschläger received an introduction into  Scandinavian mythology." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/39467/2> ;
        ont:person          <http://dbpedia.org/resource/Edvard_Storm> , <http://dbpedia.org/resource/Poet> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/39467/2/4> ;
        ont:subject         <http://dbpedia.org/resource/Adam_Oehlenschläger> ;
        ont:timeExpression  "1749–1794" .

