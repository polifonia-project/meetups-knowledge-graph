<http://w3id.org/polifonia/pilot/meetups/39467/3/27>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1817 he returned to Paris, and published Hroars Saga and the tragedy of Fostbrødrene." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/39467/3> ;
        ont:place         <http://dbpedia.org/resource/Paris> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/39467/3/26> ;
