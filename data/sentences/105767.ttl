        rdf:type          ont:Sentence ;
        ont:excerpt       "His albums had sold over 180 million copies by 2019." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/105767/21> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/105767/21/2> ;
        ont:subject       <http://dbpedia.org/resource/Camilo_Sesto> .

