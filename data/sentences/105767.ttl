        ont:person          <http://dbpedia.org/resource/Composer> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/105767/12/0> ;
        ont:subject         <http://dbpedia.org/resource/Camilo_Sesto> ;
