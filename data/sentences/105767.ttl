<http://w3id.org/polifonia/pilot/meetups/105767/5/30>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Sesto returned to the stage in 2004 at the Viña del Mar International Song Festival in Chile." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/105767/5> ;
        ont:place           <http://dbpedia.org/resource/Chile> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/105767/5/29> ;
        ont:subject         <http://dbpedia.org/resource/Camilo_Sesto> ;
        ont:timeExpression  "stage in 2004" .

<http://w3id.org/polifonia/pilot/meetups/105767/5/15>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He has received several platinum certifications." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/105767/5> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/105767/5/14> ;
        ont:subject       <http://dbpedia.org/resource/Camilo_Sesto> .

<http://w3id.org/polifonia/pilot/meetups/105767/5/28>
        rdf:type          ont:Sentence ;
        ont:excerpt       "The track was nominated for Pop Song of the Year at the Lo Nuestro Awards of 1992 and the American Society of Composers, Authors and Publishers Latin Awards of 1993, respectively." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/105767/5> ;
        ont:place         <http://dbpedia.org/resource/United_States> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/105767/5/27> ;
        ont:subject       <http://dbpedia.org/resource/Camilo_Sesto> .

<http://w3id.org/polifonia/pilot/meetups/105767/5/8>
        rdf:type          ont:Sentence ;
        ont:excerpt       "After meeting producer, Juan Pardo in 1972, he launched a solo career under the stage name Camilo Sesto." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/105767/5> ;
        ont:person        <http://dbpedia.org/resource/Camilo_Sesto> , <http://dbpedia.org/resource/Juan_Pardo_(explorer)> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/105767/5/7> ;
        ont:subject       <http://dbpedia.org/resource/Camilo_Sesto> .

