<http://w3id.org/polifonia/pilot/meetups/105767/0/2>
        rdf:type            ont:Sentence ;
        ont:excerpt         "There are various sales figures for him, ranging from 70 to 200 million records sold." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/105767/0> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/105767/0/1> ;
