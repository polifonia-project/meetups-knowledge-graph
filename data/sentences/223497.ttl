        rdf:type          ont:Sentence ;
        ont:excerpt       "He is a multi-instrumentalist who, in addition to flute and acoustic guitar, plays keyboards, electric guitar, bass guitar, bouzouki, balalaika, saxophone, harmonica, and a variety of whistles." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/223497/0> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/223497/0/0> ;
