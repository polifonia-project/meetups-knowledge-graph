<http://w3id.org/polifonia/pilot/meetups/223497/7/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Anderson was known for his famous one-legged flute stance, and was once referred to as a \"deranged flamingo\"." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/223497/7> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/223497/7/0> ;
