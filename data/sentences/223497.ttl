        rdf:type            ont:Sentence ;
        ont:excerpt         "As he himself tells it in the introduction to the video Nothing Is Easy: Live at the Isle of Wight 1970, he traded his electric guitar in for a flute which, after some weeks of practice, he found he could play fairly well in a rock and blues style." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/223497/6> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/223497/6/0> ;
        ont:subject         <http://dbpedia.org/resource/Ian_Anderson> ;
        ont:timeExpression  "Wight 1970" .

