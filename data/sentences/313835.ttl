<http://w3id.org/polifonia/pilot/meetups/313835/7/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1922, Trenet moved to Perpignan, this time as a day pupil." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/313835/7> ;
        ont:place         <http://dbpedia.org/resource/Perpignan> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/313835/7/0> ;
        ont:subject       <http://dbpedia.org/resource/Charles_Trenet> .

