        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/313835/31/1> .

<http://w3id.org/polifonia/pilot/meetups/313835/4/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Besides \"La Mer\", the other Trenet song to receive numerous recordings in English is \"Que reste-t-il de nos amours?\", which lyricist Albert Beach adapted as \"I Wish You Love\"." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/313835/4> ;
        ont:person        <http://dbpedia.org/resource/Albert_I._Beach> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/313835/4/0> ;
        ont:subject       <http://dbpedia.org/resource/Charles_Trenet> .

<http://w3id.org/polifonia/pilot/meetups/313835/10>
        rdf:type  ont:Paragraph ;
