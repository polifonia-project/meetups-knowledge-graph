<http://w3id.org/polifonia/pilot/meetups/313835/17/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He incorporated ten new songs into his act, including \"De la fenêtre d'en haut\" and \"La Folle Complainte\"." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/313835/17> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/313835/17/0> ;
