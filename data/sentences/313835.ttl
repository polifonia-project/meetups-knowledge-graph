        ont:subject       <http://dbpedia.org/resource/Charles_Trenet> .

<http://w3id.org/polifonia/pilot/meetups/313835/17/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "On 14 September 1951, Trenet returned to Paris and made a comeback at the Théâtre de l'Étoile." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/313835/17> ;
        ont:place           <http://dbpedia.org/resource/Paris> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/313835/17/0> ;
        ont:subject         <http://dbpedia.org/resource/Charles_Trenet> ;
        ont:timeExpression  "14 September 1951" .

<http://w3id.org/polifonia/pilot/meetups/313835/49/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "1939: \"Il pleut dans ma chambre\"" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/313835/49> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/313835/49/0> ;
        ont:subject       <http://dbpedia.org/resource/Charles_Trenet> .

<http://w3id.org/polifonia/pilot/meetups/313835/3/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "His song \"La Mer\", which according to legend he composed with Léo Chauliac on a train in 1943, was recorded in 1946." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/313835/3> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/313835/3/0> ;
        ont:subject         <http://dbpedia.org/resource/Charles_Trenet> ;
        ont:timeExpression  "train in 1943" .

<http://w3id.org/polifonia/pilot/meetups/313835/26/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Trenet then returned to his peaceful semi-retirement in Occitanie, occasionally reappearing for a special gala performance in France or abroad." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/313835/26> ;
        ont:place         <http://dbpedia.org/resource/France> , <http://dbpedia.org/resource/Occitanie> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/313835/26/0> ;
        ont:subject       <http://dbpedia.org/resource/Charles_Trenet> .

<http://w3id.org/polifonia/pilot/meetups/313835/73>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/313835/73/0> .

<http://w3id.org/polifonia/pilot/meetups/313835/52>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/313835/52/0> .

<http://w3id.org/polifonia/pilot/meetups/313835/18/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1958, Trenet was the headlining act at the Bobino and the Alhambra." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/313835/18> ;
        ont:place         <http://dbpedia.org/resource/Alhambra-Maurice_Chevalier> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/313835/18/0> ;
        ont:subject       <http://dbpedia.org/resource/Charles_Trenet> .

<http://w3id.org/polifonia/pilot/meetups/313835/31>
