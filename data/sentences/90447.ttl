        ont:excerpt       "Today, three simultaneous blindfold games would be fairly unremarkable among many chess masters." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/90447/19> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/90447/19/2> ;
