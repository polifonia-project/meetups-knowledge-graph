<http://w3id.org/polifonia/pilot/meetups/90447/19/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "In England, Philidor astounded his peers by playing three blindfold chess games simultaneously in the chess club of St. James Street on 9 May 1783." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/90447/19> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/90447/19/0> ;
        ont:subject         <http://dbpedia.org/resource/François-André_Danican_Philidor> ;
        ont:timeExpression  "9 May 1783" .

<http://w3id.org/polifonia/pilot/meetups/90447/8/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Anne Danican Philidor (1681–1728) was François-André Danican Philidor's oldest brother." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/90447/8> ;
        ont:person          <http://dbpedia.org/resource/Anne_Danican_Philidor> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/90447/8/0> ;
        ont:subject         <http://dbpedia.org/resource/François-André_Danican_Philidor> ;
        ont:timeExpression  "1681–1728" .

<http://w3id.org/polifonia/pilot/meetups/90447/36/2>
        rdf:type            ont:Sentence ;
        ont:excerpt         "He died on 31 August 1795 in London, and was buried in St James, Piccadilly." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/90447/36> ;
        ont:place           <http://dbpedia.org/resource/Westminster_St_James> , <http://dbpedia.org/resource/London> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/90447/36/1> ;
        ont:subject         <http://dbpedia.org/resource/François-André_Danican_Philidor> ;
        ont:timeExpression  "31 August 1795" .

<http://w3id.org/polifonia/pilot/meetups/90447/21/4>
        rdf:type            ont:Sentence ;
        ont:excerpt         "In fact, he was probably about 200 rating points better than anyone else yet alive—set apart by the mysteries of the game he had solved." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/90447/21> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/90447/21/3> ;
        ont:subject         <http://dbpedia.org/resource/François-André_Danican_Philidor> ;
        ont:timeExpression  "200 rating" .

