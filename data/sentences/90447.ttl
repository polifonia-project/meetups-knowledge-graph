        rdf:type            ont:Sentence ;
        ont:excerpt         "His three most successful works were Le sorcier (1764), Tom Jones (after Henry Fielding, 1765), and Ernelinde, princesse de Norvège (1767)." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/90447/11> ;
