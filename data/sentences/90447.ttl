<http://w3id.org/polifonia/pilot/meetups/90447/17/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Here, Philidor encountered George Atwood, a famous mathematician, physician and lecturer at Cambridge University." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/90447/17> ;
        ont:person        <http://dbpedia.org/resource/Margaret_Atwood> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/90447/17/2> ;
        ont:subject       <http://dbpedia.org/resource/François-André_Danican_Philidor> .

<http://w3id.org/polifonia/pilot/meetups/90447/52>
        rdf:type  ont:Paragraph ;
