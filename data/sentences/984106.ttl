        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/984106/18> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/984106/18/0> ;
        ont:subject       <http://dbpedia.org/resource/Antonio_Paoli> .

<http://w3id.org/polifonia/pilot/meetups/984106/4/10>
        rdf:type          ont:Sentence ;
        ont:excerpt       "When Paoli was young, his parents would often take him to operas at Ponce's La Perla Theater located a block from their residence." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/984106/4> ;
        ont:person        <http://dbpedia.org/resource/Antonio_Paoli> ;
        ont:place         <http://dbpedia.org/resource/La_Perla_District> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/984106/4/9> ;
        ont:subject       <http://dbpedia.org/resource/Antonio_Paoli> .

<http://w3id.org/polifonia/pilot/meetups/984106/1/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "After the War ended, following medical advice and performing vocal exercises, Paoli regained his voice and returned to the international stage, in all the glory of days past." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/984106/1> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/984106/1/3> ;
        ont:subject       <http://dbpedia.org/resource/Antonio_Paoli> .

<http://w3id.org/polifonia/pilot/meetups/984106/8/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He performed in Italy, Luxembourg and in the United Kingdom, with performances in London, Scotland, Edinburgh, and Brighton, closing the year with concerts in Corsica (his father's homeland) and Turkey." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/984106/8> ;
        ont:place         <http://dbpedia.org/resource/Corsica> , <http://dbpedia.org/resource/Italy> , <http://dbpedia.org/resource/Turkey> , <http://dbpedia.org/resource/Luxembourg> , <http://dbpedia.org/resource/London> , <http://dbpedia.org/resource/Brighton> , <http://dbpedia.org/resource/Edinburgh> , <http://dbpedia.org/resource/United_Kingdom> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/984106/8/0> ;
        ont:subject       <http://dbpedia.org/resource/Antonio_Paoli> .

<http://w3id.org/polifonia/pilot/meetups/984106/19/1>
        rdf:type            ont:Sentence ;
        ont:excerpt         "After living in New York City for almost the entire year of 1927, Paoli decided to return to Puerto Rico to live and work with his sister Amalia, and give voice lessons at the Academia Paoli." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/984106/19> ;
        ont:person          <http://dbpedia.org/resource/Antonio_Paoli> , <http://dbpedia.org/resource/Amalia_Paoli> ;
        ont:place           <http://dbpedia.org/resource/New_York_City> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/984106/19/0> ;
        ont:subject         <http://dbpedia.org/resource/Antonio_Paoli> ;
        ont:timeExpression  "year of 1927" .

<http://w3id.org/polifonia/pilot/meetups/984106/9/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Paoli purchased a villa in Porto Cereso, Lugano, Italy, where his son Antonio Arnaldo was born." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/984106/9> ;
        ont:person        <http://dbpedia.org/resource/Antonio_Paoli> ;
        ont:place         <http://dbpedia.org/resource/Lugano> , <http://dbpedia.org/resource/Porto> , <http://dbpedia.org/resource/Italy> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/984106/9/0> ;
        ont:subject       <http://dbpedia.org/resource/Antonio_Paoli> .

<http://w3id.org/polifonia/pilot/meetups/984106/28>
        rdf:type  ont:Paragraph ;
