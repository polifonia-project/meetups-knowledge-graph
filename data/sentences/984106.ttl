<http://w3id.org/polifonia/pilot/meetups/984106/11/4>
        rdf:type            ont:Sentence ;
        ont:excerpt         "The recording was arranged in the following manner; the musicians were placed at the end of the recording room and the back-up singers in a semi-arc in front of the gramophone, while Paoli stood alone 20 feet (6.1 m) away from the gramophone." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/984106/11> ;
        ont:person          <http://dbpedia.org/resource/Antonio_Paoli> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/984106/11/3> ;
        ont:subject         <http://dbpedia.org/resource/Antonio_Paoli> ;
        ont:timeExpression  "6.1 m" .

<http://w3id.org/polifonia/pilot/meetups/984106/18/1>
        rdf:type          ont:Sentence ;
