        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/984106/28/1> .

<http://w3id.org/polifonia/pilot/meetups/984106/7>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/984106/7/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/984106/7/1> .

<http://w3id.org/polifonia/pilot/meetups/984106/14/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "After a period of training in Spain, he moved to England to start his new \"career.\"" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/984106/14> ;
        ont:place         <http://dbpedia.org/resource/Spain> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/984106/14/3> ;
        ont:subject       <http://dbpedia.org/resource/Antonio_Paoli> .

<http://w3id.org/polifonia/pilot/meetups/984106/19/9>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He affectionately called her \"Adina\"." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/984106/19> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/984106/19/8> ;
        ont:subject       <http://dbpedia.org/resource/Antonio_Paoli> .

<http://w3id.org/polifonia/pilot/meetups/984106/4/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "The young couple escaped to the Dominican Republic without getting married and later returned to Puerto Rico." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/984106/4> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/984106/4/3> ;
        ont:subject       <http://dbpedia.org/resource/Antonio_Paoli> .

<http://w3id.org/polifonia/pilot/meetups/984106/23/5>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1920, the tenor was knighted by Vittorio Emmanuelle III, King of Italy, making him Cavaliere De La Corona Italiana and Commendatore Dell Popolo Romano (Knight of the Italian Crown and Commandant of the Roman People)." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/984106/23> ;
        ont:person        <http://dbpedia.org/resource/George_III> , <http://dbpedia.org/resource/Tenor> ;
        ont:place         <http://dbpedia.org/resource/Rome> , <http://dbpedia.org/resource/Italy> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/984106/23/4> ;
        ont:subject       <http://dbpedia.org/resource/Antonio_Paoli> .

<http://w3id.org/polifonia/pilot/meetups/984106/15>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/984106/15/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/984106/15/1> .

<http://w3id.org/polifonia/pilot/meetups/984106/24/5>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Among other honors bestowed on Paoli after his death are a music school named in his honor (\"Escuela Libre de Música Antonio Paoli\"), in the city of Caguas, Puerto Rico, and in his hometown of Ponce, there is a theater at the Interamerican University of Puerto Rico, Ponce named in his memory, Teatro Paoli." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/984106/24> ;
        ont:person        <http://dbpedia.org/resource/Antonio_Paoli> ;
        ont:place         <http://dbpedia.org/resource/Caguas,_Puerto_Rico> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/984106/24/4> ;
        ont:subject       <http://dbpedia.org/resource/Antonio_Paoli> .

<http://w3id.org/polifonia/pilot/meetups/984106/20/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Paoli died of prostate cancer, in San Juan on 24 August 1946, and was buried in the Puerto Rico Memorial Cemetery in Carolina, Puerto Rico." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/984106/20> ;
        ont:person          <http://dbpedia.org/resource/Antonio_Paoli> ;
        ont:place           <http://dbpedia.org/resource/San_Juan,_Puerto_Rico> , <http://dbpedia.org/resource/Carolina,_Puerto_Rico> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/984106/20/0> ;
        ont:subject         <http://dbpedia.org/resource/Antonio_Paoli> ;
        ont:timeExpression  "24 August 1946" .

<http://w3id.org/polifonia/pilot/meetups/984106/6/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He was assigned to the Queen's escort and named personal custodian and guard of the child King Alfonso XIII." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/984106/6> ;
        ont:person        <http://dbpedia.org/resource/Alfonso_XIII> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/984106/6/3> ;
        ont:subject       <http://dbpedia.org/resource/Antonio_Paoli> .

<http://w3id.org/polifonia/pilot/meetups/984106/17/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He had wanted to sing in the Metropolitan Opera House in New York, but apparently his competitor Enrico Caruso opposed this." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/984106/17> ;
        ont:person        <http://dbpedia.org/resource/Enrico_Caruso> ;
        ont:place         <http://dbpedia.org/resource/New_York_City> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/984106/17/3> ;
        ont:subject       <http://dbpedia.org/resource/Antonio_Paoli> .

