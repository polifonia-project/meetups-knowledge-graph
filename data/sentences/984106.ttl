<http://w3id.org/polifonia/pilot/meetups/984106/26/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "The following are part of Antonio Paoli's discography:" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/984106/26> ;
        ont:person        <http://dbpedia.org/resource/Antonio_Paoli> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/984106/26/0> ;
        ont:subject       <http://dbpedia.org/resource/Antonio_Paoli> .

<http://w3id.org/polifonia/pilot/meetups/984106/11/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Paoli was the first operatic artist to record an entire opera when he participated in a performance of Pagliacci by Ruggiero Leoncavallo in Milan, Italy." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/984106/11> ;
        ont:person        <http://dbpedia.org/resource/Ruggero_Leoncavallo> , <http://dbpedia.org/resource/Antonio_Paoli> ;
        ont:place         <http://dbpedia.org/resource/Italy> , <http://dbpedia.org/resource/Milan> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/984106/11/1> ;
        ont:subject       <http://dbpedia.org/resource/Antonio_Paoli> .

<http://w3id.org/polifonia/pilot/meetups/984106/1/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "When World War I forced the closure of all European opera houses, Paoli made his living as a professional boxer." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/984106/1> ;
        ont:person        <http://dbpedia.org/resource/Antonio_Paoli> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/984106/1/1> ;
        ont:subject       <http://dbpedia.org/resource/Antonio_Paoli> .

<http://w3id.org/polifonia/pilot/meetups/984106/27/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Antonio Paoli. (2000)" ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/984106/27> ;
