<http://w3id.org/polifonia/pilot/meetups/984106/22/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Antonio Paoli was the first Puerto Rican to gain international recognition in the performing arts." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/984106/22> ;
        ont:person        <http://dbpedia.org/resource/Antonio_Paoli> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/984106/22/0> ;
        ont:subject       <http://dbpedia.org/resource/Antonio_Paoli> .

<http://w3id.org/polifonia/pilot/meetups/984106/4/13>
        rdf:type          ont:Sentence ;
        ont:excerpt       "His parents were very supportive of his ambition and guided him on this route during his youth, enrolling him in a school of \"voice\" directed by Ramon Marin." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/984106/4> ;
        ont:person        <http://dbpedia.org/resource/Ramón_Marín> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/984106/4/12> ;
        ont:subject       <http://dbpedia.org/resource/Antonio_Paoli> .

