<http://w3id.org/polifonia/pilot/meetups/984106/22/1>
        rdf:type            ont:Sentence ;
        ont:excerpt         "He gave 1,725 performances between 1888 and 1942, and interpreted Verdi's Otello 575 times." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/984106/22> ;
        ont:person          <http://dbpedia.org/resource/Giuseppe_Verdi> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/984106/22/0> ;
        ont:subject         <http://dbpedia.org/resource/Antonio_Paoli> ;
        ont:timeExpression  "between 1888 and 1942" , "Otello 575" .

<http://w3id.org/polifonia/pilot/meetups/984106/4/14>
        rdf:type          ont:Sentence ;
        ont:excerpt       "His sister Amalia was a soprano who performed at La Perla in Emilio Arrieta's opera, Marina." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/984106/4> ;
        ont:person        <http://dbpedia.org/resource/Soprano> , <http://dbpedia.org/resource/Amalia_Paoli> ;
        ont:place         <http://dbpedia.org/resource/La_Perla_District> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/984106/4/13> ;
        ont:subject       <http://dbpedia.org/resource/Antonio_Paoli> .

