        rdf:type          ont:Sentence ;
        ont:excerpt       "He performed in Europe, North and South America, and finally settled with his sister Amalia in San Juan, Puerto Rico, where she had opened a singing school." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/984106/1> ;
