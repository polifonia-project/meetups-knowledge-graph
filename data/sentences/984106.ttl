        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/984106/27/0> ;
        ont:subject         <http://dbpedia.org/resource/Antonio_Paoli> ;
        ont:timeExpression  "2000" .

<http://w3id.org/polifonia/pilot/meetups/984106/6/7>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Paris newspapers commented on Paoli's success and said, \"We should declare Paoli the Tenor of France.\"" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/984106/6> ;
        ont:person        <http://dbpedia.org/resource/Antonio_Paoli> , <http://dbpedia.org/resource/Tenor> ;
        ont:place         <http://dbpedia.org/resource/Paris> , <http://dbpedia.org/resource/France> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/984106/6/6> ;
        ont:subject       <http://dbpedia.org/resource/Antonio_Paoli> .

<http://w3id.org/polifonia/pilot/meetups/984106/12/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Paoli received a contract to inaugurate Teatro Colón, in Buenos Aires, Argentina, where he sang Otello and Di quella pira." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/984106/12> ;
        ont:person        <http://dbpedia.org/resource/Antonio_Paoli> ;
        ont:place         <http://dbpedia.org/resource/Buenos_Aires> , <http://dbpedia.org/resource/Argentina> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/984106/12/1> ;
        ont:subject       <http://dbpedia.org/resource/Antonio_Paoli> .

<http://w3id.org/polifonia/pilot/meetups/984106/2/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He was buried in San Juan, but his remains were later transferred to a mausoleum at Panteón Nacional Román Baldorioty de Castro in his birth city of Ponce." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/984106/2> ;
        ont:person        <http://dbpedia.org/resource/Fidel_Castro> , <http://dbpedia.org/resource/Juan_Federico_Ponce_Vaides> ;
        ont:place         <http://dbpedia.org/resource/San_Juan,_Puerto_Rico> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/984106/2/1> ;
        ont:subject       <http://dbpedia.org/resource/Antonio_Paoli> .

