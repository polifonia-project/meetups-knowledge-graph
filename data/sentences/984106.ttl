<http://w3id.org/polifonia/pilot/meetups/984106/14/6>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He broke his right wrist on his sixth challenge, which ended his boxing career." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/984106/14> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/984106/14/5> ;
        ont:subject       <http://dbpedia.org/resource/Antonio_Paoli> .

<http://w3id.org/polifonia/pilot/meetups/984106/4/6>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Later they moved to Ponce, into a house given to them by Amalia's aunt, Teresa Intriago." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/984106/4> ;
        ont:person        <http://dbpedia.org/resource/Teresa_Brambilla> , <http://dbpedia.org/resource/Amalia_Paoli> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/984106/4/5> ;
        ont:subject       <http://dbpedia.org/resource/Antonio_Paoli> .

<http://w3id.org/polifonia/pilot/meetups/984106/10/1>
        rdf:type            ont:Sentence ;
        ont:excerpt         "The Czar of Russia Nicholas II was present during one of the performances." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/984106/10> ;
        ont:person          <http://dbpedia.org/resource/Nicholas_II_of_Russia> ;
        ont:place           <http://dbpedia.org/resource/Russia> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/984106/10/0> ;
        ont:subject         <http://dbpedia.org/resource/Antonio_Paoli> ;
        ont:timeExpression  "one of" .

<http://w3id.org/polifonia/pilot/meetups/984106/23/7>
        rdf:type          ont:Sentence ;
        ont:excerpt       "This dream was finally realized shortly after his death." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/984106/23> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/984106/23/6> ;
        ont:subject       <http://dbpedia.org/resource/Antonio_Paoli> .

<http://w3id.org/polifonia/pilot/meetups/984106/0/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "At the height of his fame, he was known as \"The King of Tenors and The Tenor of Kings.\"" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/984106/0> ;
        ont:person        <http://dbpedia.org/resource/Tenor> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/984106/0/0> ;
        ont:subject       <http://dbpedia.org/resource/Antonio_Paoli> .

<http://w3id.org/polifonia/pilot/meetups/984106/17>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/984106/17/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/984106/17/1> ;
        rdf:_3    <http://w3id.org/polifonia/pilot/meetups/984106/17/2> ;
        rdf:_4    <http://w3id.org/polifonia/pilot/meetups/984106/17/3> ;
        rdf:_5    <http://w3id.org/polifonia/pilot/meetups/984106/17/4> ;
        rdf:_6    <http://w3id.org/polifonia/pilot/meetups/984106/17/5> ;
        rdf:_7    <http://w3id.org/polifonia/pilot/meetups/984106/17/6> .

<http://w3id.org/polifonia/pilot/meetups/984106/11/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "That same year he was named as a \"Singer of the Royal Court\" by William II of Germany." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/984106/11> ;
        ont:person        <http://dbpedia.org/resource/William_II_of_England> ;
        ont:place         <http://dbpedia.org/resource/Germany> , <http://dbpedia.org/resource/Royal_Court_Theatre> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/984106/11/0> ;
        ont:subject       <http://dbpedia.org/resource/Antonio_Paoli> .

<http://w3id.org/polifonia/pilot/meetups/984106/24/7>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Included is the work by Emilio J. Pasarell titled \"Orígenes y desarrollo de la afición teatral en Puerto Rico\" (Origins and development of the theatrical pursuit in Puerto Rico) where he describes the life of Paoli in detail." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/984106/24> ;
        ont:person        <http://dbpedia.org/resource/Antonio_Paoli> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/984106/24/6> ;
        ont:subject       <http://dbpedia.org/resource/Antonio_Paoli> .

<http://w3id.org/polifonia/pilot/meetups/984106/1/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He performed in Moscow, Paris, London, Madrid, New York, Buenos Aires, Havana, Santiago, Milan, Rome, Naples, Barcelona, and Vienna." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/984106/1> ;
        ont:place         <http://dbpedia.org/resource/Havana> , <http://dbpedia.org/resource/Santiago> , <http://dbpedia.org/resource/Vienna> , <http://dbpedia.org/resource/Moscow> , <http://dbpedia.org/resource/London> , <http://dbpedia.org/resource/Paris> , <http://dbpedia.org/resource/Milan> , <http://dbpedia.org/resource/Naples> , <http://dbpedia.org/resource/New_York_City> , <http://dbpedia.org/resource/Rome> , <http://dbpedia.org/resource/Barcelona> , <http://dbpedia.org/resource/Madrid> , <http://dbpedia.org/resource/Buenos_Aires> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/984106/1/0> ;
        ont:subject       <http://dbpedia.org/resource/Antonio_Paoli> .

<http://w3id.org/polifonia/pilot/meetups/984106/20/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "The bust of Paoli at the Pantheon's rotunda is the work of Puerto Rican sculptor Gladys Nieves." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/984106/20> ;
        ont:person        <http://dbpedia.org/resource/Antonio_Paoli> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/984106/20/1> ;
        ont:subject       <http://dbpedia.org/resource/Antonio_Paoli> .

<http://w3id.org/polifonia/pilot/meetups/984106/6/6>
        rdf:type            ont:Sentence ;
        ont:excerpt         "On 26 April 1899, he made his debut in Gioacchino Rossini's opera William Tell in Paris, France." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/984106/6> ;
        ont:person          <http://dbpedia.org/resource/Gioachino_Rossini> , <http://dbpedia.org/resource/William_Tell> ;
        ont:place           <http://dbpedia.org/resource/Paris> , <http://dbpedia.org/resource/France> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/984106/6/5> ;
        ont:subject         <http://dbpedia.org/resource/Antonio_Paoli> ;
        ont:timeExpression  "26 April 1899" .

<http://w3id.org/polifonia/pilot/meetups/984106/12/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1910, the singer was signed as the First Tenor by La Scala, Milan, the most prestigious opera company in the world at the time." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/984106/12> ;
        ont:person        <http://dbpedia.org/resource/Tenor> ;
        ont:place         <http://dbpedia.org/resource/Milan> , <http://dbpedia.org/resource/La_Scala> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/984106/12/0> ;
        ont:subject       <http://dbpedia.org/resource/Antonio_Paoli> .

<http://w3id.org/polifonia/pilot/meetups/984106/17/6>
        rdf:type          ont:Sentence ;
        ont:excerpt       "From the U.S., the tenor would visit Curaçao, Cuba, Colombia, Haiti and Ecuador." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/984106/17> ;
        ont:person        <http://dbpedia.org/resource/Tenor> ;
        ont:place         <http://dbpedia.org/resource/Haiti> , <http://dbpedia.org/resource/Cuba> , <http://dbpedia.org/resource/United_States> , <http://dbpedia.org/resource/Colombia> , <http://dbpedia.org/resource/Ecuador> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/984106/17/5> ;
        ont:subject       <http://dbpedia.org/resource/Antonio_Paoli> .

<http://w3id.org/polifonia/pilot/meetups/984106/2/1>
        rdf:type            ont:Sentence ;
        ont:excerpt         "He would not see this last dream come true, because he developed cancer and died at age 75." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/984106/2> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/984106/2/0> ;
        ont:subject         <http://dbpedia.org/resource/Antonio_Paoli> ;
        ont:timeExpression  "at age 75" .

