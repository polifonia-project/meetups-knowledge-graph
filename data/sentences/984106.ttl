<http://w3id.org/polifonia/pilot/meetups/984106/19/11>
        rdf:type          ont:Sentence ;
        ont:excerpt       "The service was conducted at the Chapel of the University of the Sacred Heart (Santurce)." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/984106/19> ;
        ont:person        <http://dbpedia.org/resource/Sacred_Heart> ;
        ont:place         <http://dbpedia.org/resource/Santurtzi> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/984106/19/10> ;
        ont:subject       <http://dbpedia.org/resource/Antonio_Paoli> .

<http://w3id.org/polifonia/pilot/meetups/984106/17/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "At the time, his sister Amalia Paoli was residing in San Juan (in the ward of Santurce) where she was running a singing school (Academia Paoli)." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/984106/17> ;
        ont:person        <http://dbpedia.org/resource/Amalia_Paoli> , <http://dbpedia.org/resource/Antonio_Paoli> ;
