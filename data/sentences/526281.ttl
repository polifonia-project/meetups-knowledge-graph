<http://w3id.org/polifonia/pilot/meetups/526281/6/1>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Subsequently, he worked on George Lucas's THX 1138 and American Graffiti and Coppola's The Godfather before editing picture and mixing sound on Coppola's The Conversation, for which he received an Academy Award nomination in sound in 1974." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/526281/6> ;
        ont:person          <http://dbpedia.org/resource/George_Lucas> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/526281/6/0> ;
        ont:subject         <http://dbpedia.org/resource/Walter_Murch> ;
        ont:timeExpression  "sound in 1974" , "THX 1138" .

