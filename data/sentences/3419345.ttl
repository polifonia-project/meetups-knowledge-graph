<http://w3id.org/polifonia/pilot/meetups/3419345/4/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Blackwell was then recorded in 1961, in Indianapolis, by the young Art Rosenbaum for the Prestige/Bluesville Records label." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/3419345/4> ;
        ont:place         <http://dbpedia.org/resource/Indianapolis> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/3419345/4/0> ;
        ont:subject       <http://dbpedia.org/resource/Scrapper_Blackwell> .

<http://w3id.org/polifonia/pilot/meetups/3419345/3/11>
        rdf:type          ont:Sentence ;
        ont:excerpt       "A short time later Blackwell retired from the music industry." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/3419345/3> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/3419345/3/10> ;
        ont:subject       <http://dbpedia.org/resource/Scrapper_Blackwell> .

<http://w3id.org/polifonia/pilot/meetups/3419345/2/6>
        rdf:type          ont:Sentence ;
        ont:excerpt       "By his teens, Blackwell was a part-time musician, traveling as far as Chicago." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/3419345/2> ;
        ont:person        <http://dbpedia.org/resource/Ian_Blackwell> ;
        ont:place         <http://dbpedia.org/resource/Chicago> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/3419345/2/5> ;
        ont:subject       <http://dbpedia.org/resource/Scrapper_Blackwell> .

<http://w3id.org/polifonia/pilot/meetups/3419345/2>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/3419345/2/0> ;
