<http://w3id.org/polifonia/pilot/meetups/1476033/0/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Rubén Fuentes (born 15 February 1926) is a Mexican classical violinist and composer, who is best known for his contributions to mariachi music." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1476033/0> ;
        ont:person          <http://dbpedia.org/resource/Composer> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1476033/0/0> ;
        ont:subject         <http://dbpedia.org/resource/Rubén_Fuentes> ;
        ont:timeExpression  "15 February 1926" .

<http://w3id.org/polifonia/pilot/meetups/1476033/2/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "With domestic sales of two-and-a-half million copies, Canciones de Mi Padre stands as the biggest-selling non-English language record in US history." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1476033/2> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1476033/2/3> ;
        ont:subject       <http://dbpedia.org/resource/Rubén_Fuentes> .

<http://w3id.org/polifonia/pilot/meetups/1476033/4/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Most of them were accompanied by Mariachi Vargas de Tecalitlán." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1476033/4> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1476033/4/1> ;
        ont:subject       <http://dbpedia.org/resource/Rubén_Fuentes> .

