<http://w3id.org/polifonia/pilot/meetups/1059399/7/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He was survived by his sons, Jeffrey and Andrew, by his daughter, Alyce, and by two grandchildren." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1059399/7> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1059399/7/1> ;
        ont:subject       <http://dbpedia.org/resource/Karl_Haas> .

<http://w3id.org/polifonia/pilot/meetups/1059399/2/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Faced with the rise of Nazism, the Jewish Haas fled Germany for the United States in 1936." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1059399/2> ;
        ont:person        <http://dbpedia.org/resource/Walter_A._Haas> ;
        ont:place         <http://dbpedia.org/resource/United_States> , <http://dbpedia.org/resource/Germany> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1059399/2/2> ;
        ont:subject       <http://dbpedia.org/resource/Karl_Haas> .

<http://w3id.org/polifonia/pilot/meetups/1059399/4/8>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Haas also twice won the George Foster Peabody Award for excellence in broadcasting." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1059399/4> ;
        ont:person        <http://dbpedia.org/resource/George_Gershwin> , <http://dbpedia.org/resource/David_Foster> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1059399/4/7> ;
        ont:subject       <http://dbpedia.org/resource/Karl_Haas> .

<http://w3id.org/polifonia/pilot/meetups/1059399/4/13>
        rdf:type          ont:Sentence ;
        ont:excerpt       "The announcement explained that the number of stations that carried the show had dropped from more than 400 to fewer than 20, which made it unfeasible to continue the program's national distribution." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1059399/4> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1059399/4/12> ;
        ont:subject       <http://dbpedia.org/resource/Karl_Haas> .

<http://w3id.org/polifonia/pilot/meetups/1059399/0/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He was a respected musicologist, as well as an accomplished pianist and conductor." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1059399/0> ;
        ont:person        <http://dbpedia.org/resource/Conducting> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1059399/0/2> ;
        ont:subject       <http://dbpedia.org/resource/Karl_Haas> .

<http://w3id.org/polifonia/pilot/meetups/1059399/4/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "The show was eventually syndicated to commercial and public radio stations around the world and became the world's most widely listened-to classical music radio program." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1059399/4> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1059399/4/1> ;
        ont:subject       <http://dbpedia.org/resource/Karl_Haas> .

<http://w3id.org/polifonia/pilot/meetups/1059399/3>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/1059399/3/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/1059399/3/1> .

<http://w3id.org/polifonia/pilot/meetups/1059399/7/1>
        rdf:type            ont:Sentence ;
        ont:excerpt         "He died at the age of 91 on 6 February 2005 at a hospital in Royal Oak, Michigan." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1059399/7> ;
        ont:place           <http://dbpedia.org/resource/Michigan> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1059399/7/0> ;
        ont:subject         <http://dbpedia.org/resource/Karl_Haas> ;
        ont:timeExpression  "age of 91" , "6 February 2005" .

<http://w3id.org/polifonia/pilot/meetups/1059399/2/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He studied piano with Artur Schnabel." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1059399/2> ;
        ont:person        <http://dbpedia.org/resource/Artur_Schnabel> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1059399/2/1> ;
        ont:subject       <http://dbpedia.org/resource/Karl_Haas> .

<http://w3id.org/polifonia/pilot/meetups/1059399/6/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Death ==" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1059399/6> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1059399/6/0> ;
        ont:subject       <http://dbpedia.org/resource/Karl_Haas> .

<http://w3id.org/polifonia/pilot/meetups/1059399/4/7>
        rdf:type          ont:Sentence ;
        ont:excerpt       "President George H. W. Bush presented the award to him at the White House." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1059399/4> ;
        ont:person        <http://dbpedia.org/resource/George_W._Bush> , <http://dbpedia.org/resource/George_Gershwin> , <http://dbpedia.org/resource/President_of_the_United_States> ;
        ont:place         <http://dbpedia.org/resource/White_House> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1059399/4/6> ;
        ont:subject       <http://dbpedia.org/resource/Karl_Haas> .

<http://w3id.org/polifonia/pilot/meetups/1059399/4/12>
        rdf:type            ont:Sentence ;
        ont:excerpt         "That month, WCLV announced \"with great regret\" that it would broadcast and syndicate its last Adventures in Good Music program on June 29, 2007." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1059399/4> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1059399/4/11> ;
        ont:subject         <http://dbpedia.org/resource/Karl_Haas> ;
        ont:timeExpression  "June 29 , 2007" .

<http://w3id.org/polifonia/pilot/meetups/1059399/0/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He also published the book Inside Music." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1059399/0> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1059399/0/1> ;
        ont:subject       <http://dbpedia.org/resource/Karl_Haas> .

<http://w3id.org/polifonia/pilot/meetups/1059399/4/1>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Syndicated broadcasts of the show across the United States began in 1970 on WCLV, a Cleveland, Ohio radio station." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1059399/4> ;
        ont:place           <http://dbpedia.org/resource/Cleveland> , <http://dbpedia.org/resource/United_States> , <http://dbpedia.org/resource/Ohio> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1059399/4/0> ;
        ont:subject         <http://dbpedia.org/resource/Karl_Haas> ;
        ont:timeExpression  "1970 on" .

