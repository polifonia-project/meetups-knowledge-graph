<http://w3id.org/polifonia/pilot/meetups/1059399/2/5>
        rdf:type            ont:Sentence ;
        ont:excerpt         "He and his wife, Trudie, had two sons and one daughter." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1059399/2> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1059399/2/4> ;
        ont:subject         <http://dbpedia.org/resource/Karl_Haas> ;
        ont:timeExpression  "one daughter" .

<http://w3id.org/polifonia/pilot/meetups/1059399/5>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/1059399/5/0> .

<http://w3id.org/polifonia/pilot/meetups/1059399/4/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He started every show with his trademark greeting \"Hello everyone\", and later entitled a track of his CD with those words." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1059399/4> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1059399/4/3> ;
        ont:subject       <http://dbpedia.org/resource/Karl_Haas> .

<http://w3id.org/polifonia/pilot/meetups/1059399/2/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He first settled in Detroit, Michigan, then lived in other places, returning to Detroit near the end of his life." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1059399/2> ;
        ont:place         <http://dbpedia.org/resource/Michigan> , <http://dbpedia.org/resource/Detroit> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1059399/2/3> ;
        ont:subject       <http://dbpedia.org/resource/Karl_Haas> .

<http://w3id.org/polifonia/pilot/meetups/1059399/4/9>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1997 he became the first classical music broadcaster to be named to the National Radio Hall of Fame." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1059399/4> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1059399/4/8> ;
        ont:subject       <http://dbpedia.org/resource/Karl_Haas> .

<http://w3id.org/polifonia/pilot/meetups/1059399/0/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1996, he received an honorary degree in Doctor of Letters from Oglethorpe University." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1059399/0> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1059399/0/3> ;
        ont:subject       <http://dbpedia.org/resource/Karl_Haas> .

<http://w3id.org/polifonia/pilot/meetups/1059399/4/3>
        rdf:type            ont:Sentence ;
        ont:excerpt         "The theme music for Adventures in Good Music was the second movement from Beethoven's \"Pathétique\" Sonata (Sonata No. 8 in C minor), performed by Haas live for each program." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1059399/4> ;
        ont:person          <http://dbpedia.org/resource/Ludwig_van_Beethoven> , <http://dbpedia.org/resource/Pavel_Haas> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1059399/4/2> ;
        ont:subject         <http://dbpedia.org/resource/Karl_Haas> ;
        ont:timeExpression  "8 in" .

