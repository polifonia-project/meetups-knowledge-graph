        rdf:type          ont:Sentence ;
        ont:excerpt       "Near the end of his life, Haas returned to Detroit." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1059399/7> ;
        ont:place         <http://dbpedia.org/resource/Detroit> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1059399/7/0> ;
        ont:subject       <http://dbpedia.org/resource/Karl_Haas> .

<http://w3id.org/polifonia/pilot/meetups/1059399/2/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He studied at the Mannheim Conservatory and earned a doctorate in music literature from Heidelberg University." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1059399/2> ;
        ont:place         <http://dbpedia.org/resource/Mannheim> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1059399/2/0> ;
        ont:subject       <http://dbpedia.org/resource/Karl_Haas> .

