<http://w3id.org/polifonia/pilot/meetups/1059399/4/11>
        rdf:type            ont:Sentence ;
        ont:excerpt         "WCLV continued to syndicate recordings of his previous shows until June 2007." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1059399/4> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1059399/4/10> ;
