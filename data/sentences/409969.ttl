<http://w3id.org/polifonia/pilot/meetups/409969/5/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Price and Little Richard were both recording for Specialty Records." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/409969/5> ;
        ont:person        <http://dbpedia.org/resource/Little_Richard> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/409969/5/2> ;
        ont:subject       <http://dbpedia.org/resource/Larry_Williams> .

<http://w3id.org/polifonia/pilot/meetups/409969/28>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/409969/28/0> .

<http://w3id.org/polifonia/pilot/meetups/409969/8/8>
        rdf:type          ont:Sentence ;
        ont:excerpt       "His death was deemed suicide, though there was speculation that he was murdered because of his involvement in drugs and allegedly prostitution." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/409969/8> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/409969/8/7> ;
        ont:subject       <http://dbpedia.org/resource/Larry_Williams> .

<http://w3id.org/polifonia/pilot/meetups/409969/5/20>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Williams also recorded and released material of his own and with Watson, with some moderate chart success." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/409969/5> ;
        ont:person        <http://dbpedia.org/resource/James_Watson> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/409969/5/19> ;
        ont:subject       <http://dbpedia.org/resource/Larry_Williams> .
