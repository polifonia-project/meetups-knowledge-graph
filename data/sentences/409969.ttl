<http://w3id.org/polifonia/pilot/meetups/409969/10/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "An Illinois drummer and blues singer named Martin Albritton claimed to be Larry Williams, alive and well." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/409969/10> ;
        ont:person        <http://dbpedia.org/resource/Purple_martin> , <http://dbpedia.org/resource/Larry_Williams> ;
        ont:place         <http://dbpedia.org/resource/Illinois> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/409969/10/0> ;
        ont:subject       <http://dbpedia.org/resource/Larry_Williams> .

<http://w3id.org/polifonia/pilot/meetups/409969/5/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He was introduced to Robert Blackwell, Specialty's house producer, and was signed to the label." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/409969/5> ;
        ont:person        <http://dbpedia.org/resource/Robert_Blackwell> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/409969/5/3> ;
        ont:subject       <http://dbpedia.org/resource/Larry_Williams> .

<http://w3id.org/polifonia/pilot/meetups/409969/8/9>
        rdf:type          ont:Sentence ;
        ont:excerpt       "No suspects were arrested or charged." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/409969/8> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/409969/8/8> ;
        ont:subject       <http://dbpedia.org/resource/Larry_Williams> .

<http://w3id.org/polifonia/pilot/meetups/409969/10>
        rdf:type  ont:Paragraph ;
