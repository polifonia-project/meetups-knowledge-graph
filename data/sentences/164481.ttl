        ont:place           <http://dbpedia.org/resource/Harrison,_Arkansas> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/164481/2/0> ;
        ont:subject         <http://dbpedia.org/resource/Little_Willie_John> ;
        ont:timeExpression  "November 15 , 1937" .

<http://w3id.org/polifonia/pilot/meetups/164481/2/10>
        rdf:type            ont:Sentence ;
        ont:excerpt         "One of his biggest hits, \"Fever\" (1956) (Pop number 24, R&B number 1), sold over one million copies and was awarded a gold disc." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/164481/2> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/164481/2/9> ;
