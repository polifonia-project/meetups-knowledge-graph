        ont:timeExpression  "1956" , "One of" .

<http://w3id.org/polifonia/pilot/meetups/164481/3/15>
        rdf:type          ont:Sentence ;
        ont:excerpt       "His interment was in Detroit Memorial Park East, in Warren, Michigan." ;
