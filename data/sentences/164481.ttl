<http://w3id.org/polifonia/pilot/meetups/164481/4/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "His son Keith John is a backing vocalist for Stevie Wonder." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/164481/4> ;
        ont:person        <http://dbpedia.org/resource/Stevie_Wonder> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/164481/4/1> ;
        ont:subject       <http://dbpedia.org/resource/Little_Willie_John> .

<http://w3id.org/polifonia/pilot/meetups/164481/11>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/164481/11/0> .

<http://w3id.org/polifonia/pilot/meetups/164481/2/8>
        rdf:type            ont:Sentence ;
        ont:excerpt         "John's first recording, a version of Titus Turner's \"All Around the World\", was a hit, reaching number 5 on the Billboard R&B chart." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/164481/2> ;
        ont:person          <http://dbpedia.org/resource/Titus_Turner> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/164481/2/7> ;
        ont:subject         <http://dbpedia.org/resource/Little_Willie_John> ;
        ont:timeExpression  "5 on" .

<http://w3id.org/polifonia/pilot/meetups/164481/2/18>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Another of his songs to be covered was \"Leave My Kitten Alone\" (1959), recorded by The Beatles in 1964 and intended for their Beatles for Sale album." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/164481/2> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/164481/2/17> ;
        ont:subject         <http://dbpedia.org/resource/Little_Willie_John> ;
        ont:timeExpression  "1959" .

<http://w3id.org/polifonia/pilot/meetups/164481/8/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Singles discography ==" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/164481/8> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/164481/8/0> ;
        ont:subject       <http://dbpedia.org/resource/Little_Willie_John> .

<http://w3id.org/polifonia/pilot/meetups/164481/3/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Sammy Davis Jr. was there to crown the winner of the Miss Cavalcade of Jazz beauty contest." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/164481/3> ;
        ont:person        <http://dbpedia.org/resource/Miles_Davis> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/164481/3/1> ;
        ont:subject       <http://dbpedia.org/resource/Little_Willie_John> .

