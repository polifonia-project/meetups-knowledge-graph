        ont:person          <http://dbpedia.org/resource/Derek_Trucks> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1733334/7/1> ;
        ont:subject         <http://dbpedia.org/resource/Butch_Trucks> ;
        ont:timeExpression  "one of" .

