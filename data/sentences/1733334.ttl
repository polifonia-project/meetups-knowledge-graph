        rdf:type          ont:Sentence ;
        ont:excerpt       "Trucks laid down a powerful conventional beat while the jazz-influenced Johanson added a second layer of percussion and ad libitum cymbal flourishes, seamlessly melded into one syncopated sound." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1733334/4> ;
        ont:person        <http://dbpedia.org/resource/Ad_libitum> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1733334/4/3> ;
