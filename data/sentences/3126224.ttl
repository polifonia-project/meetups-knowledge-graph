<http://w3id.org/polifonia/pilot/meetups/3126224/3/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Hinch's drumming managed tempo and style changes from swing to jazz and rock, using a single bass drum." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/3126224/3> ;
        ont:person        <http://dbpedia.org/resource/Tempo> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/3126224/3/3> ;
        ont:subject       <http://dbpedia.org/resource/John_Hinch_(musician)> .

<http://w3id.org/polifonia/pilot/meetups/3126224/6/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Tributes included that paid by Halford, who stated, \"His style was strong, direct and unique." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/3126224/6> ;
        ont:person        <http://dbpedia.org/resource/Rob_Halford> ;
