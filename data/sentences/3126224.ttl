<http://w3id.org/polifonia/pilot/meetups/3126224/0/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "John Frederick Hinch (19 June 1947 – 29 April 2021) was a British drummer from Lichfield, Staffordshire." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/3126224/0> ;
        ont:place           <http://dbpedia.org/resource/Staffordshire> , <http://dbpedia.org/resource/Lichfield> , <http://dbpedia.org/resource/United_Kingdom> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/3126224/0/0> ;
        ont:subject         <http://dbpedia.org/resource/John_Hinch_(musician)> ;
        ont:timeExpression  "19 June 1947" , "29 April 2021" .

<http://w3id.org/polifonia/pilot/meetups/3126224/3/5>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Hinch always used traditional jazz-rock drum kits with a snare drum, single bass drum, single tom-tom, and single floor tom, in contrast to his numerous successors who used big kits and equipment." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/3126224/3> ;
        ont:person        <http://dbpedia.org/resource/Drum> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/3126224/3/4> ;
        ont:subject       <http://dbpedia.org/resource/John_Hinch_(musician)> .

<http://w3id.org/polifonia/pilot/meetups/3126224/6/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "I'll be blasting Rocka Rolla today!\"" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/3126224/6> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/3126224/6/3> ;
        ont:subject       <http://dbpedia.org/resource/John_Hinch_(musician)> .

