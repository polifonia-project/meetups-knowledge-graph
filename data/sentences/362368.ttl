        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/362368/9/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/362368/9/1> .

<http://w3id.org/polifonia/pilot/meetups/362368/4/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Johnson sang as well as whistled, and also was able to give a boisterous laugh in musical pitch." ;
