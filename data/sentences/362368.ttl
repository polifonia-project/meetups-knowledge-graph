<http://w3id.org/polifonia/pilot/meetups/362368/2/5>
        rdf:type          ont:Sentence ;
        ont:excerpt       "By the late 1870s he was making his living as a street entertainer in New York, specializing in whistling popular tunes." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/362368/2> ;
        ont:place         <http://dbpedia.org/resource/New_York_City> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/362368/2/4> ;
        ont:subject       <http://dbpedia.org/resource/George_W._Johnson_(singer)> .

<http://w3id.org/polifonia/pilot/meetups/362368/10/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "False rumors have circulated that the first black recording star died either in a racially motivated lynching, or alternatively that he was hanged after he committed murder." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/362368/10> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/362368/10/0> ;
        ont:subject       <http://dbpedia.org/resource/George_W._Johnson_(singer)> .

<http://w3id.org/polifonia/pilot/meetups/362368/6/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "They remained in the Edison and Columbia catalogs for years, although neither was as popular as his two original tunes." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/362368/6> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/362368/6/3> ;
        ont:subject       <http://dbpedia.org/resource/George_W._Johnson_(singer)> .

