<http://w3id.org/polifonia/pilot/meetups/362368/2/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "During his time with this family, he developed his musical ability and even learned to read and write, which was illegal for a black child in Virginia before the American Civil War." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/362368/2> ;
        ont:place         <http://dbpedia.org/resource/Virginia> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/362368/2/2> ;
        ont:subject       <http://dbpedia.org/resource/George_W._Johnson_(singer)> .

<http://w3id.org/polifonia/pilot/meetups/362368/5/8>
        rdf:type            ont:Sentence ;
        ont:excerpt         "At least one of his 1891 recording sessions was held at Thomas Edison's laboratory in West Orange, New Jersey." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/362368/5> ;
        ont:person          <http://dbpedia.org/resource/Thomas_Edison> ;
        ont:place           <http://dbpedia.org/resource/New_Jersey> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/362368/5/7> ;
        ont:subject         <http://dbpedia.org/resource/George_W._Johnson_(singer)> ;
        ont:timeExpression  "1891 recording" , "one of" .

<http://w3id.org/polifonia/pilot/meetups/362368/6/2>
        rdf:type            ont:Sentence ;
        ont:excerpt         "In addition to Berliner, Johnson recorded for Edison Records, Columbia, the Victor Talking Machine Company, the Chicago Talking Machine Company, Bettini and numerous other small cylinder and disc companies through the 1890s and up to 1909 or 1910." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/362368/6> ;
        ont:person          <http://dbpedia.org/resource/Gianni_Bettini> , <http://dbpedia.org/resource/Lyndon_B._Johnson> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/362368/6/1> ;
        ont:subject         <http://dbpedia.org/resource/George_W._Johnson_(singer)> ;
        ont:timeExpression  "through the 1890s" .

<http://w3id.org/polifonia/pilot/meetups/362368/21>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/362368/21/0> .

<http://w3id.org/polifonia/pilot/meetups/362368/5/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Johnson would sometimes sing the same song over and over again in the recording studio fifty or more times a day." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/362368/5> ;
        ont:person        <http://dbpedia.org/resource/Lyndon_B._Johnson> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/362368/5/1> ;
        ont:subject       <http://dbpedia.org/resource/George_W._Johnson_(singer)> .

<http://w3id.org/polifonia/pilot/meetups/362368/8/7>
        rdf:type            ont:Sentence ;
        ont:excerpt         "On April 12, 2014, a century after his death, the singer was finally recognized in a ceremony including displays, presentations and a performance by actor Larry Marshall who impersonated Johnson." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/362368/8> ;
        ont:person          <http://dbpedia.org/resource/Lyndon_B._Johnson> , <http://dbpedia.org/resource/Larry_R._Marshall> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/362368/8/6> ;
        ont:subject         <http://dbpedia.org/resource/George_W._Johnson_(singer)> ;
        ont:timeExpression  "April 12 , 2014" .

