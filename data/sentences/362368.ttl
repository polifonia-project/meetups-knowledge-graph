        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/362368/5> ;
        ont:person          <http://dbpedia.org/resource/Lyndon_B._Johnson> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/362368/5/3> ;
        ont:subject         <http://dbpedia.org/resource/George_W._Johnson_(singer)> ;
        ont:timeExpression  "at 25,000 to 50,000" , "between 1890 and 1895" , "one" .

