<http://w3id.org/polifonia/pilot/meetups/362368/4/3>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Although Johnson could whistle all the tunes of the day, one of his first recordings for both companies was a popular vaudeville novelty song called \"The Whistling Coon\"." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/362368/4> ;
        ont:person          <http://dbpedia.org/resource/Carleton_S._Coon> , <http://dbpedia.org/resource/Lyndon_B._Johnson> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/362368/4/2> ;
        ont:subject         <http://dbpedia.org/resource/George_W._Johnson_(singer)> ;
        ont:timeExpression  "one of" .

<http://w3id.org/polifonia/pilot/meetups/362368/8/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "His friend Len Spencer, now a successful artist and booking agent, hired Johnson as an office doorman." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/362368/8> ;
        ont:person        <http://dbpedia.org/resource/Lyndon_B._Johnson> , <http://dbpedia.org/resource/Len_Spencer> , <http://dbpedia.org/resource/Talent_agent> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/362368/8/1> ;
        ont:subject       <http://dbpedia.org/resource/George_W._Johnson_(singer)> .

