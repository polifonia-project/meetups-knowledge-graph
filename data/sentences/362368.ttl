<http://w3id.org/polifonia/pilot/meetups/362368/7/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Later life ==" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/362368/7> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/362368/7/0> ;
