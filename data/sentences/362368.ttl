<http://w3id.org/polifonia/pilot/meetups/362368/8/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "New recording technology enabled the pressing of thousands of duplicate records from a single master, and Johnson was no longer needed to record each copy individually." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/362368/8> ;
        ont:person        <http://dbpedia.org/resource/Lyndon_B._Johnson> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/362368/8/0> ;
        ont:subject       <http://dbpedia.org/resource/George_W._Johnson_(singer)> .

