<http://w3id.org/polifonia/pilot/meetups/362368/4/5>
        rdf:type          ont:Sentence ;
        ont:excerpt       "From this he developed the second performance that made him famous, \"The Laughing Song\"." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/362368/4> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/362368/4/4> ;
        ont:subject       <http://dbpedia.org/resource/George_W._Johnson_(singer)> .

<http://w3id.org/polifonia/pilot/meetups/362368/0/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "George Washington Johnson (c. October 1846 – January 23, 1914) was an American singer and pioneer sound recording artist, the first African-American recording star of the phonograph." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/362368/0> ;
        ont:person          <http://dbpedia.org/resource/Lyndon_B._Johnson> , <http://dbpedia.org/resource/George_Gershwin> ;
        ont:place           <http://dbpedia.org/resource/United_States> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/362368/0/0> ;
        ont:subject         <http://dbpedia.org/resource/George_W._Johnson_(singer)> ;
        ont:timeExpression  "January 23 , 1914" , "October 1846" .

<http://w3id.org/polifonia/pilot/meetups/362368/12/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "In April, 2014, his c. 1896 recording of \"The Laughing Song\" was included in the Library of Congress' National Recording Registry." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/362368/12> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/362368/12/0> ;
        ont:subject         <http://dbpedia.org/resource/George_W._Johnson_(singer)> ;
        ont:timeExpression  "1896 recording" , "2014" .

<http://w3id.org/polifonia/pilot/meetups/362368/8/4>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Johnson died from pneumonia and myocarditis in 1914 at the age of 67." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/362368/8> ;
        ont:person          <http://dbpedia.org/resource/Lyndon_B._Johnson> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/362368/8/3> ;
        ont:subject         <http://dbpedia.org/resource/George_W._Johnson_(singer)> ;
        ont:timeExpression  "myocarditis in 1914" , "age of 67" .

