        ont:subject       <http://dbpedia.org/resource/Peter_Gustav_Lejeune_Dirichlet> .

<http://w3id.org/polifonia/pilot/meetups/240002/16/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "They were accompanied to Italy by Ludwig Schläfli, who came as a translator; as he was strongly interested in mathematics, both Dirichlet and Jacobi lectured to him during the trip, and he later became an important mathematician himself." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/240002/16> ;
        ont:place         <http://dbpedia.org/resource/Italy> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/240002/16/1> ;
