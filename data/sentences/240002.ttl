<http://w3id.org/polifonia/pilot/meetups/240002/30/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Inspired by the work of his mentor in Paris, Dirichlet published in 1829 a famous memoir giving the conditions, showing for which functions the convergence of the Fourier series holds." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/240002/30> ;
        ont:place         <http://dbpedia.org/resource/Paris> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/240002/30/0> ;
        ont:subject       <http://dbpedia.org/resource/Peter_Gustav_Lejeune_Dirichlet> .

<http://w3id.org/polifonia/pilot/meetups/240002/8/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "With the support of Humboldt and Gauss, Dirichlet was offered a teaching position at the University of Breslau." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/240002/8> ;
        ont:person        <http://dbpedia.org/resource/Carl_Friedrich_Gauss> ;
        ont:place         <http://dbpedia.org/resource/Wrocław> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/240002/8/3> ;
        ont:subject       <http://dbpedia.org/resource/Peter_Gustav_Lejeune_Dirichlet> .

<http://w3id.org/polifonia/pilot/meetups/240002/18/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Kummer was called to assume his position as a professor of mathematics in Berlin." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/240002/18> ;
        ont:place         <http://dbpedia.org/resource/Berlin> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/240002/18/2> ;
        ont:subject       <http://dbpedia.org/resource/Peter_Gustav_Lejeune_Dirichlet> .

