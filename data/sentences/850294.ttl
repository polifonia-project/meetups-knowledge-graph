        rdf:type            ont:Sentence ;
        ont:excerpt         "By the 1990s Adkins had gained a cult following and began touring regularly, receiving offers from more record labels." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/850294/7> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/850294/7/0> ;
