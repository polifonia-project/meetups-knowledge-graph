        rdf:type          ont:Sentence ;
        ont:excerpt       "\"She Said\" revealed his imaginative tone in writing, in which he compared the woman of a one-night stand he had to \"a dying can of that commodity meat.\"" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/850294/6> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/850294/6/2> ;
