<http://w3id.org/polifonia/pilot/meetups/850294/11/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Recurring themes in Adkins' catalogue included sex, heartbreak, decapitation, aliens, hot dogs and poultry." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/850294/11> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/850294/11/3> ;
        ont:subject       <http://dbpedia.org/resource/Hasil_Adkins> .

