        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/850294/11/4> ;
        ont:subject       <http://dbpedia.org/resource/Hasil_Adkins> .

<http://w3id.org/polifonia/pilot/meetups/850294/6/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "\"No More Hot Dogs\" was a song about decapitating a girlfriend and keeping her head as a wall mount." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/850294/6> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/850294/6/3> ;
        ont:subject       <http://dbpedia.org/resource/Hasil_Adkins> .

<http://w3id.org/polifonia/pilot/meetups/850294/3/18>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1983 he was living with his girlfriend who was still a minor." ;
