        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/850294/7/9> ;
        ont:subject       <http://dbpedia.org/resource/Hasil_Adkins> .

<http://w3id.org/polifonia/pilot/meetups/850294/17/1>
        rdf:type            ont:Sentence ;
        ont:excerpt         "(Norton, 1993)" ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/850294/17> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/850294/17/0> ;
