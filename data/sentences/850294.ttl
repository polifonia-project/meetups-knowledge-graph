        rdf:type          ont:Sentence ;
        ont:excerpt       "Aside from his fondness for meat, Adkins claimed to have but three loves in life, \"girls, guitars, and cars." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/850294/3> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/850294/3/11> ;
