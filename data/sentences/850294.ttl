        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/850294/38> ;
        ont:person        <http://dbpedia.org/resource/Hasil_Adkins> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/850294/38/0> ;
        ont:subject       <http://dbpedia.org/resource/Hasil_Adkins> .

<http://w3id.org/polifonia/pilot/meetups/850294/9/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Adkins is often cited as an important precursor to the psychobilly genre." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/850294/9> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/850294/9/0> ;
        ont:subject       <http://dbpedia.org/resource/Hasil_Adkins> .

<http://w3id.org/polifonia/pilot/meetups/850294/7/8>
        rdf:type            ont:Sentence ;
        ont:excerpt         "He played himself as a street musician in 2004's The Heart Is Deceitful Above All Things, partially narrated The Red's Breakfast Experience and starred in a comedic horror film entitled Die You Zombie Bastards!." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/850294/7> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/850294/7/7> ;
        ont:subject         <http://dbpedia.org/resource/Hasil_Adkins> ;
        ont:timeExpression  "street musician in 2004" .

