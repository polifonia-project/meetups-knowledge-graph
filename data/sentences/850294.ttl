<http://w3id.org/polifonia/pilot/meetups/850294/3/23>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He never married." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/850294/3> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/850294/3/22> ;
