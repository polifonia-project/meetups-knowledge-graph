        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/850294/8> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/850294/8/0> ;
        ont:subject       <http://dbpedia.org/resource/Hasil_Adkins> .

<http://w3id.org/polifonia/pilot/meetups/850294/6/8>
        rdf:type          ont:Sentence ;
        ont:excerpt       "\"The early 1980s saw a resurgence in Adkins' fan base when the American punk rock band The Cramps did a remake of Adkins' \"She Said\"." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/850294/6> ;
        ont:place         <http://dbpedia.org/resource/United_States> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/850294/6/7> ;
        ont:subject       <http://dbpedia.org/resource/Hasil_Adkins> .

<http://w3id.org/polifonia/pilot/meetups/850294/3/22>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Adkins was said to have suffered from manic depression and insomnia among other mental illnesses." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/850294/3> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/850294/3/21> ;
        ont:subject       <http://dbpedia.org/resource/Hasil_Adkins> .

<http://w3id.org/polifonia/pilot/meetups/850294/38/0>
