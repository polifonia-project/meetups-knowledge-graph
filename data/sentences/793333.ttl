<http://w3id.org/polifonia/pilot/meetups/793333/2/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "His father, an auto mechanic, played the clarinet, performing on the weekends." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/793333/2> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/793333/2/0> ;
        ont:subject       <http://dbpedia.org/resource/Jimmy_Heath> .

<http://w3id.org/polifonia/pilot/meetups/793333/6/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "He notably played in a jazz concert at the White House, when President Bill Clinton borrowed his saxophone for one number." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/793333/6> ;
        ont:person          <http://dbpedia.org/resource/Bill_Clinton> , <http://dbpedia.org/resource/President_of_the_United_States> ;
        ont:place           <http://dbpedia.org/resource/White_House> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/793333/6/0> ;
        ont:subject         <http://dbpedia.org/resource/Jimmy_Heath> ;
        ont:timeExpression  "one number" .

