<http://w3id.org/polifonia/pilot/meetups/52267409/0/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Anna King (December 9, 1937 â€“ October 21, 2002) was an American soul and gospel singer who performed with both James Brown and Duke Ellington." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/52267409/0> ;
        ont:person          <http://dbpedia.org/resource/Anna_King_(artist)> , <http://dbpedia.org/resource/Duke_Ellington> , <http://dbpedia.org/resource/James_Brown> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/52267409/0/0> ;
        ont:subject         <http://dbpedia.org/resource/Anna_King_(singer)> ;
        ont:timeExpression  "October 21 , 2002" , "December 9 , 1937" .

<http://w3id.org/polifonia/pilot/meetups/52267409/2/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1963, she auditioned successfully to replace Tammy Montgomery (later known as Tammi Terrell) in James Brown's touring revue." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/52267409/2> ;
        ont:person        <http://dbpedia.org/resource/James_Brown> , <http://dbpedia.org/resource/Tammy_Duckworth> , <http://dbpedia.org/resource/Tammi_Terrell> , <http://dbpedia.org/resource/Bernard_Montgomery> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/52267409/2/3> ;
        ont:subject       <http://dbpedia.org/resource/Anna_King_(singer)> .

<http://w3id.org/polifonia/pilot/meetups/52267409/2/14>
        rdf:type          ont:Sentence ;
        ont:excerpt       "She died in Philadelphia in 2002, aged 64." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/52267409/2> ;
        ont:place         <http://dbpedia.org/resource/Philadelphia> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/52267409/2/13> ;
        ont:subject       <http://dbpedia.org/resource/Anna_King_(singer)> .

<http://w3id.org/polifonia/pilot/meetups/52267409/1>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/52267409/1/0> ;
