<http://w3id.org/polifonia/pilot/meetups/52267409/2/13>
        rdf:type          ont:Sentence ;
        ont:excerpt       "By the mid-1970s, she gave up singing and turned to preaching, as an ordained minister of the church." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/52267409/2> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/52267409/2/12> ;
        ont:subject       <http://dbpedia.org/resource/Anna_King_(singer)> .

<http://w3id.org/polifonia/pilot/meetups/52267409/2/3>
        rdf:type            ont:Sentence ;
        ont:excerpt         "She toured with Chuck Jackson and the Shirelles, and recorded for Dixon's own Ludix label in 1962." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/52267409/2> ;
        ont:person          <http://dbpedia.org/resource/Chuck_Jackson> , <http://dbpedia.org/resource/Willie_Dixon> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/52267409/2/2> ;
        ont:subject         <http://dbpedia.org/resource/Anna_King_(singer)> ;
        ont:timeExpression  "label in 1962" .

<http://w3id.org/polifonia/pilot/meetups/52267409/2/9>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Released on the End label, it was written and produced by Ronald Moseley and Robert Bateman, but was not a hit." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/52267409/2> ;
        ont:person        <http://dbpedia.org/resource/Robert_Bateman_(painter)> ;
        ont:place         <http://dbpedia.org/resource/Moseley> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/52267409/2/8> ;
        ont:subject       <http://dbpedia.org/resource/Anna_King_(singer)> .

