<http://w3id.org/polifonia/pilot/meetups/52267409/2/8>
        rdf:type          ont:Sentence ;
        ont:excerpt       "After releasing two more singles, King left the James Brown revue in late 1964, and recorded \"Mama's Got a Bag of Her Own\", an answer record to Brown's \"Papa's Got a Brand New Bag\"." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/52267409/2> ;
        ont:person        <http://dbpedia.org/resource/James_Brown> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/52267409/2/7> ;
        ont:subject       <http://dbpedia.org/resource/Anna_King_(singer)> .

<http://w3id.org/polifonia/pilot/meetups/52267409/2/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He persuaded her to record secular music, and she recorded two unsuccessful singles in 1961, released on the Malibu label." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/52267409/2> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/52267409/2/1> ;
        ont:subject       <http://dbpedia.org/resource/Anna_King_(singer)> .

<http://w3id.org/polifonia/pilot/meetups/52267409/2/12>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In the late 1960s she joined the Brockington Choral Ensemble, a gospel choir, and recorded with them on the Arctic and Hob labels." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/52267409/2> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/52267409/2/11> ;
        ont:subject       <http://dbpedia.org/resource/Anna_King_(singer)> .

