        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/52267409/1> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/52267409/1/0> ;
        ont:subject       <http://dbpedia.org/resource/Anna_King_(singer)> .

<http://w3id.org/polifonia/pilot/meetups/52267409/2/6>
        rdf:type            ont:Sentence ;
        ont:excerpt         "As a follow-up, she recorded a duet with Famous Flames member Bobby Byrd, \"Baby Baby Baby\", which rose to number 52 on the pop chart later that year." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/52267409/2> ;
        ont:person          <http://dbpedia.org/resource/Bobby_Byrd> ;
        ont:place           <http://dbpedia.org/resource/Pennsylvania_State_Game_Lands_Number_52> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/52267409/2/5> ;
        ont:subject         <http://dbpedia.org/resource/Anna_King_(singer)> ;
        ont:timeExpression  "52 on" .

<http://w3id.org/polifonia/pilot/meetups/52267409/2/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "She was born Anna Dolores Williams in Philadelphia, and grew up singing in her church." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/52267409/2> ;
        ont:person        <http://dbpedia.org/resource/Anna_the_Prophetess> ;
        ont:place         <http://dbpedia.org/resource/Dolores_Hidalgo> , <http://dbpedia.org/resource/Philadelphia> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/52267409/2/0> ;
        ont:subject       <http://dbpedia.org/resource/Anna_King_(singer)> .

<http://w3id.org/polifonia/pilot/meetups/52267409/2/10>
        rdf:type          ont:Sentence ;
        ont:excerpt       "She then formed a band with another former member of Brown's revue, Sam Lathan, and toured for a year with other soul musicians, before giving up the secular music business." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/52267409/2> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/52267409/2/9> ;
        ont:subject       <http://dbpedia.org/resource/Anna_King_(singer)> .

