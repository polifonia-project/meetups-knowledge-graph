<http://w3id.org/polifonia/pilot/meetups/579599/3/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Soon, however, he was playing piano and singing in local taverns." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/579599/3> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/579599/3/0> ;
