<http://w3id.org/polifonia/pilot/meetups/579599/8/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "==" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/579599/8> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/579599/8/1> ;
        ont:subject       <http://dbpedia.org/resource/Gene_Austin> .

<http://w3id.org/polifonia/pilot/meetups/579599/3/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "The act ended when Bergere married." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/579599/3> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/579599/3/2> ;
        ont:subject       <http://dbpedia.org/resource/Gene_Austin> .

<http://w3id.org/polifonia/pilot/meetups/579599/10>
