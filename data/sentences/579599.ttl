<http://w3id.org/polifonia/pilot/meetups/579599/3/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "On returning to the United States in 1919, Austin settled in Baltimore, Maryland, where he briefly studied dentistry and law." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/579599/3> ;
        ont:place         <http://dbpedia.org/resource/Maryland> , <http://dbpedia.org/resource/United_States> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/579599/3/0> ;
        ont:subject       <http://dbpedia.org/resource/Gene_Austin> .

<http://w3id.org/polifonia/pilot/meetups/579599/17/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1962, he campaigned unsuccessfully for the Democratic nomination for governor of Nevada." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/579599/17> ;
        ont:place         <http://dbpedia.org/resource/Nevada> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/579599/17/0> ;
