        rdf:type            ont:Sentence ;
        ont:excerpt         "After cutting a handful of unsuccessful pop vocal recordings for labels such as Fraternity and Torch, Braun signed to Decca Records and charted his only Top 40 hit, \"Till Death Do Us Part\", in 1962." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1869640/4> ;
        ont:person          <http://dbpedia.org/resource/Scooter_Braun> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1869640/4/2> ;
        ont:subject         <http://dbpedia.org/resource/Bob_Braun> ;
