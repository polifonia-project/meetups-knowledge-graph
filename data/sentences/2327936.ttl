<http://w3id.org/polifonia/pilot/meetups/2327936/0/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Vern Yocum (April 15, 1909 - September 13, 1991) is best known as copyist and librarian for Frank Sinatra, Nat King Cole, and Nelson Riddle." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/2327936/0> ;
        ont:person          <http://dbpedia.org/resource/Nelson_Riddle> , <http://dbpedia.org/resource/Nat_King_Cole> , <http://dbpedia.org/resource/Frank_Sinatra> , <http://dbpedia.org/resource/Librarian> , <http://dbpedia.org/resource/Vern_Yocum> , <http://dbpedia.org/resource/Copyist> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/2327936/0/0> ;
        ont:subject         <http://dbpedia.org/resource/Vern_Yocum> ;
        ont:timeExpression  "September 13 , 1991" , "April 15 , 1909" .

<http://w3id.org/polifonia/pilot/meetups/2327936/3>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/2327936/3/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/2327936/3/1> ;
        rdf:_3    <http://w3id.org/polifonia/pilot/meetups/2327936/3/2> ;
        rdf:_4    <http://w3id.org/polifonia/pilot/meetups/2327936/3/3> .

<http://w3id.org/polifonia/pilot/meetups/2327936/2/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "By 1950 he had completely transitioned into this field after managing and playing with The Bob Keane (Keene) Band." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2327936/2> ;
        ont:person        <http://dbpedia.org/resource/Bob_Keane> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2327936/2/3> ;
        ont:subject       <http://dbpedia.org/resource/Vern_Yocum> .

<http://w3id.org/polifonia/pilot/meetups/2327936/3/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "His collection resides at the University of Arizona." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2327936/3> ;
        ont:place         <http://dbpedia.org/resource/Arizona> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2327936/3/2> ;
        ont:subject       <http://dbpedia.org/resource/Vern_Yocum> .

<http://w3id.org/polifonia/pilot/meetups/2327936/2>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/2327936/2/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/2327936/2/1> ;
