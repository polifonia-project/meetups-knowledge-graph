<http://w3id.org/polifonia/pilot/meetups/1081839/0/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Shirley Alston Reeves (born June 10, 1941 in Henderson, North Carolina), born Shirley Owens, is an American soul singer who was the main lead singer of the hit girl group the Shirelles." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1081839/0> ;
        ont:person          <http://dbpedia.org/resource/Shirley_Owens> ;
        ont:place           <http://dbpedia.org/resource/Henderson,_North_Carolina> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1081839/0/0> ;
        ont:subject         <http://dbpedia.org/resource/Shirley_Owens> ;
        ont:timeExpression  "June 10 , 1941" .

<http://w3id.org/polifonia/pilot/meetups/1081839/1/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "She left the Shirelles in 1975 to begin a solo career, initially recording under the name \"Lady Rose\"." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1081839/1> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1081839/1/0> ;
        ont:subject       <http://dbpedia.org/resource/Shirley_Owens> .

