<http://w3id.org/polifonia/pilot/meetups/56386/40/1>
        rdf:type            ont:Sentence ;
        ont:excerpt         "While undergoing dialysis and awaiting a kidney transplant in May 2003, White suffered a severe stroke, which forced him to retire from public life." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/56386/40> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/56386/40/0> ;
        ont:subject         <http://dbpedia.org/resource/Barry_White> ;
        ont:timeExpression  "May 2003" .

<http://w3id.org/polifonia/pilot/meetups/56386/1/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "White recorded 19 studio albums during the course of his career, but multiple versions and compilations were released worldwide that were certified gold, 41 of which also attained platinum status." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/56386/1> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/56386/1/0> ;
        ont:subject         <http://dbpedia.org/resource/Barry_White> ;
        ont:timeExpression  "19 studio" , "41 of" .

<http://w3id.org/polifonia/pilot/meetups/56386/13/6>
