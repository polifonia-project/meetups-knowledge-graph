<http://w3id.org/polifonia/pilot/meetups/56386/0/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Barry Eugene Carter (September 12, 1944  July 4, 2003), better known by his stage name Barry White, was an American singer-songwriter, musician, record producer and composer." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/56386/0> ;
        ont:person          <http://dbpedia.org/resource/Composer> , <http://dbpedia.org/resource/Barry_White> ;
