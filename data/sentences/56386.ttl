        ont:subject         <http://dbpedia.org/resource/Barry_White> ;
        ont:timeExpression  "1999 by" .

<http://w3id.org/polifonia/pilot/meetups/56386/12/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "The album, 1972's From A Girl's Point of View We Give to You... Love Unlimited, became the first of White's string of long-titled albums and singles." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/56386/12> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/56386/12/3> ;
