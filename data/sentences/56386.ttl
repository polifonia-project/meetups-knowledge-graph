        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/56386/0/0> ;
        ont:subject         <http://dbpedia.org/resource/Barry_White> ;
        ont:timeExpression  "September 12 , 1944" , "July 4 , 2003" .

<http://w3id.org/polifonia/pilot/meetups/56386/63/0>
        rdf:type            ont:Sentence ;
