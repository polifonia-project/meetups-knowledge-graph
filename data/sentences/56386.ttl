        ont:subject         <http://dbpedia.org/resource/Barry_White> ;
        ont:timeExpression  "July 4 , 2003" .

<http://w3id.org/polifonia/pilot/meetups/56386/15/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1973, White created The Love Unlimited Orchestra, a 40-piece orchestral group to be used originally as a backing band for the girl-group Love Unlimited." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/56386/15> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/56386/15/0> ;
        ont:subject       <http://dbpedia.org/resource/Barry_White> .

