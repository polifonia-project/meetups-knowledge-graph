        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/56386/13> ;
        ont:person          <http://dbpedia.org/resource/Glodean_White> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/56386/13/7> ;
        ont:subject         <http://dbpedia.org/resource/Barry_White> ;
