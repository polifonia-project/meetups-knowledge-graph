<http://w3id.org/polifonia/pilot/meetups/56386/13/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "White produced, wrote and arranged their classic soul ballad \"Walkin' in the Rain with the One I Love\", which climbed to No. 14 in the Billboard Hot 100 Pop chart and No. 6 on the Billboard R&B chart in late 1972." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/56386/13> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/56386/13/0> ;
        ont:subject         <http://dbpedia.org/resource/Barry_White> ;
        ont:timeExpression  "with the One" , "Hot 100" , "14 in" , "6 on" .

<http://w3id.org/polifonia/pilot/meetups/56386/45/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "White was nominated for 11 Grammy Awards; winning two for Staying Power at the 42nd Annual Grammy Awards in 2000." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/56386/45> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/56386/45/0> ;
        ont:subject         <http://dbpedia.org/resource/Barry_White> ;
        ont:timeExpression  "two for" , "at the 42nd" .

<http://w3id.org/polifonia/pilot/meetups/56386/30/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "White also provided voice-over for Arby's Restaurant commercials on television and radio to promote its Market Fresh menu." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/56386/30> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/56386/30/1> ;
        ont:subject       <http://dbpedia.org/resource/Barry_White> .

