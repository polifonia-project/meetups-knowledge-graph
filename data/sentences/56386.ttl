<http://w3id.org/polifonia/pilot/meetups/56386/31/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Personal life ==" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/56386/31> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/56386/31/0> ;
