        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/238175/14/10> ;
        ont:subject       <http://dbpedia.org/resource/Horace_Silver> .

<http://w3id.org/polifonia/pilot/meetups/238175/14/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Silver stated in the same year that he had reduced his touring to four months a year, so that he could spend more time with his son." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/238175/14> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/238175/14/0> ;
        ont:subject       <http://dbpedia.org/resource/Horace_Silver> .

<http://w3id.org/polifonia/pilot/meetups/238175/31/3>
        rdf:type            ont:Sentence ;
