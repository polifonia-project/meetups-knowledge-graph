<http://w3id.org/polifonia/pilot/meetups/238175/10/19>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Recordings and personnel changes – sometimes expanding the band to a sextet – continued in the mid-1960s." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/238175/10> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/238175/10/18> ;
