<http://w3id.org/polifonia/pilot/meetups/238175/17/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 2005, the National Academy of Recording Arts and Sciences awarded him its President's Merit Award." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/238175/17> ;
        ont:person        <http://dbpedia.org/resource/President_of_the_United_States> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/238175/17/1> ;
        ont:subject       <http://dbpedia.org/resource/Horace_Silver> .

<http://w3id.org/polifonia/pilot/meetups/238175/12/13>
        rdf:type          ont:Sentence ;
        ont:excerpt       "The personnel in his band continued to change, and continued to contain young musicians who made telling contributions." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/238175/12> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/238175/12/12> ;
        ont:subject       <http://dbpedia.org/resource/Horace_Silver> .

<http://w3id.org/polifonia/pilot/meetups/238175/3>
        rdf:type  ont:Paragraph ;
