<http://w3id.org/polifonia/pilot/meetups/238175/30/3>
        rdf:type            ont:Sentence ;
        ont:excerpt         "ISBN 978-0-19-993739-4." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/238175/30> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/238175/30/2> ;
        ont:subject         <http://dbpedia.org/resource/Horace_Silver> ;
        ont:timeExpression  "ISBN 978-0-19-993739-4" .

<http://w3id.org/polifonia/pilot/meetups/238175/8/16>
        rdf:type          ont:Sentence ;
        ont:excerpt       "This set of studio and concert recordings was pivotal in the development and defining of hard bop, which combined elements of blues, gospel, and R&B, with bebop-based harmony and rhythm." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/238175/8> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/238175/8/15> ;
        ont:subject       <http://dbpedia.org/resource/Horace_Silver> .

<http://w3id.org/polifonia/pilot/meetups/238175/2/2>
        rdf:type            ont:Sentence ;
        ont:excerpt         "The last two of these were often combined, resulting in commercially unsuccessful releases such as The United States of Mind series." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/238175/2> ;
        ont:place           <http://dbpedia.org/resource/United_States> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/238175/2/1> ;
        ont:subject         <http://dbpedia.org/resource/Horace_Silver> ;
        ont:timeExpression  "two of" .

<http://w3id.org/polifonia/pilot/meetups/238175/37/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Horace Silver Discography at the Hard Bop Home Page" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/238175/37> ;
