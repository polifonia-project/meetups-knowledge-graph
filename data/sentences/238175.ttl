        rdf:type          ont:Sentence ;
        ont:excerpt       "Around this time, according to saxophonist Dave Liebman, Silver's reputation among aspiring young jazz musicians was that he was \"a little – not commercial, but not quite the real deal [in jazz].\"" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/238175/12> ;
        ont:person        <http://dbpedia.org/resource/Dave_Liebman> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/238175/12/8> ;
