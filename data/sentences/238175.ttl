        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/238175/33/0> ;
        ont:subject         <http://dbpedia.org/resource/Horace_Silver> ;
        ont:timeExpression  "2006" .

<http://w3id.org/polifonia/pilot/meetups/238175/14/6>
        rdf:type          ont:Sentence ;
        ont:excerpt       "His band for performances in the UK and elsewhere in 1987 included trumpeter Dave Douglas and saxophonist Vincent Herring." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/238175/14> ;
        ont:person        <http://dbpedia.org/resource/Vincent_Herring> , <http://dbpedia.org/resource/Dave_Douglas_(trumpeter)> ;
        ont:place         <http://dbpedia.org/resource/United_Kingdom> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/238175/14/5> ;
        ont:subject       <http://dbpedia.org/resource/Horace_Silver> .

<http://w3id.org/polifonia/pilot/meetups/238175/10/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Later that year, he left Blakey after one and a half years, in part because of the heroin use prevalent in the band, which Silver did not want to be involved in." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/238175/10> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/238175/10/0> ;
        ont:subject       <http://dbpedia.org/resource/Horace_Silver> .

<http://w3id.org/polifonia/pilot/meetups/238175/27>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/238175/27/0> .

<http://w3id.org/polifonia/pilot/meetups/238175/15/6>
        rdf:type            ont:Sentence ;
        ont:excerpt         "One continuation from his early career was that Silver recorded his own compositions for his later albums and they were typically new, rather than re-workings of previous releases." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/238175/15> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/238175/15/5> ;
        ont:subject         <http://dbpedia.org/resource/Horace_Silver> ;
        ont:timeExpression  "One continuation" .

