        rdf:type          ont:Sentence ;
        ont:excerpt       "Later life ==" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/3620666/13> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/3620666/13/0> ;
        ont:subject       <http://dbpedia.org/resource/Dorothy_Gibson> .

<http://w3id.org/polifonia/pilot/meetups/3620666/8/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Saved From the Titanic, released a month after the sinking, was the first of many films about the event." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/3620666/8> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/3620666/8/0> ;
        ont:subject       <http://dbpedia.org/resource/Dorothy_Gibson> .

<http://w3id.org/polifonia/pilot/meetups/3620666/19>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/3620666/19/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/3620666/19/1> .

<http://w3id.org/polifonia/pilot/meetups/3620666/17/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "The character of Susan Alexander in Orson Wellesâ Citizen Kane (1941) may have been partly based on Dorothy, along with other real-life figures Marion Davies, Hope Hampton, and Ganna Walska." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/3620666/17> ;
        ont:person          <http://dbpedia.org/resource/Ganna_Walska> , <http://dbpedia.org/resource/Alexander_the_Great> , <http://dbpedia.org/resource/Marion_Davies> ;
