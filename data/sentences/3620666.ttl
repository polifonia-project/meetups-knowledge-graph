<http://w3id.org/polifonia/pilot/meetups/3620666/9/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "The women had been playing bridge with friends in the lounge on the night of the ship's fatal collision with the iceberg." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/3620666/9> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/3620666/9/1> ;
        ont:subject       <http://dbpedia.org/resource/Dorothy_Gibson> .

<http://w3id.org/polifonia/pilot/meetups/3620666/17/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "She was also the inspiration for a character in her friend Indro Montanelliâs novel General della" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/3620666/17> ;
        ont:person        <http://dbpedia.org/resource/General_officer> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/3620666/17/0> ;
        ont:subject       <http://dbpedia.org/resource/Dorothy_Gibson> .

<http://w3id.org/polifonia/pilot/meetups/3620666/12/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Brulatour married film actress Hope Hampton in 1923." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/3620666/12> ;
        ont:place         <http://dbpedia.org/resource/Hampton,_Virginia> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/3620666/12/1> ;
        ont:subject       <http://dbpedia.org/resource/Dorothy_Gibson> .

<http://w3id.org/polifonia/pilot/meetups/3620666/2/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "She was also a regular chorus member in shows produced by the Shubert Brothers at the Hippodrome Theatre." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/3620666/2> ;
        ont:place         <http://dbpedia.org/resource/New_York_Hippodrome> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/3620666/2/2> ;
        ont:subject       <http://dbpedia.org/resource/Dorothy_Gibson> .

<http://w3id.org/polifonia/pilot/meetups/3620666/16/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Salvaged by collectors David and Margo Navone in 2001, it was preserved by the American Film Institute and is now archived at the Library of Congress." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/3620666/16> ;
        ont:person        <http://dbpedia.org/resource/Margo_Winkler> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/3620666/16/0> ;
        ont:subject       <http://dbpedia.org/resource/Dorothy_Gibson> .

<http://w3id.org/polifonia/pilot/meetups/3620666/11/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "A year later, while driving Brulatour's sports car in New York, Dorothy struck and killed a pedestrian." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/3620666/11> ;
        ont:place         <http://dbpedia.org/resource/New_York_City> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/3620666/11/1> ;
        ont:subject       <http://dbpedia.org/resource/Dorothy_Gibson> .

