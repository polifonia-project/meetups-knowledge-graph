<http://w3id.org/polifonia/pilot/meetups/3620666/9/5>
        rdf:type          ont:Sentence ;
        ont:excerpt       "She not only starred in the one-reel drama but also wrote the scenario." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/3620666/9> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/3620666/9/4> ;
