<http://w3id.org/polifonia/pilot/meetups/52557970/0/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Ricky Shayne (born 4 June 1944) is a pop singer and an actor of French and Lebanese descent who was popular in Europe in the 1960s, especially in Germanophone countries." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/52557970/0> ;
        ont:person          <http://dbpedia.org/resource/Ricky_Shayne> ;
        ont:place           <http://dbpedia.org/resource/Europe> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/52557970/0/0> ;
        ont:subject         <http://dbpedia.org/resource/Ricky_Shayne> ;
        ont:timeExpression  "in the 1960s" , "4 June 1944" .

