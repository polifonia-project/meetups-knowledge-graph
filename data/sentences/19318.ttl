        rdf:type          ont:Sentence ;
        ont:excerpt       "==" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/19318/20> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/19318/20/1> ;
