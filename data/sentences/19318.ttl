        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/19318/10/1> ;
        ont:subject       <http://dbpedia.org/resource/Marilyn_Monroe> .

<http://w3id.org/polifonia/pilot/meetups/19318/40/5>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Monroe often wore white to emphasize her blondness and drew attention by wearing revealing outfits that showed off her figure." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/19318/40> ;
        ont:person        <http://dbpedia.org/resource/Greg_Monroe> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/19318/40/4> ;
        ont:subject       <http://dbpedia.org/resource/Marilyn_Monroe> .

<http://w3id.org/polifonia/pilot/meetups/19318/3/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "She dedicated 1955 to building the company and began studying method acting under Lee Strasberg at the Actors Studio." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/19318/3> ;
        ont:person        <http://dbpedia.org/resource/Lee_Strasberg> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/19318/3/0> ;
        ont:subject       <http://dbpedia.org/resource/Marilyn_Monroe> .

<http://w3id.org/polifonia/pilot/meetups/19318/17/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Rising star ===" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/19318/17> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/19318/17/1> ;
        ont:subject       <http://dbpedia.org/resource/Marilyn_Monroe> .

<http://w3id.org/polifonia/pilot/meetups/19318/27/3>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Bus Stop was released in August 1956 and became critical and commercial success." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/19318/27> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/19318/27/2> ;
        ont:subject         <http://dbpedia.org/resource/Marilyn_Monroe> ;
        ont:timeExpression  "August 1956" .

<http://w3id.org/polifonia/pilot/meetups/19318/8/15>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Monroe was otherwise a mediocre student, but excelled in writing and contributed to the school newspaper." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/19318/8> ;
        ont:person        <http://dbpedia.org/resource/Greg_Monroe> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/19318/8/14> ;
        ont:subject       <http://dbpedia.org/resource/Marilyn_Monroe> .

<http://w3id.org/polifonia/pilot/meetups/19318/21/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Monroe had become one of 20th Century-Fox's biggest stars, but her contract had not changed since 1950, meaning that she was paid far less than other stars of her stature and could not choose her projects." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/19318/21> ;
        ont:person          <http://dbpedia.org/resource/Greg_Monroe> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/19318/21/0> ;
        ont:subject         <http://dbpedia.org/resource/Marilyn_Monroe> ;
        ont:timeExpression  "one of" .

<http://w3id.org/polifonia/pilot/meetups/19318/7/14>
        rdf:type          ont:Sentence ;
        ont:excerpt       "They shared the house with lodgers, actors George and Maude Atkinson and their daughter, Nellie." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/19318/7> ;
        ont:person        <http://dbpedia.org/resource/George_Gershwin> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/19318/7/13> ;
        ont:subject       <http://dbpedia.org/resource/Marilyn_Monroe> .

<http://w3id.org/polifonia/pilot/meetups/19318/36>
