        rdf:type          ont:Sentence ;
        ont:excerpt       "She took classes with Constance Collier and attended workshops on method acting at the Actors Studio, run by Lee Strasberg." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/19318/24> ;
        ont:person        <http://dbpedia.org/resource/Constance_Collier> , <http://dbpedia.org/resource/Lee_Strasberg> ;
