<http://w3id.org/polifonia/pilot/meetups/19318/11/10>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Despite her enthusiasm, her teachers thought her too shy and insecure to have a future in acting, and Fox did not renew her contract in August 1947." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/19318/11> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/19318/11/9> ;
        ont:subject         <http://dbpedia.org/resource/Marilyn_Monroe> ;
        ont:timeExpression  "August 1947" .

<http://w3id.org/polifonia/pilot/meetups/19318/8/20>
        rdf:type            ont:Sentence ;
        ont:excerpt         "As a solution, she married their neighbors' 21-year-old son, factory worker James Dougherty, on June 19, 1942, just after her 16th birthday." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/19318/8> ;
        ont:person          <http://dbpedia.org/resource/LeBron_James> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/19318/8/19> ;
        ont:subject         <http://dbpedia.org/resource/Marilyn_Monroe> ;
        ont:timeExpression  "16th birthday" , "June 19 , 1942" .

<http://w3id.org/polifonia/pilot/meetups/19318/7/9>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Although Gladys was mentally and financially unprepared for a child, Monroe's early childhood was stable and happy." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/19318/7> ;
        ont:person        <http://dbpedia.org/resource/Greg_Monroe> , <http://dbpedia.org/resource/Mary_Pickford> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/19318/7/8> ;
        ont:subject       <http://dbpedia.org/resource/Marilyn_Monroe> .

<http://w3id.org/polifonia/pilot/meetups/19318/41>
        rdf:type  ont:Paragraph ;
