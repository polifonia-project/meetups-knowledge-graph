        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/19318/33/3> ;
        ont:subject         <http://dbpedia.org/resource/Marilyn_Monroe> ;
        ont:timeExpression  "May 19" .

<http://w3id.org/polifonia/pilot/meetups/19318/16/9>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Monroe's problems have been attributed to a combination of perfectionism, low self-esteem, and stage fright." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/19318/16> ;
        ont:person        <http://dbpedia.org/resource/Greg_Monroe> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/19318/16/8> ;
        ont:subject       <http://dbpedia.org/resource/Marilyn_Monroe> .

<http://w3id.org/polifonia/pilot/meetups/19318/10/6>
        rdf:type          ont:Sentence ;
        ont:excerpt       "To make herself more employable, she straightened her hair and dyed it blonde." ;
