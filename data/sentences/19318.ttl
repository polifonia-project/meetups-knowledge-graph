        ont:excerpt       "Monroe was also briefly hospitalized due to a barbiturate overdose." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/19318/28> ;
        ont:person        <http://dbpedia.org/resource/Greg_Monroe> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/19318/28/8> ;
        ont:subject       <http://dbpedia.org/resource/Marilyn_Monroe> .

<http://w3id.org/polifonia/pilot/meetups/19318/12/5>
        rdf:type            ont:Sentence ;
        ont:excerpt         "She began working with the studio's head drama coach, Natasha Lytess, who would remain her mentor until 1955." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/19318/12> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/19318/12/4> ;
        ont:subject         <http://dbpedia.org/resource/Marilyn_Monroe> ;
        ont:timeExpression  "mentor until 1955" .

<http://w3id.org/polifonia/pilot/meetups/19318/22/6>
        rdf:type            ont:Sentence ;
        ont:excerpt         "In April 1954, Otto Preminger's western River of No Return, the last film that Monroe had filmed prior to the suspension, was released." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/19318/22> ;
        ont:person          <http://dbpedia.org/resource/Greg_Monroe> , <http://dbpedia.org/resource/Otto_Preminger> ;
        ont:place           <http://dbpedia.org/resource/Western_River,_South_Australia> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/19318/22/5> ;
        ont:subject         <http://dbpedia.org/resource/Marilyn_Monroe> ;
        ont:timeExpression  "April 1954" .

<http://w3id.org/polifonia/pilot/meetups/19318/32/7>
        rdf:type          ont:Sentence ;
        ont:excerpt       "She underwent a cholecystectomy and surgery for her endometriosis, and spent four weeks hospitalized for depression." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/19318/32> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/19318/32/6> ;
