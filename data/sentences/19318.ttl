<http://w3id.org/polifonia/pilot/meetups/19318/8/21>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Monroe subsequently dropped out of high school and became a housewife." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/19318/8> ;
