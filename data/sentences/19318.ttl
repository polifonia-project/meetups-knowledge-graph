<http://w3id.org/polifonia/pilot/meetups/19318/35/9>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Empty medicine bottles were found next to her bed." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/19318/35> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/19318/35/8> ;
