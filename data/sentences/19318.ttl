        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/19318/37/0> ;
        ont:subject       <http://dbpedia.org/resource/Marilyn_Monroe> .

<http://w3id.org/polifonia/pilot/meetups/19318/47/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Legacy ==" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/19318/47> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/19318/47/0> ;
        ont:subject       <http://dbpedia.org/resource/Marilyn_Monroe> .

<http://w3id.org/polifonia/pilot/meetups/19318/0/1>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Famous for playing comedic \"blonde bombshell\" characters, she became one of the most popular sex symbols of the 1950s and early 1960s and was emblematic of the era's sexual revolution." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/19318/0> ;
        ont:person          <http://dbpedia.org/resource/Sex_symbol> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/19318/0/0> ;
        ont:subject         <http://dbpedia.org/resource/Marilyn_Monroe> ;
        ont:timeExpression  "of the 1950s" , "one of" .

<http://w3id.org/polifonia/pilot/meetups/19318/8/11>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Some of my foster families used to send me to the movies to get me out of the house and there I'd sit all day and way into the night." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/19318/8> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/19318/8/10> ;
        ont:subject       <http://dbpedia.org/resource/Marilyn_Monroe> .

<http://w3id.org/polifonia/pilot/meetups/19318/24/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "She replaced her old acting coach, Natasha Lytess, with Paula; the Strasbergs remained an important influence for the rest of her career." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/19318/24> ;
        ont:person        <http://dbpedia.org/resource/Paula_of_Rome> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/19318/24/2> ;
        ont:subject       <http://dbpedia.org/resource/Marilyn_Monroe> .

