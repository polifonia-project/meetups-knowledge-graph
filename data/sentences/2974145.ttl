        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2974145/10/0> ;
        ont:subject       <http://dbpedia.org/resource/Chuck_Jackson> .

<http://w3id.org/polifonia/pilot/meetups/2974145/8/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 2021, his song \"Any Day Now\" was used in a Volkswagen commercial." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2974145/8> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2974145/8/0> ;
