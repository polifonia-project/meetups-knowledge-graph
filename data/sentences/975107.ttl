<http://w3id.org/polifonia/pilot/meetups/975107/14/1>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Greatly inspired by Rao, Ellis sought to implement odd meters in a Western improvised context and (with Rao) co-authored the 1965 article \"An Introduction to Indian Music for the Jazz Musician\"." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/975107/14> ;
        ont:person          <http://dbpedia.org/resource/Musician> , <http://dbpedia.org/resource/Albert_Ellis> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/975107/14/0> ;
        ont:subject         <http://dbpedia.org/resource/Don_Ellis> ;
        ont:timeExpression  "1965 article" .

<http://w3id.org/polifonia/pilot/meetups/975107/38/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1974, Ellis became interested in the music of Brazil, even studying Portuguese so as to better communicate with indigenous musicians." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/975107/38> ;
        ont:person        <http://dbpedia.org/resource/Monta_Ellis> ;
        ont:place         <http://dbpedia.org/resource/Brazil> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/975107/38/0> ;
        ont:subject       <http://dbpedia.org/resource/Don_Ellis> .

<http://w3id.org/polifonia/pilot/meetups/975107/48/1>
        rdf:type            ont:Sentence ;
        ont:excerpt         "The New Rhythm Book (1972) presents methods of practice and performance in unusual meters and features a companion play-along LP/cassette entitled New Rhythms." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/975107/48> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/975107/48/0> ;
        ont:subject         <http://dbpedia.org/resource/Don_Ellis> ;
        ont:timeExpression  "1972" .

