        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/975107/44/0> ;
        ont:subject         <http://dbpedia.org/resource/Don_Ellis> ;
        ont:timeExpression  "48 on" .

<http://w3id.org/polifonia/pilot/meetups/975107/54/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "The horn section was more than halved, the string quartet was removed, a vocal quartet was added, and no electronics (save for amplification) were used to alter the band's sound." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/975107/54> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/975107/54/0> ;
        ont:subject       <http://dbpedia.org/resource/Don_Ellis> .

