<http://w3id.org/polifonia/pilot/meetups/975107/33/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Ellis's final album for Columbia, Connection, was recorded in August 1972." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/975107/33> ;
        ont:person          <http://dbpedia.org/resource/Monta_Ellis> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/975107/33/0> ;
        ont:subject         <http://dbpedia.org/resource/Don_Ellis> ;
        ont:timeExpression  "August 1972" .

<http://w3id.org/polifonia/pilot/meetups/975107/43/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "The album was thrown together and released as Music from Other Galaxies and Planets; all the songs were retitled with novelty space-related names such as \"Orion's Sword\" and \"Crypton\"." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/975107/43> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/975107/43/0> ;
        ont:subject       <http://dbpedia.org/resource/Don_Ellis> .

<http://w3id.org/polifonia/pilot/meetups/975107/53/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "These new timbres offered Ellis a wellspring of creative possibilities." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/975107/53> ;
        ont:person        <http://dbpedia.org/resource/Monta_Ellis> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/975107/53/1> ;
        ont:subject       <http://dbpedia.org/resource/Don_Ellis> .

<http://w3id.org/polifonia/pilot/meetups/975107/81>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/975107/81/0> .

<http://w3id.org/polifonia/pilot/meetups/975107/30/4>
        rdf:type          ont:Sentence ;
