        rdf:type            ont:Sentence ;
        ont:excerpt         "The album featured \"The Theme from 'The French Connection'\", an abbreviated version of Ellis's movie score, and \"Chain Reaction\", a 13/8 tour de force by longtime contributor Hank Levy." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/975107/33> ;
        ont:person          <http://dbpedia.org/resource/Monta_Ellis> , <http://dbpedia.org/resource/Hank_Levy> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/975107/33/0> ;
        ont:subject         <http://dbpedia.org/resource/Don_Ellis> ;
        ont:timeExpression  "13/8 tour" .

