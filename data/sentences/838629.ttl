<http://w3id.org/polifonia/pilot/meetups/838629/52/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Despite his illness, Albert exercised regularly until shortly before his death." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/838629/52> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/838629/52/1> ;
        ont:subject       <http://dbpedia.org/resource/Eddie_Albert> .

<http://w3id.org/polifonia/pilot/meetups/838629/20/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "A review in Broadcasting magazine panned the program, saying, \"Mr. Albert with the help of Miss Hanley, conducts an interview, talks a little, sings a little and looks all-thumbs a lot.\"" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/838629/20> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/838629/20/1> ;
