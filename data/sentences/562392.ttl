<http://w3id.org/polifonia/pilot/meetups/562392/8/2>
        rdf:type            ont:Sentence ;
        ont:excerpt         "At the age of 73, she sang with her daughter at the famous Paris Olympia and appeared on several French television shows." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/562392/8> ;
        ont:place           <http://dbpedia.org/resource/France> , <http://dbpedia.org/resource/Olympia_(Paris)> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/562392/8/1> ;
        ont:subject         <http://dbpedia.org/resource/Lucienne_Boyer> ;
        ont:timeExpression  "age of 73" .

