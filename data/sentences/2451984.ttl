<http://w3id.org/polifonia/pilot/meetups/2451984/13/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1960, Capitol Records induced Livingston to return as president and, eventually, chairman of the board." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2451984/13> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2451984/13/0> ;
        ont:subject       <http://dbpedia.org/resource/Alan_W._Livingston> .

<http://w3id.org/polifonia/pilot/meetups/2451984/26>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/2451984/26/0> .

<http://w3id.org/polifonia/pilot/meetups/2451984/9/2>
        rdf:type            ont:Sentence ;
        ont:excerpt         "When The Beatles released their 1966 album Yesterday and Today it was initially released with a surreal Robert Whitaker cover photograph depicting the group draped in slabs of meat and dismembered doll parts." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/2451984/9> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/2451984/9/1> ;
        ont:subject         <http://dbpedia.org/resource/Alan_W._Livingston> ;
        ont:timeExpression  "1966 album Yesterday" .

<http://w3id.org/polifonia/pilot/meetups/2451984/12/0>
        rdf:type          ont:Sentence ;
