        rdf:type          ont:Sentence ;
        ont:excerpt       "Livingston also wrote a novel titled Ronnie Finkelhof, Superstar about a shy Harvard pre-law student who becomes an overnight success as a rock musician." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2451984/17> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2451984/17/0> ;
