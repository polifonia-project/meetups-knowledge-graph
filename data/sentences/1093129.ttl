<http://w3id.org/polifonia/pilot/meetups/1093129/3/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Elvera Sanchez was born in New York City to Cuban immigrants Luisa Valentina (née Aguiar; February 14, 1884 – October 5, 1996) and Marco Sanchez." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1093129/3> ;
        ont:person          <http://dbpedia.org/resource/Marco_Sanchez> , <http://dbpedia.org/resource/Elvera_Sanchez> ;
        ont:place           <http://dbpedia.org/resource/New_York_City> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1093129/3/0> ;
        ont:subject         <http://dbpedia.org/resource/Elvera_Sanchez> ;
        ont:timeExpression  "February 14 , 1884" , "October 5 , 1996" .

