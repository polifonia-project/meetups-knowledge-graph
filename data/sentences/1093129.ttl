        ont:place         <http://dbpedia.org/resource/Apollo_Theater> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1093129/3/5> ;
        ont:subject       <http://dbpedia.org/resource/Elvera_Sanchez> .

<http://w3id.org/polifonia/pilot/meetups/1093129/4/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "After retiring from her show business career at the age of 35, she began working as a barmaid for Grace's Little Belmont in Atlantic City, New Jersey." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1093129/4> ;
        ont:person          <http://dbpedia.org/resource/Bartender> ;
        ont:place           <http://dbpedia.org/resource/New_Jersey> , <http://dbpedia.org/resource/Atlantic_City_Race_Course> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1093129/4/0> ;
        ont:subject         <http://dbpedia.org/resource/Elvera_Sanchez> ;
        ont:timeExpression  "age of 35" .

