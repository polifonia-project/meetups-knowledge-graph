<http://w3id.org/polifonia/pilot/meetups/12945/2/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Biography ==" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/12945/2> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/12945/2/0> ;
        ont:subject       <http://dbpedia.org/resource/George_Benson> .

<http://w3id.org/polifonia/pilot/meetups/12945/11/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "He also recorded the original version of \"The Greatest Love of All\" for the 1977 Muhammad Ali bio-pic, The Greatest, which was later covered by Whitney Houston as \"Greatest Love of All.\"" ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/12945/11> ;
        ont:person          <http://dbpedia.org/resource/Whitney_Houston> , <http://dbpedia.org/resource/Muhammad_Ali> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/12945/11/0> ;
        ont:subject         <http://dbpedia.org/resource/George_Benson> ;
        ont:timeExpression  "for the 1977" .

<http://w3id.org/polifonia/pilot/meetups/12945/5/6>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Miles Davis employed Benson in the mid-1960s, featuring his guitar on \"Paraphernalia\" on his 1968 Columbia release, Miles in the Sky before Benson went to Verve Records." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/12945/5> ;
        ont:person        <http://dbpedia.org/resource/Miles_Davis> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/12945/5/5> ;
        ont:subject       <http://dbpedia.org/resource/George_Benson> .

<http://w3id.org/polifonia/pilot/meetups/12945/15>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/12945/15/0> ;
