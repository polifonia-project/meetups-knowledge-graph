        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/346303/28/1> .

<http://w3id.org/polifonia/pilot/meetups/346303/2/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "\"Viv\" soon rebelled, often sneaking out of her bedroom and staying out after curfew." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/346303/2> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/346303/2/2> ;
        ont:subject       <http://dbpedia.org/resource/Vivian_Vance> .

<http://w3id.org/polifonia/pilot/meetups/346303/9/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "She appeared on The Lucy Show from 1962 to 1965, as Vivian Bagley, a divorced mother of one son, sharing a house with Ball's character." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/346303/9> ;
        ont:person          <http://dbpedia.org/resource/John_Lambrick_Vivian> , <http://dbpedia.org/resource/Mark_Bagley> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/346303/9/0> ;
        ont:subject         <http://dbpedia.org/resource/Vivian_Vance> ;
        ont:timeExpression  "one son" , "from 1962 to 1965" .

<http://w3id.org/polifonia/pilot/meetups/346303/9/10>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Without Vance on the show, Ball seriously considered ending the series, feeling she couldn't continue without her." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/346303/9> ;
        ont:person        <http://dbpedia.org/resource/Zebulon_Baird_Vance> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/346303/9/9> ;
        ont:subject       <http://dbpedia.org/resource/Vivian_Vance> .

<http://w3id.org/polifonia/pilot/meetups/346303/16/3>
        rdf:type            ont:Sentence ;
        ont:excerpt         "On January 16, 1961, Vance married literary agent, editor, and publisher John Dodds." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/346303/16> ;
        ont:person          <http://dbpedia.org/resource/Vance_Palmer> , <http://dbpedia.org/resource/John_Dodds_(motorcyclist)> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/346303/16/2> ;
        ont:subject         <http://dbpedia.org/resource/Vivian_Vance> ;
        ont:timeExpression  "January 16 , 1961" .

