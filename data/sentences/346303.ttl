<http://w3id.org/polifonia/pilot/meetups/346303/14/3>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Her most successful stage role was that of Nancy Collister in the Cole Porter musical Let's Face It! (1941), in which she starred alongside Danny Kaye and Eve Arden for over 500 performances." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/346303/14> ;
        ont:person          <http://dbpedia.org/resource/Eve_Arden> , <http://dbpedia.org/resource/Danny_Kaye> , <http://dbpedia.org/resource/Nancy_Sinatra> , <http://dbpedia.org/resource/Cole_Porter> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/346303/14/2> ;
        ont:subject         <http://dbpedia.org/resource/Vivian_Vance> ;
        ont:timeExpression  "1941" .

<http://w3id.org/polifonia/pilot/meetups/346303/6/8>
        rdf:type          ont:Sentence ;
        ont:excerpt       "She used to skim through the script before she memorized her lines to see how many scenes she had with \"that stubborn-headed little Irishman.\"" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/346303/6> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/346303/6/7> ;
        ont:subject       <http://dbpedia.org/resource/Vivian_Vance> .

<http://w3id.org/polifonia/pilot/meetups/346303/6/11>
        rdf:type            ont:Sentence ;
        ont:excerpt         "She was nominated an additional three times (for 1954, 1956, and 1957) before the series ended." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/346303/6> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/346303/6/10> ;
        ont:subject         <http://dbpedia.org/resource/Vivian_Vance> ;
        ont:timeExpression  "1956" .

<http://w3id.org/polifonia/pilot/meetups/346303/28>
        rdf:type  ont:Paragraph ;
