<http://w3id.org/polifonia/pilot/meetups/226142/8/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "It was Freed who disagreed with the original structure of the show's second act, in which more than twenty years pass between most of the act and the final three scenes of the musical." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/226142/8> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/226142/8/0> ;
        ont:subject       <http://dbpedia.org/resource/Arthur_Freed> .

<http://w3id.org/polifonia/pilot/meetups/226142/2/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He soon began to write songs, and was eventually hired by Metro-Goldwyn-Mayer." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/226142/2> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/226142/2/1> ;
        ont:subject       <http://dbpedia.org/resource/Arthur_Freed> .

<http://w3id.org/polifonia/pilot/meetups/226142/7>
        rdf:type  ont:Paragraph ;
