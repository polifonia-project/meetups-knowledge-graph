<http://w3id.org/polifonia/pilot/meetups/399509/10/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "You don't have to sing a song about it.\"" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/399509/10> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/399509/10/3> ;
        ont:subject       <http://dbpedia.org/resource/Thomas_Mapfumo> .

<http://w3id.org/polifonia/pilot/meetups/399509/9/8>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Host Georges Collinet describes Mapfumo as living in the low-density suburbs with his wife, who worked at a law office in downtown Harare, and his two children—a boy and a girl." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/399509/9> ;
        ont:person          <http://dbpedia.org/resource/Georges_Collinet> ;
        ont:place           <http://dbpedia.org/resource/Harare> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/399509/9/7> ;
        ont:subject         <http://dbpedia.org/resource/Thomas_Mapfumo> ;
        ont:timeExpression  "two children—a boy" .

<http://w3id.org/polifonia/pilot/meetups/399509/32>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/399509/32/0> .

<http://w3id.org/polifonia/pilot/meetups/399509/5/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He played mostly covers of American rock and soul tunes, such as Otis Redding or Elvis Presley, until he was in the Hallelujah Chicken Run Band." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/399509/5> ;
        ont:person        <http://dbpedia.org/resource/Elvis_Presley> , <http://dbpedia.org/resource/Otis_Redding> ;
        ont:place         <http://dbpedia.org/resource/United_States> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/399509/5/2> ;
        ont:subject       <http://dbpedia.org/resource/Thomas_Mapfumo> .

<http://w3id.org/polifonia/pilot/meetups/399509/17>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/399509/17/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/399509/17/1> .

<http://w3id.org/polifonia/pilot/meetups/399509/9/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "They were unable to stop his records from being played in discos or on radio stations they did not control, including the Voice of Mozambique." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/399509/9> ;
        ont:place         <http://dbpedia.org/resource/Mozambique> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/399509/9/1> ;
        ont:subject       <http://dbpedia.org/resource/Thomas_Mapfumo> .

