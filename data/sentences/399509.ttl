<http://w3id.org/polifonia/pilot/meetups/399509/7/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He called his new style of music Chimurenga." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/399509/7> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/399509/7/2> ;
        ont:subject       <http://dbpedia.org/resource/Thomas_Mapfumo> .

<http://w3id.org/polifonia/pilot/meetups/399509/11/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Things got uncomfortable enough that Mapfumo moved to Eugene, Oregon in the late 1990s." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/399509/11> ;
        ont:place         <http://dbpedia.org/resource/Oregon> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/399509/11/3> ;
