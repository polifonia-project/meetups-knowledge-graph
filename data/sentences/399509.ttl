<http://w3id.org/polifonia/pilot/meetups/399509/7/5>
        rdf:type          ont:Sentence ;
        ont:excerpt       "His songs openly called for the violent overthrow of the government, with lyrics like \"Mothers, send your sons to war.\"" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/399509/7> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/399509/7/4> ;
