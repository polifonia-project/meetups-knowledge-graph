<http://w3id.org/polifonia/pilot/meetups/399509/7/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In Shona it means \"a little flame\", and was the name of a previous revolutionary movement in the late 19th century." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/399509/7> ;
        ont:person        <http://dbpedia.org/resource/Revolutionary> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/399509/7/3> ;
        ont:subject       <http://dbpedia.org/resource/Thomas_Mapfumo> .

<http://w3id.org/polifonia/pilot/meetups/399509/11/5>
        rdf:type            ont:Sentence ;
        ont:excerpt         "After Mugabe's overthrow in 2017, he returned to Zimbabwe, to a massive show named 'Homecoming Bira' at Glamis Arena in Harare on April 28, 2018." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/399509/11> ;
        ont:person          <http://dbpedia.org/resource/Robert_Mugabe> ;
        ont:place           <http://dbpedia.org/resource/Glamis> , <http://dbpedia.org/resource/Harare> , <http://dbpedia.org/resource/Zimbabwe> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/399509/11/4> ;
        ont:subject         <http://dbpedia.org/resource/Thomas_Mapfumo> ;
        ont:timeExpression  "overthrow in 2017" , "April 28 , 2018" .

<http://w3id.org/polifonia/pilot/meetups/399509/39>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/399509/39/0> .

<http://w3id.org/polifonia/pilot/meetups/399509/9/9>
        rdf:type          ont:Sentence ;
        ont:excerpt       "And he drove a blue Ford with fake leopard-skin seat covers." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/399509/9> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/399509/9/8> ;
        ont:subject       <http://dbpedia.org/resource/Thomas_Mapfumo> .

<http://w3id.org/polifonia/pilot/meetups/399509/10/5>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Collinet also observes that Mapfumo can not sing anything he wants: \"Clearly he can't sing 'Down with President Mugabe' – but he wouldn't want to." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/399509/10> ;
        ont:person        <http://dbpedia.org/resource/President_of_the_United_States> , <http://dbpedia.org/resource/Robert_Mugabe> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/399509/10/4> ;
        ont:subject       <http://dbpedia.org/resource/Thomas_Mapfumo> .

<http://w3id.org/polifonia/pilot/meetups/399509/20>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/399509/20/0> .

<http://w3id.org/polifonia/pilot/meetups/399509/33>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/399509/33/0> .

<http://w3id.org/polifonia/pilot/meetups/399509/5/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "There he introduced the innovation of adapting traditional Shona music to modern rock instrumentation." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/399509/5> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/399509/5/3> ;
        ont:subject       <http://dbpedia.org/resource/Thomas_Mapfumo> .

<http://w3id.org/polifonia/pilot/meetups/399509/18>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/399509/18/0> .

<http://w3id.org/polifonia/pilot/meetups/399509/9/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Large demonstrations in protest of his arrest and an inability to find charges against him forced the government to release him after three months." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/399509/9> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/399509/9/2> ;
        ont:subject       <http://dbpedia.org/resource/Thomas_Mapfumo> .

