<http://w3id.org/polifonia/pilot/meetups/155107/20/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "On his 1969 album Folkjokeopus, English guitarist/singer-songwriter Roy Harper, dedicated the song \"One for All\" (\"One for Al\") to Albert Ayler, \"who I knew and loved during my time in Copenhagen\"." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/155107/20> ;
        ont:person          <http://dbpedia.org/resource/Albert_Ayler> , <http://dbpedia.org/resource/Roy_Harper_(singer)> ;
        ont:place           <http://dbpedia.org/resource/Copenhagen> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/155107/20/0> ;
        ont:subject         <http://dbpedia.org/resource/Albert_Ayler> ;
        ont:timeExpression  "One for" , "1969 album" .

