<http://w3id.org/polifonia/pilot/meetups/155107/18/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "His wild sound foreshadowed contemporary hardcore, noise, and experimental rock styles." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/155107/18> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/155107/18/2> ;
