<http://w3id.org/polifonia/pilot/meetups/481738/0/2>
        rdf:type            ont:Sentence ;
        ont:excerpt         "The man behind several songs by big French and American names, he was the first to use a microphone on a French stage in 1936." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/481738/0> ;
        ont:place           <http://dbpedia.org/resource/France> , <http://dbpedia.org/resource/United_States> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/481738/0/1> ;
        ont:subject         <http://dbpedia.org/resource/Jean_Sablon> ;
        ont:timeExpression  "stage in 1936" .

<http://w3id.org/polifonia/pilot/meetups/481738/14/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "The following year found him again in New York, as well as in Hollywood, Boston, Brussels (ABC), Paris (ABC), Mexico (Ciro's) and Canada." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/481738/14> ;
        ont:person        <http://dbpedia.org/resource/Ciro_Terranova> ;
        ont:place         <http://dbpedia.org/resource/Paris> , <http://dbpedia.org/resource/Hollywood> , <http://dbpedia.org/resource/Canada> , <http://dbpedia.org/resource/New_York_City> , <http://dbpedia.org/resource/Brussels> , <http://dbpedia.org/resource/Boston> , <http://dbpedia.org/resource/Mexico> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/481738/14/1> ;
        ont:subject       <http://dbpedia.org/resource/Jean_Sablon> .

<http://w3id.org/polifonia/pilot/meetups/481738/38/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Madame, by Albert Willemetz, music by Henri Christiné, Théâtre des Bouffes-Parisiens." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/481738/38> ;
        ont:person        <http://dbpedia.org/resource/Albert_Willemetz> ;
