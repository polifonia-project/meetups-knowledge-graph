        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/481738/0/0> ;
        ont:subject         <http://dbpedia.org/resource/Jean_Sablon> ;
        ont:timeExpression  "25 March 1906" , "24 February 1994" .

<http://w3id.org/polifonia/pilot/meetups/481738/14/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Meanwhile, in 1943, in London, his sister Germaine was the first to perform Le Chant des Partisans, which became the anthem of the French Resistance." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/481738/14> ;
        ont:place         <http://dbpedia.org/resource/London> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/481738/14/0> ;
        ont:subject       <http://dbpedia.org/resource/Jean_Sablon> .

