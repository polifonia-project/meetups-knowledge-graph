<http://w3id.org/polifonia/pilot/meetups/1790603/8/2>
        rdf:type            ont:Sentence ;
        ont:excerpt         "In the 1980s, after his 13-year-old daughter died in a house fire, he developed a drug habit and ended up living in a homeless shelter." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1790603/8> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1790603/8/1> ;
