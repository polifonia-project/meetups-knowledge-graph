        ont:place           <http://dbpedia.org/resource/Elberton,_Georgia> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1790603/3/0> ;
        ont:subject         <http://dbpedia.org/resource/Howard_Tate> ;
        ont:timeExpression  "1940 census" .

