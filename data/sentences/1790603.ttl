        ont:person          <http://dbpedia.org/resource/Disc_jockey> ;
        ont:place           <http://dbpedia.org/resource/New_Jersey> , <http://dbpedia.org/resource/New_Orleans> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1790603/10/0> ;
        ont:subject         <http://dbpedia.org/resource/Howard_Tate> ;
        ont:timeExpression  "in spring 2001" .

