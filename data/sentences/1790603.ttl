        rdf:type          ont:Sentence ;
        ont:excerpt       "According to the census record, Tate's father was named Hult Tate and his mother Roberta Tate." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1790603/3> ;
        ont:place         <http://dbpedia.org/resource/Hult,_Sweden> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1790603/3/2> ;
        ont:subject       <http://dbpedia.org/resource/Howard_Tate> .

