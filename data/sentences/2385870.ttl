        ont:timeExpression  "1943" , "1941" , "1954" .

<http://w3id.org/polifonia/pilot/meetups/2385870/2/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Grigori Kozintsev was born in the family of a doctor, therapist and pediatrician Moisei Isaakovich Kozintsev (1859-1930) and his wife Anna Grigorievna Lurie was from a rabbinical family from Kyiv." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2385870/2> ;
        ont:person        <http://dbpedia.org/resource/Grigori_Kozintsev> , <http://dbpedia.org/resource/Anna_Freud> ;
        ont:place         <http://dbpedia.org/resource/Kyiv> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2385870/2/0> ;
        ont:subject       <http://dbpedia.org/resource/Grigori_Kozintsev> .

<http://w3id.org/polifonia/pilot/meetups/2385870/15/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "He died on May 11, 1973 in Leningrad and was buried at Literatorskie jetty Volkovo Cemetery." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/2385870/15> ;
        ont:place           <http://dbpedia.org/resource/Volkovo_Cemetery> , <http://dbpedia.org/resource/Saint_Petersburg> ;
