<http://w3id.org/polifonia/pilot/meetups/2385870/11/1>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Kozintsev worked briefly in theater, staging the plays King Lear (1941), Othello (1943) and Hamlet (1954)." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/2385870/11> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/2385870/11/0> ;
