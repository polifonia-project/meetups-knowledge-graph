<http://w3id.org/polifonia/pilot/meetups/2385870/4/1>
        rdf:type            ont:Sentence ;
        ont:excerpt         "The father admitted patients at the commercial clinic \"Kvisisana\" on the Large Zhytomyr Street, 19, and at a free dispensary in the surgical hospital, built by philanthropist Babushkin on the Tverskaya Street, 7." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/2385870/4> ;
        ont:place           <http://dbpedia.org/resource/Babushkin_(town)> , <http://dbpedia.org/resource/Zhytomyr> , <http://dbpedia.org/resource/Tverskaya_Street> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/2385870/4/0> ;
        ont:subject         <http://dbpedia.org/resource/Grigori_Kozintsev> ;
        ont:timeExpression  "19" , "7" .

