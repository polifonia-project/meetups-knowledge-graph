        rdf:_3    <http://w3id.org/polifonia/pilot/meetups/1595398/5/2> ;
        rdf:_4    <http://w3id.org/polifonia/pilot/meetups/1595398/5/3> .

<http://w3id.org/polifonia/pilot/meetups/1595398/9/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "===" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1595398/9> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1595398/9/0> ;
        ont:subject       <http://dbpedia.org/resource/Jack_Scott_(singer)> .

<http://w3id.org/polifonia/pilot/meetups/1595398/4/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He wrote all of his own hits, except one: \"Burning Bridges.\"It has been said that \"with the exception of Roy Orbison and Elvis Presley, no white rock and roller of the time ever developed a finer voice with a better range than Jack Scott, or cut a more convincing body of work in rockabilly, rock and roll, country-soul, gospel or blues\"." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1595398/4> ;
        ont:person        <http://dbpedia.org/resource/Jack_Scott_(singer)> , <http://dbpedia.org/resource/Elvis_Presley> , <http://dbpedia.org/resource/Roy_Orbison> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1595398/4/0> ;
        ont:subject       <http://dbpedia.org/resource/Jack_Scott_(singer)> .

<http://w3id.org/polifonia/pilot/meetups/1595398/10/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "===" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1595398/10> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1595398/10/0> ;
        ont:subject       <http://dbpedia.org/resource/Jack_Scott_(singer)> .

<http://w3id.org/polifonia/pilot/meetups/1595398/2/7>
        rdf:type            ont:Sentence ;
        ont:excerpt         "\"My True Love\" (#3)." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1595398/2> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1595398/2/6> ;
        ont:subject         <http://dbpedia.org/resource/Jack_Scott_(singer)> ;
        ont:timeExpression  "3" .

<http://w3id.org/polifonia/pilot/meetups/1595398/8/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "==" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1595398/8> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1595398/8/0> ;
        ont:subject       <http://dbpedia.org/resource/Jack_Scott_(singer)> .

<http://w3id.org/polifonia/pilot/meetups/1595398/2/10>
        rdf:type            ont:Sentence ;
        ont:excerpt         "In all, six of 12 songs on his first album became hit singles." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1595398/2> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1595398/2/9> ;
        ont:subject         <http://dbpedia.org/resource/Jack_Scott_(singer)> ;
        ont:timeExpression  "six of" .

<http://w3id.org/polifonia/pilot/meetups/1595398/3/1>
        rdf:type            ont:Sentence ;
        ont:excerpt         "He then recorded four Billboard Hot 100 hits – \"What in the World's Come Over You\" (#5), \"Burning Bridges\" (#3) b/w \"Oh Little One\" (#34), and \"It Only Happened Yesterday\" (#38)." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1595398/3> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1595398/3/0> ;
        ont:subject         <http://dbpedia.org/resource/Jack_Scott_(singer)> ;
        ont:timeExpression  "34" , "38" , "Hot 100" , "3" , "5" .

<http://w3id.org/polifonia/pilot/meetups/1595398/7/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Scott died of congestive heart failure on December 12, 2019, at the age of 83." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1595398/7> ;
