<http://w3id.org/polifonia/pilot/meetups/1595398/8/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Discography ==" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1595398/8> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1595398/8/0> ;
        ont:subject       <http://dbpedia.org/resource/Jack_Scott_(singer)> .

<http://w3id.org/polifonia/pilot/meetups/1595398/3/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "\"What in the World's Come Over You\" was Scott's second gold disc winner." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1595398/3> ;
        ont:person        <http://dbpedia.org/resource/Walter_Scott> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1595398/3/1> ;
        ont:subject       <http://dbpedia.org/resource/Jack_Scott_(singer)> .

<http://w3id.org/polifonia/pilot/meetups/1595398/7/1>
        rdf:type            ont:Sentence ;
        ont:excerpt         "His niece, singer and actress Rio Scafone, announced his death on her Facebook page, stating that he had suffered a massive heart attack on December 8 and there was nothing they could do for him." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1595398/7> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1595398/7/0> ;
        ont:subject         <http://dbpedia.org/resource/Jack_Scott_(singer)> ;
        ont:timeExpression  "December 8" .

<http://w3id.org/polifonia/pilot/meetups/1595398/2/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He grew up listening to hillbilly music and was taught to play the guitar by his mother, Laura." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1595398/2> ;
        ont:person        <http://dbpedia.org/resource/Laura_Bush> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1595398/2/1> ;
        ont:subject       <http://dbpedia.org/resource/Jack_Scott_(singer)> .

<http://w3id.org/polifonia/pilot/meetups/1595398/5/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 2011, he was inducted into the Canadian Songwriters Hall of Fame." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1595398/5> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1595398/5/0> ;
        ont:subject       <http://dbpedia.org/resource/Jack_Scott_(singer)> .

<http://w3id.org/polifonia/pilot/meetups/1595398/5>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/1595398/5/0> ;
