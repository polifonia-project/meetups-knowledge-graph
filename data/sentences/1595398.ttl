<http://w3id.org/polifonia/pilot/meetups/1595398/5/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 2007, Jack Scott was voted into the Michigan Rock and Roll Legends Hall of Fame." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1595398/5> ;
        ont:person        <http://dbpedia.org/resource/Jack_Scott_(singer)> ;
        ont:place         <http://dbpedia.org/resource/Michigan> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1595398/5/0> ;
        ont:subject       <http://dbpedia.org/resource/Jack_Scott_(singer)> .

<http://w3id.org/polifonia/pilot/meetups/1595398/0/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He was inducted into the Canadian Songwriters Hall of Fame in 2011 and was called \"undeniably the greatest Canadian rock and roll singer of all time.\"" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1595398/0> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1595398/0/0> ;
        ont:subject       <http://dbpedia.org/resource/Jack_Scott_(singer)> .

