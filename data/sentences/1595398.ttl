<http://w3id.org/polifonia/pilot/meetups/1595398/2/12>
        rdf:type            ont:Sentence ;
        ont:excerpt         "He served in the United States Army during most of 1959, just after \"Goodbye Baby\" (#8) made the Top Ten. 1959 also saw him chart with \"The Way I Walk\" (#35)." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1595398/2> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1595398/2/11> ;
        ont:subject         <http://dbpedia.org/resource/Jack_Scott_(singer)> ;
        ont:timeExpression  "8" , "35" .

<http://w3id.org/polifonia/pilot/meetups/1595398/3/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Scott continued to record and perform during the 1960s and 1970s." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1595398/3> ;
        ont:person        <http://dbpedia.org/resource/Walter_Scott> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1595398/3/2> ;
        ont:subject       <http://dbpedia.org/resource/Jack_Scott_(singer)> .

<http://w3id.org/polifonia/pilot/meetups/1595398/7/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Scott died four days later, in the afternoon." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1595398/7> ;
        ont:person        <http://dbpedia.org/resource/Walter_Scott> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1595398/7/1> ;
        ont:subject       <http://dbpedia.org/resource/Jack_Scott_(singer)> .

<http://w3id.org/polifonia/pilot/meetups/1595398/2/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "As a teenager, he pursued a singing career and recorded as \"Jack Scott\"." ;
