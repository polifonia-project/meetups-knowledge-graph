        ont:subject         <http://dbpedia.org/resource/James_Brown> ;
        ont:timeExpression  "four fellow" .

<http://w3id.org/polifonia/pilot/meetups/15766/16/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "\"Bring it Up\" has an Afro-Cuban guajeo-like structure." ;
