        rdf:type          ont:Sentence ;
        ont:excerpt       "Brown's name was supposed to have been Joseph James Brown, but his first and middle names were mistakenly reversed on his birth certificate." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/15766/5> ;
        ont:person        <http://dbpedia.org/resource/Saint_Joseph> , <http://dbpedia.org/resource/James_Brown> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/15766/5/0> ;
