<http://w3id.org/polifonia/pilot/meetups/1127222/5/5>
        rdf:type            ont:Sentence ;
        ont:excerpt         "They married on 18 February 1892 and Sorabji was their only child." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1127222/5> ;
        ont:person          <http://dbpedia.org/resource/Kaikhosru_Shapurji_Sorabji> ;
