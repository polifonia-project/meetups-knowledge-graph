        rdf:type            ont:Sentence ;
        ont:excerpt         "Sorabji joined the Parsi community in 1913 or 1914 by attending a Navjote ceremony (probably performed in his home by a priest) and changed his name." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1127222/8> ;
        ont:person          <http://dbpedia.org/resource/Kaikhosru_Shapurji_Sorabji> , <http://dbpedia.org/resource/Priest> ;
