        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/1127222/27/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/1127222/27/1> .

<http://w3id.org/polifonia/pilot/meetups/1127222/21/9>
        rdf:type            ont:Sentence ;
        ont:excerpt         "One of them, Nicolas Slonimsky, who in 1978 erroneously wrote that Sorabji owned a castle, once called him \"the most enigmatic composer now living\"." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1127222/21> ;
        ont:person          <http://dbpedia.org/resource/Nicolas_Slonimsky> , <http://dbpedia.org/resource/Kaikhosru_Shapurji_Sorabji> , <http://dbpedia.org/resource/Composer> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1127222/21/8> ;
        ont:subject         <http://dbpedia.org/resource/Kaikhosru_Shapurji_Sorabji> ;
        ont:timeExpression  "One of" .

<http://w3id.org/polifonia/pilot/meetups/1127222/63/10>
        rdf:type          ont:Sentence ;
        ont:excerpt       "The music critic Andrew Clements calls Sorabji \"just another 20th-century English eccentric ... whose talent never matched [his] musical ambition\"." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1127222/63> ;
        ont:person        <http://dbpedia.org/resource/Andrew_Clements> , <http://dbpedia.org/resource/Kaikhosru_Shapurji_Sorabji> ;
