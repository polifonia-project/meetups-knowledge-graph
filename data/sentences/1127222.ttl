<http://w3id.org/polifonia/pilot/meetups/1127222/24/10>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Sorabji once called him \"one of the two people on earth most precious to me\"." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1127222/24> ;
        ont:person          <http://dbpedia.org/resource/Kaikhosru_Shapurji_Sorabji> ;
