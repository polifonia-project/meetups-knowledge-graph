        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/1127222/25/1> .

<http://w3id.org/polifonia/pilot/meetups/1127222/21/7>
        rdf:type          ont:Sentence ;
        ont:excerpt       "The villagers in Corfe Castle sometimes referred to him as \"Sir Abji\" and \"Indian Prince\"." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1127222/21> ;
        ont:person        <http://dbpedia.org/resource/Prince> ;
        ont:place         <http://dbpedia.org/resource/Corfe_Castle> ;
