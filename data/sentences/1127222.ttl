<http://w3id.org/polifonia/pilot/meetups/1127222/1/2>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Although Sorabji was a reluctant performer and not a virtuoso, he played some of his music publicly between 1920 and 1936." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1127222/1> ;
        ont:person          <http://dbpedia.org/resource/Kaikhosru_Shapurji_Sorabji> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1127222/1/1> ;
        ont:subject         <http://dbpedia.org/resource/Kaikhosru_Shapurji_Sorabji> ;
        ont:timeExpression  "between 1920 and 1936" .

<http://w3id.org/polifonia/pilot/meetups/1127222/29/1>
        rdf:type          ont:Sentence ;
