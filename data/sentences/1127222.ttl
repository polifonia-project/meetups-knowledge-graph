        ont:subject       <http://dbpedia.org/resource/Kaikhosru_Shapurji_Sorabji> .

<http://w3id.org/polifonia/pilot/meetups/1127222/14/16>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Although the tapes were not intended for public circulation, leaks occurred and some of the recordings were included in a 55-minute WBAI broadcast from 1969 and a three-hour programme produced by WNCN in 1970." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1127222/14> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1127222/14/15> ;
        ont:subject         <http://dbpedia.org/resource/Kaikhosru_Shapurji_Sorabji> ;
        ont:timeExpression  "broadcast from 1969" .

<http://w3id.org/polifonia/pilot/meetups/1127222/7/5>
        rdf:type          ont:Sentence ;
        ont:excerpt       "For unknown reasons, Sorabji was not conscripted during World War I, and though he later praised conscientious objectors for their courage, there is no proof he tried to register as one." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1127222/7> ;
        ont:person        <http://dbpedia.org/resource/Kaikhosru_Shapurji_Sorabji> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1127222/7/4> ;
        ont:subject       <http://dbpedia.org/resource/Kaikhosru_Shapurji_Sorabji> .

<http://w3id.org/polifonia/pilot/meetups/1127222/19/0>
        rdf:type          ont:Sentence ;
