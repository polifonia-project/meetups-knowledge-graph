        ont:subject       <http://dbpedia.org/resource/Kaikhosru_Shapurji_Sorabji> .

<http://w3id.org/polifonia/pilot/meetups/1127222/18/3>
        rdf:type            ont:Sentence ;
        ont:excerpt         "In March 1987, he moved into Marley House Nursing Home, a private nursing home in Winfrith Newburgh (near Dorchester, Dorset), where he was permanently chairbound and received daily nursing care." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1127222/18> ;
        ont:place           <http://dbpedia.org/resource/Winfrith_Newburgh> , <http://dbpedia.org/resource/Dorchester,_Dorset> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1127222/18/2> ;
        ont:subject         <http://dbpedia.org/resource/Kaikhosru_Shapurji_Sorabji> ;
        ont:timeExpression  "March 1987" .

<http://w3id.org/polifonia/pilot/meetups/1127222/28/4>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Bromage acted as joint trustee of the Shapurji Sorabji Trust between 1933 and 1941 and produced a defective index for Sorabji's book Around Music, which the composer was displeased with." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1127222/28> ;
        ont:person          <http://dbpedia.org/resource/Composer> , <http://dbpedia.org/resource/Kaikhosru_Shapurji_Sorabji> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1127222/28/3> ;
        ont:subject         <http://dbpedia.org/resource/Kaikhosru_Shapurji_Sorabji> ;
        ont:timeExpression  "between 1933 and 1941" .

<http://w3id.org/polifonia/pilot/meetups/1127222/65/1>
