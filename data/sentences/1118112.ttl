<http://w3id.org/polifonia/pilot/meetups/1118112/0/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Ronald Wycherley (17 April 1940 – 28 January 1983), better known as Billy Fury, was an English singer, musician, songwriter, and actor." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1118112/0> ;
        ont:person          <http://dbpedia.org/resource/Billy_Fury> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1118112/0/0> ;
        ont:subject         <http://dbpedia.org/resource/Billy_Fury> ;
        ont:timeExpression  "17 April 1940" , "28 January 1983" .

<http://w3id.org/polifonia/pilot/meetups/1118112/22/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "References in popular culture ==" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1118112/22> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1118112/22/0> ;
        ont:subject       <http://dbpedia.org/resource/Billy_Fury> .

<http://w3id.org/polifonia/pilot/meetups/1118112/14/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Fury was a keen birdwatcher." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1118112/14> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1118112/14/0> ;
        ont:subject       <http://dbpedia.org/resource/Billy_Fury> .

<http://w3id.org/polifonia/pilot/meetups/1118112/5/5>
        rdf:type            ont:Sentence ;
        ont:excerpt         "In October 1959, the UK music magazine, NME, commented that Fury's stage antics had been drawing much press criticism." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1118112/5> ;
        ont:place           <http://dbpedia.org/resource/United_Kingdom> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1118112/5/4> ;
        ont:subject         <http://dbpedia.org/resource/Billy_Fury> ;
        ont:timeExpression  "October 1959" .

<http://w3id.org/polifonia/pilot/meetups/1118112/12/8>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He returned to touring later that year, and his next two singles, \"Love or Money\" and \"Devil or Angel\", just dented the UK chart." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1118112/12> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1118112/12/7> ;
        ont:subject       <http://dbpedia.org/resource/Billy_Fury> .

