        rdf:type            ont:Sentence ;
        ont:excerpt         "A new release, \"Be Mine Tonight\" (1981), failed to make an appearance in the UK Singles Chart." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1118112/12> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1118112/12/5> ;
