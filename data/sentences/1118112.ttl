        ont:subject       <http://dbpedia.org/resource/Billy_Fury> .

<http://w3id.org/polifonia/pilot/meetups/1118112/21/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Fury's backing band from 1970 until 1976, when he stopped touring due to ill health, were Fury's Tornados, named by Fury and his then manager Hal Carter." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1118112/21> ;
        ont:person          <http://dbpedia.org/resource/Jimmy_Carter> , <http://dbpedia.org/resource/Talent_manager> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1118112/21/0> ;
        ont:subject         <http://dbpedia.org/resource/Billy_Fury> ;
        ont:timeExpression  "from 1970 until 1976" .

