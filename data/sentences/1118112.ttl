        ont:subject         <http://dbpedia.org/resource/Billy_Fury> ;
        ont:timeExpression  "group in 1955" .

<http://w3id.org/polifonia/pilot/meetups/1118112/7/7>
        rdf:type          ont:Sentence ;
        ont:excerpt       "It featured Helen Shapiro, Danny Williams, Shane Fenton and Bobby Vee, who appeared with the Vernons Girls." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1118112/7> ;
        ont:person        <http://dbpedia.org/resource/Helen_Shapiro> , <http://dbpedia.org/resource/Bobby_Vee> , <http://dbpedia.org/resource/Alvin_Stardust> , <http://dbpedia.org/resource/Girl> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1118112/7/6> ;
        ont:subject       <http://dbpedia.org/resource/Billy_Fury> .

<http://w3id.org/polifonia/pilot/meetups/1118112/17/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He was taken to St Mary's Hospital in Paddington, but died later in the afternoon, aged 42." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1118112/17> ;
        ont:person        <http://dbpedia.org/resource/Mary_I_of_England> ;
