        ont:timeExpression  "Eight of" , "June 2010" .

<http://w3id.org/polifonia/pilot/meetups/1118112/5/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Wycherley went to meet pop manager and impresario Larry Parnes at the Essoldo Theatre in Birkenhead, hoping to interest one of Parnes' protégés," ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1118112/5> ;
        ont:person          <http://dbpedia.org/resource/Impresario> , <http://dbpedia.org/resource/Talent_manager> , <http://dbpedia.org/resource/Larry_Parnes> ;
        ont:place           <http://dbpedia.org/resource/Birkenhead> , <http://dbpedia.org/resource/Parnitha> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1118112/5/0> ;
        ont:subject         <http://dbpedia.org/resource/Billy_Fury> ;
        ont:timeExpression  "one of" .

<http://w3id.org/polifonia/pilot/meetups/1118112/12/3>
