<http://w3id.org/polifonia/pilot/meetups/51997395/3/4>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Between April and June 1938, Esperanza provided shorthand transcription for a fine arts conference organized by the anti-Nazi Liga pro-Cultura Alemana en México (Pro-German Culture League in Mexico)." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/51997395/3> ;
        ont:person          <http://dbpedia.org/resource/Fine_art> ;
        ont:place           <http://dbpedia.org/resource/Esperanza_Base> , <http://dbpedia.org/resource/Xico,_Veracruz> , <http://dbpedia.org/resource/Mexico> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/51997395/3/3> ;
        ont:subject         <http://dbpedia.org/resource/Esperanza_López_Mateos> ;
        ont:timeExpression  "June 1938" .

<http://w3id.org/polifonia/pilot/meetups/51997395/2/12>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1934 Esperanza married Roberto Figueroa." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/51997395/2> ;
        ont:person        <http://dbpedia.org/resource/Roberto_Figueroa> ;
        ont:place         <http://dbpedia.org/resource/Esperanza_Base> ;
