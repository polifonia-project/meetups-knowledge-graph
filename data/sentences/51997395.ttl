        ont:timeExpression  "1951" .

<http://w3id.org/polifonia/pilot/meetups/51997395/5/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Traven was very pleased and named her his representative for Latin America." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/51997395/5> ;
        ont:place         <http://dbpedia.org/resource/Latin_America> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/51997395/5/1> ;
        ont:subject       <http://dbpedia.org/resource/Esperanza_López_Mateos> .

<http://w3id.org/polifonia/pilot/meetups/51997395/0/3>
        rdf:type            ont:Sentence ;
        ont:excerpt         "She participated in the strike by the miners of Nueva Rosita (1950–1951) and worked with Vicente Lombardo Toledano; together they supported many Jewish exiles fleeing European wars and seeking refuge in Mexico." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/51997395/0> ;
        ont:person          <http://dbpedia.org/resource/Vicente_Lombardo_Toledano> ;
        ont:place           <http://dbpedia.org/resource/Nueva_Rosita> , <http://dbpedia.org/resource/Mexico> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/51997395/0/2> ;
        ont:subject         <http://dbpedia.org/resource/Esperanza_López_Mateos> ;
        ont:timeExpression  "1950–1951" .

<http://w3id.org/polifonia/pilot/meetups/51997395/15/5>
        rdf:type          ont:Sentence ;
        ont:excerpt       "From there she went to Europe, acting as a representative of B. Traven in Switzerland, France, Czechoslovakia, and Italy." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/51997395/15> ;
        ont:place         <http://dbpedia.org/resource/France> , <http://dbpedia.org/resource/Switzerland> , <http://dbpedia.org/resource/Europe> , <http://dbpedia.org/resource/Czechoslovakia> , <http://dbpedia.org/resource/Italy> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/51997395/15/4> ;
        ont:subject       <http://dbpedia.org/resource/Esperanza_López_Mateos> .

<http://w3id.org/polifonia/pilot/meetups/51997395/11/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "El barco de la muerte (The Death Ship) (1950)" ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/51997395/11> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/51997395/11/0> ;
        ont:subject         <http://dbpedia.org/resource/Esperanza_López_Mateos> ;
        ont:timeExpression  "1950" .

<http://w3id.org/polifonia/pilot/meetups/51997395/21/5>
        rdf:type          ont:Sentence ;
        ont:excerpt       "President Alemán declined to meet with the miners, and in early April the strike was declared non-existent." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/51997395/21> ;
        ont:person        <http://dbpedia.org/resource/President_of_the_United_States> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/51997395/21/4> ;
        ont:subject       <http://dbpedia.org/resource/Esperanza_López_Mateos> .

<http://w3id.org/polifonia/pilot/meetups/51997395/2/8>
        rdf:type          ont:Sentence ;
        ont:excerpt       "This marriage produced two older sons, Mariano (born 1900) and Elena." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/51997395/2> ;
        ont:person        <http://dbpedia.org/resource/Elena_of_Gallura> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/51997395/2/7> ;
        ont:subject       <http://dbpedia.org/resource/Esperanza_López_Mateos> .

<http://w3id.org/polifonia/pilot/meetups/51997395/22>
