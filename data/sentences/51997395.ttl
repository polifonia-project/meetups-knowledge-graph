<http://w3id.org/polifonia/pilot/meetups/51997395/12/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Gobierno (Government) (1951)" ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/51997395/12> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/51997395/12/0> ;
