        ont:subject       <http://dbpedia.org/resource/Esperanza_López_Mateos> .

<http://w3id.org/polifonia/pilot/meetups/51997395/21/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He was intimidated and forced to leave Coahuila." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/51997395/21> ;
        ont:place         <http://dbpedia.org/resource/Coahuila> ;
