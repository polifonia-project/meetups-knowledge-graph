<http://w3id.org/polifonia/pilot/meetups/51997395/6/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "This was the first Spanish translation authorized by Traven, completed by Esperanza López Mateos." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/51997395/6> ;
        ont:place         <http://dbpedia.org/resource/Esperanza_Base> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/51997395/6/0> ;
        ont:subject       <http://dbpedia.org/resource/Esperanza_López_Mateos> .

<http://w3id.org/polifonia/pilot/meetups/51997395/23/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "However they also reported that according to José Álvarez Amézquita, the doctor who attended Esperanza and prepared the death certificate, she was shot in the neck." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/51997395/23> ;
        ont:place         <http://dbpedia.org/resource/Esperanza_Base> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/51997395/23/3> ;
        ont:subject       <http://dbpedia.org/resource/Esperanza_López_Mateos> .

<http://w3id.org/polifonia/pilot/meetups/51997395/14>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/51997395/14/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/51997395/14/1> .

<http://w3id.org/polifonia/pilot/meetups/51997395/5/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Although Traven refused at first, Esperanza insisted and sent her translation of The Bridge in the Jungle to persuade him." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/51997395/5> ;
        ont:place         <http://dbpedia.org/resource/Esperanza_Base> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/51997395/5/0> ;
        ont:subject       <http://dbpedia.org/resource/Esperanza_López_Mateos> .

<http://w3id.org/polifonia/pilot/meetups/51997395/0/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "She was the sister of politician Adolfo López Mateos and sister-in-law of cinematographer Gabriel Figueroa." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/51997395/0> ;
        ont:person        <http://dbpedia.org/resource/Cinematographer> , <http://dbpedia.org/resource/Gabriel> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/51997395/0/1> ;
        ont:subject       <http://dbpedia.org/resource/Esperanza_López_Mateos> .

<http://w3id.org/polifonia/pilot/meetups/51997395/15/4>
        rdf:type            ont:Sentence ;
        ont:excerpt         "In November 1947, she traveled to New York, where she met with Henry Schnautz." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/51997395/15> ;
        ont:place           <http://dbpedia.org/resource/New_York_City> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/51997395/15/3> ;
        ont:subject         <http://dbpedia.org/resource/Esperanza_López_Mateos> ;
        ont:timeExpression  "November 1947" .

<http://w3id.org/polifonia/pilot/meetups/51997395/9/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "La carreta (The Carreta) (1949)" ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/51997395/9> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/51997395/9/0> ;
        ont:subject         <http://dbpedia.org/resource/Esperanza_López_Mateos> ;
        ont:timeExpression  "1949" .

<http://w3id.org/polifonia/pilot/meetups/51997395/2/15>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In Iztaccihuatl there is a mountain hut named for Esperanza." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/51997395/2> ;
        ont:place         <http://dbpedia.org/resource/Iztaccihuatl> , <http://dbpedia.org/resource/Esperanza_Base> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/51997395/2/14> ;
        ont:subject       <http://dbpedia.org/resource/Esperanza_López_Mateos> .

<http://w3id.org/polifonia/pilot/meetups/51997395/4/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Translations ==" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/51997395/4> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/51997395/4/0> ;
        ont:subject       <http://dbpedia.org/resource/Esperanza_López_Mateos> .

<http://w3id.org/polifonia/pilot/meetups/51997395/21/4>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Meanwhile, the majority of the miners stayed at the Deportivo 18 de Marzo sports complex." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/51997395/21> ;
        ont:place           <http://dbpedia.org/resource/Deportivo_18_de_Marzo_metro_station> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/51997395/21/3> ;
        ont:subject         <http://dbpedia.org/resource/Esperanza_López_Mateos> ;
        ont:timeExpression  "Deportivo 18" .

<http://w3id.org/polifonia/pilot/meetups/51997395/2/7>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In either case, Esperanza and Adolfo took the name of Elena's late husband, Mariano López." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/51997395/2> ;
        ont:person        <http://dbpedia.org/resource/Elena_of_Gallura> ;
        ont:place         <http://dbpedia.org/resource/Esperanza_Base> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/51997395/2/6> ;
