<http://w3id.org/polifonia/pilot/meetups/51997395/17/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "In June 1948 Esperanza began a painful spinal treatment to recover from the aftermath of an alpine accident." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/51997395/17> ;
        ont:place           <http://dbpedia.org/resource/Esperanza_Base> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/51997395/17/0> ;
        ont:subject         <http://dbpedia.org/resource/Esperanza_López_Mateos> ;
        ont:timeExpression  "June 1948" .

<http://w3id.org/polifonia/pilot/meetups/51997395/5/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "From then on she worked as Traven's secretary, translator, and agent." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/51997395/5> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/51997395/5/2> ;
        ont:subject       <http://dbpedia.org/resource/Esperanza_López_Mateos> .

<http://w3id.org/polifonia/pilot/meetups/51997395/15/6>
        rdf:type          ont:Sentence ;
        ont:excerpt       "She met with Josef Wieder (Traven's agent) in Zürich and visited Davos before returning to New York." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/51997395/15> ;
        ont:place         <http://dbpedia.org/resource/Davos> , <http://dbpedia.org/resource/New_York_City> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/51997395/15/5> ;
        ont:subject       <http://dbpedia.org/resource/Esperanza_López_Mateos> .

<http://w3id.org/polifonia/pilot/meetups/51997395/23/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Esperanza López Mateos died on September 19, 1951." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/51997395/23> ;
        ont:place           <http://dbpedia.org/resource/Esperanza_Base> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/51997395/23/0> ;
        ont:subject         <http://dbpedia.org/resource/Esperanza_López_Mateos> ;
        ont:timeExpression  "September 19 , 1951" .

<http://w3id.org/polifonia/pilot/meetups/51997395/16/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1948 she participated with Vicente Lombardo Toledano in the foundation of the Partido Popular (later the Popular Socialist Party)." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/51997395/16> ;
        ont:person        <http://dbpedia.org/resource/Vicente_Lombardo_Toledano> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/51997395/16/0> ;
        ont:subject       <http://dbpedia.org/resource/Esperanza_López_Mateos> .

<http://w3id.org/polifonia/pilot/meetups/51997395/10>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/51997395/10/0> .

