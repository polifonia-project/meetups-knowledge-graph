        ont:subject       <http://dbpedia.org/resource/Esperanza_López_Mateos> .

<http://w3id.org/polifonia/pilot/meetups/51997395/2/11>
        rdf:type          ont:Sentence ;
        ont:excerpt       "The children grew up in close contact with their cousins, Gabriel and Roberto Figueroa Mateos." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/51997395/2> ;
        ont:person        <http://dbpedia.org/resource/Roberto_Figueroa> , <http://dbpedia.org/resource/Gabriel> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/51997395/2/10> ;
        ont:subject       <http://dbpedia.org/resource/Esperanza_López_Mateos> .

<http://w3id.org/polifonia/pilot/meetups/51997395/21/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In Nueva Rosita, Esperanza faced Pliego Garduño, commander in charge of the militarized zone." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/51997395/21> ;
        ont:place         <http://dbpedia.org/resource/Pliego> , <http://dbpedia.org/resource/Esperanza_Base> , <http://dbpedia.org/resource/Nueva_Rosita> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/51997395/21/0> ;
        ont:subject       <http://dbpedia.org/resource/Esperanza_López_Mateos> .

<http://w3id.org/polifonia/pilot/meetups/51997395/14/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "==" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/51997395/14> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/51997395/14/0> ;
        ont:subject       <http://dbpedia.org/resource/Esperanza_López_Mateos> .

<http://w3id.org/polifonia/pilot/meetups/51997395/2/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "These dates are consistent with those of her birth certificate, in which her name only appears as Esperanza Mateos, natural daughter of Elena Mateos." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/51997395/2> ;
        ont:person        <http://dbpedia.org/resource/Elena_of_Gallura> ;
