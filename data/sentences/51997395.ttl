<http://w3id.org/polifonia/pilot/meetups/51997395/10/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "La rebelión de los colgados (The Rebellion of the Hanged) (1950)" ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/51997395/10> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/51997395/10/0> ;
        ont:subject         <http://dbpedia.org/resource/Esperanza_López_Mateos> ;
        ont:timeExpression  "1950" .

<http://w3id.org/polifonia/pilot/meetups/51997395/3/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "There she met many intellectuals of German origin who had come to Mexico to escape the Nazis, as well as Mexican intellectuals who sought to transform education to counteract the power of the Catholic Church." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/51997395/3> ;
        ont:place         <http://dbpedia.org/resource/Mexico> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/51997395/3/1> ;
        ont:subject       <http://dbpedia.org/resource/Esperanza_López_Mateos> .

<http://w3id.org/polifonia/pilot/meetups/51997395/2/10>
        rdf:type          ont:Sentence ;
        ont:excerpt       "According to a story told by Gabriel Figueroa in his Memorias, the Spanish Gonzalo de Murga had a daughter with an aristocratic English woman who left him; he later entrusted this child to Elena Mateos, who named her Esperanza." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/51997395/2> ;
        ont:person        <http://dbpedia.org/resource/Gonzalo_Pizarro> , <http://dbpedia.org/resource/Elena_of_Gallura> , <http://dbpedia.org/resource/Gabriel> ;
        ont:place         <http://dbpedia.org/resource/Spain> , <http://dbpedia.org/resource/Esperanza_Base> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/51997395/2/9> ;
        ont:subject       <http://dbpedia.org/resource/Esperanza_López_Mateos> .

<http://w3id.org/polifonia/pilot/meetups/51997395/2/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "The name of her father is not mentioned." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/51997395/2> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/51997395/2/1> ;
        ont:subject       <http://dbpedia.org/resource/Esperanza_López_Mateos> .

