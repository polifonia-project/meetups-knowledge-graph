        rdf:type          ont:Sentence ;
        ont:excerpt       "Esperanza's illegitimate birth gave rise to many legends to protect the reputation of Elena and her two minor children, Esperanza and Adolfo." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/51997395/2> ;
        ont:person        <http://dbpedia.org/resource/Elena_of_Gallura> ;
        ont:place         <http://dbpedia.org/resource/Esperanza_Base> ;
