        rdf:_3    <http://w3id.org/polifonia/pilot/meetups/1788051/7/2> .

<http://w3id.org/polifonia/pilot/meetups/1788051/2/5>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He was considered by his peers to be the finest arranger in the world, and his talents influenced many composer-arrangers, including Quincy Jones, all of whom acknowledged his contributions to their work." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1788051/2> ;
        ont:place         <http://dbpedia.org/resource/Episcopal_Diocese_of_Quincy> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1788051/2/4> ;
        ont:subject       <http://dbpedia.org/resource/Robert_Farnon> .

