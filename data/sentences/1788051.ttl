<http://w3id.org/polifonia/pilot/meetups/1788051/3/2>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Robert Farnon died at the age of 87, at a hospice near his home of 40 years in Guernsey." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1788051/3> ;
        ont:person          <http://dbpedia.org/resource/Robert_Farnon> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1788051/3/1> ;
