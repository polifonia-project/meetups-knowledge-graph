        ont:excerpt       "Works ==" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1788051/4> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1788051/4/0> ;
        ont:subject       <http://dbpedia.org/resource/Robert_Farnon> .

<http://w3id.org/polifonia/pilot/meetups/1788051/2/7>
        rdf:type            ont:Sentence ;
        ont:excerpt         "He was the musical director and conductor for Tony Bennett's 1972 series for Thames Television, Tony Bennett at the Talk of the Town." ;
