<http://w3id.org/polifonia/pilot/meetups/1788051/3/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Farnon's older brother Brian and his younger brother Dennis were also orchestral composers, arrangers and conductors." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1788051/3> ;
        ont:person        <http://dbpedia.org/resource/Dennis_Wilson> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1788051/3/3> ;
        ont:subject       <http://dbpedia.org/resource/Robert_Farnon> .

<http://w3id.org/polifonia/pilot/meetups/1788051/2/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "His friend and fellow composer Wally Stott composed \"A Canadian in Mayfair\" as a tribute." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1788051/2> ;
        ont:person        <http://dbpedia.org/resource/Composer> , <http://dbpedia.org/resource/Angela_Morley> ;
        ont:place         <http://dbpedia.org/resource/Mayfair> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1788051/2/3> ;
        ont:subject       <http://dbpedia.org/resource/Robert_Farnon> .

<http://w3id.org/polifonia/pilot/meetups/1788051/0>
        rdf:type  ont:Paragraph ;
