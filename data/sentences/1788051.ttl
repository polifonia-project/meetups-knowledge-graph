<http://w3id.org/polifonia/pilot/meetups/1788051/0/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Robert Joseph Farnon CM (24 July 1917 – 23 April 2005) was a Canadian-born composer, conductor, musical arranger and trumpet player." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1788051/0> ;
        ont:person          <http://dbpedia.org/resource/Robert_Joseph> , <http://dbpedia.org/resource/Conducting> , <http://dbpedia.org/resource/Composer> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1788051/0/0> ;
        ont:subject         <http://dbpedia.org/resource/Robert_Farnon> ;
        ont:timeExpression  "24 July 1917" , "23 April 2005" .

<http://w3id.org/polifonia/pilot/meetups/1788051/7>
        rdf:type  ont:Paragraph ;
