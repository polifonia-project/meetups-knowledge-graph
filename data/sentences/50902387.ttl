        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/50902387/2/20> ;
        ont:subject       <http://dbpedia.org/resource/Michael_Morrow> .

<http://w3id.org/polifonia/pilot/meetups/50902387/6/11>
        rdf:type          ont:Sentence ;
        ont:excerpt       "The group shot to fame after this and thereafter it was frequently engaged for concerts and recordings; see the separate entry Musica Reservata." ;
