        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/306480/7/1> ;
        rdf:_3    <http://w3id.org/polifonia/pilot/meetups/306480/7/2> ;
        rdf:_4    <http://w3id.org/polifonia/pilot/meetups/306480/7/3> .

<http://w3id.org/polifonia/pilot/meetups/306480/8/9>
        rdf:type            ont:Sentence ;
        ont:excerpt         "F-111: In 1965, James Rosenquist completed F-111, one of the largest and most ambitious works in his collection." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/306480/8> ;
        ont:person          <http://dbpedia.org/resource/James_Rosenquist> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/306480/8/8> ;
        ont:subject         <http://dbpedia.org/resource/James_Rosenquist> ;
        ont:timeExpression  "one of" .

<http://w3id.org/polifonia/pilot/meetups/306480/10>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/306480/10/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/306480/10/1> ;
        rdf:_3    <http://w3id.org/polifonia/pilot/meetups/306480/10/2> ;
        rdf:_4    <http://w3id.org/polifonia/pilot/meetups/306480/10/3> ;
        rdf:_5    <http://w3id.org/polifonia/pilot/meetups/306480/10/4> .

<http://w3id.org/polifonia/pilot/meetups/306480/4/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Rosenquist applied sign-painting techniques to the large-scale paintings he began creating in 1960." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/306480/4> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/306480/4/3> ;
        ont:subject       <http://dbpedia.org/resource/James_Rosenquist> .

<http://w3id.org/polifonia/pilot/meetups/306480/5/22>
        rdf:type            ont:Sentence ;
        ont:excerpt         "This includes the three-painting suite The Swimmer in the Econo-mist (1997–1998) for Deutsche Guggenheim, Berlin, Germany, and a painting that was planned for the ceiling of the Palais de Chaillot in Paris, France." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/306480/5> ;
        ont:place           <http://dbpedia.org/resource/Paris> , <http://dbpedia.org/resource/Berlin> , <http://dbpedia.org/resource/France> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/306480/5/21> ;
        ont:subject         <http://dbpedia.org/resource/James_Rosenquist> ;
        ont:timeExpression  "1997–1998" .

<http://w3id.org/polifonia/pilot/meetups/306480/23>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/306480/23/0> .

<http://w3id.org/polifonia/pilot/meetups/306480/1>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/306480/1/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/306480/1/1> .

<http://w3id.org/polifonia/pilot/meetups/306480/8/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "With President Elect, Rosenquist seeks to make a statement on the new role that advertising and mass media had during the Kennedy's campaign." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/306480/8> ;
        ont:person        <http://dbpedia.org/resource/John_F._Kennedy> , <http://dbpedia.org/resource/President_of_the_United_States> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/306480/8/2> ;
        ont:subject       <http://dbpedia.org/resource/James_Rosenquist> .

<http://w3id.org/polifonia/pilot/meetups/306480/7/3>
        rdf:type            ont:Sentence ;
        ont:excerpt         "The two images are divided into separate zones, which serve to focus on visual parallels such as the arch of the tomato stem and the woman's eyelashes, as well as illustrating Rosenquist's signature, often surreal, fragmented composition." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/306480/7> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/306480/7/2> ;
        ont:subject         <http://dbpedia.org/resource/James_Rosenquist> ;
        ont:timeExpression  "two" .

<http://w3id.org/polifonia/pilot/meetups/306480/2/4>
        rdf:type            ont:Sentence ;
        ont:excerpt         "In 1955, at the age of 21, he moved to New York City on scholarship to study at the Art Students League, studying under painters such as Edwin Dickinson and George Grosz." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/306480/2> ;
        ont:person          <http://dbpedia.org/resource/George_Gershwin> , <http://dbpedia.org/resource/Edwin_Dickinson> ;
        ont:place           <http://dbpedia.org/resource/New_York_City> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/306480/2/3> ;
        ont:subject         <http://dbpedia.org/resource/James_Rosenquist> ;
        ont:timeExpression  "age of 21" .

<http://w3id.org/polifonia/pilot/meetups/306480/12/4>
        rdf:type            ont:Sentence ;
        ont:excerpt         "His second wife was Mimi Thompson, whom he married on April 18, 1987, by whom he had one child: Lily." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/306480/12> ;
        ont:person          <http://dbpedia.org/resource/Klay_Thompson> , <http://dbpedia.org/resource/Mimi_Smith> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/306480/12/3> ;
        ont:subject         <http://dbpedia.org/resource/James_Rosenquist> ;
        ont:timeExpression  "one child" , "April 18 , 1987" .

<http://w3id.org/polifonia/pilot/meetups/306480/5/9>
        rdf:type            ont:Sentence ;
        ont:excerpt         "\"Rosenquist had his first two solo exhibitions at the Green Gallery in 1962 and 1963." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/306480/5> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/306480/5/8> ;
        ont:subject         <http://dbpedia.org/resource/James_Rosenquist> ;
        ont:timeExpression  "in 1962 and 1963" .

<http://w3id.org/polifonia/pilot/meetups/306480/30>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/306480/30/0> .

<http://w3id.org/polifonia/pilot/meetups/306480/5/14>
        rdf:type          ont:Sentence ;
        ont:excerpt       "I did not really know Andy or Roy Lichtenstein that well." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/306480/5> ;
        ont:person        <http://dbpedia.org/resource/Roy_Lichtenstein> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/306480/5/13> ;
        ont:subject       <http://dbpedia.org/resource/James_Rosenquist> .

<http://w3id.org/polifonia/pilot/meetups/306480/15>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/306480/15/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/306480/15/1> .

<http://w3id.org/polifonia/pilot/meetups/306480/8/12>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Painted during the Vietnam war, F-111 contrasts pictures from the war with commercial imagery from advertisements, showing tires, a cake, lightbulbs, a girl in a salon hairdryer, bubbles, and spaghetti." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/306480/8> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/306480/8/11> ;
        ont:subject       <http://dbpedia.org/resource/James_Rosenquist> .

<http://w3id.org/polifonia/pilot/meetups/306480/28>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/306480/28/0> .

<http://w3id.org/polifonia/pilot/meetups/306480/5/3>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Time magazine stated that \"his powerful graphic style and painted montages helped define the 1960s Pop Art movement." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/306480/5> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/306480/5/2> ;
        ont:subject         <http://dbpedia.org/resource/James_Rosenquist> ;
        ont:timeExpression  "1960s" .

<http://w3id.org/polifonia/pilot/meetups/306480/6>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/306480/6/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/306480/6/1> .

<http://w3id.org/polifonia/pilot/meetups/306480/8/8>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In the painting, Rosenquist contrasts the portrait of Kennedy with the cake and the Chevrolet to show how each element is marketed to American consumers." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/306480/8> ;
        ont:person        <http://dbpedia.org/resource/John_F._Kennedy> ;
        ont:place         <http://dbpedia.org/resource/United_States> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/306480/8/7> ;
        ont:subject       <http://dbpedia.org/resource/James_Rosenquist> .

<http://w3id.org/polifonia/pilot/meetups/306480/10/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "His F-111, shown at The Jewish Museum in 1965, was mentioned in a chapter of Polaroids from the Dead by Douglas Coupland." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/306480/10> ;
        ont:person        <http://dbpedia.org/resource/Douglas_Coupland> ;
        ont:place         <http://dbpedia.org/resource/Jewish_Museum_(Manhattan)> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/306480/10/3> ;
        ont:subject       <http://dbpedia.org/resource/James_Rosenquist> .

<http://w3id.org/polifonia/pilot/meetups/306480/4/3>
        rdf:type            ont:Sentence ;
        ont:excerpt         "From 1957 to 1960, Rosenquist earned his living as a billboard painter." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/306480/4> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/306480/4/2> ;
        ont:subject         <http://dbpedia.org/resource/James_Rosenquist> ;
        ont:timeExpression  "From 1957 to 1960" .

<http://w3id.org/polifonia/pilot/meetups/306480/5/21>
        rdf:type          ont:Sentence ;
        ont:excerpt       "After his acclaim, Rosenquist produced large-scale commissions." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/306480/5> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/306480/5/20> ;
        ont:subject       <http://dbpedia.org/resource/James_Rosenquist> .

<http://w3id.org/polifonia/pilot/meetups/306480/22>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/306480/22/0> .

<http://w3id.org/polifonia/pilot/meetups/306480/2/9>
        rdf:type          ont:Sentence ;
        ont:excerpt       "It sounds ambitious, but I wanted to go to mural school\"." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/306480/2> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/306480/2/8> ;
        ont:subject       <http://dbpedia.org/resource/James_Rosenquist> .

<http://w3id.org/polifonia/pilot/meetups/306480/0>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/306480/0/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/306480/0/1> ;
        rdf:_3    <http://w3id.org/polifonia/pilot/meetups/306480/0/2> ;
        rdf:_4    <http://w3id.org/polifonia/pilot/meetups/306480/0/3> .

<http://w3id.org/polifonia/pilot/meetups/306480/8/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Rosenquist uses icons in pop culture to examine fame and the relationship between advertising and the consumer, exploring the kind of fame and iconography that comes with American politics." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/306480/8> ;
        ont:person        <http://dbpedia.org/resource/Politics_of_the_United_States> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/306480/8/1> ;
        ont:subject       <http://dbpedia.org/resource/James_Rosenquist> .

<http://w3id.org/polifonia/pilot/meetups/306480/5/19>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Rosenquist's paintings have been on display in the lobby of Key Tower in Cleveland, Ohio." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/306480/5> ;
        ont:place         <http://dbpedia.org/resource/Ohio> , <http://dbpedia.org/resource/Key_Tower> , <http://dbpedia.org/resource/Cleveland> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/306480/5/18> ;
        ont:subject       <http://dbpedia.org/resource/James_Rosenquist> .

<http://w3id.org/polifonia/pilot/meetups/306480/7/2>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Done in oil on two separate  pieces of canvas, the work exemplifies the beginnings of the pop art movement in the way that Rosenquist takes imagery from mass media, using a picture of a tomato and a clipping from an ad for hand cream." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/306480/7> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/306480/7/1> ;
        ont:subject         <http://dbpedia.org/resource/James_Rosenquist> ;
        ont:timeExpression  "oil on two" .

<http://w3id.org/polifonia/pilot/meetups/306480/2/3>
        rdf:type            ont:Sentence ;
        ont:excerpt         "In junior high school, Rosenquist won a short-term scholarship to study at the Minneapolis School of Art and subsequently studied painting at the University of Minnesota from 1952 to 1954." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/306480/2> ;
        ont:place           <http://dbpedia.org/resource/Minnesota> , <http://dbpedia.org/resource/Minneapolis> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/306480/2/2> ;
        ont:subject         <http://dbpedia.org/resource/James_Rosenquist> ;
        ont:timeExpression  "from 1952 to 1954" .

<http://w3id.org/polifonia/pilot/meetups/306480/5/8>
        rdf:type          ont:Sentence ;
        ont:excerpt       "It's as if someone were inviting you to inspect the fist with which he simultaneously punches you." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/306480/5> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/306480/5/7> ;
        ont:subject       <http://dbpedia.org/resource/James_Rosenquist> .

<http://w3id.org/polifonia/pilot/meetups/306480/12/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1976, a year after his divorce, he moved to Aripeka, Florida." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/306480/12> ;
        ont:person        <http://dbpedia.org/resource/Divorce> ;
        ont:place         <http://dbpedia.org/resource/Aripeka,_Florida> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/306480/12/2> ;
        ont:subject       <http://dbpedia.org/resource/James_Rosenquist> .

<http://w3id.org/polifonia/pilot/meetups/306480/5/13>
        rdf:type          ont:Sentence ;
        ont:excerpt       "I didn't meet Andy Warhol until 1964." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/306480/5> ;
        ont:person        <http://dbpedia.org/resource/Andy_Warhol> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/306480/5/12> ;
        ont:subject       <http://dbpedia.org/resource/James_Rosenquist> .

<http://w3id.org/polifonia/pilot/meetups/306480/14>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/306480/14/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/306480/14/1> .

<http://w3id.org/polifonia/pilot/meetups/306480/8/11>
        rdf:type            ont:Sentence ;
        ont:excerpt         "The painting initially was intended to cover all four walls of the main room within the Castelli gallery in Manhattan, occupying the entirety of each wall without any kind of visual relief, to cast an imposing, continuous view of the war." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/306480/8> ;
