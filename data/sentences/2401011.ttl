<http://w3id.org/polifonia/pilot/meetups/2401011/4/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "==" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2401011/4> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2401011/4/2> ;
