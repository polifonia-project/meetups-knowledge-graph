<http://w3id.org/polifonia/pilot/meetups/1153177/10/3>
        rdf:type            ont:Sentence ;
        ont:excerpt         "He also runs a website, Al Lowe's Humor Site, and CyberJoke 3000, a daily joke mailing list." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1153177/10> ;
        ont:person          <http://dbpedia.org/resource/Al_Lowe> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1153177/10/2> ;
