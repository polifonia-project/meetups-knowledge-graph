        rdf:type          ont:Sentence ;
        ont:excerpt       "Following this setback, Lowe expressed serious doubts whether he'd ever reenter the gaming industry again." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1153177/6> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1153177/6/3> ;
