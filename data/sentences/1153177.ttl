        rdf:type            ont:Sentence ;
        ont:excerpt         "Al Lowe (born July 24, 1946) is an American video game designer, programmer, and musician who developed several adventure games, mostly for Sierra On-Line." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1153177/0> ;
        ont:person          <http://dbpedia.org/resource/Al_Lowe> ;
