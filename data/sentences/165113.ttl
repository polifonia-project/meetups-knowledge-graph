        ont:place           <http://dbpedia.org/resource/Hollywood> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/165113/34/2> ;
        ont:subject         <http://dbpedia.org/resource/Neil_Diamond> ;
        ont:timeExpression  "August 10 , 2012" .

<http://w3id.org/polifonia/pilot/meetups/165113/3/16>
        rdf:type          ont:Sentence ;
        ont:excerpt       "26 Diamond next attended New York University as a pre-med major on a fencing scholarship, again on the fencing team with Herb Cohen." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/165113/3> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/165113/3/15> ;
