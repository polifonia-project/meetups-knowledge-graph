        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/165113/10/9> ;
        ont:subject       <http://dbpedia.org/resource/Neil_Diamond> .

<http://w3id.org/polifonia/pilot/meetups/165113/0/3>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Thirty-eight songs by Diamond have been featured in the Top 10 on the Billboard Adult Contemporary charts." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/165113/0> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/165113/0/2> ;
        ont:subject         <http://dbpedia.org/resource/Neil_Diamond> ;
        ont:timeExpression  "10 on" .

<http://w3id.org/polifonia/pilot/meetups/165113/12/12>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Bartlett angrily responded to Diamond's lawsuit by criticizing his music as having become \"too slick...and it's not as much from his heart as it used to be.\"" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/165113/12> ;
        ont:person        <http://dbpedia.org/resource/Doveman> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/165113/12/11> ;
        ont:subject       <http://dbpedia.org/resource/Neil_Diamond> .

