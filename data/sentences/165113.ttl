        rdf:type            ont:Sentence ;
        ont:excerpt         "The performance of August 24, 1972, was recorded and released as the live double album Hot August Night." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/165113/10> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/165113/10/1> ;
