        ont:person        <http://dbpedia.org/resource/Caroline_Kennedy> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/165113/9/3> ;
        ont:subject       <http://dbpedia.org/resource/Neil_Diamond> .

<http://w3id.org/polifonia/pilot/meetups/165113/3/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "His parents were Rose (née Rapoport) and Akeeba \"Kieve\" Diamond, a dry-goods merchant." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/165113/3> ;
        ont:person        <http://dbpedia.org/resource/Solomon_Judah_Loeb_Rapoport> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/165113/3/0> ;
        ont:subject       <http://dbpedia.org/resource/Neil_Diamond> .

<http://w3id.org/polifonia/pilot/meetups/165113/41/8>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Though Sardina had died in 2006, Diamond invited his widow and her family to be his front-row guests at his show in Milwaukee, where he told them he was moved by the film." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/165113/41> ;
