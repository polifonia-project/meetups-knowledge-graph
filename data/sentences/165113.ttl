<http://w3id.org/polifonia/pilot/meetups/165113/35/20>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 2019, \"Sweet Caroline\" was selected by the Library of Congress for preservation in the National Recording Registry for being \"culturally, historically, or aesthetically significant\"." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/165113/35> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/165113/35/19> ;
