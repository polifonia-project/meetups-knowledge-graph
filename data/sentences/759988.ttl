<http://w3id.org/polifonia/pilot/meetups/759988/4/5>
        rdf:type            ont:Sentence ;
        ont:excerpt         "In 1965 he bought Pier 54." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/759988/4> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/759988/4/4> ;
        ont:subject         <http://dbpedia.org/resource/Ivar_Haglund> ;
        ont:timeExpression  "Pier 54" .

<http://w3id.org/polifonia/pilot/meetups/759988/5/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "With no direct heirs, he left his estate principally to the University of Washington School of Business in support of the University Restaurant Program." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/759988/5> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/759988/5/3> ;
        ont:subject       <http://dbpedia.org/resource/Ivar_Haglund> .

<http://w3id.org/polifonia/pilot/meetups/759988/1/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Background ==" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/759988/1> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/759988/1/0> ;
        ont:subject       <http://dbpedia.org/resource/Ivar_Haglund> .

<http://w3id.org/polifonia/pilot/meetups/759988/2/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Ivar Johan Haglund was born in Seattle, Washington, the son of pioneers Johan Ivar Haglund, a Swedish immigrant and Daisy Hanson Haglund, daughter of Norwegian immigrants." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/759988/2> ;
        ont:person        <http://dbpedia.org/resource/Ivar_Haglund> , <http://dbpedia.org/resource/Ivar_the_Boneless> ;
        ont:place         <http://dbpedia.org/resource/Seattle> , <http://dbpedia.org/resource/Norway> , <http://dbpedia.org/resource/Sweden> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/759988/2/0> ;
        ont:subject       <http://dbpedia.org/resource/Ivar_Haglund> .

<http://w3id.org/polifonia/pilot/meetups/759988/2/6>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Ivar himself was treated by Hazzard several times after his mother's death, likely due to his father's belief that Hazzard's treatment had eased his late mother's suffering and extended her life." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/759988/2> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/759988/2/5> ;
        ont:subject       <http://dbpedia.org/resource/Ivar_Haglund> .

<http://w3id.org/polifonia/pilot/meetups/759988/2>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/759988/2/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/759988/2/1> ;
        rdf:_3    <http://w3id.org/polifonia/pilot/meetups/759988/2/2> ;
        rdf:_4    <http://w3id.org/polifonia/pilot/meetups/759988/2/3> ;
        rdf:_5    <http://w3id.org/polifonia/pilot/meetups/759988/2/4> ;
        rdf:_6    <http://w3id.org/polifonia/pilot/meetups/759988/2/5> ;
        rdf:_7    <http://w3id.org/polifonia/pilot/meetups/759988/2/6> ;
        rdf:_8    <http://w3id.org/polifonia/pilot/meetups/759988/2/7> .

<http://w3id.org/polifonia/pilot/meetups/759988/4/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He expanded the fine dining and fish and chips restaurants into a regional chain." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/759988/4> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/759988/4/3> ;
        ont:subject       <http://dbpedia.org/resource/Ivar_Haglund> .

<http://w3id.org/polifonia/pilot/meetups/759988/5/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He died of a heart attack just over a year later." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/759988/5> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/759988/5/2> ;
        ont:subject       <http://dbpedia.org/resource/Ivar_Haglund> .

<http://w3id.org/polifonia/pilot/meetups/759988/1/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "==" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/759988/1> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/759988/1/0> ;
        ont:subject       <http://dbpedia.org/resource/Ivar_Haglund> .

<http://w3id.org/polifonia/pilot/meetups/759988/2/5>
        rdf:type          ont:Sentence ;
        ont:excerpt       "The official cause of death was reported as stomach cancer." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/759988/2> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/759988/2/4> ;
        ont:subject       <http://dbpedia.org/resource/Ivar_Haglund> .

<http://w3id.org/polifonia/pilot/meetups/759988/1>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/759988/1/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/759988/1/1> .

<http://w3id.org/polifonia/pilot/meetups/759988/4/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He coined its motto, \"Keep Clam.\"" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/759988/4> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/759988/4/2> ;
        ont:subject       <http://dbpedia.org/resource/Ivar_Haglund> .

<http://w3id.org/polifonia/pilot/meetups/759988/5/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1983, he was elected port commissioner after filing as a prank." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/759988/5> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/759988/5/1> ;
        ont:subject       <http://dbpedia.org/resource/Ivar_Haglund> .

<http://w3id.org/polifonia/pilot/meetups/759988/0/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Ivar Johan Haglund (March 21, 1905 – January 30, 1985) was a Seattle folk singer, restaurateur and the founder of Ivar's." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/759988/0> ;
        ont:person          <http://dbpedia.org/resource/Restaurateur> ;
        ont:place           <http://dbpedia.org/resource/Seattle> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/759988/0/0> ;
        ont:subject         <http://dbpedia.org/resource/Ivar_Haglund> ;
        ont:timeExpression  "January 30 , 1985" , "March 21 , 1905" .

<http://w3id.org/polifonia/pilot/meetups/759988/2/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "However, following autopsy, it was determined that his mother had been suffering from terminal stomach cancer, possibly for years, and that she would have died even without Hazzard's \"treatment\"." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/759988/2> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/759988/2/3> ;
        ont:subject       <http://dbpedia.org/resource/Ivar_Haglund> .
