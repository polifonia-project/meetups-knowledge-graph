<http://w3id.org/polifonia/pilot/meetups/759988/4/7>
        rdf:type            ont:Sentence ;
        ont:excerpt         "The 1980s saw the addition of multiple Ivar's Fish Bars." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/759988/4> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/759988/4/6> ;
        ont:subject         <http://dbpedia.org/resource/Ivar_Haglund> ;
        ont:timeExpression  "1980s" .

<http://w3id.org/polifonia/pilot/meetups/759988/5/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "After his neighbor on Pier 56 put up a sign reading \"Don't Feed Sea Gulls, Health Regulation\" in 1971, Haglund responded with his own sign encouraging customers to feed the seagulls." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/759988/5> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/759988/5/0> ;
        ont:subject         <http://dbpedia.org/resource/Ivar_Haglund> ;
        ont:timeExpression  "Pier 56" .

<http://w3id.org/polifonia/pilot/meetups/759988/2/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "The house on the property, which is now located at 3045 64th Avenue SW in West Seattle, is considered to be the oldest house in Seattle." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/759988/2> ;
        ont:place         <http://dbpedia.org/resource/Seattle> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/759988/2/1> ;
        ont:subject       <http://dbpedia.org/resource/Ivar_Haglund> .

<http://w3id.org/polifonia/pilot/meetups/759988/3/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Career ==" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/759988/3> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/759988/3/0> ;
        ont:subject       <http://dbpedia.org/resource/Ivar_Haglund> .

<http://w3id.org/polifonia/pilot/meetups/759988/4/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "In 1938, he opened Seattle's first aquarium along with a fish and chips counter on Pier 54." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/759988/4> ;
        ont:place           <http://dbpedia.org/resource/Seattle> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/759988/4/0> ;
        ont:subject         <http://dbpedia.org/resource/Ivar_Haglund> ;
        ont:timeExpression  "Pier 54" .

