<http://w3id.org/polifonia/pilot/meetups/2617984/12/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Xian developed pulmonary tuberculosis due to overwork and malnutrition." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2617984/12> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2617984/12/0> ;
        ont:subject       <http://dbpedia.org/resource/Xian_Xinghai> .

<http://w3id.org/polifonia/pilot/meetups/2617984/5/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He worked for film studios before going to the Communist headquarters in Yan'an, where he became dean of the Music Department at Lu Xun Institute of Arts in 1938." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2617984/5> ;
        ont:person        <http://dbpedia.org/resource/Dean_(education)> , <http://dbpedia.org/resource/Lu_Xun> ;
        ont:place         <http://dbpedia.org/resource/Yan'an> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2617984/5/2> ;
        ont:subject       <http://dbpedia.org/resource/Xian_Xinghai> .

