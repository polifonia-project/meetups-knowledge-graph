        ont:place           <http://dbpedia.org/resource/Moscow> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/2617984/12/0> ;
        ont:subject         <http://dbpedia.org/resource/Xian_Xinghai> ;
        ont:timeExpression  "age of 40" , "30 October 1945" .

