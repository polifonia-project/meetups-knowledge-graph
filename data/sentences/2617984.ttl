<http://w3id.org/polifonia/pilot/meetups/2617984/18/4>
        rdf:type            ont:Sentence ;
        ont:excerpt         "The movie relate Xian's life story from 1941, when he was in Moskow, to his death in 1945." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/2617984/18> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/2617984/18/3> ;
