        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/2617984/17/0> ;
        ont:subject         <http://dbpedia.org/resource/Xian_Xinghai> ;
        ont:timeExpression  "in the 1970s" .

<http://w3id.org/polifonia/pilot/meetups/2617984/12/1>
        rdf:type            ont:Sentence ;
        ont:excerpt         "After the war, Xian returned to Moscow for medical treatment but died of pulmonary disease on 30 October 1945 at the age of 40." ;
