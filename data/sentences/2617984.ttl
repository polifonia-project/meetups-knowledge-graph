<http://w3id.org/polifonia/pilot/meetups/2617984/15/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "During the Cultural Revolution (1966–1976), when all the Western and some Chinese art was forbidden, the pianist Yin Chengzong arranged the Yellow River Cantata into a concerto for piano and orchestra, by the name of Yellow River Piano Concerto (1969)." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/2617984/15> ;
        ont:person          <http://dbpedia.org/resource/Yin_Chengzong> , <http://dbpedia.org/resource/Chinese_art> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/2617984/15/0> ;
        ont:subject         <http://dbpedia.org/resource/Xian_Xinghai> ;
        ont:timeExpression  "1969" , "1966–1976" .

<http://w3id.org/polifonia/pilot/meetups/2617984/8>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/2617984/8/0> .

<http://w3id.org/polifonia/pilot/meetups/2617984/3/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "During this period he composed Wind, Song of a Wanderer, Violin Sonata in D Minor, and other works." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2617984/3> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2617984/3/3> ;
        ont:subject       <http://dbpedia.org/resource/Xian_Xinghai> .

<http://w3id.org/polifonia/pilot/meetups/2617984/21/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "A statue of Xian has been erected in People's Park of central Guangzhou." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2617984/21> ;
        ont:place         <http://dbpedia.org/resource/Guangzhou> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2617984/21/0> ;
        ont:subject       <http://dbpedia.org/resource/Xian_Xinghai> .

<http://w3id.org/polifonia/pilot/meetups/2617984/14/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Xian's influence in Chinese music won him the title \"People's Composer\"." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2617984/14> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2617984/14/0> ;
        ont:subject       <http://dbpedia.org/resource/Xian_Xinghai> .

<http://w3id.org/polifonia/pilot/meetups/2617984/7/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "It was here that he composed the symphonies Liberation of the Nation and Sacred War, and the suites Red All Over the River and Chinese Rhapsody for winds and strings." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2617984/7> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2617984/7/2> ;
        ont:subject       <http://dbpedia.org/resource/Xian_Xinghai> .

<http://w3id.org/polifonia/pilot/meetups/2617984/2/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "His teacher, Ou Jianfu, first noticed Xian Xinghai's musical talent, and he was enrolled into the school's military band." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2617984/2> ;
        ont:person        <http://dbpedia.org/resource/Xian_Xinghai> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2617984/2/3> ;
        ont:subject       <http://dbpedia.org/resource/Xian_Xinghai> .

<http://w3id.org/polifonia/pilot/meetups/2617984/20/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Both the Xinghai Conservatory of Music and the Xinghai Concert Hall in Guangzhou are named after Xian." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2617984/20> ;
        ont:place         <http://dbpedia.org/resource/Guangzhou> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2617984/20/0> ;
        ont:subject       <http://dbpedia.org/resource/Xian_Xinghai> .

