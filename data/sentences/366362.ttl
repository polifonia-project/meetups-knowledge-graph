        rdf:type          ont:Sentence ;
        ont:excerpt       "She was also employed as a payroll manager and did some modelling for local clothing stores." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/366362/2> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/366362/2/2> ;
