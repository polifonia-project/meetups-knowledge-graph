<http://w3id.org/polifonia/pilot/meetups/300931/17/5>
        rdf:type          ont:Sentence ;
        ont:excerpt       "After recording it, she returned to Knoxville to live with her daughter." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/300931/17> ;
        ont:place         <http://dbpedia.org/resource/Knoxville,_Tennessee> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/300931/17/4> ;
        ont:subject       <http://dbpedia.org/resource/Ida_Cox> .

<http://w3id.org/polifonia/pilot/meetups/300931/7/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1927, she married Jesse \"Tiny\" Crump, a blues piano accompanist active on the Theater Owners Booking Association vaudeville circuit." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/300931/7> ;
        ont:person        <http://dbpedia.org/resource/Accompaniment> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/300931/7/0> ;
        ont:subject       <http://dbpedia.org/resource/Ida_Cox> .

<http://w3id.org/polifonia/pilot/meetups/300931/2/1>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Many sources give her birth date as February 26, 1896, but the researchers Bob Eagle and Eric LeBlanc have suggested she was born in 1888 and noted other evidence suggesting 1894." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/300931/2> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/300931/2/0> ;
        ont:subject         <http://dbpedia.org/resource/Ida_Cox> ;
        ont:timeExpression  "February 26 , 1896" .

<http://w3id.org/polifonia/pilot/meetups/300931/20/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "==" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/300931/20> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/300931/20/0> ;
        ont:subject       <http://dbpedia.org/resource/Ida_Cox> .

<http://w3id.org/polifonia/pilot/meetups/300931/13/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1929, Cox and Crump formed their tent show revue, Raisin' Cain (after the biblical story of Cain and Abel and the resulting colloquialism)." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/300931/13> ;
        ont:person        <http://dbpedia.org/resource/Cain_and_Abel> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/300931/13/0> ;
        ont:subject       <http://dbpedia.org/resource/Ida_Cox> .

<http://w3id.org/polifonia/pilot/meetups/300931/6/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In the early 1920s, she married Eugene Williams and gave birth to a daughter, Helen." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/300931/6> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/300931/6/0> ;
        ont:subject       <http://dbpedia.org/resource/Ida_Cox> .

<http://w3id.org/polifonia/pilot/meetups/300931/1/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Childhood and early career ==" ;
