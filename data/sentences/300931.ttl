        rdf:_7    <http://w3id.org/polifonia/pilot/meetups/300931/11/6> .

<http://w3id.org/polifonia/pilot/meetups/300931/19/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "At the height of the classic female blues era, competition was stiff, with numerous talented blueswomen performing, and Cox's singing was only part of her act." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/300931/19> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/300931/19/1> ;
        ont:subject       <http://dbpedia.org/resource/Ida_Cox> .

<http://w3id.org/polifonia/pilot/meetups/300931/21/3>
        rdf:type            ont:Sentence ;
        ont:excerpt         "She was one of the few female blues singers of the time to write her own songs." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/300931/21> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/300931/21/2> ;
        ont:subject         <http://dbpedia.org/resource/Ida_Cox> ;
        ont:timeExpression  "one of" .

<http://w3id.org/polifonia/pilot/meetups/300931/14/3>
        rdf:type            ont:Sentence ;
        ont:excerpt         "In the early 1930s drummer Earl Palmer entered show business as a tap dancer in Cox's Darktown Scandals revue." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/300931/14> ;
        ont:person          <http://dbpedia.org/resource/Earl_Palmer> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/300931/14/2> ;
        ont:subject         <http://dbpedia.org/resource/Ida_Cox> ;
        ont:timeExpression  "1930s drummer" .

<http://w3id.org/polifonia/pilot/meetups/300931/24>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/300931/24/0> .

<http://w3id.org/polifonia/pilot/meetups/300931/2/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Cox joined the local African Methodist Choir at an early age and developed an interest in gospel music and performance." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/300931/2> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/300931/2/3> ;
        ont:subject       <http://dbpedia.org/resource/Ida_Cox> .

<http://w3id.org/polifonia/pilot/meetups/300931/13/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "The show had sixteen chorus girls, comics, and backup singers." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/300931/13> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/300931/13/2> ;
        ont:subject       <http://dbpedia.org/resource/Ida_Cox> .

