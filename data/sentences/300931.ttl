        rdf:_8    <http://w3id.org/polifonia/pilot/meetups/300931/2/7> .

<http://w3id.org/polifonia/pilot/meetups/300931/0/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Ida Cox (born Ida M. Prather, February 26, 1888 or 1896 – November 10, 1967) was an American singer and vaudeville performer, best known for her blues performances and recordings." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/300931/0> ;
        ont:person          <http://dbpedia.org/resource/Ida_Cox> ;
        ont:place           <http://dbpedia.org/resource/United_States> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/300931/0/0> ;
        ont:subject         <http://dbpedia.org/resource/Ida_Cox> ;
        ont:timeExpression  "February 26 , 1888" , "November 10 , 1967" .

<http://w3id.org/polifonia/pilot/meetups/300931/3/5>
        rdf:type          ont:Sentence ;
        ont:excerpt       "When she was not singing, Cox performed as a sharp-witted comedian in vaudeville shows, gaining stage experience and cultivating her stage presence." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/300931/3> ;
        ont:person        <http://dbpedia.org/resource/Comedian> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/300931/3/4> ;
        ont:subject       <http://dbpedia.org/resource/Ida_Cox> .

<http://w3id.org/polifonia/pilot/meetups/300931/12>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/300931/12/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/300931/12/1> .

<http://w3id.org/polifonia/pilot/meetups/300931/21/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Through her raw and sharp lyricism, Cox in her songs described the complex social realities of poor and working class African Americans in the early twentieth century." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/300931/21> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/300931/21/3> ;
        ont:subject       <http://dbpedia.org/resource/Ida_Cox> .

<http://w3id.org/polifonia/pilot/meetups/300931/19/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Nevertheless, as her career developed, she assumed and embodied the title bestowed on her, \"The Uncrowned Queen of the Blues\"." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/300931/19> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/300931/19/2> ;
        ont:subject       <http://dbpedia.org/resource/Ida_Cox> .

<http://w3id.org/polifonia/pilot/meetups/300931/25>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/300931/25/0> .

<http://w3id.org/polifonia/pilot/meetups/300931/2/5>
        rdf:type            ont:Sentence ;
        ont:excerpt         "At the age of 14, she left home to tour with White and Clark's Black & Tan Minstrels." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/300931/2> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/300931/2/4> ;
        ont:subject         <http://dbpedia.org/resource/Ida_Cox> ;
        ont:timeExpression  "age of 14" .

<http://w3id.org/polifonia/pilot/meetups/300931/13/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "The Raisin' Cain tent show proved so popular that in 1929 it became the first show associated with the Theater Owners' Booking Association circuit to open at the famed Apollo Theater, in Harlem, New York." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/300931/13> ;
        ont:place         <http://dbpedia.org/resource/Harlem> , <http://dbpedia.org/resource/Apollo_Theater> , <http://dbpedia.org/resource/New_York_City> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/300931/13/3> ;
        ont:subject       <http://dbpedia.org/resource/Ida_Cox> .

<http://w3id.org/polifonia/pilot/meetups/300931/7>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/300931/7/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/300931/7/1> .

<http://w3id.org/polifonia/pilot/meetups/300931/17/3>
        rdf:type            ont:Sentence ;
        ont:excerpt         "A review in The New York Times said that Cox at the age of 65 had lost quality in range and intonation but retained her charismatic and expressive delivery." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/300931/17> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/300931/17/2> ;
        ont:subject         <http://dbpedia.org/resource/Ida_Cox> ;
        ont:timeExpression  "age of 65" .

<http://w3id.org/polifonia/pilot/meetups/300931/32>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/300931/32/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/300931/32/1> .

<http://w3id.org/polifonia/pilot/meetups/300931/16/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "She continued to perform until 1945, when she was forced into retirement after a debilitating stroke which occurred during a performance at a nightclub in Buffalo, New York." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/300931/16> ;
        ont:place         <http://dbpedia.org/resource/Buffalo,_New_York> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/300931/16/2> ;
        ont:subject       <http://dbpedia.org/resource/Ida_Cox> .

<http://w3id.org/polifonia/pilot/meetups/300931/1>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/300931/1/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/300931/1/1> .

<http://w3id.org/polifonia/pilot/meetups/300931/11/4>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Between September 1923 and October 1929, she recorded 78 titles for Paramount." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/300931/11> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/300931/11/3> ;
        ont:subject         <http://dbpedia.org/resource/Ida_Cox> ;
        ont:timeExpression  "October 1929" , "September 1923" .

<http://w3id.org/polifonia/pilot/meetups/300931/17>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/300931/17/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/300931/17/1> ;
        rdf:_3    <http://w3id.org/polifonia/pilot/meetups/300931/17/2> ;
        rdf:_4    <http://w3id.org/polifonia/pilot/meetups/300931/17/3> ;
        rdf:_5    <http://w3id.org/polifonia/pilot/meetups/300931/17/4> ;
        rdf:_6    <http://w3id.org/polifonia/pilot/meetups/300931/17/5> ;
        rdf:_7    <http://w3id.org/polifonia/pilot/meetups/300931/17/6> ;
        rdf:_8    <http://w3id.org/polifonia/pilot/meetups/300931/17/7> .

<http://w3id.org/polifonia/pilot/meetups/300931/9/3>
        rdf:type            ont:Sentence ;
        ont:excerpt         "In March 1922, a performance by Cox at the Beale Street Palace, in Memphis, Tennessee, was aired on radio station WMC with positive reviews, leading to exposure to a wider audience." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/300931/9> ;
        ont:place           <http://dbpedia.org/resource/Tennessee> , <http://dbpedia.org/resource/Beale_Street> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/300931/9/2> ;
        ont:subject         <http://dbpedia.org/resource/Ida_Cox> ;
        ont:timeExpression  "March 1922" .

<http://w3id.org/polifonia/pilot/meetups/300931/3/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "There were no microphones; the weaker voiced singers used a megaphone, but most of the featured women blues singers scorned such aids to volume.\"" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/300931/3> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/300931/3/3> ;
        ont:subject       <http://dbpedia.org/resource/Ida_Cox> .

