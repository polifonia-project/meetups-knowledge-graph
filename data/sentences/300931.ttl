        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/300931/15/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/300931/15/1> .

<http://w3id.org/polifonia/pilot/meetups/300931/9/1>
        rdf:type            ont:Sentence ;
        ont:excerpt         "In 1920, she left the vaudeville circuit briefly to appear as a headline act at the 81 Theatre in Atlanta, with the pianist Jelly Roll Morton." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/300931/9> ;
        ont:person          <http://dbpedia.org/resource/Jelly_Roll_Morton> ;
        ont:place           <http://dbpedia.org/resource/Atlanta> ;
