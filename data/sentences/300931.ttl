<http://w3id.org/polifonia/pilot/meetups/300931/11/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Paramount called her \"The Uncrowned Queen of Blues\"." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/300931/11> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/300931/11/2> ;
        ont:subject       <http://dbpedia.org/resource/Ida_Cox> .

<http://w3id.org/polifonia/pilot/meetups/300931/16/2>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Cox recorded for Vocalion in 1939 and Okeh in 1940 with bands that included Charlie Christian, Hot Lips Page, Red Allen, J. C. Higginbotham, Lionel Hampton." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/300931/16> ;
        ont:person          <http://dbpedia.org/resource/Charlie_Christian> , <http://dbpedia.org/resource/Red_Allen> , <http://dbpedia.org/resource/Hot_Lips_Page> ;
        ont:place           <http://dbpedia.org/resource/Hampton,_Virginia> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/300931/16/1> ;
        ont:subject         <http://dbpedia.org/resource/Ida_Cox> ;
        ont:timeExpression  "1940 with" .

<http://w3id.org/polifonia/pilot/meetups/300931/16>
