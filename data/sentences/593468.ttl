<http://w3id.org/polifonia/pilot/meetups/593468/11/5>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Lavoe was part of the group when the All-Stars returned to Yankee Stadium in 1975, where the band recorded a two volume production entitled Live at Yankee Stadium." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/593468/11> ;
        ont:place           <http://dbpedia.org/resource/Yankee_Stadium> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/593468/11/4> ;
        ont:subject         <http://dbpedia.org/resource/Héctor_Lavoe> ;
        ont:timeExpression  "two volume production" .

