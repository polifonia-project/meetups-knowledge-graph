        rdf:type            ont:Sentence ;
        ont:excerpt         "His last major concert performance was the Music for Montserrat all-star charity concert at London's Royal Albert Hall on September 15, 1997, four months before his death." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/98513/32> ;
