        ont:person          <http://dbpedia.org/resource/Johnny_Cash> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/98513/22/0> ;
        ont:subject         <http://dbpedia.org/resource/Carl_Perkins> ;
        ont:timeExpression  "troupe in 1968" .

<http://w3id.org/polifonia/pilot/meetups/98513/6/28>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Years later the musician Gene Vincent told an interviewer, rather than \"Blue Moon of Kentucky\" being a \"new sound\", \"a lot of people were doing it before that, especially Carl Perkins.\"" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/98513/6> ;
        ont:person        <http://dbpedia.org/resource/Gene_Vincent> , <http://dbpedia.org/resource/Carl_Perkins> ;
