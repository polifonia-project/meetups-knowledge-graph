<http://w3id.org/polifonia/pilot/meetups/1433271/2/6>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Ayatollah Khomeini had decreed: \"Women's voices should not be heard by men other than members of their own families." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1433271/2> ;
        ont:person        <http://dbpedia.org/resource/Ruhollah_Khomeini> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1433271/2/5> ;
        ont:subject       <http://dbpedia.org/resource/Marzieh_(singer)> .

