<http://w3id.org/polifonia/pilot/meetups/1433271/4/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1994, Marzieh left Iran forever due to the political repression, making her new home in Paris." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1433271/4> ;
        ont:place         <http://dbpedia.org/resource/Paris> , <http://dbpedia.org/resource/Iran> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1433271/4/0> ;
        ont:subject       <http://dbpedia.org/resource/Marzieh_(singer)> .

<http://w3id.org/polifonia/pilot/meetups/1433271/14>
        rdf:type  ont:Paragraph ;
