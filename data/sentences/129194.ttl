<http://w3id.org/polifonia/pilot/meetups/129194/7/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Epstein's parents moved him from one boarding school to another, including Clayesmore School in Dorset." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/129194/7> ;
        ont:place         <http://dbpedia.org/resource/Dorset> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/129194/7/3> ;
