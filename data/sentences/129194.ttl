<http://w3id.org/polifonia/pilot/meetups/129194/0/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Brian Samuel Epstein (; 19 September 1934 – 27 August 1967) was a British music entrepreneur who managed the Beatles from 1962 until his death." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/129194/0> ;
        ont:person          <http://dbpedia.org/resource/Samuel_Epstein> ;
