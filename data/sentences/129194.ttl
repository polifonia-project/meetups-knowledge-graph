<http://w3id.org/polifonia/pilot/meetups/129194/37/14>
        rdf:type          ont:Sentence ;
        ont:excerpt       "McCartney sent him a letter directing him to have no contact with any of them in the future." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/129194/37> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/129194/37/13> ;
