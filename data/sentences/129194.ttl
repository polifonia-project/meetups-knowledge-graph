        ont:subject         <http://dbpedia.org/resource/Brian_Epstein> ;
        ont:timeExpression  "27 August 1967" , "19 September 1934" , "1962 until" .

<http://w3id.org/polifonia/pilot/meetups/129194/45/11>
        rdf:type          ont:Sentence ;
        ont:excerpt       "She attended and failing to rouse Epstein called his doctor who, along with the butler, broke down the door and found Epstein in his bed, appearing to be asleep, a book opened near his hand and some digestive biscuits on the nightstand." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/129194/45> ;
        ont:person        <http://dbpedia.org/resource/Butler> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/129194/45/10> ;
        ont:subject       <http://dbpedia.org/resource/Brian_Epstein> .

<http://w3id.org/polifonia/pilot/meetups/129194/47/13>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Tiwary said about that project that the film would \"be less a music bio and more of an inspirational human-interest story about an outsider\"." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/129194/47> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/129194/47/12> ;
        ont:subject       <http://dbpedia.org/resource/Brian_Epstein> .

