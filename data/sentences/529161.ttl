        ont:timeExpression  "1981" .

<http://w3id.org/polifonia/pilot/meetups/529161/15/1>
        rdf:type            ont:Sentence ;
        ont:excerpt         "In 1990, the four joined forces with the 1983–88 line-up of Yes (Chris Squire, Alan White, Trevor Rabin, and Tony Kaye) to contribute songs for the Yes album Union (1991)." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/529161/15> ;
        ont:person          <http://dbpedia.org/resource/Tony_Kaye_(musician)> , <http://dbpedia.org/resource/Chris_Squire> , <http://dbpedia.org/resource/Trevor_Rabin> , <http://dbpedia.org/resource/Alan_White_(Yes_drummer)> ;
        ont:place           <http://dbpedia.org/resource/United_States> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/529161/15/0> ;
        ont:subject         <http://dbpedia.org/resource/Steve_Howe> ;
        ont:timeExpression  "1991" , "four" , "1983–88 line-up" .

