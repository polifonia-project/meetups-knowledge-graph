<http://w3id.org/polifonia/pilot/meetups/529161/26/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Personal life ==" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/529161/26> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/529161/26/0> ;
