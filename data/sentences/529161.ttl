        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/529161/30> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/529161/30/0> ;
        ont:subject       <http://dbpedia.org/resource/Steve_Howe> .

<http://w3id.org/polifonia/pilot/meetups/529161/4/1>
        rdf:type            ont:Sentence ;
        ont:excerpt         "He grew up in a middle-class family apartment with three older siblings, brothers Phillip and John, and sister Stella." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/529161/4> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/529161/4/0> ;
        ont:subject         <http://dbpedia.org/resource/Steve_Howe> ;
        ont:timeExpression  "family apartment with three" .

<http://w3id.org/polifonia/pilot/meetups/529161/11/6>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Howe continued with the band until Yes officially split up in early 1981." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/529161/11> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/529161/11/5> ;
        ont:subject       <http://dbpedia.org/resource/Steve_Howe> .

<http://w3id.org/polifonia/pilot/meetups/529161/4/12>
        rdf:type            ont:Sentence ;
        ont:excerpt         "The only book he read, he claimed, was Dance Band Chords for the Guitar (1946) by Eric Kershaw." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/529161/4> ;
        ont:person          <http://dbpedia.org/resource/Ian_Kershaw> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/529161/4/11> ;
        ont:subject         <http://dbpedia.org/resource/Steve_Howe> ;
        ont:timeExpression  "1946" .

<http://w3id.org/polifonia/pilot/meetups/529161/9/6>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Howe's proficiency with a wide range of guitars, and his strong contributions to the songwriting, made him a prolific member who was an essential part of the band's change in musical direction towards progressive rock." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/529161/9> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/529161/9/5> ;
        ont:subject       <http://dbpedia.org/resource/Steve_Howe> .

<http://w3id.org/polifonia/pilot/meetups/529161/14/10>
        rdf:type            ont:Sentence ;
        ont:excerpt         "The lead single, \"When the Heart Rules the Mind\", went to No. 14 on the Billboard Hot 100 chart." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/529161/14> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/529161/14/9> ;
        ont:subject         <http://dbpedia.org/resource/Steve_Howe> ;
        ont:timeExpression  "14 on" , "100 chart" .

<http://w3id.org/polifonia/pilot/meetups/529161/27/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1972, Howe became a vegetarian and avoids taking unnecessary pharmaceutical drugs." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/529161/27> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/529161/27/3> ;
        ont:subject       <http://dbpedia.org/resource/Steve_Howe> .

<http://w3id.org/polifonia/pilot/meetups/529161/5/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Career ==" ;
