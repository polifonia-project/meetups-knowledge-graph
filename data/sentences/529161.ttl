        rdf:type          ont:Sentence ;
        ont:excerpt       "===" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/529161/8> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/529161/8/0> ;
