        ont:timeExpression  "May 1965" , "48 on" .

<http://w3id.org/polifonia/pilot/meetups/529161/14/9>
        rdf:type            ont:Sentence ;
        ont:excerpt         "It reached No. 11 in the US and was certified gold, and it peaked at No. 41 in the UK." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/529161/14> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/529161/14/8> ;
        ont:subject         <http://dbpedia.org/resource/Steve_Howe> ;
        ont:timeExpression  "41 in" , "11 in" .

