<http://w3id.org/polifonia/pilot/meetups/529161/1/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Upon joining Yes in 1970, Howe helped to change the band's musical direction, leading to more commercial and critical success." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/529161/1> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/529161/1/0> ;
