<http://w3id.org/polifonia/pilot/meetups/529161/7/14>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Howe released the tracks for the proposed album after he obtained the recordings and remixed them himself as The Bodast Tapes (1981)." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/529161/7> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/529161/7/13> ;
