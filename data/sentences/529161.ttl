        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/529161/37> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/529161/37/1> ;
        ont:subject       <http://dbpedia.org/resource/Steve_Howe> .

<http://w3id.org/polifonia/pilot/meetups/529161/22/4>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Howe released his new studio album Love Is on 31 July 2020, his first one in nine years." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/529161/22> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/529161/22/3> ;
        ont:subject         <http://dbpedia.org/resource/Steve_Howe> ;
        ont:timeExpression  "one in" , "31 July 2020" .

<http://w3id.org/polifonia/pilot/meetups/529161/4/17>
        rdf:type          ont:Sentence ;
        ont:excerpt       "I stood on the side of the stage, played my songs, never looked up\"." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/529161/4> ;
