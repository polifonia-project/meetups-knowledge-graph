<http://w3id.org/polifonia/pilot/meetups/529161/17/5>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Upon release, it reached No. 15 on the Billboard Top New Age Albums chart." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/529161/17> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/529161/17/4> ;
        ont:subject         <http://dbpedia.org/resource/Steve_Howe> ;
        ont:timeExpression  "15 on" .

<http://w3id.org/polifonia/pilot/meetups/529161/15>
        rdf:type  ont:Paragraph ;
