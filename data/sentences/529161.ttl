<http://w3id.org/polifonia/pilot/meetups/529161/21/9>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He was replaced by Sam Coulson." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/529161/21> ;
