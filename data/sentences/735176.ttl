<http://w3id.org/polifonia/pilot/meetups/735176/4/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Bingham supported the introduction of market values along with Christianity." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/735176/4> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/735176/4/3> ;
        ont:subject       <http://dbpedia.org/resource/Hiram_Bingham_I> .

<http://w3id.org/polifonia/pilot/meetups/735176/13/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "His hymns are still sung in Hawaii at the churches and by the choruses in concert." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/735176/13> ;
        ont:place         <http://dbpedia.org/resource/Hawaii> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/735176/13/3> ;
        ont:subject       <http://dbpedia.org/resource/Hiram_Bingham_I> .

