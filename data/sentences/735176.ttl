<http://w3id.org/polifonia/pilot/meetups/735176/0/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Hiram Bingham, formally Hiram Bingham I (October 30, 1789 – November 11, 1869), was leader of the first group of American Protestant missionaries to introduce Christianity to the Hawaiian islands." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/735176/0> ;
        ont:person          <http://dbpedia.org/resource/Missionary> , <http://dbpedia.org/resource/Hiram_Bingham_I> ;
        ont:place           <http://dbpedia.org/resource/Bingham_railway_station> , <http://dbpedia.org/resource/United_States> , <http://dbpedia.org/resource/Hawaiian_Islands> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/735176/0/0> ;
        ont:subject         <http://dbpedia.org/resource/Hiram_Bingham_I> ;
        ont:timeExpression  "October 30 , 1789" , "November 11 , 1869" .

<http://w3id.org/polifonia/pilot/meetups/735176/7/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "==" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/735176/7> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/735176/7/0> ;
        ont:subject       <http://dbpedia.org/resource/Hiram_Bingham_I> .

<http://w3id.org/polifonia/pilot/meetups/735176/6/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Bingham used his influence with Queen Kaʻahumanu to instigate a strongly anti-Catholic policy in Hawaii, considerably impeding the work of the French Catholic missionary Alexis Bachelot and resulting in decades of persecution of Hawaiians who were converted to Catholicism." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/735176/6> ;
        ont:person        <http://dbpedia.org/resource/Alexis_of_Russia> ;
        ont:place         <http://dbpedia.org/resource/Bingham_railway_station> , <http://dbpedia.org/resource/Hawaiian_Kingdom> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/735176/6/0> ;
        ont:subject       <http://dbpedia.org/resource/Hiram_Bingham_I> .

<http://w3id.org/polifonia/pilot/meetups/735176/6>
        rdf:type  ont:Paragraph ;
