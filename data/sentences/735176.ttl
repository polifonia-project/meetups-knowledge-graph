<http://w3id.org/polifonia/pilot/meetups/735176/2/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He needed to be married to be accepted as a missionary." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/735176/2> ;
        ont:person        <http://dbpedia.org/resource/Missionary> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/735176/2/3> ;
        ont:subject       <http://dbpedia.org/resource/Hiram_Bingham_I> .

<http://w3id.org/polifonia/pilot/meetups/735176/11/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Bingham remained in New England, where he served as the pastor of an African-American church." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/735176/11> ;
        ont:place         <http://dbpedia.org/resource/New_England> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/735176/11/3> ;
        ont:subject       <http://dbpedia.org/resource/Hiram_Bingham_I> .

<http://w3id.org/polifonia/pilot/meetups/735176/15>
        rdf:type  ont:Paragraph ;
