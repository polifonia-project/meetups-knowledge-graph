<http://w3id.org/polifonia/pilot/meetups/735176/2/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "After breaking his first engagement, Bingham found a new bride, Sybil Moseley." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/735176/2> ;
        ont:person        <http://dbpedia.org/resource/Bride> ;
        ont:place         <http://dbpedia.org/resource/Moseley> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/735176/2/2> ;
        ont:subject       <http://dbpedia.org/resource/Hiram_Bingham_I> .

<http://w3id.org/polifonia/pilot/meetups/735176/11/3>
        rdf:type            ont:Sentence ;
        ont:excerpt         "He published a memoir, A Residence of Twenty-One Years in the Sandwich Islands in 1847." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/735176/11> ;
        ont:place           <http://dbpedia.org/resource/Hawaiian_Islands> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/735176/11/2> ;
