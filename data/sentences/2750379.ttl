        ont:subject       <http://dbpedia.org/resource/Tommy_Reilly_(harmonica_player)> .

<http://w3id.org/polifonia/pilot/meetups/2750379/4/4>
        rdf:type            ont:Sentence ;
        ont:excerpt         "More than 30 concert works were composed for Reilly, including Michael Spivakovsky's Harmonica Concerto of 1951 and fellow Canadian Robert Farnon's Prelude and Dance for Harmonica and Orchestra." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/2750379/4> ;
        ont:person          <http://dbpedia.org/resource/Robert_Farnon> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/2750379/4/3> ;
        ont:subject         <http://dbpedia.org/resource/Tommy_Reilly_(harmonica_player)> ;
        ont:timeExpression  "30 concert" .

