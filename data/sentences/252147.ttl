        rdf:type            ont:Sentence ;
        ont:excerpt         "It later appeared on the 1981 album Dead Ringer with a new title, \"Nocturnal Pleasure\"." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/252147/18> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/252147/18/2> ;
        ont:subject         <http://dbpedia.org/resource/Jim_Steinman> ;
        ont:timeExpression  "1981 album" .

<http://w3id.org/polifonia/pilot/meetups/252147/55/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "===" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/252147/55> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/252147/55/0> ;
        ont:subject       <http://dbpedia.org/resource/Jim_Steinman> .

<http://w3id.org/polifonia/pilot/meetups/252147/28/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Steven Rinkoff was, as usual, credited as co-producer, and others received co-producer credits." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/252147/28> ;
        ont:person        <http://dbpedia.org/resource/Steven_Rinkoff> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/252147/28/3> ;
        ont:subject       <http://dbpedia.org/resource/Jim_Steinman> .

<http://w3id.org/polifonia/pilot/meetups/252147/38/5>
        rdf:type          ont:Sentence ;
        ont:excerpt       "This demo was part of Steinman's preparations for the unrealized Batman: The Musical." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/252147/38> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/252147/38/4> ;
        ont:subject       <http://dbpedia.org/resource/Jim_Steinman> .

<http://w3id.org/polifonia/pilot/meetups/252147/32/2>
        rdf:type            ont:Sentence ;
        ont:excerpt         "He also produced \"I Want to Spend My Lifetime Loving You\" (music by James Horner, lyric by Will Jennings) for the film The Mask of Zorro (1998)." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/252147/32> ;
        ont:person          <http://dbpedia.org/resource/Will_Jennings> , <http://dbpedia.org/resource/James_Horner> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/252147/32/1> ;
        ont:subject         <http://dbpedia.org/resource/Jim_Steinman> ;
        ont:timeExpression  "1998" .

<http://w3id.org/polifonia/pilot/meetups/252147/12/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1971, Steinman provided music for a puppet show titled Ubu." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/252147/12> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/252147/12/0> ;
        ont:subject       <http://dbpedia.org/resource/Jim_Steinman> .

<http://w3id.org/polifonia/pilot/meetups/252147/22/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "All of the songs on the album were written by Steinman." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/252147/22> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/252147/22/0> ;
        ont:subject       <http://dbpedia.org/resource/Jim_Steinman> .

<http://w3id.org/polifonia/pilot/meetups/252147/52/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 2017, Velvet Valve Records was preparing to release music with Karine Hannah singing." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/252147/52> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/252147/52/3> ;
        ont:subject       <http://dbpedia.org/resource/Jim_Steinman> .

<http://w3id.org/polifonia/pilot/meetups/252147/42/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "However, the Steinman song was not released until 2005." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/252147/42> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/252147/42/2> ;
        ont:subject       <http://dbpedia.org/resource/Jim_Steinman> .

<http://w3id.org/polifonia/pilot/meetups/252147/15/7>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Orchestrations were by Steven Margoshes, a frequent Steinman collaborator." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/252147/15> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/252147/15/6> ;
        ont:subject       <http://dbpedia.org/resource/Jim_Steinman> .

<http://w3id.org/polifonia/pilot/meetups/252147/25/8>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Steinman produced the track \"Love Can Make You Cry\", written by Michael Kehr, Don Kehr and Ian Hunter, for the soundtrack album for the 1986 film Iron Eagle." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/252147/25> ;
        ont:person          <http://dbpedia.org/resource/Ian_Tyson> ;
        ont:place           <http://dbpedia.org/resource/Hunter_Region> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/252147/25/7> ;
        ont:subject         <http://dbpedia.org/resource/Jim_Steinman> ;
        ont:timeExpression  "1986 film" .

<http://w3id.org/polifonia/pilot/meetups/252147/22/12>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Barry Manilow's compilation album Greatest Hits Vol. II (1983) included the song \"Read 'Em and Weep\", written, composed, and produced by Steinman." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/252147/22> ;
        ont:person          <http://dbpedia.org/resource/Barry_Manilow> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/252147/22/11> ;
        ont:subject         <http://dbpedia.org/resource/Jim_Steinman> ;
        ont:timeExpression  "1983" .

<http://w3id.org/polifonia/pilot/meetups/252147/25/15>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Four of the songs on the album were written and/or composed by Steinman." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/252147/25> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/252147/25/14> ;
        ont:subject         <http://dbpedia.org/resource/Jim_Steinman> ;
        ont:timeExpression  "Four of" .

