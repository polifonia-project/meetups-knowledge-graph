        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/252147/22/1> ;
        ont:subject         <http://dbpedia.org/resource/Jim_Steinman> ;
        ont:timeExpression  "One of" .

<http://w3id.org/polifonia/pilot/meetups/252147/12/1>
        rdf:type            ont:Sentence ;
        ont:excerpt         "The show, put on by puppeteer and filmmaker Demian, was an adaptation of Ubu on the Hill, an 1888 play by Alfred Jarry." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/252147/12> ;
        ont:person          <http://dbpedia.org/resource/Alfred_Jarry> , <http://dbpedia.org/resource/Puppeteer> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/252147/12/0> ;
        ont:subject         <http://dbpedia.org/resource/Jim_Steinman> ;
        ont:timeExpression  "1888 play" .

<http://w3id.org/polifonia/pilot/meetups/252147/42/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "This song is different from the song with the same title in Whistle Down the Wind." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/252147/42> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/252147/42/3> ;
        ont:subject       <http://dbpedia.org/resource/Jim_Steinman> .

<http://w3id.org/polifonia/pilot/meetups/252147/32/3>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Lead vocals were a duet between Tina Arena and Marc Anthony, and the track also appeared on Tina Arena's album In Deep (1997)." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/252147/32> ;
        ont:person          <http://dbpedia.org/resource/Tina_Arena> , <http://dbpedia.org/resource/Marc_Anthony> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/252147/32/2> ;
        ont:subject         <http://dbpedia.org/resource/Jim_Steinman> ;
        ont:timeExpression  "1997" .

<http://w3id.org/polifonia/pilot/meetups/252147/15/8>
        rdf:type            ont:Sentence ;
        ont:excerpt         "One song from The Confidence Man, \"Milady\", was recorded by Barry Manilow, but never released." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/252147/15> ;
        ont:person          <http://dbpedia.org/resource/Barry_Manilow> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/252147/15/7> ;
        ont:subject         <http://dbpedia.org/resource/Jim_Steinman> ;
        ont:timeExpression  "One song" .

<http://w3id.org/polifonia/pilot/meetups/252147/52/5>
        rdf:type            ont:Sentence ;
        ont:excerpt         "In March 2017, Velvet Valve Records digitally released a recording of Hannah singing \"Braver Than We Are\"." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/252147/52> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/252147/52/4> ;
        ont:subject         <http://dbpedia.org/resource/Jim_Steinman> ;
        ont:timeExpression  "March 2017" .

<http://w3id.org/polifonia/pilot/meetups/252147/25/9>
        rdf:type            ont:Sentence ;
        ont:excerpt         "This was a modified version of the original recording of the same song, which had appeared on Urgent's 1985 album Cast The First Stone." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/252147/25> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/252147/25/8> ;
        ont:subject         <http://dbpedia.org/resource/Jim_Steinman> ;
        ont:timeExpression  "1985 album" .

<http://w3id.org/polifonia/pilot/meetups/252147/22/13>
        rdf:type            ont:Sentence ;
        ont:excerpt         "It had appeared on Meat Loaf's Dead Ringer album in 1981, but with a slightly different lyric." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/252147/22> ;
        ont:person          <http://dbpedia.org/resource/Meat_Loaf> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/252147/22/12> ;
        ont:subject         <http://dbpedia.org/resource/Jim_Steinman> ;
        ont:timeExpression  "album in 1981" .

<http://w3id.org/polifonia/pilot/meetups/252147/25/16>
        rdf:type          ont:Sentence ;
        ont:excerpt       "One, \"Holding Out for a Hero\", was the same track from the Footloose soundtrack." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/252147/25> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/252147/25/15> ;
        ont:subject       <http://dbpedia.org/resource/Jim_Steinman> .

<http://w3id.org/polifonia/pilot/meetups/252147/7/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "In March 1968, Steinman contributed music for an Amherst College adaptation of Bertolt Brecht's A Man's a Man." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/252147/7> ;
        ont:person          <http://dbpedia.org/resource/Bertolt_Brecht> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/252147/7/0> ;
        ont:subject         <http://dbpedia.org/resource/Jim_Steinman> ;
        ont:timeExpression  "March 1968" .

<http://w3id.org/polifonia/pilot/meetups/252147/41>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/252147/41/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/252147/41/1> ;
        rdf:_3    <http://w3id.org/polifonia/pilot/meetups/252147/41/2> ;
        rdf:_4    <http://w3id.org/polifonia/pilot/meetups/252147/41/3> ;
        rdf:_5    <http://w3id.org/polifonia/pilot/meetups/252147/41/4> ;
        rdf:_6    <http://w3id.org/polifonia/pilot/meetups/252147/41/5> ;
        rdf:_7    <http://w3id.org/polifonia/pilot/meetups/252147/41/6> .

<http://w3id.org/polifonia/pilot/meetups/252147/4/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Steinman received his bachelor's degree from Amherst College in 1969." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/252147/4> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/252147/4/3> ;
        ont:subject       <http://dbpedia.org/resource/Jim_Steinman> .

<http://w3id.org/polifonia/pilot/meetups/252147/3>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/252147/3/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/252147/3/1> .

<http://w3id.org/polifonia/pilot/meetups/252147/11>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/252147/11/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/252147/11/1> .

<http://w3id.org/polifonia/pilot/meetups/252147/19/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Sean Kelly and Tony Hendra wrote the lyrics." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/252147/19> ;
        ont:person        <http://dbpedia.org/resource/Tony_Hendra> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/252147/19/0> ;
        ont:subject       <http://dbpedia.org/resource/Jim_Steinman> .

<http://w3id.org/polifonia/pilot/meetups/252147/29/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Steinman also produced this version, with lead vocals by Taylor Dayne." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/252147/29> ;
        ont:person        <http://dbpedia.org/resource/Taylor_Dayne> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/252147/29/1> ;
        ont:subject       <http://dbpedia.org/resource/Jim_Steinman> .

<http://w3id.org/polifonia/pilot/meetups/252147/39/3>
        rdf:type            ont:Sentence ;
        ont:excerpt         "According to an unnamed source Burton was attracted to the project because it was as dark as his 1989 film Batman rather than \"the goofy, campy turn the franchise took with Batman Forever and Batman and Robin, both of which were directed by Joel Schumacher.\"" ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/252147/39> ;
        ont:person          <http://dbpedia.org/resource/Joel_Schumacher> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/252147/39/2> ;
        ont:subject         <http://dbpedia.org/resource/Jim_Steinman> ;
        ont:timeExpression  "1989 film" .

<http://w3id.org/polifonia/pilot/meetups/252147/49/4>
        rdf:type            ont:Sentence ;
        ont:excerpt         "On April 17, 2013, the college announced that Steinman would speak at a \"conversation\" on campus that will be open to the public on May 25, and would receive an honorary degree, a doctorate of humane letters, at the college's 192nd commencement on May 26." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/252147/49> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/252147/49/3> ;
        ont:subject         <http://dbpedia.org/resource/Jim_Steinman> ;
        ont:timeExpression  "192nd commencement" , "April 17 , 2013" , "May 26" , "May 25" .

<http://w3id.org/polifonia/pilot/meetups/252147/33/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Tanz der Vampire (Dance of the Vampires), opened in Vienna, Austria on October 4, 1997." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/252147/33> ;
        ont:place           <http://dbpedia.org/resource/Vienna> , <http://dbpedia.org/resource/Austria> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/252147/33/0> ;
        ont:subject         <http://dbpedia.org/resource/Jim_Steinman> ;
        ont:timeExpression  "October 4 , 1997" .

<http://w3id.org/polifonia/pilot/meetups/252147/43/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "After the original director left the show, it was directed by John Rando, who joined very shortly before the opening." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/252147/43> ;
        ont:person        <http://dbpedia.org/resource/John_Rando> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/252147/43/0> ;
        ont:subject       <http://dbpedia.org/resource/Jim_Steinman> .

<http://w3id.org/polifonia/pilot/meetups/252147/16/5>
        rdf:type          ont:Sentence ;
        ont:excerpt       "\"Great Bolero's of Fire\") which was later used at many live shows featuring Steinman work." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/252147/16> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/252147/16/4> ;
        ont:subject       <http://dbpedia.org/resource/Jim_Steinman> .

<http://w3id.org/polifonia/pilot/meetups/252147/46/8>
        rdf:type          ont:Sentence ;
        ont:excerpt       "It contained a cover of \"Total Eclipse of the Heart\", produced by Steve Mac." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/252147/46> ;
        ont:person        <http://dbpedia.org/resource/Steve_Mac> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/252147/46/7> ;
        ont:subject       <http://dbpedia.org/resource/Jim_Steinman> .

<http://w3id.org/polifonia/pilot/meetups/252147/10/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Nonetheless, Steinman worked under Papp after his years at Amherst College." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/252147/10> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/252147/10/1> ;
        ont:subject       <http://dbpedia.org/resource/Jim_Steinman> .

<http://w3id.org/polifonia/pilot/meetups/252147/30/4>
        rdf:type            ont:Sentence ;
        ont:excerpt         "It was released on the album Nobody Else (1995) and reached the No. 1 position on the UK singles chart." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/252147/30> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/252147/30/3> ;
        ont:subject         <http://dbpedia.org/resource/Jim_Steinman> ;
        ont:timeExpression  "1 position" , "1995" .

<http://w3id.org/polifonia/pilot/meetups/252147/40/5>
        rdf:type          ont:Sentence ;
        ont:excerpt       "It also includes the first-ever release of the song \"If It Ain't Broke (Break It)\", written by Steinman, with lead vocals by Mike Vogel." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/252147/40> ;
        ont:person        <http://dbpedia.org/resource/Mike_Vogel> , <http://dbpedia.org/resource/Philip_Broke> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/252147/40/4> ;
        ont:subject       <http://dbpedia.org/resource/Jim_Steinman> .

<http://w3id.org/polifonia/pilot/meetups/252147/50/6>
        rdf:type          ont:Sentence ;
        ont:excerpt       "The track \"Going All The Way\" is a mixture of some previously released material, some previously leaked material, and a little bit of material neither previously released nor previously leaked." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/252147/50> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/252147/50/5> ;
        ont:subject       <http://dbpedia.org/resource/Jim_Steinman> .

<http://w3id.org/polifonia/pilot/meetups/252147/25/28>
        rdf:type          ont:Sentence ;
        ont:excerpt       "The official four female singers were Ellen Foley, Elaine Caswell, Gina Taylor and Deliria Wilde (with Holly Sherwood doing lead vocals on \"Good Girls Go to Heaven\")." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/252147/25> ;
        ont:person        <http://dbpedia.org/resource/Ellen_Foley> , <http://dbpedia.org/resource/Girl> , <http://dbpedia.org/resource/Oscar_Wilde> ;
        ont:place         <http://dbpedia.org/resource/Fort_Caswell_Historic_District> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/252147/25/27> ;
        ont:subject       <http://dbpedia.org/resource/Jim_Steinman> .

<http://w3id.org/polifonia/pilot/meetups/252147/53>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/252147/53/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/252147/53/1> .

<http://w3id.org/polifonia/pilot/meetups/252147/39/10>
        rdf:type          ont:Sentence ;
        ont:excerpt       "That includes the songs \"We're Still The Children We Were Then\" and \"Not Allowed To Love\"." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/252147/39> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/252147/39/9> ;
        ont:subject       <http://dbpedia.org/resource/Jim_Steinman> .

<http://w3id.org/polifonia/pilot/meetups/252147/5/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Career ==" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/252147/5> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/252147/5/0> ;
        ont:subject       <http://dbpedia.org/resource/Jim_Steinman> .

<http://w3id.org/polifonia/pilot/meetups/252147/32/14>
        rdf:type          ont:Sentence ;
        ont:excerpt       "However, it received poor reviews and the Broadway run, scheduled for the following April, was cancelled." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/252147/32> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/252147/32/13> ;
        ont:subject       <http://dbpedia.org/resource/Jim_Steinman> .

<http://w3id.org/polifonia/pilot/meetups/252147/23>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/252147/23/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/252147/23/1> ;
        rdf:_3    <http://w3id.org/polifonia/pilot/meetups/252147/23/2> ;
        rdf:_4    <http://w3id.org/polifonia/pilot/meetups/252147/23/3> .

<http://w3id.org/polifonia/pilot/meetups/252147/15/10>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Some music from this show later appeared in the hit song \"Making Love Out of Nothing at All\", and in the score of the film A Small Circle of Friends." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/252147/15> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/252147/15/9> ;
        ont:subject       <http://dbpedia.org/resource/Jim_Steinman> .

<http://w3id.org/polifonia/pilot/meetups/252147/17/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "CBS executive Clive Davis even claimed that Steinman knew nothing about writing, or rock music in general." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/252147/17> ;
        ont:person        <http://dbpedia.org/resource/Miles_Davis> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/252147/17/1> ;
        ont:subject       <http://dbpedia.org/resource/Jim_Steinman> .

<http://w3id.org/polifonia/pilot/meetups/252147/27/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Tabb's website has shared a recording of the song \"Bring Me The Head of Jerry Garcia\", with Steinman credited as executive producer." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/252147/27> ;
        ont:person        <http://dbpedia.org/resource/Derrick_Tabb> , <http://dbpedia.org/resource/Jerry_Garcia> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/252147/27/2> ;
        ont:subject       <http://dbpedia.org/resource/Jim_Steinman> .

<http://w3id.org/polifonia/pilot/meetups/252147/64/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Steinmania – Features the 1969 Recording of the Dream Engine" ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/252147/64> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/252147/64/0> ;
        ont:subject         <http://dbpedia.org/resource/Jim_Steinman> ;
        ont:timeExpression  "1969" .

<http://w3id.org/polifonia/pilot/meetups/252147/37/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "\"Scarpia\", a singer on many demo recordings for Steinman, sang the lead vocal." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/252147/37> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/252147/37/3> ;
        ont:subject       <http://dbpedia.org/resource/Jim_Steinman> .

<http://w3id.org/polifonia/pilot/meetups/252147/47/5>
        rdf:type          ont:Sentence ;
        ont:excerpt       "The last of those songs is partly adapted from \"If It Ain't Broke (Break It)\"." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/252147/47> ;
        ont:person        <http://dbpedia.org/resource/Philip_Broke> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/252147/47/4> ;
        ont:subject       <http://dbpedia.org/resource/Jim_Steinman> .

<http://w3id.org/polifonia/pilot/meetups/252147/21/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1980, the film A Small Circle of Friends was released." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/252147/21> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/252147/21/0> ;
        ont:subject       <http://dbpedia.org/resource/Jim_Steinman> .

<http://w3id.org/polifonia/pilot/meetups/252147/31/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He also produced, but not authored, two other songs on the album: \"River Deep, Mountain High\" and \"Call the Man\"." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/252147/31> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/252147/31/0> ;
        ont:subject       <http://dbpedia.org/resource/Jim_Steinman> .

<http://w3id.org/polifonia/pilot/meetups/252147/41/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Steven Rinkoff was credited with co-production, recording and mixing." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/252147/41> ;
        ont:person        <http://dbpedia.org/resource/Steven_Rinkoff> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/252147/41/1> ;
        ont:subject       <http://dbpedia.org/resource/Jim_Steinman> .

<http://w3id.org/polifonia/pilot/meetups/252147/51/3>
        rdf:type            ont:Sentence ;
        ont:excerpt         "The musical performed at Toronto's Ed Mirvish Theatre, then finished in January 2018 to make a return to London in 2018, this time the Dominion Theatre." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/252147/51> ;
        ont:person          <http://dbpedia.org/resource/Ed_Mirvish> ;
        ont:place           <http://dbpedia.org/resource/Toronto> , <http://dbpedia.org/resource/London> , <http://dbpedia.org/resource/Dominion_Theatre> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/252147/51/2> ;
        ont:subject         <http://dbpedia.org/resource/Jim_Steinman> ;
        ont:timeExpression  "January 2018" .

<http://w3id.org/polifonia/pilot/meetups/252147/35>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/252147/35/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/252147/35/1> .

<http://w3id.org/polifonia/pilot/meetups/252147/28/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "After a series of financial and legal disputes during the 1980s, Steinman and Meat Loaf met at the singer's house in Connecticut at Christmas 1989 or 1990 and sang Bat out of Hell on piano." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/252147/28> ;
        ont:person          <http://dbpedia.org/resource/Meat_Loaf> ;
        ont:place           <http://dbpedia.org/resource/Connecticut> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/252147/28/0> ;
