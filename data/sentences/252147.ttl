<http://w3id.org/polifonia/pilot/meetups/252147/29/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "\"Original Sin\", the eponymous song on the 1989 Pandora's Box album, featured on the soundtrack album for the film The Shadow (1994)." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/252147/29> ;
        ont:person          <http://dbpedia.org/resource/Pandora> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/252147/29/0> ;
        ont:subject         <http://dbpedia.org/resource/Jim_Steinman> ;
        ont:timeExpression  "on the 1989" , "1994" .

<http://w3id.org/polifonia/pilot/meetups/252147/39/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Preparation for a stage musical based on the Batman comic book series began early in the decade, with Steinman working on the music and lyrics." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/252147/39> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/252147/39/0> ;
        ont:subject       <http://dbpedia.org/resource/Jim_Steinman> .

<http://w3id.org/polifonia/pilot/meetups/252147/49/2>
        rdf:type            ont:Sentence ;
        ont:excerpt         "On April 10, 2012, Amherst College announced that Steinman would receive an honorary degree at the college's 191st commencement on May 20." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/252147/49> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/252147/49/1> ;
        ont:subject         <http://dbpedia.org/resource/Jim_Steinman> ;
        ont:timeExpression  "191st commencement" , "May 20" , "April 10 , 2012" .

<http://w3id.org/polifonia/pilot/meetups/252147/16/3>
        rdf:type            ont:Sentence ;
        ont:excerpt         "The three songs were \"Bat Out of Hell\", \"Heaven Can Wait\" and \"The Formation of the Pack\", which was retitled \"All Revved Up With No Place to Go\"." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/252147/16> ;
        ont:place           <http://dbpedia.org/resource/No_Place> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/252147/16/2> ;
        ont:subject         <http://dbpedia.org/resource/Jim_Steinman> ;
        ont:timeExpression  "three" .

<http://w3id.org/polifonia/pilot/meetups/252147/53/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "==" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/252147/53> ;
