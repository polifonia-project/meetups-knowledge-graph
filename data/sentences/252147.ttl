        rdf:type            ont:Sentence ;
        ont:excerpt         "Steinman was executive producer for the 2003 MTV television film Wuthering Heights." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/252147/40> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/252147/40/0> ;
        ont:subject         <http://dbpedia.org/resource/Jim_Steinman> ;
        ont:timeExpression  "for the 2003" .

<http://w3id.org/polifonia/pilot/meetups/252147/50/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "It contains ten songs composed by Steinman." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/252147/50> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/252147/50/0> ;
        ont:subject       <http://dbpedia.org/resource/Jim_Steinman> .

<http://w3id.org/polifonia/pilot/meetups/252147/25/23>
        rdf:type          ont:Sentence ;
        ont:excerpt       "One was the song \"Rude Awakening\", with lead vocals by Bill Medley and was written by Rick Rose and Paul Rothchild." ;
