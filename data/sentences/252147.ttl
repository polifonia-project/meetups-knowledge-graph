        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/252147/41/0> ;
        ont:subject       <http://dbpedia.org/resource/Jim_Steinman> .

<http://w3id.org/polifonia/pilot/meetups/252147/51/1>
        rdf:type            ont:Sentence ;
        ont:excerpt         "This musical was based on multiple projects, including Steinman's Neverland musical from the 1970s, the hit albums Bat Out of Hell, Bat Out of Hell II: Back into Hell, and on assorted other recordings from Steinman's career." ;
