        ont:timeExpression  "11 on" .

<http://w3id.org/polifonia/pilot/meetups/252147/24/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "However, Steinman was fired, and the recording work he made with the band was not released." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/252147/24> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/252147/24/0> ;
        ont:subject       <http://dbpedia.org/resource/Jim_Steinman> .

<http://w3id.org/polifonia/pilot/meetups/252147/14/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1973, a single of the song \"More Than You Deserve\", from the musical sharing that name, was released." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/252147/14> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/252147/14/0> ;
        ont:subject       <http://dbpedia.org/resource/Jim_Steinman> .

<http://w3id.org/polifonia/pilot/meetups/252147/25/34>
        rdf:type          ont:Sentence ;
        ont:excerpt       "It was not released at all in North America." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/252147/25> ;
        ont:place         <http://dbpedia.org/resource/North_America> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/252147/25/33> ;
        ont:subject       <http://dbpedia.org/resource/Jim_Steinman> .

<http://w3id.org/polifonia/pilot/meetups/252147/34/2>
        rdf:type            ont:Sentence ;
        ont:excerpt         "The compilation also included three new recordings of songs co-written by Steinman, all three of which he produced." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/252147/34> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/252147/34/1> ;
        ont:subject         <http://dbpedia.org/resource/Jim_Steinman> ;
        ont:timeExpression  "three of" .

<http://w3id.org/polifonia/pilot/meetups/252147/44/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Steinman did not attend the opening night, in order to show his disgust with the show." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/252147/44> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/252147/44/2> ;
        ont:subject       <http://dbpedia.org/resource/Jim_Steinman> .

<http://w3id.org/polifonia/pilot/meetups/252147/54/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "I've never been dumped... but probably because I don't allow myself to be dumped." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/252147/54> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/252147/54/3> ;
        ont:subject       <http://dbpedia.org/resource/Jim_Steinman> .

<http://w3id.org/polifonia/pilot/meetups/252147/21/5>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Most songs are sung by Steinman himself." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/252147/21> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/252147/21/4> ;
        ont:subject       <http://dbpedia.org/resource/Jim_Steinman> .

<http://w3id.org/polifonia/pilot/meetups/252147/31/6>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Steinman's production team prepared a recording of the Steinman/Black song \"Is Nothing Sacred\" for that album, but it was removed shortly before the album's release." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/252147/31> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/252147/31/5> ;
        ont:subject       <http://dbpedia.org/resource/Jim_Steinman> .

<http://w3id.org/polifonia/pilot/meetups/252147/32/20>
        rdf:type          ont:Sentence ;
        ont:excerpt       "The same track appeared on a Boyzone album and their greatest hits album." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/252147/32> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/252147/32/19> ;
        ont:subject       <http://dbpedia.org/resource/Jim_Steinman> .

<http://w3id.org/polifonia/pilot/meetups/252147/0/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "James Richard Steinman (November 1, 1947 – April 19, 2021) was an American composer, lyricist, record producer, and playwright." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/252147/0> ;
