        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/252147/61/0> ;
        ont:subject       <http://dbpedia.org/resource/Jim_Steinman> .

<http://w3id.org/polifonia/pilot/meetups/252147/34/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Also included is the song \"Is Nothing Sacred\", on which Steinman wrote music for Don Black's lyrics." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/252147/34> ;
        ont:person        <http://dbpedia.org/resource/Don_Black_(lyricist)> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/252147/34/3> ;
        ont:subject       <http://dbpedia.org/resource/Jim_Steinman> .

<http://w3id.org/polifonia/pilot/meetups/252147/44/5>
        rdf:type            ont:Sentence ;
        ont:excerpt         "The manager he was referring to was David Sonenberg, who was one of the producers of the show and Steinman's longtime manager." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/252147/44> ;
        ont:person          <http://dbpedia.org/resource/David_Sonenberg> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/252147/44/4> ;
