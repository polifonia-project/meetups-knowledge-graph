        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/252147/49> ;
        ont:person          <http://dbpedia.org/resource/Terry_Jones> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/252147/49/0> ;
