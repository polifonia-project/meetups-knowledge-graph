        rdf:_6    <http://w3id.org/polifonia/pilot/meetups/252147/54/5> .

<http://w3id.org/polifonia/pilot/meetups/252147/32/15>
        rdf:type            ont:Sentence ;
        ont:excerpt         "A reworked and more successful West End production opened at the Aldwych Theatre on July 1, 1998." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/252147/32> ;
        ont:place           <http://dbpedia.org/resource/Aldwych_Theatre> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/252147/32/14> ;
        ont:subject         <http://dbpedia.org/resource/Jim_Steinman> ;
        ont:timeExpression  "July 1 , 1998" .

<http://w3id.org/polifonia/pilot/meetups/252147/24>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/252147/24/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/252147/24/1> ;
        rdf:_3    <http://w3id.org/polifonia/pilot/meetups/252147/24/2> .

<http://w3id.org/polifonia/pilot/meetups/252147/54/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Responding to an interviewer's assertion that his songs are tragic, Steinman said he has \"never been stomped on literally." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/252147/54> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/252147/54/0> ;
        ont:subject       <http://dbpedia.org/resource/Jim_Steinman> .

<http://w3id.org/polifonia/pilot/meetups/252147/25/30>
        rdf:type            ont:Sentence ;
        ont:excerpt         "In interview, Elaine Caswell said Pandora's Box was \"four women; three that existed and [...] Deliria Wilde who was somewhat mythical, someone [Steinman] kind of created\"." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/252147/25> ;
        ont:person          <http://dbpedia.org/resource/Oscar_Wilde> , <http://dbpedia.org/resource/Pandora> ;
        ont:place           <http://dbpedia.org/resource/Fort_Caswell_Historic_District> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/252147/25/29> ;
        ont:subject         <http://dbpedia.org/resource/Jim_Steinman> ;
        ont:timeExpression  "three that" .

<http://w3id.org/polifonia/pilot/meetups/252147/15/11>
        rdf:type          ont:Sentence ;
        ont:excerpt       "A cast album of the songs from The Confidence Man, produced by Jeff Olmstead, was released in 2003." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/252147/15> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/252147/15/10> ;
