        rdf:type            ont:Sentence ;
        ont:excerpt         "The soundtrack album for the 1989 film Rude Awakening included two tracks produced by Steinman." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/252147/25> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/252147/25/21> ;
        ont:subject         <http://dbpedia.org/resource/Jim_Steinman> ;
        ont:timeExpression  "1989 film" .

