<http://w3id.org/polifonia/pilot/meetups/252147/17/3>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Recording started in 1976 in Bearsville, near Woodstock." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/252147/17> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/252147/17/2> ;
        ont:subject         <http://dbpedia.org/resource/Jim_Steinman> ;
        ont:timeExpression  "1976 in" .

<http://w3id.org/polifonia/pilot/meetups/252147/27/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "The song's lyric says of Jerry Garcia, \"he plays guitar like diarrhea\"." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/252147/27> ;
        ont:person        <http://dbpedia.org/resource/Jerry_Garcia> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/252147/27/3> ;
        ont:subject       <http://dbpedia.org/resource/Jim_Steinman> .

<http://w3id.org/polifonia/pilot/meetups/252147/47/6>
        rdf:type          ont:Sentence ;
        ont:excerpt       "This project was also the first to perform a revised and politicized lyric to \"Braver Than We Are\"." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/252147/47> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/252147/47/5> ;
        ont:subject       <http://dbpedia.org/resource/Jim_Steinman> .

<http://w3id.org/polifonia/pilot/meetups/252147/11/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "=== 1970s" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/252147/11> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/252147/11/0> ;
        ont:subject       <http://dbpedia.org/resource/Jim_Steinman> .

<http://w3id.org/polifonia/pilot/meetups/252147/21/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "It had an orchestral score composed by Steinman, and orchestrated by his frequent collaborator Steve Margoshes." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/252147/21> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/252147/21/0> ;
        ont:subject       <http://dbpedia.org/resource/Jim_Steinman> .

<http://w3id.org/polifonia/pilot/meetups/252147/31/2>
        rdf:type            ont:Sentence ;
        ont:excerpt         "\"It's All Coming Back to Me Now\" reached No. 2 on the Billboard charts and won Steinman the award for BMI song of the year." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/252147/31> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/252147/31/1> ;
        ont:subject         <http://dbpedia.org/resource/Jim_Steinman> ;
        ont:timeExpression  "2 on" .

<http://w3id.org/polifonia/pilot/meetups/252147/41/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Like many Steinman productions, it includes work by electronic-music arranger and programmer Jeff Bova." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/252147/41> ;
        ont:person        <http://dbpedia.org/resource/Jeff_Bova> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/252147/41/2> ;
        ont:subject       <http://dbpedia.org/resource/Jim_Steinman> .

<http://w3id.org/polifonia/pilot/meetups/252147/51/4>
        rdf:type            ont:Sentence ;
        ont:excerpt         "A German version of the show was produced by Stage Entertainment, and ran at Oberhausen's Metronom Theatre from November 2018 to September 2019." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/252147/51> ;
        ont:place           <http://dbpedia.org/resource/Germany> , <http://dbpedia.org/resource/Oberhausen> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/252147/51/3> ;
        ont:subject         <http://dbpedia.org/resource/Jim_Steinman> ;
        ont:timeExpression  "September 2019" , "November 2018" .

<http://w3id.org/polifonia/pilot/meetups/252147/36>
        rdf:type  ont:Paragraph ;
