        rdf:_4    <http://w3id.org/polifonia/pilot/meetups/252147/36/3> .

<http://w3id.org/polifonia/pilot/meetups/252147/18/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1979, the Freeway Records label made a 2-LP compilation album titled L.A. Radio." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/252147/18> ;
        ont:place         <http://dbpedia.org/resource/Los_Angeles> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/252147/18/0> ;
        ont:subject       <http://dbpedia.org/resource/Jim_Steinman> .

<http://w3id.org/polifonia/pilot/meetups/252147/28/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Steinman says that \"working together again seemed like the cool thing to do." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/252147/28> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/252147/28/0> ;
        ont:subject       <http://dbpedia.org/resource/Jim_Steinman> .

<http://w3id.org/polifonia/pilot/meetups/252147/38/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Demo recordings of Hannah singing four songs by Steinman were leaked to fans and have since been available on numerous fan sites." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/252147/38> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/252147/38/1> ;
        ont:subject       <http://dbpedia.org/resource/Jim_Steinman> .

<http://w3id.org/polifonia/pilot/meetups/252147/25/42>
        rdf:type            ont:Sentence ;
        ont:excerpt         "ELO Part II released their album Electric Light Orchestra Part Two in 1990, without Steinman producing." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/252147/25> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/252147/25/41> ;
        ont:subject         <http://dbpedia.org/resource/Jim_Steinman> ;
        ont:timeExpression  "Part Two" .

<http://w3id.org/polifonia/pilot/meetups/252147/42/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 2005, The Everly Brothers released a limited edition album titled On the Wings of a Nightingale: The Mercury Studio Recordings." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/252147/42> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/252147/42/0> ;
        ont:subject       <http://dbpedia.org/resource/Jim_Steinman> .

<http://w3id.org/polifonia/pilot/meetups/252147/15/4>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Ray Fox described the 1976 production as \"a capsule version of the show\"." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/252147/15> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/252147/15/3> ;
        ont:subject         <http://dbpedia.org/resource/Jim_Steinman> ;
        ont:timeExpression  "1976 production" .

<http://w3id.org/polifonia/pilot/meetups/252147/52/1>
        rdf:type            ont:Sentence ;
        ont:excerpt         "The album was first distributed in Canada to coincide with the show's run in Toronto, then later was released again via Ghostlight Records in July 2018, both as a worldwide digital release and on CD for sale in the UK to coincide with the show's run at the Dominion Theatre, London." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/252147/52> ;
        ont:place           <http://dbpedia.org/resource/Canada> , <http://dbpedia.org/resource/London> , <http://dbpedia.org/resource/Toronto> , <http://dbpedia.org/resource/Dominion_Theatre> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/252147/52/0> ;
        ont:subject         <http://dbpedia.org/resource/Jim_Steinman> ;
        ont:timeExpression  "July 2018" .

<http://w3id.org/polifonia/pilot/meetups/252147/25/5>
        rdf:type          ont:Sentence ;
        ont:excerpt       "The selection was recorded during the recording sessions for Secret Dreams And Forbidden Fire, an album that Bonnie Tyler was then recording, and which Steinman also produced." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/252147/25> ;
        ont:person        <http://dbpedia.org/resource/Bonnie_Tyler> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/252147/25/4> ;
        ont:subject       <http://dbpedia.org/resource/Jim_Steinman> .

<http://w3id.org/polifonia/pilot/meetups/252147/45/7>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Produced by Desmond Child, Bat Out of Hell III: The Monster Is Loose was released in October 2006." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/252147/45> ;
        ont:person          <http://dbpedia.org/resource/Desmond_Child> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/252147/45/6> ;
        ont:subject         <http://dbpedia.org/resource/Jim_Steinman> ;
        ont:timeExpression  "October 2006" .

<http://w3id.org/polifonia/pilot/meetups/252147/25/12>
        rdf:type          ont:Sentence ;
        ont:excerpt       "However, Steinman declined in order to fulfill his commitments to a Bonnie Tyler album." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/252147/25> ;
        ont:person        <http://dbpedia.org/resource/Bonnie_Tyler> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/252147/25/11> ;
        ont:subject       <http://dbpedia.org/resource/Jim_Steinman> .

<http://w3id.org/polifonia/pilot/meetups/252147/22/9>
        rdf:type            ont:Sentence ;
        ont:excerpt         "The second of those appeared on Air Supply's 1983 compilation albums Greatest Hits and Making Love..." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/252147/22> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/252147/22/8> ;
        ont:subject         <http://dbpedia.org/resource/Jim_Steinman> ;
        ont:timeExpression  "1983 compilation" .

<http://w3id.org/polifonia/pilot/meetups/252147/4/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Steinman was born November 1, 1947, in Hewlett Harbor, New York, the son of Eleanor, a teacher of Latin, and Louis Steinman, who owned a steel distribution warehouse." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/252147/4> ;
        ont:person          <http://dbpedia.org/resource/Eleanor_Roosevelt> ;
        ont:place           <http://dbpedia.org/resource/Hewlett_Harbor,_New_York> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/252147/4/0> ;
        ont:subject         <http://dbpedia.org/resource/Jim_Steinman> ;
        ont:timeExpression  "November 1 , 1947" .

