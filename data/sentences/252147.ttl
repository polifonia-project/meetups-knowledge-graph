        ont:person        <http://dbpedia.org/resource/Bill_Medley> , <http://dbpedia.org/resource/Paul_A._Rothchild> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/252147/25/22> ;
        ont:subject       <http://dbpedia.org/resource/Jim_Steinman> .

<http://w3id.org/polifonia/pilot/meetups/252147/33/6>
        rdf:type          ont:Sentence ;
        ont:excerpt       "This includes the song \"Total Eclipse of the Heart\", the melody of \"Objects in the Rear View Mirror May Appear Closer than They Are\", and many other parts of earlier works." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/252147/33> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/252147/33/5> ;
