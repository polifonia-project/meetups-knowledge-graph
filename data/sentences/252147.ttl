<http://w3id.org/polifonia/pilot/meetups/252147/46/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 2006, the album's title became the subject of a legal battle between Steinman and Meat Loaf." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/252147/46> ;
        ont:person        <http://dbpedia.org/resource/Meat_Loaf> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/252147/46/0> ;
        ont:subject       <http://dbpedia.org/resource/Jim_Steinman> .

<http://w3id.org/polifonia/pilot/meetups/252147/56/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He had another stroke four years prior to his death." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/252147/56> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/252147/56/0> ;
        ont:subject       <http://dbpedia.org/resource/Jim_Steinman> .

<http://w3id.org/polifonia/pilot/meetups/252147/29/5>
        rdf:type          ont:Sentence ;
        ont:excerpt       "The album consisted of songs written by the band members." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/252147/29> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/252147/29/4> ;
        ont:subject       <http://dbpedia.org/resource/Jim_Steinman> .

<http://w3id.org/polifonia/pilot/meetups/252147/39/6>
        rdf:type          ont:Sentence ;
        ont:excerpt       "It was my dream that he do this.\"" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/252147/39> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/252147/39/5> ;
        ont:subject       <http://dbpedia.org/resource/Jim_Steinman> .

<http://w3id.org/polifonia/pilot/meetups/252147/13/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "This was the first commercially released recording of a song written by Steinman." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/252147/13> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/252147/13/0> ;
        ont:subject       <http://dbpedia.org/resource/Jim_Steinman> .

<http://w3id.org/polifonia/pilot/meetups/252147/25/20>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1987, the Sisters of Mercy released their second album Floodland." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/252147/25> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/252147/25/19> ;
        ont:subject       <http://dbpedia.org/resource/Jim_Steinman> .

<http://w3id.org/polifonia/pilot/meetups/252147/23/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "The song was a hit single, the soundtrack album was successful, and the film was successful." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/252147/23> ;
