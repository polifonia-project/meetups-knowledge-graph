        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/252147/36/0> ;
        ont:subject       <http://dbpedia.org/resource/Jim_Steinman> .

<http://w3id.org/polifonia/pilot/meetups/252147/46/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Steinman had registered a trademark on the title \"Bat Out of Hell\" in 1995, and sought to prevent Meat Loaf from using the title." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/252147/46> ;
        ont:person        <http://dbpedia.org/resource/Meat_Loaf> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/252147/46/0> ;
        ont:subject       <http://dbpedia.org/resource/Jim_Steinman> .

<http://w3id.org/polifonia/pilot/meetups/252147/56/2>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Per the death certificate from the Connecticut Department of Health, Steinman died from aspiration pneumonia at a hospital in Danbury, Connecticut, on April 19, 2021, at age 73." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/252147/56> ;
        ont:place           <http://dbpedia.org/resource/Connecticut> , <http://dbpedia.org/resource/Danbury,_Connecticut> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/252147/56/1> ;
        ont:subject         <http://dbpedia.org/resource/Jim_Steinman> ;
        ont:timeExpression  "April 19 , 2021" , "at age 73" .

<http://w3id.org/polifonia/pilot/meetups/252147/23/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Some of the music from this song was adapted from the music used for the song \"Stark Raving Love\" from the album Bad for Good." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/252147/23> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/252147/23/2> ;
        ont:subject       <http://dbpedia.org/resource/Jim_Steinman> .

<http://w3id.org/polifonia/pilot/meetups/252147/39/7>
        rdf:type          ont:Sentence ;
        ont:excerpt       "However, Warner Bros. cancelled its plans to cast and stage the show." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/252147/39> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/252147/39/6> ;
        ont:subject       <http://dbpedia.org/resource/Jim_Steinman> .

<http://w3id.org/polifonia/pilot/meetups/252147/13/2>
        rdf:type            ont:Sentence ;
        ont:excerpt         "That same year, Steinman wrote music and lyrics for a musical titled More Than You Deserve (1973)." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/252147/13> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/252147/13/1> ;
        ont:subject         <http://dbpedia.org/resource/Jim_Steinman> ;
        ont:timeExpression  "1973" .

<http://w3id.org/polifonia/pilot/meetups/252147/25/21>
        rdf:type          ont:Sentence ;
        ont:excerpt       "The track \"This Corrosion\" was produced by Steinman, and the track \"Dominion/Mother Russia\" was co-produced by Steinman, along with Larry Alexander and Andrew Eldritch." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/252147/25> ;
        ont:person        <http://dbpedia.org/resource/Charles_Paul_Alexander> , <http://dbpedia.org/resource/Andrew_Eldritch> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/252147/25/20> ;
        ont:subject       <http://dbpedia.org/resource/Jim_Steinman> .

<http://w3id.org/polifonia/pilot/meetups/252147/60/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Media related to Jim Steinman at Wikimedia Commons" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/252147/60> ;
        ont:person        <http://dbpedia.org/resource/Jim_Steinman> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/252147/60/0> ;
        ont:subject       <http://dbpedia.org/resource/Jim_Steinman> .

<http://w3id.org/polifonia/pilot/meetups/252147/33/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Many of the various productions of Tanz der Vampire have had cast recordings released, some of them produced by Steinman, along with other co-producers." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/252147/33> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/252147/33/3> ;
        ont:subject       <http://dbpedia.org/resource/Jim_Steinman> .

<http://w3id.org/polifonia/pilot/meetups/252147/8/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Baal is the leader of a self-assembled group of wild boys called The Tribe, whose mortal enemies are Max and Emily, the parents of the Girl, a young woman with whom Baal has fallen in love." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/252147/8> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/252147/8/0> ;
        ont:subject       <http://dbpedia.org/resource/Jim_Steinman> .

