        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/252147/47/7> ;
        ont:subject       <http://dbpedia.org/resource/Jim_Steinman> .

<http://w3id.org/polifonia/pilot/meetups/252147/21/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Early in the production of a follow-up album to Bat Out of Hell, Meat Loaf developed vocal problems and was unable to continue on the project." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/252147/21> ;
        ont:person        <http://dbpedia.org/resource/Meat_Loaf> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/252147/21/2> ;
        ont:subject       <http://dbpedia.org/resource/Jim_Steinman> .

<http://w3id.org/polifonia/pilot/meetups/252147/31/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "This album as a whole won two 1997 Grammy Awards, for Best Pop Album and for Album of the Year." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/252147/31> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/252147/31/3> ;
        ont:subject       <http://dbpedia.org/resource/Jim_Steinman> .

<http://w3id.org/polifonia/pilot/meetups/252147/41/5>
        rdf:type            ont:Sentence ;
        ont:excerpt         "In 2004, the film Shrek 2 was released, along with a soundtrack album." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/252147/41> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/252147/41/4> ;
        ont:subject         <http://dbpedia.org/resource/Jim_Steinman> ;
        ont:timeExpression  "Shrek 2" .

<http://w3id.org/polifonia/pilot/meetups/252147/51/6>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Steinman had also set an American tour of the musical to take place starting in late 2018 through 2019 but it was cancelled after its 3-week run in the first city Toronto was complete." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/252147/51> ;
