        ont:excerpt       "Years later, it was reissued and became available internationally on compact disc." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/252147/25> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/252147/25/34> ;
