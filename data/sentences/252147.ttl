<http://w3id.org/polifonia/pilot/meetups/252147/17/7>
        rdf:type            ont:Sentence ;
        ont:excerpt         "The highest-charting song from the album was \"Two Out of Three Ain't Bad\", which reached No. 11 on the Billboard Charts." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/252147/17> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/252147/17/6> ;
