        rdf:type          ont:Sentence ;
        ont:excerpt       "His frequent collaborator Meat Loaf reacted to Steinman's death by saying, \"We didn't know each other, we were each other.\"" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/252147/56> ;
        ont:person        <http://dbpedia.org/resource/Meat_Loaf> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/252147/56/3> ;
