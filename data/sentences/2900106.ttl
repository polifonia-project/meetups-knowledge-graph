        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/2900106/13/0> ;
        ont:subject         <http://dbpedia.org/resource/John_Fane,_11th_Earl_of_Westmorland> ;
        ont:timeExpression  "26 June 1811" .

<http://w3id.org/polifonia/pilot/meetups/2900106/6/2>
        rdf:type            ont:Sentence ;
