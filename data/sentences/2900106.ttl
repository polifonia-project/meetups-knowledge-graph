        ont:timeExpression  "from 1806 to 1807" .

<http://w3id.org/polifonia/pilot/meetups/2900106/5/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Burghersh was a supporter of the second Portland Ministry, in which his father was again Lord Privy Seal, but without particular activity in Parliament, appearing once in debate on the price of wheat on 3 June 1808." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/2900106/5> ;
        ont:place           <http://dbpedia.org/resource/Burwash> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/2900106/5/0> ;
        ont:subject         <http://dbpedia.org/resource/John_Fane,_11th_Earl_of_Westmorland> ;
        ont:timeExpression  "3 June 1808" .

