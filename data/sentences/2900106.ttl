<http://w3id.org/polifonia/pilot/meetups/2900106/5/7>
        rdf:type            ont:Sentence ;
        ont:excerpt         "He served with the 3rd Dragoon Guards in their campaign in Portugal in 1810." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/2900106/5> ;
        ont:place           <http://dbpedia.org/resource/Portugal> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/2900106/5/6> ;
