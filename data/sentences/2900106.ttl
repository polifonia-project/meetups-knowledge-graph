        rdf:type            ont:Sentence ;
        ont:excerpt         "On 9 May 1803, Burghersh was appointed a deputy lieutenant of Northamptonshire, and after the breakdown of the Peace of Amiens, he was commissioned a lieutenant in the Northamptonshire Militia on 30 June 1803." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/2900106/4> ;
        ont:place           <http://dbpedia.org/resource/Burwash> , <http://dbpedia.org/resource/Amiens> , <http://dbpedia.org/resource/Northamptonshire> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/2900106/4/0> ;
        ont:subject         <http://dbpedia.org/resource/John_Fane,_11th_Earl_of_Westmorland> ;
        ont:timeExpression  "30 June 1803" , "9 May 1803" .

