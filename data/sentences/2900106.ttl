        rdf:type            ont:Sentence ;
        ont:excerpt         "On 10 December 1811 he exchanged back into the 7th Foot and later the same year purchased a lieutenant-colonelcy in the 63rd Foot." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/2900106/6> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/2900106/6/0> ;
