        ont:subject         <http://dbpedia.org/resource/Scott_Walker_(singer)> ;
        ont:timeExpression  "two" .

<http://w3id.org/polifonia/pilot/meetups/703488/17/11>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Apparently now fated for a stagnant career on the revival circuit, the Walker Brothers lost heart and interest, compounded by Scott's increasing reluctance to sing live." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/703488/17> ;
        ont:person        <http://dbpedia.org/resource/Kemba_Walker> , <http://dbpedia.org/resource/Walter_Scott> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/703488/17/10> ;
        ont:subject       <http://dbpedia.org/resource/Scott_Walker_(singer)> .

