<http://w3id.org/polifonia/pilot/meetups/703488/22/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "This award had been presented only twice before, the first time to Phil Spector, and the second to Brian Eno." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/703488/22> ;
        ont:person        <http://dbpedia.org/resource/Phil_Spector> , <http://dbpedia.org/resource/Brian_Eno> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/703488/22/1> ;
        ont:subject       <http://dbpedia.org/resource/Scott_Walker_(singer)> .

<http://w3id.org/polifonia/pilot/meetups/703488/17/10>
        rdf:type          ont:Sentence ;
        ont:excerpt       "The supporting tour saw the band concentrating on the old hits and ballads and ignoring the songs from their new record." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/703488/17> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/703488/17/9> ;
        ont:subject       <http://dbpedia.org/resource/Scott_Walker_(singer)> .

<http://w3id.org/polifonia/pilot/meetups/703488/14/1>
        rdf:type            ont:Sentence ;
        ont:excerpt         "His next album, 'Til the Band Comes In (1970), showed a pronounced split between its two sides." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/703488/14> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/703488/14/0> ;
        ont:subject         <http://dbpedia.org/resource/Scott_Walker_(singer)> ;
        ont:timeExpression  "1970" .

<http://w3id.org/polifonia/pilot/meetups/703488/0/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Noel Scott Engel (January 9, 1943 – March 22, 2019) better known by the stage name Scott Walker, was a British-American singer-songwriter, composer and record producer who resided in England." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/703488/0> ;
        ont:person          <http://dbpedia.org/resource/Noel_Scott> , <http://dbpedia.org/resource/Scott_Walker_(singer)> , <http://dbpedia.org/resource/Scott_Walker_(politician)> , <http://dbpedia.org/resource/Composer> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/703488/0/0> ;
        ont:subject         <http://dbpedia.org/resource/Scott_Walker_(singer)> ;
        ont:timeExpression  "March 22 , 2019" , "January 9 , 1943" .

<http://w3id.org/polifonia/pilot/meetups/703488/12/9>
        rdf:type            ont:Sentence ;
        ont:excerpt         "At the peak of his fame in 1969, Walker was given his own BBC TV series, Scott, featuring solo Walker performances of ballads, big band standards, Brel songs and his own compositions." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/703488/12> ;
        ont:person          <http://dbpedia.org/resource/Walter_Scott> , <http://dbpedia.org/resource/Kemba_Walker> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/703488/12/8> ;
        ont:subject         <http://dbpedia.org/resource/Scott_Walker_(singer)> ;
        ont:timeExpression  "fame in 1969" .

<http://w3id.org/polifonia/pilot/meetups/703488/19/6>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Like Nite Flights before it, Climate of Hunter was met with critical praise but low sales." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/703488/19> ;
        ont:place         <http://dbpedia.org/resource/Hunter_Region> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/703488/19/5> ;
        ont:subject       <http://dbpedia.org/resource/Scott_Walker_(singer)> .

