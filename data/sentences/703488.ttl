        rdf:_4    <http://w3id.org/polifonia/pilot/meetups/703488/15/3> ;
        rdf:_5    <http://w3id.org/polifonia/pilot/meetups/703488/15/4> ;
        rdf:_6    <http://w3id.org/polifonia/pilot/meetups/703488/15/5> ;
        rdf:_7    <http://w3id.org/polifonia/pilot/meetups/703488/15/6> ;
