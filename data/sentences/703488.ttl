<http://w3id.org/polifonia/pilot/meetups/703488/4/8>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In between attending art school and furthering his interests in cinema and literature, Scott played bass guitar proficiently enough to get session work in Los Angeles as a teenager." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/703488/4> ;
        ont:person        <http://dbpedia.org/resource/Walter_Scott> ;
        ont:place         <http://dbpedia.org/resource/Los_Angeles> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/703488/4/7> ;
        ont:subject       <http://dbpedia.org/resource/Scott_Walker_(singer)> .

<http://w3id.org/polifonia/pilot/meetups/703488/0/3>
        rdf:type            ont:Sentence ;
        ont:excerpt         "He lived in the UK from 1965 and became a UK citizen in 1970." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/703488/0> ;
        ont:place           <http://dbpedia.org/resource/United_Kingdom> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/703488/0/2> ;
        ont:subject         <http://dbpedia.org/resource/Scott_Walker_(singer)> ;
        ont:timeExpression  "citizen in 1970" .

<http://w3id.org/polifonia/pilot/meetups/703488/14/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Subsequent releases saw Walker revert to cover versions of popular film tunes and a serious flirtation with country music." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/703488/14> ;
        ont:person        <http://dbpedia.org/resource/Kemba_Walker> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/703488/14/3> ;
        ont:subject       <http://dbpedia.org/resource/Scott_Walker_(singer)> .

<http://w3id.org/polifonia/pilot/meetups/703488/7/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Finding suitable material was always a problem." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/703488/7> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/703488/7/0> ;
        ont:subject       <http://dbpedia.org/resource/Scott_Walker_(singer)> .

<http://w3id.org/polifonia/pilot/meetups/703488/19/9>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Soon afterwards, Walker was dropped by the label." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/703488/19> ;
        ont:person        <http://dbpedia.org/resource/Kemba_Walker> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/703488/19/8> ;
        ont:subject       <http://dbpedia.org/resource/Scott_Walker_(singer)> .

<http://w3id.org/polifonia/pilot/meetups/703488/26/17>
        rdf:type            ont:Sentence ;
        ont:excerpt         "He wrote the score for the ROH2 production of Jean Cocteau's 1932 play Duet for One, which was staged in the Linbury Studio in June 2011." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/703488/26> ;
        ont:person          <http://dbpedia.org/resource/Jean_Cocteau> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/703488/26/16> ;
        ont:subject         <http://dbpedia.org/resource/Scott_Walker_(singer)> ;
        ont:timeExpression  "June 2011" , "1932 play" .

