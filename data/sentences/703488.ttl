        ont:subject       <http://dbpedia.org/resource/Scott_Walker_(singer)> .

<http://w3id.org/polifonia/pilot/meetups/703488/17/1>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Their first comeback single, a cover of Tom Rush's song \"No Regrets\", from the album of the same title reached number 7 in the UK Singles Chart, some months after its release." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/703488/17> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/703488/17/0> ;
        ont:subject         <http://dbpedia.org/resource/Scott_Walker_(singer)> ;
        ont:timeExpression  "7 in" .

<http://w3id.org/polifonia/pilot/meetups/703488/8/5>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Artistic differences and the stresses stemming from overwhelming pop stardom led to the break-up of the Walker Brothers the same year, although they reunited briefly for a tour of Japan in 1968." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/703488/8> ;
        ont:person        <http://dbpedia.org/resource/Kemba_Walker> ;
        ont:place         <http://dbpedia.org/resource/Japan> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/703488/8/4> ;
        ont:subject       <http://dbpedia.org/resource/Scott_Walker_(singer)> .

<http://w3id.org/polifonia/pilot/meetups/703488/26/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "The World Premiere of Scott Walker: 30 Century Man took place as part of the 50th London Film Festival." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/703488/26> ;
        ont:person        <http://dbpedia.org/resource/Scott_Walker_(politician)> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/703488/26/1> ;
