        rdf:type          ont:Sentence ;
        ont:excerpt       "==" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/703488/9> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/703488/9/1> ;
        ont:subject       <http://dbpedia.org/resource/Scott_Walker_(singer)> .

<http://w3id.org/polifonia/pilot/meetups/703488/32/11>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Instead, there are whirring synthesizers, great orchestral blocks of sound, noises of unknown provenance.\"" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/703488/32> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/703488/32/10> ;
        ont:subject       <http://dbpedia.org/resource/Scott_Walker_(singer)> .

<http://w3id.org/polifonia/pilot/meetups/703488/12/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1968 (during the brief Walker Brothers reunion and tour of Japan), he produced a single with the Japanese rock group the Carnabeats, featuring Gary Walker on vocals." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/703488/12> ;
        ont:person        <http://dbpedia.org/resource/Kemba_Walker> , <http://dbpedia.org/resource/Gary_Walker_(musician)> ;
        ont:place         <http://dbpedia.org/resource/Japan> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/703488/12/0> ;
        ont:subject       <http://dbpedia.org/resource/Scott_Walker_(singer)> .

<http://w3id.org/polifonia/pilot/meetups/703488/25/7>
        rdf:type            ont:Sentence ;
        ont:excerpt         "In June 2006, Mojo and radio honored Scott Walker with the MOJO Icon Award: \"Voted for by Mojo readers and Mojo4music users, the recipient of this award has enjoyed a spectacular career on a global scale\"." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/703488/25> ;
        ont:person          <http://dbpedia.org/resource/Scott_Walker_(politician)> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/703488/25/6> ;
        ont:subject         <http://dbpedia.org/resource/Scott_Walker_(singer)> ;
        ont:timeExpression  "June 2006" .

<http://w3id.org/polifonia/pilot/meetups/703488/17/6>
        rdf:type          ont:Sentence ;
        ont:excerpt       "The opening four songs were Scott's, the final four John's, while the middle pair were by Gary." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/703488/17> ;
        ont:person        <http://dbpedia.org/resource/Walter_Scott> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/703488/17/5> ;
        ont:subject       <http://dbpedia.org/resource/Scott_Walker_(singer)> .

<http://w3id.org/polifonia/pilot/meetups/703488/19/17>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In the meantime David Bowie covered Scott's song \"Nite Flights\" on his Black Tie White Noise album, which also contained the Walker inspired 'You've Been Around'." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/703488/19> ;
        ont:person        <http://dbpedia.org/resource/David_Bowie> , <http://dbpedia.org/resource/Kemba_Walker> , <http://dbpedia.org/resource/Walter_Scott> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/703488/19/16> ;
        ont:subject       <http://dbpedia.org/resource/Scott_Walker_(singer)> .

<http://w3id.org/polifonia/pilot/meetups/703488/36/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "==" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/703488/36> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/703488/36/0> ;
        ont:subject       <http://dbpedia.org/resource/Scott_Walker_(singer)> .

<http://w3id.org/polifonia/pilot/meetups/703488/21/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1999, he wrote and produced two songs for Ute Lemper on her album Punishing Kiss, credited as N.S. Engel." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/703488/21> ;
        ont:person        <http://dbpedia.org/resource/Ute_Lemper> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/703488/21/1> ;
        ont:subject       <http://dbpedia.org/resource/Scott_Walker_(singer)> .

<http://w3id.org/polifonia/pilot/meetups/703488/13/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Compensating for his recent dip into passivity, this was his first record to be made up entirely of self-penned material." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/703488/13> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/703488/13/0> ;
        ont:subject       <http://dbpedia.org/resource/Scott_Walker_(singer)> .

<http://w3id.org/polifonia/pilot/meetups/703488/26/7>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Walker's evocation of \"Darkness\" appears as the ninth." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/703488/26> ;
        ont:person        <http://dbpedia.org/resource/Kemba_Walker> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/703488/26/6> ;
        ont:subject       <http://dbpedia.org/resource/Scott_Walker_(singer)> .

<http://w3id.org/polifonia/pilot/meetups/703488/31>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/703488/31/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/703488/31/1> .

<http://w3id.org/polifonia/pilot/meetups/703488/30/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He is survived by his girlfriend Beverly Foster, his daughter Lee and granddaughter Emmi-Lee." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/703488/30> ;
        ont:person        <http://dbpedia.org/resource/David_Foster> , <http://dbpedia.org/resource/Robert_E._Lee> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/703488/30/2> ;
        ont:subject       <http://dbpedia.org/resource/Scott_Walker_(singer)> .

