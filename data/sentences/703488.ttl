<http://w3id.org/polifonia/pilot/meetups/703488/16/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "===" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/703488/16> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/703488/16/0> ;
        ont:subject       <http://dbpedia.org/resource/Scott_Walker_(singer)> .

<http://w3id.org/polifonia/pilot/meetups/703488/0/7>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Walker's 1960s recordings were highly regarded by the 1980s UK underground music scene, and gained a cult following." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/703488/0> ;
        ont:person          <http://dbpedia.org/resource/Kemba_Walker> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/703488/0/6> ;
        ont:subject         <http://dbpedia.org/resource/Scott_Walker_(singer)> ;
        ont:timeExpression  "by the 1980s" .

<http://w3id.org/polifonia/pilot/meetups/703488/25/1>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Critical acclaim for the album garnered a Metacritic score of 85." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/703488/25> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/703488/25/0> ;
        ont:subject         <http://dbpedia.org/resource/Scott_Walker_(singer)> ;
        ont:timeExpression  "score of 85" .

<http://w3id.org/polifonia/pilot/meetups/703488/19/11>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He did not return to public attention until the early 1990s when his solo and Walker Brothers work was critically reappraised again." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/703488/19> ;
        ont:person        <http://dbpedia.org/resource/Kemba_Walker> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/703488/19/10> ;
        ont:subject       <http://dbpedia.org/resource/Scott_Walker_(singer)> .

<http://w3id.org/polifonia/pilot/meetups/703488/17/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "By the mid-1970s, Walker's career was at its nadir, and he joined back up with John Maus and Gary Leeds to revive the Walker Brothers in 1975." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/703488/17> ;
        ont:person        <http://dbpedia.org/resource/John_Maus> , <http://dbpedia.org/resource/Gary_Walker_(musician)> , <http://dbpedia.org/resource/Kemba_Walker> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/703488/17/0> ;
        ont:subject       <http://dbpedia.org/resource/Scott_Walker_(singer)> .

<http://w3id.org/polifonia/pilot/meetups/703488/9>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/703488/9/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/703488/9/1> ;
        rdf:_3    <http://w3id.org/polifonia/pilot/meetups/703488/9/2> .

<http://w3id.org/polifonia/pilot/meetups/703488/15/8>
        rdf:type          ont:Sentence ;
        ont:excerpt       "It went from bad to worse..." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/703488/15> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/703488/15/7> ;
        ont:subject       <http://dbpedia.org/resource/Scott_Walker_(singer)> .

<http://w3id.org/polifonia/pilot/meetups/703488/8/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "I think I just got irritated with it all.\"" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/703488/8> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/703488/8/3> ;
        ont:subject       <http://dbpedia.org/resource/Scott_Walker_(singer)> .

<http://w3id.org/polifonia/pilot/meetups/703488/26/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Interviews were recorded with David Bowie (executive producer of the film), Radiohead, Sting, Gavin Friday and many musicians associated with Walker over the years." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/703488/26> ;
        ont:person        <http://dbpedia.org/resource/Gavin_Friday> , <http://dbpedia.org/resource/Sting_(musician)> , <http://dbpedia.org/resource/Kemba_Walker> , <http://dbpedia.org/resource/David_Bowie> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/703488/26/0> ;
        ont:subject       <http://dbpedia.org/resource/Scott_Walker_(singer)> .

<http://w3id.org/polifonia/pilot/meetups/703488/25>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/703488/25/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/703488/25/1> ;
