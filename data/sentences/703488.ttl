<http://w3id.org/polifonia/pilot/meetups/703488/25/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In both composition and atmosphere, The Drift was a continuation of the surreal, menacing, partially abstract approach displayed on Climate of Hunter and Tilt." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/703488/25> ;
        ont:place         <http://dbpedia.org/resource/Hunter_Region> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/703488/25/1> ;
        ont:subject       <http://dbpedia.org/resource/Scott_Walker_(singer)> .

<http://w3id.org/polifonia/pilot/meetups/703488/19/12>
        rdf:type            ont:Sentence ;
        ont:excerpt         "During this period Walker's first four studio albums were issued on CD for the first time and the compilation album No Regrets – The Best of Scott Walker and The Walker Brothers 1965–1976 hit number 4 on the UK Albums Chart." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/703488/19> ;
        ont:person          <http://dbpedia.org/resource/Kemba_Walker> , <http://dbpedia.org/resource/Scott_Walker_(politician)> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/703488/19/11> ;
        ont:subject         <http://dbpedia.org/resource/Scott_Walker_(singer)> ;
        ont:timeExpression  "4 on" , "four studio" .

<http://w3id.org/polifonia/pilot/meetups/703488/3/0>
        rdf:type          ont:Sentence ;
