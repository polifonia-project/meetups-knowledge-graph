<http://w3id.org/polifonia/pilot/meetups/1359293/2/2>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Drafted into the Navy during World War II, Henderson became an arranger for the Navy band stationed at the Naval Station Great Lakes, prior to becoming the staff orchestrator for The U. S. Navy School of Music in Washington, D.C. from 1944 to 1946." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1359293/2> ;
        ont:person          <http://dbpedia.org/resource/George_Washington> , <http://dbpedia.org/resource/Gerald_Henderson_Jr.> ;
        ont:place           <http://dbpedia.org/resource/Washington,_D.C.> , <http://dbpedia.org/resource/Naval_Station_Great_Lakes> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1359293/2/1> ;
        ont:subject         <http://dbpedia.org/resource/Luther_Henderson> ;
        ont:timeExpression  "from 1944 to 1946" .

<http://w3id.org/polifonia/pilot/meetups/1359293/6/1>
        rdf:type          ont:Sentence ;
