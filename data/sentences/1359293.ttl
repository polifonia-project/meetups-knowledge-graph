<http://w3id.org/polifonia/pilot/meetups/1359293/2/5>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Henderson went on to serve as classical orchestrator for Ellington's symphonic works, receiving the nickname of being Ellington's \"classical arm.\"" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1359293/2> ;
        ont:person        <http://dbpedia.org/resource/Gerald_Henderson_Jr.> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1359293/2/4> ;
        ont:subject       <http://dbpedia.org/resource/Luther_Henderson> .

<http://w3id.org/polifonia/pilot/meetups/1359293/10/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Following a long battle with cancer, Henderson died on July 29, 2003 at the age of 84." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1359293/10> ;
        ont:person          <http://dbpedia.org/resource/Gerald_Henderson_Jr.> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1359293/10/0> ;
        ont:subject         <http://dbpedia.org/resource/Luther_Henderson> ;
        ont:timeExpression  "age of 84" , "July 29 , 2003" .

<http://w3id.org/polifonia/pilot/meetups/1359293/6/4>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Henderson's arrangements of Ellington's music were recorded in 1999 by the City of Birmingham Symphony Orchestra." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1359293/6> ;
        ont:person          <http://dbpedia.org/resource/Gerald_Henderson_Jr.> ;
