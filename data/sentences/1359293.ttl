        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1359293/0/0> ;
        ont:subject         <http://dbpedia.org/resource/Luther_Henderson> ;
        ont:timeExpression  "July 29 , 2003" , "March 14 , 1919" .

<http://w3id.org/polifonia/pilot/meetups/1359293/8/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 2008 Juilliard School of Music, Henderson's alma mater, established the Luther Henderson Scholarship Fund." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1359293/8> ;
        ont:person        <http://dbpedia.org/resource/Gerald_Henderson_Jr.> , <http://dbpedia.org/resource/Alma_mater> , <http://dbpedia.org/resource/Luther_Henderson> ;
