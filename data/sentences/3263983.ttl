<http://w3id.org/polifonia/pilot/meetups/3263983/9/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Bing Crosby went to Europe in 1950 for an extended visit and there were headlines stating that the Crosby marriage was strained, but this was subsequently denied." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/3263983/9> ;
        ont:person          <http://dbpedia.org/resource/Bing_Crosby> ;
        ont:place           <http://dbpedia.org/resource/Europe> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/3263983/9/0> ;
        ont:subject         <http://dbpedia.org/resource/Dixie_Lee> ;
        ont:timeExpression  "1950 for" .

<http://w3id.org/polifonia/pilot/meetups/3263983/4/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Dixie Lee was better known than Crosby at that time, as illustrated by the incorrect news release issued by the Associated Press, which reported she married \"Murray Crosey\"." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/3263983/4> ;
        ont:person        <http://dbpedia.org/resource/Bing_Crosby> , <http://dbpedia.org/resource/Dixie_Lee> ;
        ont:place         <http://dbpedia.org/resource/Murray_River> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/3263983/4/0> ;
        ont:subject       <http://dbpedia.org/resource/Dixie_Lee> .

