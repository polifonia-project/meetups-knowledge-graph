        rdf:type          ont:Sentence ;
        ont:excerpt       "Crosby's biographer, Gary Giddins, describes Dixie Lee as a shy, private person with a sensible approach to life." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/3263983/8> ;
        ont:person        <http://dbpedia.org/resource/Bing_Crosby> , <http://dbpedia.org/resource/Dixie_Lee> ;
