        ont:excerpt         "(When she entered show business, she gave her birth date as November 4, 1911.)" ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/3263983/2> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/3263983/2/0> ;
        ont:subject         <http://dbpedia.org/resource/Dixie_Lee> ;
        ont:timeExpression  "November 4 , 1911" .

<http://w3id.org/polifonia/pilot/meetups/3263983/5/6>
        rdf:type            ont:Sentence ;
        ont:excerpt         "To one of the girls in the crowd, she confided she was not half as angry with Crosby as she was at the parting a week ago." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/3263983/5> ;
        ont:person          <http://dbpedia.org/resource/Bing_Crosby> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/3263983/5/5> ;
        ont:subject         <http://dbpedia.org/resource/Dixie_Lee> ;
        ont:timeExpression  "one of" .

<http://w3id.org/polifonia/pilot/meetups/3263983/27>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/3263983/27/0> .

<http://w3id.org/polifonia/pilot/meetups/3263983/6/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "After the birth of the twins, she made a brief return to show business." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/3263983/6> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/3263983/6/0> ;
        ont:subject       <http://dbpedia.org/resource/Dixie_Lee> .

<http://w3id.org/polifonia/pilot/meetups/3263983/1/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Biography ==" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/3263983/1> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/3263983/1/0> ;
        ont:subject       <http://dbpedia.org/resource/Dixie_Lee> .

<http://w3id.org/polifonia/pilot/meetups/3263983/9/5>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Dixie Lee died from ovarian cancer on November 1, 1952, three days before her 43rd birthday." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/3263983/9> ;
        ont:person          <http://dbpedia.org/resource/Dixie_Lee> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/3263983/9/4> ;
        ont:subject         <http://dbpedia.org/resource/Dixie_Lee> ;
        ont:timeExpression  "November 1 , 1952" , "43rd birthday" .

<http://w3id.org/polifonia/pilot/meetups/3263983/5/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "There were early problems with the marriage, and on March 4, 1931, Dixie announced that they had separated and that she would soon be filing a divorce suit charging mental cruelty." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/3263983/5> ;
        ont:person          <http://dbpedia.org/resource/Divorce> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/3263983/5/0> ;
        ont:subject         <http://dbpedia.org/resource/Dixie_Lee> ;
        ont:timeExpression  "March 4 , 1931" .

<http://w3id.org/polifonia/pilot/meetups/3263983/21>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/3263983/21/0> .

<http://w3id.org/polifonia/pilot/meetups/3263983/0/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "She was the first wife of singer Bing Crosby." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/3263983/0> ;
        ont:person        <http://dbpedia.org/resource/Bing_Crosby> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/3263983/0/0> ;
        ont:subject       <http://dbpedia.org/resource/Dixie_Lee> .

<http://w3id.org/polifonia/pilot/meetups/3263983/8/5>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Crosby accompanied his wife to the hospital in an ambulance and remained at her bedside during the night." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/3263983/8> ;
        ont:person        <http://dbpedia.org/resource/Bing_Crosby> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/3263983/8/4> ;
        ont:subject       <http://dbpedia.org/resource/Dixie_Lee> .

<http://w3id.org/polifonia/pilot/meetups/3263983/34>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/3263983/34/0> .

<http://w3id.org/polifonia/pilot/meetups/3263983/4/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "She met Bing Crosby at the age of 20 and they married on September 29, 1930, at the Church of the Blessed Sacrament in Hollywood." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/3263983/4> ;
        ont:person          <http://dbpedia.org/resource/Bing_Crosby> ;
        ont:place           <http://dbpedia.org/resource/Hollywood> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/3263983/4/0> ;
        ont:subject         <http://dbpedia.org/resource/Dixie_Lee> ;
        ont:timeExpression  "September 29 , 1930" , "age of 20" .

<http://w3id.org/polifonia/pilot/meetups/3263983/19>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/3263983/19/0> .

<http://w3id.org/polifonia/pilot/meetups/3263983/8>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/3263983/8/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/3263983/8/1> ;
