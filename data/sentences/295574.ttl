<http://w3id.org/polifonia/pilot/meetups/295574/2/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He was introduced to Jamaican music when he lodged as a teenager in a West Indian household in Brixton, South West London." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/295574/2> ;
        ont:place         <http://dbpedia.org/resource/Brixton> , <http://dbpedia.org/resource/South_London> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/295574/2/0> ;
