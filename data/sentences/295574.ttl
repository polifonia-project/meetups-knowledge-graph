<http://w3id.org/polifonia/pilot/meetups/295574/6/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Judge Dread died from a heart attack as he walked off stage after performing at The Penny Theatre in Canterbury on 13 March 1998." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/295574/6> ;
        ont:person          <http://dbpedia.org/resource/Judge_Dread> ;
        ont:place           <http://dbpedia.org/resource/Canterbury> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/295574/6/0> ;
        ont:subject         <http://dbpedia.org/resource/Judge_Dread> ;
        ont:timeExpression  "13 March 1998" .

