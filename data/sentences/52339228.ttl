<http://w3id.org/polifonia/pilot/meetups/52339228/0/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "His most notable recording was  \"Mr. President\", a protest against cuts in social welfare undertaken by President Ronald Reagan." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/52339228/0> ;
        ont:person        <http://dbpedia.org/resource/President_of_the_United_States> , <http://dbpedia.org/resource/Ronald_Reagan> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/52339228/0/0> ;
