<http://w3id.org/polifonia/pilot/meetups/52339228/3/6>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In later years arthritis affected his guitar playing, so he often improvised, presenting a challenge to the local musicians backing him." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/52339228/3> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/52339228/3/5> ;
        ont:subject       <http://dbpedia.org/resource/Big_Boy_Henry> .

<http://w3id.org/polifonia/pilot/meetups/52339228/3/11>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He became a respected figure locally, mainly because of his generous nature and support of other musicians." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/52339228/3> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/52339228/3/10> ;
