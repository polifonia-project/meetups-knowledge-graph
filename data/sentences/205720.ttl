<http://w3id.org/polifonia/pilot/meetups/205720/15/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Ballard is buried in Detroit Memorial Park Cemetery in Warren, Michigan." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/205720/15> ;
        ont:person        <http://dbpedia.org/resource/J._G._Ballard> ;
        ont:place         <http://dbpedia.org/resource/Memorial_Park_Cemetery_(Memphis,_Tennessee)> , <http://dbpedia.org/resource/Michigan> , <http://dbpedia.org/resource/Detroit> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/205720/15/1> ;
        ont:subject       <http://dbpedia.org/resource/Florence_Ballard> .

<http://w3id.org/polifonia/pilot/meetups/205720/1/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Ballard was posthumously inducted to the Rock and Roll Hall of Fame as a member of the Supremes in 1988." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/205720/1> ;
        ont:person        <http://dbpedia.org/resource/J._G._Ballard> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/205720/1/2> ;
        ont:subject       <http://dbpedia.org/resource/Florence_Ballard> .

<http://w3id.org/polifonia/pilot/meetups/205720/8/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Ballard expressed dissatisfaction with the group's direction throughout its successful period." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/205720/8> ;
        ont:person        <http://dbpedia.org/resource/J._G._Ballard> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/205720/8/0> ;
        ont:subject       <http://dbpedia.org/resource/Florence_Ballard> .

<http://w3id.org/polifonia/pilot/meetups/205720/6/8>
        rdf:type            ont:Sentence ;
        ont:excerpt         "The group struggled in their early years with the label, releasing eight singles that failed to crack the Billboard Hot 100, giving them the nickname \"no-hit Supremes\"." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/205720/6> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/205720/6/7> ;
        ont:subject         <http://dbpedia.org/resource/Florence_Ballard> ;
        ont:timeExpression  "Hot 100" .

<http://w3id.org/polifonia/pilot/meetups/205720/3/10>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Ballard met future singing partner Mary Wilson during a middle-school talent show and they became friends while attending Northeastern High." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/205720/3> ;
        ont:person        <http://dbpedia.org/resource/Mary_Wilson_Goelet> , <http://dbpedia.org/resource/J._G._Ballard> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/205720/3/9> ;
        ont:subject       <http://dbpedia.org/resource/Florence_Ballard> .

<http://w3id.org/polifonia/pilot/meetups/205720/9/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "On July 1, the day after her 24th birthday, Ballard showed up inebriated during the group's third performance at the Flamingo and stuck her stomach out from her suit." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/205720/9> ;
        ont:person          <http://dbpedia.org/resource/J._G._Ballard> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/205720/9/0> ;
        ont:subject         <http://dbpedia.org/resource/Florence_Ballard> ;
        ont:timeExpression  "July 1" .

<http://w3id.org/polifonia/pilot/meetups/205720/17/2>
        rdf:type            ont:Sentence ;
        ont:excerpt         "In his short story \"You Know They Got a Hell of a Band\", Stephen King, through the late disc jockey Alan Freed, includes Ballard as one of the deceased artists who performs in a town called \"Rock and Roll Heaven\"." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/205720/17> ;
        ont:person          <http://dbpedia.org/resource/Stephen_IV_of_Hungary> , <http://dbpedia.org/resource/Disc_jockey> , <http://dbpedia.org/resource/J._G._Ballard> , <http://dbpedia.org/resource/Alan_Freed> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/205720/17/1> ;
        ont:subject         <http://dbpedia.org/resource/Florence_Ballard> ;
        ont:timeExpression  "one of" .

