<http://w3id.org/polifonia/pilot/meetups/205720/13/5>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Afterwards, she started receiving offers for interviews; Jet magazine was one of the first to report on Ballard and her recovery." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/205720/13> ;
        ont:person          <http://dbpedia.org/resource/J._G._Ballard> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/205720/13/4> ;
        ont:subject         <http://dbpedia.org/resource/Florence_Ballard> ;
        ont:timeExpression  "one of" .

<http://w3id.org/polifonia/pilot/meetups/205720/6/17>
        rdf:type          ont:Sentence ;
        ont:excerpt       "During live shows, Ballard often performed the Barbra Streisand standard, \"People\"." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/205720/6> ;
        ont:person        <http://dbpedia.org/resource/J._G._Ballard> , <http://dbpedia.org/resource/Barbra_Streisand> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/205720/6/16> ;
        ont:subject       <http://dbpedia.org/resource/Florence_Ballard> .

<http://w3id.org/polifonia/pilot/meetups/205720/6/3>
        rdf:type            ont:Sentence ;
        ont:excerpt         "In January 1961, Gordy agreed to sign them on the condition they change their name." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/205720/6> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/205720/6/2> ;
        ont:subject         <http://dbpedia.org/resource/Florence_Ballard> ;
        ont:timeExpression  "January 1961" .

<http://w3id.org/polifonia/pilot/meetups/205720/10/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "===" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/205720/10> ;
