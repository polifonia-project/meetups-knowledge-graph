<http://w3id.org/polifonia/pilot/meetups/205720/26>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/205720/26/0> .

<http://w3id.org/polifonia/pilot/meetups/205720/11/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "In July 1971, Ballard sued Motown for additional royalty payments she believed she was due to receive; she was defeated in court by Motown." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/205720/11> ;
        ont:person          <http://dbpedia.org/resource/J._G._Ballard> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/205720/11/0> ;
        ont:subject         <http://dbpedia.org/resource/Florence_Ballard> ;
        ont:timeExpression  "July 1971" .

<http://w3id.org/polifonia/pilot/meetups/205720/9/12>
        rdf:type          ont:Sentence ;
        ont:excerpt       "She continued to perform as a solo artist, opening for Bill Cosby that September at Chicago's Auditorium Theatre." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/205720/9> ;
        ont:person        <http://dbpedia.org/resource/Bill_Cosby> ;
        ont:place         <http://dbpedia.org/resource/Auditorium_Theatre> , <http://dbpedia.org/resource/Chicago> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/205720/9/11> ;
        ont:subject       <http://dbpedia.org/resource/Florence_Ballard> .

