<http://w3id.org/polifonia/pilot/meetups/205720/6/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Later in 1960, the Primettes signed a contract with Lu Pine Records, issuing two songs that failed to perform well." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/205720/6> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/205720/6/0> ;
        ont:subject       <http://dbpedia.org/resource/Florence_Ballard> .

<http://w3id.org/polifonia/pilot/meetups/205720/3/23>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Both Wilson and Jesse Green, an early boyfriend of Florence's, had described her as a \"generally happy if somewhat mischievous and sassy teenager.\"" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/205720/3> ;
        ont:person        <http://dbpedia.org/resource/Jesse_Green> , <http://dbpedia.org/resource/Woodrow_Wilson> ;
        ont:place         <http://dbpedia.org/resource/Florence> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/205720/3/22> ;
        ont:subject       <http://dbpedia.org/resource/Florence_Ballard> .

