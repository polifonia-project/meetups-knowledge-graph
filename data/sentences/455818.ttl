        ont:place         <http://dbpedia.org/resource/France> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/455818/7/0> ;
        ont:subject       <http://dbpedia.org/resource/Léo_Ferré> .

<http://w3id.org/polifonia/pilot/meetups/455818/6/17>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1969, Ferré settled in Tuscany, in Italy." ;
