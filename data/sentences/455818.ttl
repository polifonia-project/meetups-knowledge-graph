<http://w3id.org/polifonia/pilot/meetups/455818/6/32>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Being dropped by the \"professionals\", and disgusted for good with being \"a merchandise for producers\", Ferré refused to accept French song prizes." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/455818/6> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/455818/6/31> ;
