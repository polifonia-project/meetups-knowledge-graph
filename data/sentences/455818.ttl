<http://w3id.org/polifonia/pilot/meetups/455818/6/13>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In his absence, Ferré's chimpanzee Pépée suffered a fall and refused to be approached." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/455818/6> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/455818/6/12> ;
