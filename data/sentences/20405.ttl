        ont:person          <http://dbpedia.org/resource/Charlie_Parker> , <http://dbpedia.org/resource/Miles_Davis> ;
        ont:place           <http://dbpedia.org/resource/New_York_City> , <http://dbpedia.org/resource/Detroit> , <http://dbpedia.org/resource/Chicago> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/20405/9/14> ;
        ont:subject         <http://dbpedia.org/resource/Miles_Davis> ;
        ont:timeExpression  "April 30" , "March 1948" .

<http://w3id.org/polifonia/pilot/meetups/20405/51/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Views on his earlier work ==" ;
