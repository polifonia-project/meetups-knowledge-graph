<http://w3id.org/polifonia/pilot/meetups/20405/38/14>
        rdf:type            ont:Sentence ;
        ont:excerpt         "In August 1978, Davis hired a new manager, Mark Rothbaum, who had worked with him since 1972." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/20405/38> ;
        ont:person          <http://dbpedia.org/resource/Anthony_Davis> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/20405/38/13> ;
