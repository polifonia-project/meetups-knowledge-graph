        ont:excerpt       "In 2009, the US House of Representatives passed a resolution that honored it as a national treasure." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/20405/24> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/20405/24/2> ;
