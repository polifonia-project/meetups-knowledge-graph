        ont:timeExpression  "August 1978" .

<http://w3id.org/polifonia/pilot/meetups/20405/15/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "The doctors told him to remain silent after the operation, but he got into an argument that permanently damaged his vocal cords and gave him a raspy voice for the rest of his life." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/20405/15> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/20405/15/0> ;
        ont:subject       <http://dbpedia.org/resource/Miles_Davis> .

<http://w3id.org/polifonia/pilot/meetups/20405/11/15>
        rdf:type          ont:Sentence ;
        ont:excerpt       "The presence of white musicians in the group angered some black players, many of whom were unemployed at the time, yet Davis rebuffed their criticisms." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/20405/11> ;
        ont:person        <http://dbpedia.org/resource/Miles_Davis> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/20405/11/14> ;
        ont:subject       <http://dbpedia.org/resource/Miles_Davis> .

