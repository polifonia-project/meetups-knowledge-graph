        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/20405/34/2> ;
        ont:subject       <http://dbpedia.org/resource/Miles_Davis> .

<http://w3id.org/polifonia/pilot/meetups/20405/48/6>
        rdf:type            ont:Sentence ;
        ont:excerpt         "After returning to America, he stopped in New York City to record material for Doo-Bop, then returned to California to play at the Hollywood Bowl on August 25, his final live performance." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/20405/48> ;
        ont:place           <http://dbpedia.org/resource/Hollywood_Bowl> , <http://dbpedia.org/resource/New_York_City> , <http://dbpedia.org/resource/California> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/20405/48/5> ;
        ont:subject         <http://dbpedia.org/resource/Miles_Davis> ;
        ont:timeExpression  "August 25" .

<http://w3id.org/polifonia/pilot/meetups/20405/24/17>
        rdf:type            ont:Sentence ;
        ont:excerpt         "They married in December 1959 in Toledo, Ohio." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/20405/24> ;
        ont:place           <http://dbpedia.org/resource/Ohio> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/20405/24/16> ;
        ont:subject         <http://dbpedia.org/resource/Miles_Davis> ;
        ont:timeExpression  "December 1959" .

<http://w3id.org/polifonia/pilot/meetups/20405/43/8>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He brought in producer, composer, and keyboardist Robert Irving III, who had collaborated with him on The Man with the Horn." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/20405/43> ;
        ont:person        <http://dbpedia.org/resource/Composer> , <http://dbpedia.org/resource/The_Man> , <http://dbpedia.org/resource/Robert_Irving_III> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/20405/43/7> ;
        ont:subject       <http://dbpedia.org/resource/Miles_Davis> .

<http://w3id.org/polifonia/pilot/meetups/20405/41/1>
        rdf:type            ont:Sentence ;
        ont:excerpt         "His first post-hiatus studio appearance took place in May 1980." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/20405/41> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/20405/41/0> ;
        ont:subject         <http://dbpedia.org/resource/Miles_Davis> ;
        ont:timeExpression  "May 1980" .

<http://w3id.org/polifonia/pilot/meetups/20405/50/6>
        rdf:type          ont:Sentence ;
        ont:excerpt       "His death was attributed to the combined effects of a stroke, pneumonia, and respiratory failure." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/20405/50> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/20405/50/5> ;
        ont:subject       <http://dbpedia.org/resource/Miles_Davis> .

