<http://w3id.org/polifonia/pilot/meetups/20405/11/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Evans' Manhattan apartment had become the meeting place for several young musicians and composers such as Davis, Roach, Lewis, and Mulligan who were unhappy with the increasingly virtuoso instrumental techniques that dominated bebop." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/20405/11> ;
        ont:person        <http://dbpedia.org/resource/Miles_Davis> , <http://dbpedia.org/resource/Tyreke_Evans> ;
        ont:place         <http://dbpedia.org/resource/Manhattan> , <http://dbpedia.org/resource/Isle_of_Lewis> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/20405/11/0> ;
        ont:subject       <http://dbpedia.org/resource/Miles_Davis> .

<http://w3id.org/polifonia/pilot/meetups/20405/37/0>
        rdf:type          ont:Sentence ;
