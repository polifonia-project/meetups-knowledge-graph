        ont:subject         <http://dbpedia.org/resource/Miles_Davis> ;
        ont:timeExpression  "November 1971" .

<http://w3id.org/polifonia/pilot/meetups/20405/4/23>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Years later, Davis considered this job one of the most important of his career." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/20405/4> ;
        ont:person          <http://dbpedia.org/resource/Anthony_Davis> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/20405/4/22> ;
        ont:subject         <http://dbpedia.org/resource/Miles_Davis> ;
        ont:timeExpression  "one of" .

<http://w3id.org/polifonia/pilot/meetups/20405/58/2>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Also in 2005, a London exhibition was held of his paintings, The Last Miles: The Music of Miles Davis, 1980-1991' was released detailing his final years and eight of his albums from the 1960s and 70s were reissued in celebration of the 50th anniversary of his signing to Columbia Records." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/20405/58> ;
        ont:person          <http://dbpedia.org/resource/Miles_Davis> ;
        ont:place           <http://dbpedia.org/resource/London> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/20405/58/1> ;
        ont:subject         <http://dbpedia.org/resource/Miles_Davis> ;
        ont:timeExpression  "eight of" , "from the 1960s" .

<http://w3id.org/polifonia/pilot/meetups/20405/9/11>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He joined a quintet led by Parker that also included Max Roach." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/20405/9> ;
        ont:person        <http://dbpedia.org/resource/Charlie_Parker> , <http://dbpedia.org/resource/Max_Roach> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/20405/9/10> ;
        ont:subject       <http://dbpedia.org/resource/Miles_Davis> .

<http://w3id.org/polifonia/pilot/meetups/20405/19/8>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Wynton Kelly, his replacement, brought to the group a swinging style that contrasted with Evans's delicacy." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/20405/19> ;
        ont:person        <http://dbpedia.org/resource/Wynton_Kelly> , <http://dbpedia.org/resource/Tyreke_Evans> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/20405/19/7> ;
        ont:subject       <http://dbpedia.org/resource/Miles_Davis> .

