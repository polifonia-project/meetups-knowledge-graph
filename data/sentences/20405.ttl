<http://w3id.org/polifonia/pilot/meetups/20405/0/6>
        rdf:type          ont:Sentence ;
        ont:excerpt       "After a widely acclaimed comeback performance at the Newport Jazz Festival, he signed a long-term contract with Columbia Records and recorded the album 'Round About Midnight in 1955." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/20405/0> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/20405/0/5> ;
        ont:subject       <http://dbpedia.org/resource/Miles_Davis> .

<http://w3id.org/polifonia/pilot/meetups/20405/31/3>
        rdf:type          ont:Sentence ;
