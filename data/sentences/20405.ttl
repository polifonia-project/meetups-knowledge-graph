        ont:timeExpression  "4,000 advance" , "38,643 in" .

<http://w3id.org/polifonia/pilot/meetups/20405/38/11>
        rdf:type          ont:Sentence ;
        ont:excerpt       "After Coryell declined a spot in a band that Davis was beginning to put together, Davis returned to his reclusive lifestyle in New York City." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/20405/38> ;
        ont:person        <http://dbpedia.org/resource/Miles_Davis> ;
        ont:place         <http://dbpedia.org/resource/New_York_City> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/20405/38/10> ;
        ont:subject       <http://dbpedia.org/resource/Miles_Davis> .

