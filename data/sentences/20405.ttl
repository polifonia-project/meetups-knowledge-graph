<http://w3id.org/polifonia/pilot/meetups/20405/58/14>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Birth of the cool was later released on PBS' American Masters series." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/20405/58> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/20405/58/13> ;
        ont:subject       <http://dbpedia.org/resource/Miles_Davis> .

<http://w3id.org/polifonia/pilot/meetups/20405/22/7>
        rdf:type            ont:Sentence ;
        ont:excerpt         "The pair's final album was Quiet Nights (1962), a collection of bossa nova songs released against their wishes." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/20405/22> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/20405/22/6> ;
        ont:subject         <http://dbpedia.org/resource/Miles_Davis> ;
        ont:timeExpression  "1962" .

<http://w3id.org/polifonia/pilot/meetups/20405/39/1>
        rdf:type            ont:Sentence ;
        ont:excerpt         "The two married in November 1981, but their tumultuous marriage ended with Tyson filing for divorce in 1988, which was finalized in 1989." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/20405/39> ;
        ont:person          <http://dbpedia.org/resource/Cicely_Tyson> , <http://dbpedia.org/resource/Divorce> ;
