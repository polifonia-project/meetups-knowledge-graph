<http://w3id.org/polifonia/pilot/meetups/20405/17/9>
        rdf:type          ont:Sentence ;
        ont:excerpt       "This group appeared on his final albums for Prestige, which were recorded in two one-day sessions in 1956." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/20405/17> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/20405/17/8> ;
