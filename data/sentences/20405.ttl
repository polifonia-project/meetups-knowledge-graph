        rdf:type          ont:Sentence ;
        ont:excerpt       "The trumpet Davis used on the recording is displayed on the campus of the University of North Carolina at Greensboro." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/20405/57> ;
        ont:person        <http://dbpedia.org/resource/Miles_Davis> ;
        ont:place         <http://dbpedia.org/resource/North_Carolina> ;
