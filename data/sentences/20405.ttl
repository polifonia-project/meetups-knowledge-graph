        ont:subject         <http://dbpedia.org/resource/Miles_Davis> ;
        ont:timeExpression  "September 1968" .

<http://w3id.org/polifonia/pilot/meetups/20405/44/3>
        rdf:type            ont:Sentence ;
        ont:excerpt         "In May 1985, one month into a tour, Davis signed a contract with Warner Bros. that required him to give up his publishing rights." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/20405/44> ;
        ont:person          <http://dbpedia.org/resource/Anthony_Davis> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/20405/44/2> ;
        ont:subject         <http://dbpedia.org/resource/Miles_Davis> ;
        ont:timeExpression  "May 1985 , one" .

<http://w3id.org/polifonia/pilot/meetups/20405/58/6>
        rdf:type            ont:Sentence ;
        ont:excerpt         "It premiered at the New York Film Festival in October 2015." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/20405/58> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/20405/58/5> ;
