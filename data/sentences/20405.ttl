        ont:timeExpression  "February 1969" .

<http://w3id.org/polifonia/pilot/meetups/20405/47/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "After taking part in the recording of the 1985 protest song \"Sun City\" as a member of Artists United Against Apartheid, Davis appeared on the instrumental \"Don't Stop Me Now\" by Toto for their album Fahrenheit (1986)." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/20405/47> ;
        ont:person          <http://dbpedia.org/resource/Del_Webb> , <http://dbpedia.org/resource/Miles_Davis> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/20405/47/0> ;
        ont:subject         <http://dbpedia.org/resource/Miles_Davis> ;
        ont:timeExpression  "1985 protest song" , "1986" .

<http://w3id.org/polifonia/pilot/meetups/20405/47/16>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He was interviewed on 60 Minutes by Harry Reasoner." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/20405/47> ;
        ont:person        <http://dbpedia.org/resource/Harry_Reasoner> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/20405/47/15> ;
        ont:subject       <http://dbpedia.org/resource/Miles_Davis> .

