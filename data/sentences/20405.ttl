        rdf:type          ont:Sentence ;
        ont:excerpt       "Writer Stanley Crouch criticised Davis's work from \"In a Silent Way\" onwards." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/20405/52> ;
