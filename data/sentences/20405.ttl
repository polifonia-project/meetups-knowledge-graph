        ont:person        <http://dbpedia.org/resource/Miles_Davis> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/20405/13/11> ;
        ont:subject       <http://dbpedia.org/resource/Miles_Davis> .

<http://w3id.org/polifonia/pilot/meetups/20405/30/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "In September 1968, Davis married 23-year-old model and songwriter Betty Mabry." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/20405/30> ;
        ont:person          <http://dbpedia.org/resource/Betty_Davis> , <http://dbpedia.org/resource/Miles_Davis> ;
