        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/20405/31/9> ;
        ont:subject         <http://dbpedia.org/resource/Miles_Davis> ;
        ont:timeExpression  "1973" , "1970" .

<http://w3id.org/polifonia/pilot/meetups/20405/50/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "In early September 1991, Davis checked into St. John's Hospital near his home in Santa Monica, California, for routine tests." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/20405/50> ;
        ont:place           <http://dbpedia.org/resource/Santa_Monica,_California> ;
