        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/20405/43/6> ;
        ont:subject       <http://dbpedia.org/resource/Miles_Davis> .

<http://w3id.org/polifonia/pilot/meetups/20405/41/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Having played the trumpet little throughout the previous three years, Davis found it difficult to reclaim his embouchure." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/20405/41> ;
        ont:person        <http://dbpedia.org/resource/Miles_Davis> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/20405/41/0> ;
        ont:subject       <http://dbpedia.org/resource/Miles_Davis> .

