        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/20405/13/0> ;
        ont:subject       <http://dbpedia.org/resource/Miles_Davis> .

<http://w3id.org/polifonia/pilot/meetups/20405/27/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Davis needed medical attention for hip pain, which had worsened since his Japanese tour during the previous year." ;
