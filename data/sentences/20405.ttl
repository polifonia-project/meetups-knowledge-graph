        ont:subject         <http://dbpedia.org/resource/Miles_Davis> ;
        ont:timeExpression  "1982 interview" .

<http://w3id.org/polifonia/pilot/meetups/20405/22/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Davis accepted and worked with Gil Evans in what became a five-album collaboration from 1957 to 1962." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/20405/22> ;
        ont:person          <http://dbpedia.org/resource/Miles_Davis> , <http://dbpedia.org/resource/Gil_Evans> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/20405/22/0> ;
        ont:subject         <http://dbpedia.org/resource/Miles_Davis> ;
        ont:timeExpression  "from 1957 to 1962" .

<http://w3id.org/polifonia/pilot/meetups/20405/0/8>
        rdf:type            ont:Sentence ;
        ont:excerpt         "During this period, he alternated between orchestral jazz collaborations with arranger Gil Evans, such as the Spanish music-influenced Sketches of Spain (1960), and band recordings, such as Milestones (1958) and Kind of Blue (1959)." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/20405/0> ;
        ont:person          <http://dbpedia.org/resource/Gil_Evans> ;
        ont:place           <http://dbpedia.org/resource/Spain> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/20405/0/7> ;
        ont:subject         <http://dbpedia.org/resource/Miles_Davis> ;
        ont:timeExpression  "1959" , "1958" , "1960" .

<http://w3id.org/polifonia/pilot/meetups/20405/31/5>
        rdf:type            ont:Sentence ;
        ont:excerpt         "In March 1970, Davis began to perform as the opening act for rock acts, allowing Columbia to market Bitches Brew to a larger audience." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/20405/31> ;
        ont:person          <http://dbpedia.org/resource/Miles_Davis> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/20405/31/4> ;
        ont:subject         <http://dbpedia.org/resource/Miles_Davis> ;
        ont:timeExpression  "March 1970" .

<http://w3id.org/polifonia/pilot/meetups/20405/24/10>
        rdf:type            ont:Sentence ;
        ont:excerpt         "By January 1960, he was acquitted of disorderly conduct and third-degree assault." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/20405/24> ;
        ont:person          <http://dbpedia.org/resource/Disorderly_conduct> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/20405/24/9> ;
        ont:subject         <http://dbpedia.org/resource/Miles_Davis> ;
        ont:timeExpression  "January 1960" .

<http://w3id.org/polifonia/pilot/meetups/20405/17/12>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He played long, legato, melodic lines, while Coltrane contrasted with energetic solos." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/20405/17> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/20405/17/11> ;
