        rdf:_4    <http://w3id.org/polifonia/pilot/meetups/20405/12/3> .

<http://w3id.org/polifonia/pilot/meetups/20405/42/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "This was followed by appearances with a new band." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/20405/42> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/20405/42/1> ;
        ont:subject       <http://dbpedia.org/resource/Miles_Davis> .

<http://w3id.org/polifonia/pilot/meetups/20405/68/1>
        rdf:type          ont:Sentence ;
