        rdf:type          ont:Sentence ;
        ont:excerpt       "By 2003, it had sold one million copies." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/20405/31> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/20405/31/3> ;
