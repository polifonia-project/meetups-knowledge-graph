        ont:person          <http://dbpedia.org/resource/The_Man> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/20405/41/2> ;
        ont:subject         <http://dbpedia.org/resource/Miles_Davis> ;
        ont:timeExpression  "May 1981" , "June 1980" .

<http://w3id.org/polifonia/pilot/meetups/20405/50/8>
        rdf:type            ont:Sentence ;
        ont:excerpt         "A funeral service was held on October 5, 1991, at St. Peter's Lutheran Church on Lexington Avenue in New York City that was attended by around 500 friends, family members, and musical acquaintances, with many fans standing in the rain." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/20405/50> ;
        ont:place           <http://dbpedia.org/resource/New_York_City> , <http://dbpedia.org/resource/Lexington_Avenue> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/20405/50/7> ;
        ont:subject         <http://dbpedia.org/resource/Miles_Davis> ;
        ont:timeExpression  "October 5 , 1991" .

<http://w3id.org/polifonia/pilot/meetups/20405/3/11>
        rdf:type          ont:Sentence ;
        ont:excerpt       "At an early age he liked music, especially blues, big bands, and gospel." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/20405/3> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/20405/3/10> ;
        ont:subject       <http://dbpedia.org/resource/Miles_Davis> .

<http://w3id.org/polifonia/pilot/meetups/20405/11/21>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He began an affair with singer and actress Juliette Gréco." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/20405/11> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/20405/11/20> ;
        ont:subject       <http://dbpedia.org/resource/Miles_Davis> .

<http://w3id.org/polifonia/pilot/meetups/20405/19/3>
        rdf:type            ont:Sentence ;
        ont:excerpt         "By May 1958, he had replaced Jones with drummer Jimmy Cobb, and Garland left the group, leaving Davis to play piano on \"Sid's Ahead\" for Milestones." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/20405/19> ;
        ont:person          <http://dbpedia.org/resource/Miles_Davis> , <http://dbpedia.org/resource/Judy_Garland> , <http://dbpedia.org/resource/Jimmy_Cobb> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/20405/19/2> ;
        ont:subject         <http://dbpedia.org/resource/Miles_Davis> ;
        ont:timeExpression  "May 1958" .

<http://w3id.org/polifonia/pilot/meetups/20405/14/5>
        rdf:type            ont:Sentence ;
        ont:excerpt         "A few critics consider Walkin' (April 1954) the album that created the hard bop genre." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/20405/14> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/20405/14/4> ;
        ont:subject         <http://dbpedia.org/resource/Miles_Davis> ;
        ont:timeExpression  "April 1954" .

<http://w3id.org/polifonia/pilot/meetups/20405/9/7>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Cawthon gave birth to Davis's second child, Gregory, in East St. Louis before reuniting with Davis in New York City the following year." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/20405/9> ;
        ont:person        <http://dbpedia.org/resource/Pope_Gregory_I> , <http://dbpedia.org/resource/Miles_Davis> ;
        ont:place         <http://dbpedia.org/resource/New_York_City> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/20405/9/6> ;
        ont:subject       <http://dbpedia.org/resource/Miles_Davis> .

<http://w3id.org/polifonia/pilot/meetups/20405/26/1>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Rollins left them soon after, leaving Davis to pay over $25,000 (US$213,889 in 2020 dollars) to cancel upcoming gigs and quickly assemble a new group." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/20405/26> ;
        ont:person          <http://dbpedia.org/resource/Henry_Rollins> , <http://dbpedia.org/resource/Miles_Davis> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/20405/26/0> ;
        ont:subject         <http://dbpedia.org/resource/Miles_Davis> ;
        ont:timeExpression  "213,889 in" , "25,000" .

<http://w3id.org/polifonia/pilot/meetups/20405/4/9>
        rdf:type            ont:Sentence ;
        ont:excerpt         "According to Davis \"By the age of 12, music had become the most important thing in my life.\"" ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/20405/4> ;
        ont:person          <http://dbpedia.org/resource/Anthony_Davis> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/20405/4/8> ;
        ont:subject         <http://dbpedia.org/resource/Miles_Davis> ;
        ont:timeExpression  "age of 12" .

<http://w3id.org/polifonia/pilot/meetups/20405/7/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "In September 1944, Davis accepted his father's idea of studying at the Institute of Musical Arts, later known as the Juilliard School, in New York City." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/20405/7> ;
        ont:person          <http://dbpedia.org/resource/Miles_Davis> ;
        ont:place           <http://dbpedia.org/resource/New_York_City> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/20405/7/0> ;
        ont:subject         <http://dbpedia.org/resource/Miles_Davis> ;
        ont:timeExpression  "September 1944" .

<http://w3id.org/polifonia/pilot/meetups/20405/21/3>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Avakian then suggested that he work with a bigger ensemble, similar to Music for Brass (1957), an album of orchestral and brass-arranged music led by Gunther Schuller featuring Davis as a guest soloist." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/20405/21> ;
        ont:person          <http://dbpedia.org/resource/Gunther_Schuller> , <http://dbpedia.org/resource/Miles_Davis> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/20405/21/2> ;
        ont:subject         <http://dbpedia.org/resource/Miles_Davis> ;
        ont:timeExpression  "1957" .

<http://w3id.org/polifonia/pilot/meetups/20405/35/6>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Cosey later asserted that \"the band really advanced after the Japanese tour\", but Davis was again hospitalized, for his ulcers and a hernia, during a tour of the US while opening for Herbie Hancock." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/20405/35> ;
        ont:person        <http://dbpedia.org/resource/Herbie_Hancock> , <http://dbpedia.org/resource/Miles_Davis> ;
        ont:place         <http://dbpedia.org/resource/United_States> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/20405/35/5> ;
        ont:subject       <http://dbpedia.org/resource/Miles_Davis> .

<http://w3id.org/polifonia/pilot/meetups/20405/46>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/20405/46/0> ;
