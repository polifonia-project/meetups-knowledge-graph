<http://w3id.org/polifonia/pilot/meetups/20405/13/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "His heroin use became an expensive addiction, and Davis, yet to reach 24 years old, \"lost my sense of discipline, lost my sense of control over my life, and started to drift\"." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/20405/13> ;
        ont:person        <http://dbpedia.org/resource/Miles_Davis> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/20405/13/1> ;
        ont:subject       <http://dbpedia.org/resource/Miles_Davis> .

<http://w3id.org/polifonia/pilot/meetups/20405/27/5>
        rdf:type            ont:Sentence ;
        ont:excerpt         "He underwent hip replacement surgery in April 1965, with bone taken from his shin, but it failed." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/20405/27> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/20405/27/4> ;
        ont:subject         <http://dbpedia.org/resource/Miles_Davis> ;
        ont:timeExpression  "April 1965" .

<http://w3id.org/polifonia/pilot/meetups/20405/4/34>
        rdf:type          ont:Sentence ;
