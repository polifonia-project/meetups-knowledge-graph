<http://w3id.org/polifonia/pilot/meetups/18995/34/9>
        rdf:type          ont:Sentence ;
        ont:excerpt       "The film's central themes are consistent with the director's established concerns: New York, violence as culturally endemic, and subcultural divisions down ethnic lines." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/18995/34> ;
        ont:place         <http://dbpedia.org/resource/New_York_City> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/18995/34/8> ;
        ont:subject       <http://dbpedia.org/resource/Martin_Scorsese> .

<http://w3id.org/polifonia/pilot/meetups/18995/72/7>
        rdf:type            ont:Sentence ;
        ont:excerpt         "For his advocacy in film restoration he received the Robert Osborne Award at the 2018 TCM Film Festival." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/18995/72> ;
        ont:person          <http://dbpedia.org/resource/Robert_Osborne> ;
