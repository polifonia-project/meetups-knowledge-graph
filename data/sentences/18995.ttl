        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/18995/41/2> ;
        ont:subject         <http://dbpedia.org/resource/Martin_Scorsese> ;
        ont:timeExpression  "February 7 , 2008" .

<http://w3id.org/polifonia/pilot/meetups/18995/14/7>
        rdf:type          ont:Sentence ;
        ont:excerpt       "A period of wild partying followed, damaging the director's already fragile health." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/18995/14> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/18995/14/6> ;
