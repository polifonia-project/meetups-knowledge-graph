        rdf:type            ont:Sentence ;
        ont:excerpt         "After huge efforts and a monstrously intricate process, she was temporarily successful (September 1860)." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/67379/16> ;
