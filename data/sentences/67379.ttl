        ont:place         <http://dbpedia.org/resource/Kingdom_of_Hungary> , <http://dbpedia.org/resource/Royal_National_Theatre> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/67379/21/5> ;
        ont:subject       <http://dbpedia.org/resource/Franz_Liszt> .

<http://w3id.org/polifonia/pilot/meetups/67379/8/5>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Her father, however, insisted that the affair be broken off." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/67379/8> ;
