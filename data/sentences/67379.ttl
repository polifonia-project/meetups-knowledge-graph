        ont:subject         <http://dbpedia.org/resource/Franz_Liszt> ;
        ont:timeExpression  "4 January 1866" , "26 February 1866" .

<http://w3id.org/polifonia/pilot/meetups/67379/58/11>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Liszt attempted in the symphonic poem to extend this revitalization of the nature of musical discourse and add to it the Romantic ideal of reconciling classical formal principles to external literary concepts." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/67379/58> ;
        ont:person        <http://dbpedia.org/resource/Franz_Liszt> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/67379/58/10> ;
        ont:subject       <http://dbpedia.org/resource/Franz_Liszt> .

