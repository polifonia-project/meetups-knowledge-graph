        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/66585/2/3> ;
        ont:subject         <http://dbpedia.org/resource/Burning_Spear> ;
        ont:timeExpression  "One label" .

<http://w3id.org/polifonia/pilot/meetups/66585/21>
        rdf:type  ont:Paragraph ;
