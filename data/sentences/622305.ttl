<http://w3id.org/polifonia/pilot/meetups/622305/6/10>
        rdf:type          ont:Sentence ;
        ont:excerpt       "By 1910, Eltinge had reached the height of his fame." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/622305/6> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/622305/6/9> ;
        ont:subject       <http://dbpedia.org/resource/Julian_Eltinge> .

<http://w3id.org/polifonia/pilot/meetups/622305/6/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "The show was produced by E. E. Rice and included music by Jerome Kern among others." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/622305/6> ;
        ont:person        <http://dbpedia.org/resource/Jerome_Kern> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/622305/6/0> ;
        ont:subject       <http://dbpedia.org/resource/Julian_Eltinge> .

<http://w3id.org/polifonia/pilot/meetups/622305/1/2>
        rdf:type            ont:Sentence ;
        ont:excerpt         "By the time Eltinge arrived in Hollywood, he was considered one of the highest paid actors on the American stage; but with the arrival of the Great Depression and the death of vaudeville, Eltinge's star began to fade." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/622305/1> ;
        ont:place           <http://dbpedia.org/resource/United_States> , <http://dbpedia.org/resource/Hollywood> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/622305/1/1> ;
        ont:subject         <http://dbpedia.org/resource/Julian_Eltinge> ;
        ont:timeExpression  "one of" .

<http://w3id.org/polifonia/pilot/meetups/622305/4/7>
        rdf:type          ont:Sentence ;
        ont:excerpt       "It is said to be Mrs. Wyman who encouraged William to study the art of female impersonation." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/622305/4> ;
        ont:person        <http://dbpedia.org/resource/Drag_queen> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/622305/4/6> ;
        ont:subject       <http://dbpedia.org/resource/Julian_Eltinge> .

<http://w3id.org/polifonia/pilot/meetups/622305/16/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "This sexual duality led to Chicago Tribune drama critic Percy Hammond's using the term \"ambisextrous\" to describe him." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/622305/16> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/622305/16/0> ;
        ont:subject       <http://dbpedia.org/resource/Julian_Eltinge> .

<http://w3id.org/polifonia/pilot/meetups/622305/0/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "As his star began to rise, he appeared in vaudeville and toured Europe and the United States, even giving a command performance before King Edward VII." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/622305/0> ;
        ont:person        <http://dbpedia.org/resource/Edward_VII> ;
        ont:place         <http://dbpedia.org/resource/United_States> , <http://dbpedia.org/resource/Europe> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/622305/0/1> ;
        ont:subject       <http://dbpedia.org/resource/Julian_Eltinge> .

