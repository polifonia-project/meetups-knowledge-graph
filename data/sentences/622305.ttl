<http://w3id.org/polifonia/pilot/meetups/622305/17/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "==" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/622305/17> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/622305/17/0> ;
        ont:subject       <http://dbpedia.org/resource/Julian_Eltinge> .

<http://w3id.org/polifonia/pilot/meetups/622305/12/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "According to Anthony Slide's The Encyclopedia of Vaudeville, he also had a cameo role in a film entitled How Molly Malone Made Good in 1915." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/622305/12> ;
        ont:person        <http://dbpedia.org/resource/Anthony_Slide> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/622305/12/0> ;
        ont:subject       <http://dbpedia.org/resource/Julian_Eltinge> .

<http://w3id.org/polifonia/pilot/meetups/622305/6/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Eltinge's first appearance on Broadway was in the musical comedy Mr. Wix of Wickham which opened September 19, 1904 at the Bijou Theatre in New York City." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/622305/6> ;
        ont:person          <http://dbpedia.org/resource/Wix_Wickens> ;
        ont:place           <http://dbpedia.org/resource/New_York_City> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/622305/6/0> ;
        ont:subject         <http://dbpedia.org/resource/Julian_Eltinge> ;
        ont:timeExpression  "September 19 , 1904" .

<http://w3id.org/polifonia/pilot/meetups/622305/1/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "This led to other films, including 1918's The Isle of Love with Rudolph Valentino and Virginia Rappe." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/622305/1> ;
        ont:person        <http://dbpedia.org/resource/Virginia_Rappe> , <http://dbpedia.org/resource/Rudolph_Valentino> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/622305/1/0> ;
        ont:subject       <http://dbpedia.org/resource/Julian_Eltinge> .

<http://w3id.org/polifonia/pilot/meetups/622305/4/6>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In some versions he was taking cakewalk lessons from a Mrs. Wyman's dance studio when he demonstrated to his teacher a remarkable ability to emulate females." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/622305/4> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/622305/4/5> ;
        ont:subject       <http://dbpedia.org/resource/Julian_Eltinge> .

<http://w3id.org/polifonia/pilot/meetups/622305/16/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Despite the graceful femininity he exhibited on stage, Eltinge used a supermasculine facade in public to combat the rumours of his homosexuality." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/622305/16> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/622305/16/0> ;
        ont:subject       <http://dbpedia.org/resource/Julian_Eltinge> .

<http://w3id.org/polifonia/pilot/meetups/622305/11/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Hollywood and film career ==" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/622305/11> ;
        ont:place         <http://dbpedia.org/resource/Hollywood> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/622305/11/0> ;
        ont:subject       <http://dbpedia.org/resource/Julian_Eltinge> .

<http://w3id.org/polifonia/pilot/meetups/622305/5/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "==" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/622305/5> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/622305/5/0> ;
        ont:subject       <http://dbpedia.org/resource/Julian_Eltinge> .

<http://w3id.org/polifonia/pilot/meetups/622305/0/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "After appearing in the Boston Cadets Revue at the age of ten in feminine garb, Eltinge garnered notice from other producers and made his first appearance on Broadway in 1904." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/622305/0> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/622305/0/0> ;
        ont:subject       <http://dbpedia.org/resource/Julian_Eltinge> .

<http://w3id.org/polifonia/pilot/meetups/622305/17>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/622305/17/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/622305/17/1> .

<http://w3id.org/polifonia/pilot/meetups/622305/15/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "==" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/622305/15> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/622305/15/0> ;
        ont:subject       <http://dbpedia.org/resource/Julian_Eltinge> .

<http://w3id.org/polifonia/pilot/meetups/622305/4/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "The early film star Pauline Frederick and Eltinge were childhood friends." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/622305/4> ;
        ont:person        <http://dbpedia.org/resource/Pauline_Frederick> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/622305/4/0> ;
        ont:subject       <http://dbpedia.org/resource/Julian_Eltinge> .

<http://w3id.org/polifonia/pilot/meetups/622305/14/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "By 1920, Eltinge was very wealthy and was living in one of the most lavish mansions in Southern California, Villa Capistrano." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/622305/14> ;
        ont:place           <http://dbpedia.org/resource/Southern_California> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/622305/14/0> ;
        ont:subject         <http://dbpedia.org/resource/Julian_Eltinge> ;
        ont:timeExpression  "one of" .

<http://w3id.org/polifonia/pilot/meetups/622305/3/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Eltinge was born in Newtonville, Massachusetts to Julia Edna Baker and Michael Joseph Dalton." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/622305/3> ;
        ont:person        <http://dbpedia.org/resource/John_Gilbert_Baker> , <http://dbpedia.org/resource/John_Dalton> , <http://dbpedia.org/resource/Julia_the_Elder> ;
        ont:place         <http://dbpedia.org/resource/Massachusetts> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/622305/3/0> ;
        ont:subject       <http://dbpedia.org/resource/Julian_Eltinge> .

<http://w3id.org/polifonia/pilot/meetups/622305/11>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/622305/11/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/622305/11/1> .

<http://w3id.org/polifonia/pilot/meetups/622305/6/5>
        rdf:type          ont:Sentence ;
        ont:excerpt       "At the conclusion of his performances, he would remove his wig, revealing his true nature to the surprise of the often unknowing audience." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/622305/6> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/622305/6/4> ;
        ont:subject       <http://dbpedia.org/resource/Julian_Eltinge> .

<http://w3id.org/polifonia/pilot/meetups/622305/13/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Settling in Hollywood, Eltinge made three films in 1917 and also in 1918." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/622305/13> ;
        ont:place         <http://dbpedia.org/resource/Hollywood> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/622305/13/0> ;
        ont:subject       <http://dbpedia.org/resource/Julian_Eltinge> .

