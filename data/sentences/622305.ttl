<http://w3id.org/polifonia/pilot/meetups/622305/12/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "In 1914 Eltinge starred in silent picture versions of The Crinoline Girl followed by Cousin Lucy the next year." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/622305/12> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/622305/12/0> ;
        ont:subject         <http://dbpedia.org/resource/Julian_Eltinge> ;
        ont:timeExpression  "1914 Eltinge" .

<http://w3id.org/polifonia/pilot/meetups/622305/1/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Hollywood beckoned Eltinge and in 1917 he appeared in his first feature film, The Countess Charming." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/622305/1> ;
        ont:place         <http://dbpedia.org/resource/Hollywood> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/622305/1/0> ;
        ont:subject       <http://dbpedia.org/resource/Julian_Eltinge> .

<http://w3id.org/polifonia/pilot/meetups/622305/9/4>
        rdf:type            ont:Sentence ;
        ont:excerpt         "The theater is now part of the AMC Empire 25 cineplex having been lifted and moved in its entirety down the block from its original location." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/622305/9> ;
        ont:place           <http://dbpedia.org/resource/Roman_Empire> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/622305/9/3> ;
        ont:subject         <http://dbpedia.org/resource/Julian_Eltinge> ;
        ont:timeExpression  "25 cineplex" .

<http://w3id.org/polifonia/pilot/meetups/622305/4/5>
        rdf:type          ont:Sentence ;
        ont:excerpt       "But as to how he came to perform as a female a decade later with the Boston Cadets, sources differ." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/622305/4> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/622305/4/4> ;
