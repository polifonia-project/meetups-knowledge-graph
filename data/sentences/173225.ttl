        ont:person          <http://dbpedia.org/resource/Alan_Lomax> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/173225/43/0> ;
        ont:subject         <http://dbpedia.org/resource/Alan_Lomax> ;
        ont:timeExpression  "November 1979" .

