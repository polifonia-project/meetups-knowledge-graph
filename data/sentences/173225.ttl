        ont:timeExpression  "July 23 , 1943" .

<http://w3id.org/polifonia/pilot/meetups/173225/46/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Lomax also received a posthumous Grammy Trustees Award for his lifetime achievements in 2003." ;
