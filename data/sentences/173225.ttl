        rdf:type            ont:Sentence ;
        ont:excerpt         "Lomax produced recordings, concerts, and radio shows in the US and in England, which played an important role in preserving folk music traditions in both countries, and helped start both the American and British folk revivals of the 1940s, 1950s, and early 1960s." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/173225/0> ;
        ont:person          <http://dbpedia.org/resource/Alan_Lomax> ;
        ont:place           <http://dbpedia.org/resource/United_Kingdom> , <http://dbpedia.org/resource/United_States> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/173225/0/1> ;
        ont:subject         <http://dbpedia.org/resource/Alan_Lomax> ;
        ont:timeExpression  "1950s" , "of the 1940s" .

