<http://w3id.org/polifonia/pilot/meetups/173225/43/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Lomax was extremely nervous throughout the interview." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/173225/43> ;
