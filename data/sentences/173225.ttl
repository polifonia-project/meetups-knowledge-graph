        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/173225/34/0> ;
        ont:subject       <http://dbpedia.org/resource/Alan_Lomax> .

<http://w3id.org/polifonia/pilot/meetups/173225/17/6>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Lomax had met 20-year-old English folk singer Shirley Collins while living in London." ;
