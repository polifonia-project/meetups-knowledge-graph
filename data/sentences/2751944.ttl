<http://w3id.org/polifonia/pilot/meetups/2751944/0/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Narciso Figueroa (October 31, 1906 – September 4, 2004) is considered to be the \"Father of the Modern Danzas\"." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/2751944/0> ;
        ont:person          <http://dbpedia.org/resource/Narciso_Figueroa> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/2751944/0/0> ;
        ont:subject         <http://dbpedia.org/resource/Narciso_Figueroa> ;
        ont:timeExpression  "October 31 , 1906" , "September 4 , 2004" .

<http://w3id.org/polifonia/pilot/meetups/2751944/19/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "On April 27, 1997, Figueroa was inducted into the Puerto Rican Danza Composers Hall of Fame, located in the town of San German, Puerto Rico." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/2751944/19> ;
        ont:place           <http://dbpedia.org/resource/San_Germán,_Puerto_Rico> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/2751944/19/0> ;
        ont:subject         <http://dbpedia.org/resource/Narciso_Figueroa> ;
        ont:timeExpression  "April 27 , 1997" .

<http://w3id.org/polifonia/pilot/meetups/2751944/21/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Guillermo Figueroa Jr., Narciso Figueroa Jr. and Rafael Figueroa Jr. belong to the Metropolitan Opera Orchestra and they are the first chairs of the Metropolitan Opera House." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2751944/21> ;
        ont:person        <http://dbpedia.org/resource/Narciso_Figueroa> , <http://dbpedia.org/resource/Rafael_Figueroa> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2751944/21/0> ;
        ont:subject       <http://dbpedia.org/resource/Narciso_Figueroa> .

