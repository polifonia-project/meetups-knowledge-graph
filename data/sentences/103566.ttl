<http://w3id.org/polifonia/pilot/meetups/103566/0/2>
        rdf:type            ont:Sentence ;
        ont:excerpt         "As Senate Minority Leader from 1959 to 1969, he played a highly visible and key role in the politics of the 1960s." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/103566/0> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/103566/0/1> ;
        ont:subject         <http://dbpedia.org/resource/Everett_Dirksen> ;
        ont:timeExpression  "from 1959 to 1969" .

