        rdf:type            ont:Sentence ;
        ont:excerpt         "Dirksen was a Freemason and was a member of Pekin Lodge No. 29 in Pekin." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/103566/40> ;
        ont:place           <http://dbpedia.org/resource/Beijing> ;
