        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/103566/43/0> .

<http://w3id.org/polifonia/pilot/meetups/103566/4/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "His father Johann Friedrich Dirksen was born in Jennelt and his mother Antje (née Conrady) was born in Loquard." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/103566/4> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/103566/4/1> ;
        ont:subject       <http://dbpedia.org/resource/Everett_Dirksen> .

<http://w3id.org/polifonia/pilot/meetups/103566/22>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/103566/22/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/103566/22/1> ;
        rdf:_3    <http://w3id.org/polifonia/pilot/meetups/103566/22/2> .

<http://w3id.org/polifonia/pilot/meetups/103566/32/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "The saying, \"A billion here, a billion there, pretty soon, you're talking real money\" has been attributed to Dirksen, but there is no direct record of Dirksen saying the remark." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/103566/32> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/103566/32/0> ;
        ont:subject         <http://dbpedia.org/resource/Everett_Dirksen> ;
        ont:timeExpression  "billion" .

<http://w3id.org/polifonia/pilot/meetups/103566/41/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Death ==" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/103566/41> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/103566/41/0> ;
        ont:subject       <http://dbpedia.org/resource/Everett_Dirksen> .

<http://w3id.org/polifonia/pilot/meetups/103566/29/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "However, Senator Richard Russell, Jr. of Georgia refused to allow a vote on the amendment." ;
