        rdf:type          ont:Sentence ;
        ont:excerpt       "After the war, Dirksen invested money in an electric washing machine business, but it failed, after which he joined his brothers in running the Dirksen Brothers Bakery." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/103566/8> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/103566/8/0> ;
