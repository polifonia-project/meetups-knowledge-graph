<http://w3id.org/polifonia/pilot/meetups/103566/26/7>
        rdf:type          ont:Sentence ;
        ont:excerpt       "But we are going to have to muddle through for a while and see what we do." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/103566/26> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/103566/26/6> ;
