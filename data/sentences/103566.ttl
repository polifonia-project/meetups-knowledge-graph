        rdf:type          ont:Sentence ;
        ont:excerpt       "Civil rights legislation ===" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/103566/28> ;
        ont:person        <http://dbpedia.org/resource/Civil_and_political_rights> ;
