<http://w3id.org/polifonia/pilot/meetups/103566/2/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He helped break the Southern filibuster of the Civil Rights Act of 1964." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/103566/2> ;
        ont:place         <http://dbpedia.org/resource/Southern_United_States> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/103566/2/1> ;
