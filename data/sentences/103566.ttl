        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/103566/39> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/103566/39/1> ;
        ont:subject       <http://dbpedia.org/resource/Everett_Dirksen> .

<http://w3id.org/polifonia/pilot/meetups/103566/29/9>
        rdf:type          ont:Sentence ;
        ont:excerpt       "The time has come for equality of opportunity in sharing of government, in education, and in employment." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/103566/29> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/103566/29/8> ;
        ont:subject       <http://dbpedia.org/resource/Everett_Dirksen> .

<http://w3id.org/polifonia/pilot/meetups/103566/33/5>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Dirksen was pleased with his inclusion on the parody record and bought many copies to give out as Christmas gifts." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/103566/33> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/103566/33/4> ;
        ont:subject       <http://dbpedia.org/resource/Everett_Dirksen> .

<http://w3id.org/polifonia/pilot/meetups/103566/1/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In the House, he was considered a moderate and supported much of the New Deal; he became more conservative and isolationist over time, but reversed himself to support US involvement in World War II." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/103566/1> ;
        ont:place         <http://dbpedia.org/resource/United_States> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/103566/1/1> ;
        ont:subject       <http://dbpedia.org/resource/Everett_Dirksen> .

