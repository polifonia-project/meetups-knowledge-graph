        rdf:_5    <http://w3id.org/polifonia/pilot/meetups/103566/11/4> .

<http://w3id.org/polifonia/pilot/meetups/103566/23/10>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Dirksen successfully united the various factions of the Republican Party by granting younger Republicans more representation in the Senate leadership and better committee appointments." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/103566/23> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/103566/23/9> ;
        ont:subject       <http://dbpedia.org/resource/Everett_Dirksen> .

<http://w3id.org/polifonia/pilot/meetups/103566/26/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "I have been out there three times, once as something of an emissary for then President Eisenhower." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/103566/26> ;
        ont:person        <http://dbpedia.org/resource/Dwight_D._Eisenhower> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/103566/26/0> ;
        ont:subject       <http://dbpedia.org/resource/Everett_Dirksen> .

<http://w3id.org/polifonia/pilot/meetups/103566/11/3>
        rdf:type            ont:Sentence ;
        ont:excerpt         "He was re-elected seven times from 1934 to 1946." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/103566/11> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/103566/11/2> ;
        ont:subject         <http://dbpedia.org/resource/Everett_Dirksen> ;
        ont:timeExpression  "from 1934 to 1946" .

<http://w3id.org/polifonia/pilot/meetups/103566/18/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1950, Dirksen unseated Senate Majority Leader Scott W. Lucas." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/103566/18> ;
        ont:person        <http://dbpedia.org/resource/Walter_Scott> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/103566/18/0> ;
        ont:subject       <http://dbpedia.org/resource/Everett_Dirksen> .

<http://w3id.org/polifonia/pilot/meetups/103566/35/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "===" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/103566/35> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/103566/35/1> ;
        ont:subject       <http://dbpedia.org/resource/Everett_Dirksen> .

<http://w3id.org/polifonia/pilot/meetups/103566/27/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Some Republicans advised him that it would be to the party's advantage to oppose Johnson." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/103566/27> ;
        ont:person        <http://dbpedia.org/resource/Lyndon_B._Johnson> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/103566/27/0> ;
        ont:subject       <http://dbpedia.org/resource/Everett_Dirksen> .

<http://w3id.org/polifonia/pilot/meetups/103566/8/4>
        rdf:type            ont:Sentence ;
        ont:excerpt         "He placed first in field of eight candidates vying for four seats." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/103566/8> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/103566/8/3> ;
        ont:subject         <http://dbpedia.org/resource/Everett_Dirksen> ;
        ont:timeExpression  "field of eight" .

<http://w3id.org/polifonia/pilot/meetups/103566/19/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "In 1956, Dirksen was re-elected over Democrat Richard Stengel, 54.1% to 45.7%." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/103566/19> ;
        ont:person          <http://dbpedia.org/resource/Richard_Stengel> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/103566/19/0> ;
        ont:subject         <http://dbpedia.org/resource/Everett_Dirksen> ;
        ont:timeExpression  "54.1 %" , "45.7 %" .

