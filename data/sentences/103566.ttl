        rdf:type          ont:Sentence ;
        ont:excerpt       "Despite a number of physicians recommending that the eye be removed, Dirksen chose treatment and rest; he recovered most of the sight in the afflicted eye." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/103566/15> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/103566/15/0> ;
