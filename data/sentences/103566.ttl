<http://w3id.org/polifonia/pilot/meetups/103566/26/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "But we are in to the tune of some $350 million." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/103566/26> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/103566/26/3> ;
