        rdf:type          ont:Sentence ;
        ont:excerpt       "It also unveiled a commemorative plaque on the façade of the house where she was born." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1195717/27> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1195717/27/3> ;
