<http://w3id.org/polifonia/pilot/meetups/1195717/5/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He was the organist at the Saint-Aré church in Decize and gave piano and harmonium lessons." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1195717/5> ;
        ont:place         <http://dbpedia.org/resource/Decize> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1195717/5/1> ;
        ont:subject       <http://dbpedia.org/resource/Marguerite_Monnot> .

<http://w3id.org/polifonia/pilot/meetups/1195717/10/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Annette Lajon had sung the song originally, and Piaf wanted to acquire the rights to perform it." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1195717/10> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1195717/10/0> ;
        ont:subject       <http://dbpedia.org/resource/Marguerite_Monnot> .

<http://w3id.org/polifonia/pilot/meetups/1195717/1>
        rdf:type  ont:Paragraph ;
