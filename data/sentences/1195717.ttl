        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/1195717/31/0> .

<http://w3id.org/polifonia/pilot/meetups/1195717/26/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Monnot became ill with symptoms of appendicitis during her last year of life, 1961." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1195717/26> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1195717/26/0> ;
