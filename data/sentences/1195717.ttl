        rdf:type          ont:Sentence ;
        ont:excerpt       "The 'allmusic' Biography of Marguerite Monnot" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1195717/31> ;
        ont:person        <http://dbpedia.org/resource/Marguerite_Monnot> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1195717/31/0> ;
