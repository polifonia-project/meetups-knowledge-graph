<http://w3id.org/polifonia/pilot/meetups/1195717/6/8>
        rdf:type          ont:Sentence ;
        ont:excerpt       "It was there that she became keenly interested in Spanish folklore." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1195717/6> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1195717/6/7> ;
