<http://w3id.org/polifonia/pilot/meetups/1195717/13/4>
        rdf:type            ont:Sentence ;
        ont:excerpt         "During the war years, from 1939 to 1945, Monnot collaborated with Henri Contet, writing such songs as \"Y’a pas de printemps\", \"Histoire de coeur\", \"Le ciel est fermé\", and \"Le brun et le blond\"." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1195717/13> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1195717/13/3> ;
        ont:subject         <http://dbpedia.org/resource/Marguerite_Monnot> ;
        ont:timeExpression  "from 1939 to 1945" .

<http://w3id.org/polifonia/pilot/meetups/1195717/5/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Marguerite Monnot was born in Decize, Nièvre, a small city on the river Loire." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1195717/5> ;
        ont:person        <http://dbpedia.org/resource/Marguerite_Monnot> ;
        ont:place         <http://dbpedia.org/resource/Decize> , <http://dbpedia.org/resource/Loire> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1195717/5/0> ;
        ont:subject       <http://dbpedia.org/resource/Marguerite_Monnot> .

<http://w3id.org/polifonia/pilot/meetups/1195717/6/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "At the age of three, she composed her first little song, \"Bluette\"." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1195717/6> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1195717/6/0> ;
        ont:subject         <http://dbpedia.org/resource/Marguerite_Monnot> ;
        ont:timeExpression  "age of three" .

