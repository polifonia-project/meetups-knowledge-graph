<http://w3id.org/polifonia/pilot/meetups/58067/13/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1902 Handy traveled throughout Mississippi, listening to various styles of popular black music." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/58067/13> ;
        ont:person        <http://dbpedia.org/resource/W._C._Handy> ;
        ont:place         <http://dbpedia.org/resource/Mississippi> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/58067/13/0> ;
        ont:subject       <http://dbpedia.org/resource/W._C._Handy> .

<http://w3id.org/polifonia/pilot/meetups/58067/26/6>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Handy tried to interest black singers in his music but was unsuccessful; many musicians chose to play only the current hits, and did not want to take risks with new music." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/58067/26> ;
        ont:person        <http://dbpedia.org/resource/W._C._Handy> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/58067/26/5> ;
        ont:subject       <http://dbpedia.org/resource/W._C._Handy> .

<http://w3id.org/polifonia/pilot/meetups/58067/45/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "On May 17, 1969, the United States Postal Service issued a commemorative stamp in his honor." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/58067/45> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/58067/45/0> ;
        ont:subject         <http://dbpedia.org/resource/W._C._Handy> ;
        ont:timeExpression  "May 17 , 1969" .

