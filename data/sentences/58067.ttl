        rdf:type            ont:Sentence ;
        ont:excerpt         "In September 1892, Handy travelled to Birmingham, Alabama, to take a teaching exam." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/58067/6> ;
        ont:person          <http://dbpedia.org/resource/W._C._Handy> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/58067/6/0> ;
        ont:subject         <http://dbpedia.org/resource/W._C._Handy> ;
        ont:timeExpression  "September 1892" .

<http://w3id.org/polifonia/pilot/meetups/58067/18/5>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Handy sold the rights to the song for $100." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/58067/18> ;
        ont:person          <http://dbpedia.org/resource/W._C._Handy> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/58067/18/4> ;
        ont:subject         <http://dbpedia.org/resource/W._C._Handy> ;
        ont:timeExpression  "100" .

<http://w3id.org/polifonia/pilot/meetups/58067/20>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/58067/20/0> .

<http://w3id.org/polifonia/pilot/meetups/58067/22/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1912, he met Harry Pace at the Solvent Savings Bank in Memphis." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/58067/22> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/58067/22/0> ;
        ont:subject       <http://dbpedia.org/resource/W._C._Handy> .

<http://w3id.org/polifonia/pilot/meetups/58067/14/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "After a dispute with AAMC President Councill, Handy resigned his teaching position to return to the Mahara Minstrels and tour the Midwest and Pacific Northwest." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/58067/14> ;
        ont:person        <http://dbpedia.org/resource/W._C._Handy> ;
        ont:place         <http://dbpedia.org/resource/Midwestern_United_States> , <http://dbpedia.org/resource/Pacific_Northwest> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/58067/14/0> ;
        ont:subject       <http://dbpedia.org/resource/W._C._Handy> .

<http://w3id.org/polifonia/pilot/meetups/58067/46/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Handy was inducted in the National Academy of Popular Music Songwriters Hall of Fame in 1970." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/58067/46> ;
        ont:person        <http://dbpedia.org/resource/W._C._Handy> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/58067/46/0> ;
        ont:subject       <http://dbpedia.org/resource/W._C._Handy> .

<http://w3id.org/polifonia/pilot/meetups/58067/0/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "but was the first to publish music in the blues form, thereby taking the blues from a regional music style (Delta blues) with a limited audience to a new level of popularity." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/58067/0> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/58067/0/2> ;
        ont:subject       <http://dbpedia.org/resource/W._C._Handy> .

<http://w3id.org/polifonia/pilot/meetups/58067/7/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In his time off from his job, he organized a small string orchestra and taught musicians how to read music." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/58067/7> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/58067/7/0> ;
        ont:subject       <http://dbpedia.org/resource/W._C._Handy> .

<http://w3id.org/polifonia/pilot/meetups/58067/19/5>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He also made sure to leave gaps in the lyrics for the singer to provide improvisational filler, which was common in folk blues." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/58067/19> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/58067/19/4> ;
        ont:subject       <http://dbpedia.org/resource/W._C._Handy> .

<http://w3id.org/polifonia/pilot/meetups/58067/23/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1918, Still joined the United States Navy to serve in World War I. After the war he went to Harlem, where he continued to work for Handy." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/58067/23> ;
        ont:person        <http://dbpedia.org/resource/W._C._Handy> ;
        ont:place         <http://dbpedia.org/resource/Harlem> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/58067/23/0> ;
        ont:subject       <http://dbpedia.org/resource/W._C._Handy> .

