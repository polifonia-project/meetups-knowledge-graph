        ont:subject         <http://dbpedia.org/resource/Ruth_Brown> ;
        ont:timeExpression  "January 12 , 1928" , "November 17 , 2006" .

<http://w3id.org/polifonia/pilot/meetups/147308/5/5>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Her repertoire was mostly popular ballads, but Ertegun convinced her to switch to rhythm and blues." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/147308/5> ;
        ont:person        <http://dbpedia.org/resource/Ahmet_Ertegun> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/147308/5/4> ;
        ont:subject       <http://dbpedia.org/resource/Ruth_Brown> .

<http://w3id.org/polifonia/pilot/meetups/147308/1/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Following a resurgence that began in the mid-1970s and peaked in the 1980s, Brown used her influence to press for musicians' rights regarding royalties and contracts; these efforts led to the founding of the Rhythm and Blues Foundation." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/147308/1> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/147308/1/0> ;
        ont:subject         <http://dbpedia.org/resource/Ruth_Brown> ;
        ont:timeExpression  "in the 1980s" .

