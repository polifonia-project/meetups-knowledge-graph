<http://w3id.org/polifonia/pilot/meetups/1593504/30/6>
        rdf:type          ont:Sentence ;
        ont:excerpt       "They were rockin' live, and Mick was fun to watch." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1593504/30> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1593504/30/5> ;
