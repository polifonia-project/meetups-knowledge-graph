<http://w3id.org/polifonia/pilot/meetups/1593504/30/13>
        rdf:type          ont:Sentence ;
        ont:excerpt       "I'd hop up on the riser with him, playing guitar and watching him play." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1593504/30> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1593504/30/12> ;
