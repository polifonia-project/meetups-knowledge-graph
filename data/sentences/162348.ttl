        ont:subject         <http://dbpedia.org/resource/David_Gates> ;
        ont:timeExpression  "three of" .

<http://w3id.org/polifonia/pilot/meetups/162348/5/3>
        rdf:type            ont:Sentence ;
        ont:excerpt         "From 1970 to 1973, Bread charted 11 singles on the Billboard Hot 100, all of which were written and sung by Gates." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/162348/5> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/162348/5/2> ;
        ont:subject         <http://dbpedia.org/resource/David_Gates> ;
        ont:timeExpression  "From 1970 to 1973" , "Hot 100" .

