<http://w3id.org/polifonia/pilot/meetups/162348/7/2>
        rdf:type            ont:Sentence ;
        ont:excerpt         "At the end of 1977, Gates released what would be his most successful single as a solo artist, \"Goodbye Girl\", from the 1977 film of the same name." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/162348/7> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/162348/7/1> ;
        ont:subject         <http://dbpedia.org/resource/David_Gates> ;
        ont:timeExpression  "end of 1977" , "1977 film" .

<http://w3id.org/polifonia/pilot/meetups/162348/2/3>
        rdf:type            ont:Sentence ;
        ont:excerpt         "During a concert in 1957, the Accents backed Chuck Berry." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/162348/2> ;
        ont:person          <http://dbpedia.org/resource/Chuck_Berry> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/162348/2/2> ;
        ont:subject         <http://dbpedia.org/resource/David_Gates> ;
        ont:timeExpression  "concert in 1957" .

<http://w3id.org/polifonia/pilot/meetups/162348/11/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "The David Gates Songbook, containing earlier hit singles and new material, was released in 2002." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/162348/11> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/162348/11/0> ;
        ont:subject       <http://dbpedia.org/resource/David_Gates> .

<http://w3id.org/polifonia/pilot/meetups/162348/6/2>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Hot 100 singles chart, and No. 3 on the Adult Contemporary chart." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/162348/6> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/162348/6/1> ;
