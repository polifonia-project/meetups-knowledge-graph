        ont:timeExpression  "Hot 100" , "3 on" , "January 1964" .

<http://w3id.org/polifonia/pilot/meetups/162348/13/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "According to a 1996 article in People, Gates has remained married to high school sweetheart Jo Rita since 1959." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/162348/13> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/162348/13/0> ;
