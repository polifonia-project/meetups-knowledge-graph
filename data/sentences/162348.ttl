        ont:timeExpression  "127 on" , "Billboard 200" .

<http://w3id.org/polifonia/pilot/meetups/162348/2/9>
        rdf:type            ont:Sentence ;
        ont:excerpt         "His composition \"Popsicles and Icicles\" hit No. 3 on the US Hot 100 for The Murmaids in January 1964." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/162348/2> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/162348/2/8> ;
