        ont:person          <http://dbpedia.org/resource/David_Gates> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/162348/8/1> ;
        ont:subject         <http://dbpedia.org/resource/David_Gates> ;
        ont:timeExpression  "end of 1978" .

