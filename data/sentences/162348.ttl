<http://w3id.org/polifonia/pilot/meetups/162348/4/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1967, Gates produced and arranged the debut album of a band called The Pleasure Fair, of which Robb Royer was a member." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/162348/4> ;
        ont:person        <http://dbpedia.org/resource/Robb_Royer> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/162348/4/0> ;
