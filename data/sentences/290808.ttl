<http://w3id.org/polifonia/pilot/meetups/290808/5/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Their group was also popular in Venezuela, Colombia, Perú, Chile and Mexico, where they eventually toured." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/290808/5> ;
        ont:place         <http://dbpedia.org/resource/Venezuela> , <http://dbpedia.org/resource/Colombia> , <http://dbpedia.org/resource/Mexico> , <http://dbpedia.org/resource/Chile> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/290808/5/1> ;
        ont:subject       <http://dbpedia.org/resource/Tony_Croatto> .

<http://w3id.org/polifonia/pilot/meetups/290808/22/0>
        rdf:type          ont:Sentence ;
