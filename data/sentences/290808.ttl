<http://w3id.org/polifonia/pilot/meetups/290808/26/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He refused medical treatment, opting for natural treatments instead." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/290808/26> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/290808/26/0> ;
