        ont:excerpt       "He eventually moved to Carolina where he spent his later years." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/290808/19> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/290808/19/1> ;
