<http://w3id.org/polifonia/pilot/meetups/290808/0/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Originally his popular songs in the early days in Puerto Rico, with his sister Nelly, had a religious theme." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/290808/0> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/290808/0/0> ;
        ont:subject       <http://dbpedia.org/resource/Tony_Croatto> .

<http://w3id.org/polifonia/pilot/meetups/290808/4/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "TNT were renowned for their vocal harmonies, their onstage chemistry and Nelly's voice and magnetic personality." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/290808/4> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/290808/4/0> ;
