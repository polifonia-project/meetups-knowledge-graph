<http://w3id.org/polifonia/pilot/meetups/290808/6/7>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Tim eventually left the group in 1966 and returned to his native Italy to become a concert promoter." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/290808/6> ;
        ont:person          <http://dbpedia.org/resource/Tour_promoter> ;
        ont:place           <http://dbpedia.org/resource/Italy> ;
