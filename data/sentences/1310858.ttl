        rdf:type          ont:Sentence ;
        ont:excerpt       "Dosan was tortured and punished many times during the years of his activism." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1310858/10> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1310858/10/3> ;
