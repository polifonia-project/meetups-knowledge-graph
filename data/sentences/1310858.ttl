        ont:timeExpression  "October 1902" .

<http://w3id.org/polifonia/pilot/meetups/1310858/3/1>
        rdf:type            ont:Sentence ;
        ont:excerpt         "His birth date is equivalent to 10 November 1878 on the Gregorian calendar but he chose to use the date 9 November 1878 on his vita." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1310858/3> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1310858/3/0> ;
        ont:subject         <http://dbpedia.org/resource/Ahn_Changho> ;
        ont:timeExpression  "9 November 1878" , "10 November 1878" .

<http://w3id.org/polifonia/pilot/meetups/1310858/22/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "The Republic of Korea Navy Dosan Ahn Changho-class submarine was named in his honor and the lead ship entered service on 13 August 2021." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1310858/22> ;
        ont:place           <http://dbpedia.org/resource/Korea_under_Japanese_rule> , <http://dbpedia.org/resource/Republic_of_Ireland> ;
