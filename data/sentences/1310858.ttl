        rdf:type            ont:Sentence ;
        ont:excerpt         "Ahn was born Ahn Chi-sam, on the 6th day of the 10th lunar month 1878 in Kangso County, Pyeongan province, in present-day South Pyongan, North Korea." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1310858/3> ;
        ont:place           <http://dbpedia.org/resource/South_Pyongan_Province> , <http://dbpedia.org/resource/Kangso-guyok> , <http://dbpedia.org/resource/Pyongan_Province> , <http://dbpedia.org/resource/North_Korea> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1310858/3/0> ;
        ont:subject         <http://dbpedia.org/resource/Ahn_Changho> ;
        ont:timeExpression  "6th day" , "1878 in" .

