<http://w3id.org/polifonia/pilot/meetups/1310858/12/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Judging that Ahn Changho's death might lead to a popular demonstration, the Japanese military limited the number of people attending the funeral, forcing only a small number of relatives to attend." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1310858/12> ;
        ont:person        <http://dbpedia.org/resource/Ahn_Changho> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1310858/12/0> ;
        ont:subject       <http://dbpedia.org/resource/Ahn_Changho> .

<http://w3id.org/polifonia/pilot/meetups/1310858/17/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "In 2011, the Ellis Island Foundation installed a plaque honoring Dosan to commemorate the 100th anniversary of his entrance to the United States through Ellis Island from London on September 3, 1911." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1310858/17> ;
        ont:place           <http://dbpedia.org/resource/London> , <http://dbpedia.org/resource/United_States> , <http://dbpedia.org/resource/Ellis_Island> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1310858/17/0> ;
        ont:subject         <http://dbpedia.org/resource/Ahn_Changho> ;
        ont:timeExpression  "September 3 , 1911" .

