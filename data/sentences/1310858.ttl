        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1310858/10/4> ;
        ont:subject         <http://dbpedia.org/resource/Ahn_Changho> ;
        ont:timeExpression  "April 29 , 1932" .

<http://w3id.org/polifonia/pilot/meetups/1310858/0/7>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Besides his work for the Independence Movement, Dosan wanted to reform the Korean people's character and the entire social system of Korea." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1310858/0> ;
        ont:place         <http://dbpedia.org/resource/Korea> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1310858/0/6> ;
        ont:subject       <http://dbpedia.org/resource/Ahn_Changho> .

