<http://w3id.org/polifonia/pilot/meetups/3411455/2/5>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1933, he left Germany and moved to Prague with his wife because of the rise of anti-Semitism in Nazi Germany." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/3411455/2> ;
        ont:place         <http://dbpedia.org/resource/Germany> , <http://dbpedia.org/resource/Nazi_Germany> , <http://dbpedia.org/resource/Prague> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/3411455/2/4> ;
        ont:subject       <http://dbpedia.org/resource/Heinrich_Jalowetz> .

<http://w3id.org/polifonia/pilot/meetups/3411455/0/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Heinrich Jalowetz (December 3, 1882 – February 2, 1946) was an Austrian musicologist and conductor, who settled in the United States." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/3411455/0> ;
        ont:person          <http://dbpedia.org/resource/Heinrich_Jalowetz> , <http://dbpedia.org/resource/Conducting> ;
        ont:place           <http://dbpedia.org/resource/United_States> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/3411455/0/0> ;
        ont:subject         <http://dbpedia.org/resource/Heinrich_Jalowetz> ;
        ont:timeExpression  "December 3 , 1882" , "February 2 , 1946" .

