<http://w3id.org/polifonia/pilot/meetups/3411455/2/1>
        rdf:type            ont:Sentence ;
        ont:excerpt         "A musicology pupil of Guido Adler, Jalowetz was among Arnold Schoenberg's first students in Vienna, 1904–1908." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/3411455/2> ;
        ont:person          <http://dbpedia.org/resource/Arnold_Schoenberg> , <http://dbpedia.org/resource/Guido_Adler> ;
        ont:place           <http://dbpedia.org/resource/Vienna> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/3411455/2/0> ;
        ont:subject         <http://dbpedia.org/resource/Heinrich_Jalowetz> ;
        ont:timeExpression  "1904–1908" .

<http://w3id.org/polifonia/pilot/meetups/3411455/2/7>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Though his name is less widely known than that of many of Schoenberg’s more famous students, Schoenberg regarded Jalowetz very highly indeed." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/3411455/2> ;
        ont:person        <http://dbpedia.org/resource/Arnold_Schoenberg> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/3411455/2/6> ;
        ont:subject       <http://dbpedia.org/resource/Heinrich_Jalowetz> .

<http://w3id.org/polifonia/pilot/meetups/3411455/3/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Jalowetz died on February 2, 1946 in Black Mountain, North Carolina, United States." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/3411455/3> ;
        ont:place           <http://dbpedia.org/resource/United_States> , <http://dbpedia.org/resource/North_Carolina> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/3411455/3/0> ;
        ont:subject         <http://dbpedia.org/resource/Heinrich_Jalowetz> ;
        ont:timeExpression  "February 2 , 1946" .

