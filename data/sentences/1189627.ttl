<http://w3id.org/polifonia/pilot/meetups/1189627/20/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "She was devastated by the loss." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1189627/20> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1189627/20/0> ;
