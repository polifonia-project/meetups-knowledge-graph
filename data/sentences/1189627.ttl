<http://w3id.org/polifonia/pilot/meetups/1189627/9/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Unfortunately, William died before she arrived in Scotland." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1189627/9> ;
