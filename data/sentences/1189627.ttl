<http://w3id.org/polifonia/pilot/meetups/1189627/30/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "English Laws for Women in the Nineteenth Century, 1854" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1189627/30> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1189627/30/0> ;
        ont:subject       <http://dbpedia.org/resource/Caroline_Norton> .

<http://w3id.org/polifonia/pilot/meetups/1189627/18/3>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Norton finally became free with George's death in 1875." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1189627/18> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1189627/18/2> ;
        ont:subject         <http://dbpedia.org/resource/Caroline_Norton> ;
        ont:timeExpression  "death in 1875" .

<http://w3id.org/polifonia/pilot/meetups/1189627/9/7>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Due to her dismal domestic situation, Norton became deeply involved in the passage of laws promoting social justice, especially those granting rights to married and divorced women." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1189627/9> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1189627/9/6> ;
        ont:subject       <http://dbpedia.org/resource/Caroline_Norton> .

<http://w3id.org/polifonia/pilot/meetups/1189627/7>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/1189627/7/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/1189627/7/1> ;
        rdf:_3    <http://w3id.org/polifonia/pilot/meetups/1189627/7/2> ;
        rdf:_4    <http://w3id.org/polifonia/pilot/meetups/1189627/7/3> .

<http://w3id.org/polifonia/pilot/meetups/1189627/5/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Paid nothing by her husband, her earnings confiscated, Norton used the law to her own advantage." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1189627/5> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1189627/5/1> ;
        ont:subject       <http://dbpedia.org/resource/Caroline_Norton> .

<http://w3id.org/polifonia/pilot/meetups/1189627/31/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "A Letter to the Queen on Lord Chancellor Cranworth's Marriage & Divorce Bill, 1855" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1189627/31> ;
        ont:person        <http://dbpedia.org/resource/Divorce> ;
        ont:place         <http://dbpedia.org/resource/Cranworth> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1189627/31/0> ;
        ont:subject       <http://dbpedia.org/resource/Caroline_Norton> .

<http://w3id.org/polifonia/pilot/meetups/1189627/21/7>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Despite her scandalous introduction to British society, Katharine went on to become a successful London hostess." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1189627/21> ;
        ont:place         <http://dbpedia.org/resource/London> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1189627/21/6> ;
        ont:subject       <http://dbpedia.org/resource/Caroline_Norton> .

