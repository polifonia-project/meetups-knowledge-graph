<http://w3id.org/polifonia/pilot/meetups/1189627/15/2>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Under the Custody of Children Act, legally separated or divorced wives, provided they were not found guilty of criminal conversation, were granted the custody of their children up to the age of seven, and periodic access thereafter." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1189627/15> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1189627/15/1> ;
        ont:subject         <http://dbpedia.org/resource/Caroline_Norton> ;
        ont:timeExpression  "age of seven" .

