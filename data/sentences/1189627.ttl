<http://w3id.org/polifonia/pilot/meetups/1189627/6/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "I thought it for the best." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1189627/6> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1189627/6/1> ;
