<http://w3id.org/polifonia/pilot/meetups/1189627/2/2>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Caroline's Scottish mother was the daughter of a landed gentleman, Col. Sir James Callander of Craigforth and Lady Elizabeth MacDonnell, sister of an Irish peer, the 1st Marquess of Antrim." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1189627/2> ;
        ont:person          <http://dbpedia.org/resource/Princess_Caroline_of_Great_Britain> , <http://dbpedia.org/resource/Elizabeth_I> , <http://dbpedia.org/resource/James_II_of_England> ;
        ont:place           <http://dbpedia.org/resource/Callendar_House> , <http://dbpedia.org/resource/County_Antrim> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1189627/2/1> ;
        ont:subject         <http://dbpedia.org/resource/Caroline_Norton> ;
        ont:timeExpression  "1st" .

<http://w3id.org/polifonia/pilot/meetups/1189627/16/10>
        rdf:type          ont:Sentence ;
        ont:excerpt       "\"While Norton fought to extend women's legal rights, she eschewed further social activism and had no interest in the 19th-century women's movement on issues such as women's suffrage." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1189627/16> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1189627/16/9> ;
        ont:subject       <http://dbpedia.org/resource/Caroline_Norton> .

<http://w3id.org/polifonia/pilot/meetups/1189627/16/3>
        rdf:type            ont:Sentence ;
        ont:excerpt         "The Married Women's Property Act 1870 allowed married women to inherit property and take court action on their own behalf." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1189627/16> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1189627/16/2> ;
        ont:subject         <http://dbpedia.org/resource/Caroline_Norton> ;
        ont:timeExpression  "Act 1870" .

<http://w3id.org/polifonia/pilot/meetups/1189627/3/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "The union quickly proved unhappy due to his mental and physical abuse." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1189627/3> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1189627/3/1> ;
        ont:subject       <http://dbpedia.org/resource/Caroline_Norton> .

<http://w3id.org/polifonia/pilot/meetups/1189627/3/11>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Her first book, The Sorrows of Rosalie (1829), was well received." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1189627/3> ;
        ont:place           <http://dbpedia.org/resource/Rosalie_Mansion> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1189627/3/10> ;
        ont:subject         <http://dbpedia.org/resource/Caroline_Norton> ;
        ont:timeExpression  "1829" .

<http://w3id.org/polifonia/pilot/meetups/1189627/49>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/1189627/49/0> .

