<http://w3id.org/polifonia/pilot/meetups/472821/0/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He was a choral conductor on television and a recordings executive." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/472821/0> ;
        ont:person        <http://dbpedia.org/resource/Conducting> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/472821/0/3> ;
        ont:subject       <http://dbpedia.org/resource/Mitch_Miller> .

<http://w3id.org/polifonia/pilot/meetups/472821/19/1>
        rdf:type            ont:Sentence ;
        ont:excerpt         "He also recorded a string of successful albums and singles, featuring a male chorale and his own arrangements, under the name \"Mitch Miller and the Gang\" beginning in 1950." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/472821/19> ;
        ont:person          <http://dbpedia.org/resource/Mitch_Miller> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/472821/19/0> ;
        ont:subject         <http://dbpedia.org/resource/Mitch_Miller> ;
        ont:timeExpression  "beginning in 1950" .

<http://w3id.org/polifonia/pilot/meetups/472821/10>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/472821/10/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/472821/10/1> ;
        rdf:_3    <http://w3id.org/polifonia/pilot/meetups/472821/10/2> .

<http://w3id.org/polifonia/pilot/meetups/472821/7/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "This was a pivotal position in a recording company, because the A&R executive decided which musicians and songs would be recorded and promoted by that particular record label." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/472821/7> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/472821/7/0> ;
        ont:subject       <http://dbpedia.org/resource/Mitch_Miller> .

<http://w3id.org/polifonia/pilot/meetups/472821/5/9>
        rdf:type            ont:Sentence ;
        ont:excerpt         "As part of the CBS Symphony, Miller participated in the musical accompaniment on the 1938 radio broadcast of Orson Welles's Mercury Theater on the Air production of The War of the Worlds." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/472821/5> ;
        ont:person          <http://dbpedia.org/resource/Orson_Welles> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/472821/5/8> ;
        ont:subject         <http://dbpedia.org/resource/Mitch_Miller> ;
        ont:timeExpression  "1938 radio broadcast" .

<http://w3id.org/polifonia/pilot/meetups/472821/28/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "The sketch spoofed the show's production values, including cameras panning among the vocalists, going out of control and knocking them over, then chasing Allen out of the studio and onto the roof." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/472821/28> ;
        ont:person        <http://dbpedia.org/resource/Steve_Allen> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/472821/28/1> ;
        ont:subject       <http://dbpedia.org/resource/Mitch_Miller> .
