        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/592843/35/0> ;
        ont:subject         <http://dbpedia.org/resource/Gertrude_Lawrence> ;
        ont:timeExpression  "1950" .

<http://w3id.org/polifonia/pilot/meetups/592843/45/1>
        rdf:type            ont:Sentence ;
        ont:excerpt         "On 26 February 1935, the Daily Mirror reported her assets were valued at £1,879 but her liabilities were nearly £35,000, with an additional £10,000 owed to the Inland Revenue on her earnings in the United States." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/592843/45> ;
        ont:place           <http://dbpedia.org/resource/United_States> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/592843/45/0> ;
        ont:subject         <http://dbpedia.org/resource/Gertrude_Lawrence> ;
        ont:timeExpression  "26 February 1935" .

<http://w3id.org/polifonia/pilot/meetups/592843/28/6>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Du Maurier was quoted as saying she called Lawrence by the nickname \"Cinders,\" short for Cinderella." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/592843/28> ;
        ont:person        <http://dbpedia.org/resource/Saint_Lawrence> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/592843/28/5> ;
        ont:subject       <http://dbpedia.org/resource/Gertrude_Lawrence> .

<http://w3id.org/polifonia/pilot/meetups/592843/22/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Shows were given in shell-torn cinemas and hastily lighted casinos." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/592843/22> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/592843/22/2> ;
        ont:subject       <http://dbpedia.org/resource/Gertrude_Lawrence> .

<http://w3id.org/polifonia/pilot/meetups/592843/12/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "When Lawrence divorced Gordon-Howley, she and Taylor became engaged and remained so for two years, with each free to enjoy a social life separate from the other." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/592843/12> ;
        ont:person        <http://dbpedia.org/resource/Saint_Lawrence> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/592843/12/1> ;
        ont:subject       <http://dbpedia.org/resource/Gertrude_Lawrence> .

