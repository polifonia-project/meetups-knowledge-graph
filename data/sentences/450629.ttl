<http://w3id.org/polifonia/pilot/meetups/450629/5/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "His own creative perseverance paid off eventually, as his \"No Matter What\" became another top-ten worldwide hit in late 1970." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/450629/5> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/450629/5/1> ;
        ont:subject       <http://dbpedia.org/resource/Pete_Ham> .

<http://w3id.org/polifonia/pilot/meetups/450629/9/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "His most widespread effect in popular music was with the ballad \"Without You\", which he wrote with Badfinger bandmate Tom Evans." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/450629/9> ;
        ont:person        <http://dbpedia.org/resource/Tom_Evans_(musician)> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/450629/9/0> ;
        ont:subject       <http://dbpedia.org/resource/Pete_Ham> .

<http://w3id.org/polifonia/pilot/meetups/450629/7/7>
        rdf:type          ont:Sentence ;
