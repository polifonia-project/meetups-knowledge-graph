        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/450629/5/0> ;
        ont:subject       <http://dbpedia.org/resource/Pete_Ham> .

<http://w3id.org/polifonia/pilot/meetups/450629/0/2>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Ham was granted two Ivor Novello Awards related to the song in 1973." ;
