<http://w3id.org/polifonia/pilot/meetups/2839253/25/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "He died on 23 July 1829, age 72, at Warsaw." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/2839253/25> ;
        ont:place           <http://dbpedia.org/resource/Warsaw> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/2839253/25/0> ;
        ont:subject         <http://dbpedia.org/resource/Wojciech_Bogusławski> ;
        ont:timeExpression  "23 July 1829" .

<http://w3id.org/polifonia/pilot/meetups/2839253/18/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "As an actor, he began by playing leading men, but his greatest acting triumphs came later, during his second term as director of the National Theatre, playing Old Dominic in \"Taczka Occiarza\" (1793 – his own adaptation of Sebastian Mercier's play \"La Brouette de Vinagrier\"), Ferdinand Kokiel in \"Henry VI on a Hunting Excursion\" and Bardos in \"Krakovians and Highlanders\"." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/2839253/18> ;
        ont:person          <http://dbpedia.org/resource/Sebastian_of_Portugal> , <http://dbpedia.org/resource/Auguste_Mercier> , <http://dbpedia.org/resource/Henry_VI_of_England> , <http://dbpedia.org/resource/Ferdinand_II_of_Aragon> ;
        ont:place           <http://dbpedia.org/resource/Scottish_Highlands> , <http://dbpedia.org/resource/Royal_National_Theatre> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/2839253/18/0> ;
        ont:subject         <http://dbpedia.org/resource/Wojciech_Bogusławski> ;
        ont:timeExpression  "1793 –" .

<http://w3id.org/polifonia/pilot/meetups/2839253/13/1>
        rdf:type            ont:Sentence ;
        ont:excerpt         "In Lviv he started another Polish theatre which operated under his guidance until 1799." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/2839253/13> ;
        ont:place           <http://dbpedia.org/resource/Lviv> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/2839253/13/0> ;
        ont:subject         <http://dbpedia.org/resource/Wojciech_Bogusławski> ;
        ont:timeExpression  "guidance until 1799" .

