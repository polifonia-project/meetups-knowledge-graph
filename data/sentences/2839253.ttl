        rdf:_3    <http://w3id.org/polifonia/pilot/meetups/2839253/2/2> ;
        rdf:_4    <http://w3id.org/polifonia/pilot/meetups/2839253/2/3> .

<http://w3id.org/polifonia/pilot/meetups/2839253/10>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/2839253/10/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/2839253/10/1> ;
        rdf:_3    <http://w3id.org/polifonia/pilot/meetups/2839253/10/2> .

<http://w3id.org/polifonia/pilot/meetups/2839253/15/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He directed the plays of Jean Racine, Molière, Voltaire, Pierre Beaumarchais, Denis Diderot, Friedrich Schiller and Gotthold Ephraim Lessing." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2839253/15> ;
        ont:person        <http://dbpedia.org/resource/Friedrich_Schiller> , <http://dbpedia.org/resource/Denis_Diderot> , <http://dbpedia.org/resource/Jean_Racine> , <http://dbpedia.org/resource/Voltaire> , <http://dbpedia.org/resource/Jean_Baptiste_Louis_Pierre> , <http://dbpedia.org/resource/Gotthold_Ephraim_Lessing> , <http://dbpedia.org/resource/Pierre_Beaumarchais> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2839253/15/0> ;
        ont:subject       <http://dbpedia.org/resource/Wojciech_Bogusławski> .

<http://w3id.org/polifonia/pilot/meetups/2839253/10/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "However, the public immediately understood the political allusions and soon people in Warsaw's streets were singing passages from “Krakovians”." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2839253/10> ;
        ont:place         <http://dbpedia.org/resource/Warsaw> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2839253/10/1> ;
        ont:subject       <http://dbpedia.org/resource/Wojciech_Bogusławski> .

<http://w3id.org/polifonia/pilot/meetups/2839253/23>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/2839253/23/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/2839253/23/1> ;
        rdf:_3    <http://w3id.org/polifonia/pilot/meetups/2839253/23/2> ;
        rdf:_4    <http://w3id.org/polifonia/pilot/meetups/2839253/23/3> .

<http://w3id.org/polifonia/pilot/meetups/2839253/9/1>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Boguslawski saw theatre primarily as a force for good, treating it as a platform for disseminating nationalist ideals: during the turbulent Great Sejm, 1788–92, state reforms were the subject of many productions at the National Theatre." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/2839253/9> ;
        ont:place           <http://dbpedia.org/resource/Royal_National_Theatre> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/2839253/9/0> ;
        ont:subject         <http://dbpedia.org/resource/Wojciech_Bogusławski> ;
        ont:timeExpression  "1788–92" .

<http://w3id.org/polifonia/pilot/meetups/2839253/26/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He was commemorated on a Polish postage stamp issued as part of a set depicting dramatists in 1978." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2839253/26> ;
        ont:place         <http://dbpedia.org/resource/Poland> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2839253/26/0> ;
        ont:subject       <http://dbpedia.org/resource/Wojciech_Bogusławski> .

<http://w3id.org/polifonia/pilot/meetups/2839253/21/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "During this period he also performed in a number of other Polish cities, including Poznań, Kalisz, Łowicz, Kraków and Gdańsk." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2839253/21> ;
        ont:place         <http://dbpedia.org/resource/Kalisz> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2839253/21/0> ;
        ont:subject       <http://dbpedia.org/resource/Wojciech_Bogusławski> .

<http://w3id.org/polifonia/pilot/meetups/2839253/19/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "As a stage director, Bogusławski was adept at working with designers and musicians." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2839253/19> ;
        ont:person        <http://dbpedia.org/resource/Theatre_director> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2839253/19/0> ;
        ont:subject       <http://dbpedia.org/resource/Wojciech_Bogusławski> .

<http://w3id.org/polifonia/pilot/meetups/2839253/14/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "The works were Polonized because it was believed that viewers would not be stunned by the strangeness and exoticism of foreign customs only if they saw themselves as if in a mirror on stage." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2839253/14> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2839253/14/0> ;
        ont:subject       <http://dbpedia.org/resource/Wojciech_Bogusławski> .

