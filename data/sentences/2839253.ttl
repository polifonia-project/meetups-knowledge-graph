        rdf:_3    <http://w3id.org/polifonia/pilot/meetups/2839253/8/2> .

<http://w3id.org/polifonia/pilot/meetups/2839253/17/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Actors who emerged from his \"school\" also founded new theatres." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2839253/17> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2839253/17/0> ;
        ont:subject       <http://dbpedia.org/resource/Wojciech_Bogusławski> .

