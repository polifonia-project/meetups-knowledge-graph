<http://w3id.org/polifonia/pilot/meetups/1239372/6/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "She was survived by her five children." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1239372/6> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1239372/6/1> ;
        ont:subject       <http://dbpedia.org/resource/Maureen_Forrester> .

<http://w3id.org/polifonia/pilot/meetups/1239372/4/8>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Forrester converted to Judaism." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1239372/4> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1239372/4/7> ;
        ont:subject       <http://dbpedia.org/resource/Maureen_Forrester> .

<http://w3id.org/polifonia/pilot/meetups/1239372/17/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 2000, Forrester received a star on Canada's Walk of Fame." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1239372/17> ;
        ont:place         <http://dbpedia.org/resource/Canada> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1239372/17/0> ;
        ont:subject       <http://dbpedia.org/resource/Maureen_Forrester> .

<http://w3id.org/polifonia/pilot/meetups/1239372/16/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1995, Forrester received a Governor General's Performing Arts Award." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1239372/16> ;
        ont:person        <http://dbpedia.org/resource/Governor_General_of_Canada> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1239372/16/0> ;
        ont:subject       <http://dbpedia.org/resource/Maureen_Forrester> .

<http://w3id.org/polifonia/pilot/meetups/1239372/4/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Bruno Walter invited her to sing for him; he was looking for the right contralto for a performance and recording of the Mahler Symphony No. 2 \"Resurrection\"." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1239372/4> ;
        ont:person        <http://dbpedia.org/resource/Gustav_Mahler> , <http://dbpedia.org/resource/Bruno_Walter> , <http://dbpedia.org/resource/Contralto> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1239372/4/1> ;
        ont:subject       <http://dbpedia.org/resource/Maureen_Forrester> .

<http://w3id.org/polifonia/pilot/meetups/1239372/18>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/1239372/18/0> .

<http://w3id.org/polifonia/pilot/meetups/1239372/15/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1990, Forrester was inducted into the Canadian Music Hall of Fame (aka Juno Hall of Fame)." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1239372/15> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1239372/15/0> ;
        ont:subject       <http://dbpedia.org/resource/Maureen_Forrester> .

<http://w3id.org/polifonia/pilot/meetups/1239372/14/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "From 1986-90, Maureen Forrester was Chancellor of Wilfrid Laurier University; the University's recital hall is named in her honour." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1239372/14> ;
        ont:person        <http://dbpedia.org/resource/Wilfrid> , <http://dbpedia.org/resource/Maureen_Forrester> , <http://dbpedia.org/resource/Chancellor_of_Germany> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1239372/14/0> ;
        ont:subject       <http://dbpedia.org/resource/Maureen_Forrester> .

<http://w3id.org/polifonia/pilot/meetups/1239372/12>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/1239372/12/0> .

