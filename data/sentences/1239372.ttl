<http://w3id.org/polifonia/pilot/meetups/1239372/0/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Maureen Kathleen Stewart Forrester,  (July 25, 1930 – June 16, 2010) was a Canadian operatic contralto." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1239372/0> ;
        ont:person          <http://dbpedia.org/resource/Maureen_Reagan> , <http://dbpedia.org/resource/Kathleen_Stewart> , <http://dbpedia.org/resource/Contralto> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1239372/0/0> ;
        ont:subject         <http://dbpedia.org/resource/Maureen_Forrester> ;
        ont:timeExpression  "June 16 , 2010" , "July 25 , 1930" .

