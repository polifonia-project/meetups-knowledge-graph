        ont:excerpt       "This was the start of a warm relationship with great rapport." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1239372/4> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1239372/4/2> ;
