<http://w3id.org/polifonia/pilot/meetups/1239372/6/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "She was predeceased by Eugene Kash, her former husband, whom she had divorced in 1974, and who died in 2004." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1239372/6> ;
        ont:person        <http://dbpedia.org/resource/MC_Kash> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1239372/6/0> ;
        ont:subject       <http://dbpedia.org/resource/Maureen_Forrester> .

<http://w3id.org/polifonia/pilot/meetups/1239372/4/7>
        rdf:type          ont:Sentence ;
        ont:excerpt       "The couple had five children, including actors Linda Kash and Daniel Kash." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1239372/4> ;
        ont:person        <http://dbpedia.org/resource/Daniel_Kash> , <http://dbpedia.org/resource/Linda_Kash> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1239372/4/6> ;
        ont:subject       <http://dbpedia.org/resource/Maureen_Forrester> .

<http://w3id.org/polifonia/pilot/meetups/1239372/12/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1980, Forrester received the diplôme d'honneur from the Canadian Conference of the Arts" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1239372/12> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1239372/12/0> ;
        ont:subject       <http://dbpedia.org/resource/Maureen_Forrester> .

<http://w3id.org/polifonia/pilot/meetups/1239372/5/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Death ==" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1239372/5> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1239372/5/0> ;
        ont:subject       <http://dbpedia.org/resource/Maureen_Forrester> .

<http://w3id.org/polifonia/pilot/meetups/1239372/11/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1979, Forrester received the Loyola Medal from Concordia University." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1239372/11> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1239372/11/0> ;
        ont:subject       <http://dbpedia.org/resource/Maureen_Forrester> .

<http://w3id.org/polifonia/pilot/meetups/1239372/9/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "In 1969, Forrester received an honorary doctorate from Sir George Williams University, one of Concordia University's founding institutions." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1239372/9> ;
        ont:person          <http://dbpedia.org/resource/George_V> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1239372/9/0> ;
        ont:subject         <http://dbpedia.org/resource/Maureen_Forrester> ;
        ont:timeExpression  "one of" .

