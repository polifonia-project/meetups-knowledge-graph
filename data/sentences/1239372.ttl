        rdf:type            ont:Sentence ;
        ont:excerpt         "On April 8, 1969, Forrester was chosen to sing the Canadian national anthem at the first Montreal Expos regular-season baseball game, at Shea Stadium in New York City." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1239372/10> ;
        ont:place           <http://dbpedia.org/resource/Shea_Stadium> , <http://dbpedia.org/resource/New_York_City> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1239372/10/0> ;
        ont:subject         <http://dbpedia.org/resource/Maureen_Forrester> ;
        ont:timeExpression  "April 8 , 1969" .

<http://w3id.org/polifonia/pilot/meetups/1239372/4/16>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1986, she co-authored her autobiography, Out of Character (ISBN 0-7710-3228-5), with journalist Marci McDonald." ;
