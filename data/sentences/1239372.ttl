        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/1239372/13/0> .

<http://w3id.org/polifonia/pilot/meetups/1239372/2/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "When her brother came home from the war he persuaded her to take singing lessons." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1239372/2> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1239372/2/2> ;
        ont:subject       <http://dbpedia.org/resource/Maureen_Forrester> .

<http://w3id.org/polifonia/pilot/meetups/1239372/4/12>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Forrester also provided the voice of the Bianca Castafiore character in the television series The Adventures of Tintin." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1239372/4> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1239372/4/11> ;
        ont:subject       <http://dbpedia.org/resource/Maureen_Forrester> .

<http://w3id.org/polifonia/pilot/meetups/1239372/18/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 2003, Forrester was made an Officer of the National Order of Quebec." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1239372/18> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1239372/18/0> ;
        ont:subject       <http://dbpedia.org/resource/Maureen_Forrester> .

<http://w3id.org/polifonia/pilot/meetups/1239372/5>
