        rdf:type          ont:Sentence ;
        ont:excerpt       "Honours ==" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1239372/7> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1239372/7/0> ;
        ont:subject       <http://dbpedia.org/resource/Maureen_Forrester> .

<http://w3id.org/polifonia/pilot/meetups/1239372/2/2>
        rdf:type            ont:Sentence ;
        ont:excerpt         "At age 13, she dropped out of school to help support the family, working as a secretary at Bell Telephone." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1239372/2> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1239372/2/1> ;
        ont:subject         <http://dbpedia.org/resource/Maureen_Forrester> ;
        ont:timeExpression  "At age 13" .

<http://w3id.org/polifonia/pilot/meetups/1239372/4/11>
        rdf:type            ont:Sentence ;
        ont:excerpt         "She sang at the Metropolitan Opera in New York in 1975 in Das Rheingold (Erda), Siegfried, and Un ballo in maschera." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1239372/4> ;
        ont:person          <http://dbpedia.org/resource/Siegfried_Wagner> ;
        ont:place           <http://dbpedia.org/resource/New_York_City> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1239372/4/10> ;
        ont:subject         <http://dbpedia.org/resource/Maureen_Forrester> ;
        ont:timeExpression  "1975 in" .

<http://w3id.org/polifonia/pilot/meetups/1239372/13/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1983, Forrester was awarded Yale University's Sanford Medal." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1239372/13> ;
        ont:person        <http://dbpedia.org/resource/Samuel_Sanford> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1239372/13/0> ;
