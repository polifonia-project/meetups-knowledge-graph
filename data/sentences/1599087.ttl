<http://w3id.org/polifonia/pilot/meetups/1599087/4/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1954, he became a pupil of the Spanish classical guitarist Andrés Segovia and spent time studying with him in Italy." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1599087/4> ;
        ont:person        <http://dbpedia.org/resource/Classical_guitar> ;
        ont:place         <http://dbpedia.org/resource/Italy> , <http://dbpedia.org/resource/Segovia> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1599087/4/2> ;
        ont:subject       <http://dbpedia.org/resource/Charlie_Byrd> .

<http://w3id.org/polifonia/pilot/meetups/1599087/10/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Byrd played for several years at a jazz club in Silver Spring, Maryland, called The Showboat II which was owned and managed by his manager, Peter Lambros." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1599087/10> ;
        ont:person        <http://dbpedia.org/resource/Talent_manager> ;
        ont:place         <http://dbpedia.org/resource/Silver_Spring,_Maryland> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1599087/10/0> ;
        ont:subject       <http://dbpedia.org/resource/Charlie_Byrd> .

<http://w3id.org/polifonia/pilot/meetups/1599087/8/12>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Jazz Samba was released in April 1962, and by September it had entered the Billboard pop album chart." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1599087/8> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1599087/8/11> ;
        ont:subject         <http://dbpedia.org/resource/Charlie_Byrd> ;
        ont:timeExpression  "April 1962" .

<http://w3id.org/polifonia/pilot/meetups/1599087/3/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1942, Byrd entered the Virginia Polytechnic Institute and played in the school orchestra." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1599087/3> ;
        ont:place         <http://dbpedia.org/resource/Virginia> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1599087/3/2> ;
        ont:subject       <http://dbpedia.org/resource/Charlie_Byrd> .

<http://w3id.org/polifonia/pilot/meetups/1599087/8/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Byrd invited Getz back to his home to listen to some bossa nova recordings by João Gilberto and Antonio Carlos Jobim which he had brought back." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1599087/8> ;
        ont:person        <http://dbpedia.org/resource/Antônio_Carlos_Jobim> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1599087/8/1> ;
        ont:subject       <http://dbpedia.org/resource/Charlie_Byrd> .

<http://w3id.org/polifonia/pilot/meetups/1599087/12>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/1599087/12/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/1599087/12/1> .

<http://w3id.org/polifonia/pilot/meetups/1599087/8/25>
        rdf:type          ont:Sentence ;
        ont:excerpt       "The jury agreed with Byrd and awarded him half the royalties from the album." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1599087/8> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1599087/8/24> ;
        ont:subject       <http://dbpedia.org/resource/Charlie_Byrd> .

<http://w3id.org/polifonia/pilot/meetups/1599087/9>
        rdf:type  ont:Paragraph ;
