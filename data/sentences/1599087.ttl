        ont:person          <http://dbpedia.org/resource/Charlie_Byrd> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1599087/10/2> ;
        ont:subject         <http://dbpedia.org/resource/Charlie_Byrd> ;
        ont:timeExpression  "1973 publication" .

<http://w3id.org/polifonia/pilot/meetups/1599087/5/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Byrd's earliest and greatest influence was the gypsy guitarist Django Reinhardt, whom he saw perform in Paris." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1599087/5> ;
        ont:person        <http://dbpedia.org/resource/Django_Reinhardt> ;
        ont:place         <http://dbpedia.org/resource/Paris> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1599087/5/0> ;
        ont:subject       <http://dbpedia.org/resource/Charlie_Byrd> .

<http://w3id.org/polifonia/pilot/meetups/1599087/8/15>
        rdf:type          ont:Sentence ;
        ont:excerpt       "The album remained on the charts for seventy weeks, and Getz soon beat John Coltrane in a DownBeat poll." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1599087/8> ;
        ont:person        <http://dbpedia.org/resource/John_Coltrane> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1599087/8/14> ;
        ont:subject       <http://dbpedia.org/resource/Charlie_Byrd> .

<http://w3id.org/polifonia/pilot/meetups/1599087/0/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Byrd was best known for his association with Brazilian music, especially bossa nova." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1599087/0> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1599087/0/0> ;
        ont:subject       <http://dbpedia.org/resource/Charlie_Byrd> .

<http://w3id.org/polifonia/pilot/meetups/1599087/8/5>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Getz convinced Creed Taylor at Verve Records to produce the album." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1599087/8> ;
        ont:person        <http://dbpedia.org/resource/Creed_Taylor> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1599087/8/4> ;
        ont:subject       <http://dbpedia.org/resource/Charlie_Byrd> .

