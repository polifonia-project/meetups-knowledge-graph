        rdf:type          ont:Sentence ;
        ont:excerpt       "Byrd collaborated with Venezuelan pianist and composer Aldemaro Romero on the album Onda Nueva/The New Wave." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1599087/8> ;
        ont:person        <http://dbpedia.org/resource/Composer> , <http://dbpedia.org/resource/Aldemaro_Romero> ;
