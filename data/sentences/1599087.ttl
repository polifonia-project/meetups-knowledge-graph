        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1599087/12/0> ;
        ont:subject       <http://dbpedia.org/resource/Charlie_Byrd> .

<http://w3id.org/polifonia/pilot/meetups/1599087/8/18>
        rdf:type            ont:Sentence ;
        ont:excerpt         "On March 13, 14, 15, 16, 1963, Byrd travelled two hours south of Washington, DC to the University of Virginia in Charlottesville to provide music for an original musical, Lament For Guitar and Two Lovers." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1599087/8> ;
        ont:person          <http://dbpedia.org/resource/Reza_Abbasi> ;
        ont:place           <http://dbpedia.org/resource/Washington,_D.C.> , <http://dbpedia.org/resource/Virginia> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1599087/8/17> ;
        ont:subject         <http://dbpedia.org/resource/Charlie_Byrd> ;
        ont:timeExpression  "March 13 , 14" , "1963" , "15" .

<http://w3id.org/polifonia/pilot/meetups/1599087/8/8>
        rdf:type          ont:Sentence ;
        ont:excerpt       "The group included his brother Gene (\"Joe\") Byrd, as well as Keter Betts, Bill Reichenbach and Buddy Deppenschmidt." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1599087/8> ;
        ont:person        <http://dbpedia.org/resource/Keter_Betts> , <http://dbpedia.org/resource/Heinrich_Gustav_Reichenbach> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1599087/8/7> ;
        ont:subject       <http://dbpedia.org/resource/Charlie_Byrd> .

