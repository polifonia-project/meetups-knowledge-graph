        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1599087/4/0> ;
        ont:subject       <http://dbpedia.org/resource/Charlie_Byrd> .

<http://w3id.org/polifonia/pilot/meetups/1599087/7/5>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Byrd was also active as a teacher in the late 1950s; he trained guitar students at his home in Washington, D.C., each being required to audition before he agreed to be their teacher." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1599087/7> ;
        ont:person        <http://dbpedia.org/resource/George_Washington> ;
        ont:place         <http://dbpedia.org/resource/Washington,_D.C.> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1599087/7/4> ;
        ont:subject       <http://dbpedia.org/resource/Charlie_Byrd> .

