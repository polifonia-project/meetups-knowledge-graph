        rdf:_3    <http://w3id.org/polifonia/pilot/meetups/1209484/0/2> .

<http://w3id.org/polifonia/pilot/meetups/1209484/8/2>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Then they made a list of about 27 songs and told Malewana that he had no right to sing them." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1209484/8> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1209484/8/1> ;
        ont:subject         <http://dbpedia.org/resource/Annesley_Malewana> ;
        ont:timeExpression  "list of about 27" .
