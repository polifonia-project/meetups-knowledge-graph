        ont:subject         <http://dbpedia.org/resource/Vito_Acconci> ;
        ont:timeExpression  "1978" , "1980" .

<http://w3id.org/polifonia/pilot/meetups/222210/6/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In Seedbed Acconci lay hidden underneath a gallery-wide ramp installed at the Sonnabend Gallery, masturbating while vocalizing into a loudspeaker his fantasies about the visitors walking above him on the ramp." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/222210/6> ;
        ont:person        <http://dbpedia.org/resource/Ileana_Sonnabend> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/222210/6/0> ;
        ont:subject       <http://dbpedia.org/resource/Vito_Acconci> .

