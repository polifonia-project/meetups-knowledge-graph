<http://w3id.org/polifonia/pilot/meetups/222210/5/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Most of his early work incorporated subversive social comment." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/222210/5> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/222210/5/2> ;
