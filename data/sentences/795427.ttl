        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/795427/11> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/795427/11/0> ;
        ont:subject       <http://dbpedia.org/resource/Massimo_d'Azeglio> .

<http://w3id.org/polifonia/pilot/meetups/795427/8/7>
        rdf:type          ont:Sentence ;
        ont:excerpt       "That same year d'Azeglio published a pamphlet in French entitled De la Politique et du droit chr√©tien au point de vue de la question italienne, with the object of inducing Napoleon III to continue his pro-Italian policy." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/795427/8> ;
        ont:person        <http://dbpedia.org/resource/Massimo_d'Azeglio> , <http://dbpedia.org/resource/Napoleon> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/795427/8/6> ;
        ont:subject       <http://dbpedia.org/resource/Massimo_d'Azeglio> .

<http://w3id.org/polifonia/pilot/meetups/795427/4/2>
        rdf:type            ont:Sentence ;
        ont:excerpt         "In 1830 he returned to Turin and, after his father's death in 1831, moved to Milan." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/795427/4> ;
        ont:place           <http://dbpedia.org/resource/Turin> , <http://dbpedia.org/resource/Milan> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/795427/4/1> ;
        ont:subject         <http://dbpedia.org/resource/Massimo_d'Azeglio> ;
        ont:timeExpression  "death in 1831" .

<http://w3id.org/polifonia/pilot/meetups/795427/9/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Role as politician ==" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/795427/9> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/795427/9/0> ;
        ont:subject       <http://dbpedia.org/resource/Massimo_d'Azeglio> .

<http://w3id.org/polifonia/pilot/meetups/795427/9>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/795427/9/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/795427/9/1> .

<http://w3id.org/polifonia/pilot/meetups/795427/10/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "He played a significant role in the rehabilitation of Piedmont after the chaos of the 1848‚Äì49 revolutions." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/795427/10> ;
        ont:place           <http://dbpedia.org/resource/Piedmont> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/795427/10/0> ;
        ont:subject         <http://dbpedia.org/resource/Massimo_d'Azeglio> ;
        ont:timeExpression  "of the 1848‚Äì49" .

<http://w3id.org/polifonia/pilot/meetups/795427/8/12>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He continued to hold that the pope should enjoy nominal sovereignty over Rome with full spiritual independence, with the capital of Italy being established elsewhere but the Romans being Italian citizens." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/795427/8> ;
        ont:place         <http://dbpedia.org/resource/Rome> , <http://dbpedia.org/resource/Italy> , <http://dbpedia.org/resource/Pope> , <http://dbpedia.org/resource/Ancient_Rome> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/795427/8/11> ;
        ont:subject       <http://dbpedia.org/resource/Massimo_d'Azeglio> .

