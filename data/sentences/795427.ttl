<http://w3id.org/polifonia/pilot/meetups/795427/6/7>
        rdf:type            ont:Sentence ;
        ont:excerpt         "He invited Count Camillo Cavour, then a rising young politician, to enter the ministry in 1850." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/795427/6> ;
        ont:person          <http://dbpedia.org/resource/Camillo_Benso,_Count_of_Cavour> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/795427/6/6> ;
        ont:subject         <http://dbpedia.org/resource/Massimo_d'Azeglio> ;
        ont:timeExpression  "ministry in 1850" .

<http://w3id.org/polifonia/pilot/meetups/795427/3>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/795427/3/0> ;
