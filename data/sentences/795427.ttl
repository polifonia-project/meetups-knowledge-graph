<http://w3id.org/polifonia/pilot/meetups/795427/2/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He was descended from an ancient and noble Piedmontese family." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/795427/2> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/795427/2/0> ;
        ont:subject       <http://dbpedia.org/resource/Massimo_d'Azeglio> .

<http://w3id.org/polifonia/pilot/meetups/795427/4/13>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He was now a famous man, and early in 1849 Charles Albert, king of Sardinia, invited him to form a cabinet." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/795427/4> ;
        ont:place         <http://dbpedia.org/resource/Sardinia> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/795427/4/12> ;
        ont:subject       <http://dbpedia.org/resource/Massimo_d'Azeglio> .

<http://w3id.org/polifonia/pilot/meetups/795427/6/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "After the defeat at the Battle of Novara (23 March 1849), Charles Albert abdicated and was succeeded by Victor Emmanuel II." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/795427/6> ;
        ont:person          <http://dbpedia.org/resource/Victor_Emmanuel_II_of_Italy> , <http://dbpedia.org/resource/Charles_IV_of_Spain> ;
        ont:place           <http://dbpedia.org/resource/Novara> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/795427/6/0> ;
        ont:subject         <http://dbpedia.org/resource/Massimo_d'Azeglio> ;
        ont:timeExpression  "23 March 1849" .

<http://w3id.org/polifonia/pilot/meetups/795427/1/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Early life ==" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/795427/1> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/795427/1/0> ;
        ont:subject       <http://dbpedia.org/resource/Massimo_d'Azeglio> .

<http://w3id.org/polifonia/pilot/meetups/795427/4/6>
        rdf:type          ont:Sentence ;
        ont:excerpt       "The novels had a strong political context, with d'Azeglio aiming to illustrate the evils of foreign domination in Italy and to reawaken national feeling." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/795427/4> ;
        ont:person        <http://dbpedia.org/resource/Massimo_d'Azeglio> ;
        ont:place         <http://dbpedia.org/resource/Italy> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/795427/4/5> ;
        ont:subject       <http://dbpedia.org/resource/Massimo_d'Azeglio> .

<http://w3id.org/polifonia/pilot/meetups/795427/10/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He convinced the king to accept constitutional government, with the proviso the King could control military and foreign policy without parliamentary interference." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/795427/10> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/795427/10/3> ;
        ont:subject       <http://dbpedia.org/resource/Massimo_d'Azeglio> .

<http://w3id.org/polifonia/pilot/meetups/795427/5/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "==" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/795427/5> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/795427/5/0> ;
        ont:subject       <http://dbpedia.org/resource/Massimo_d'Azeglio> .

<http://w3id.org/polifonia/pilot/meetups/795427/0/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He was Prime Minister of Sardinia for almost three years, until his rival Camillo Benso, Count of Cavour succeeded him." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/795427/0> ;
        ont:person        <http://dbpedia.org/resource/Camillo_Benso,_Count_of_Cavour> , <http://dbpedia.org/resource/Prime_Minister_of_Italy> ;
        ont:place         <http://dbpedia.org/resource/Kingdom_of_Sardinia> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/795427/0/0> ;
        ont:subject       <http://dbpedia.org/resource/Massimo_d'Azeglio> .
