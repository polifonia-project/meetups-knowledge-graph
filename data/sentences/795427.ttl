<http://w3id.org/polifonia/pilot/meetups/795427/8/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Cavour continued to consult him." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/795427/8> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/795427/8/0> ;
