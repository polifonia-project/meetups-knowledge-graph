<http://w3id.org/polifonia/pilot/meetups/795427/8/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "For the next four years he lived modestly at Turin, devoting himself once more to art, although he also continued to take an active interest in politics." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/795427/8> ;
        ont:place         <http://dbpedia.org/resource/Turin> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/795427/8/0> ;
        ont:subject       <http://dbpedia.org/resource/Massimo_d'Azeglio> .

<http://w3id.org/polifonia/pilot/meetups/795427/3/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Early political career" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/795427/3> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/795427/3/0> ;
        ont:subject       <http://dbpedia.org/resource/Massimo_d'Azeglio> .

<http://w3id.org/polifonia/pilot/meetups/795427/6/6>
        rdf:type          ont:Sentence ;
        ont:excerpt       "When the bishops protested they were punished or exiled, inspiring liberal anticlerical elements across Italy." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/795427/6> ;
        ont:place         <http://dbpedia.org/resource/Italy> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/795427/6/5> ;
        ont:subject       <http://dbpedia.org/resource/Massimo_d'Azeglio> .

<http://w3id.org/polifonia/pilot/meetups/795427/12/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "\"His landscape paintings influenced Salvatore Mazza and Luigi Riccardi." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/795427/12> ;
        ont:person        <http://dbpedia.org/resource/Luigi_Riccardi> , <http://dbpedia.org/resource/Salvatore_Mazza> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/795427/12/3> ;
        ont:subject       <http://dbpedia.org/resource/Massimo_d'Azeglio> .

