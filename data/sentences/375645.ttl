        rdf:type            ont:Sentence ;
        ont:excerpt         "He is regarded as one of the British Invasion's most distinctive singers with his deep, powerful blues-rock voice." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/375645/0> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/375645/0/1> ;
        ont:subject         <http://dbpedia.org/resource/Eric_Burdon> ;
        ont:timeExpression  "one of" .

<http://w3id.org/polifonia/pilot/meetups/375645/3/11>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Burdon was also a good friend of the Beatles' John Lennon and was mentioned in one of their songs, \"I Am the Walrus\" as \"the eggman.\"" ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/375645/3> ;
        ont:person          <http://dbpedia.org/resource/John_Lennon> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/375645/3/10> ;
        ont:subject         <http://dbpedia.org/resource/Eric_Burdon> ;
        ont:timeExpression  "one of" .

