        rdf:type          ont:Sentence ;
        ont:excerpt       "In mid 1980, they recorded the album The Last Drive." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/375645/18> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/375645/18/3> ;
