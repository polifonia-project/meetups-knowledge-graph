        rdf:type          ont:Sentence ;
        ont:excerpt       "Professional career" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/3337272/6> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/3337272/6/0> ;
        ont:subject       <http://dbpedia.org/resource/Dmitry_Pokrass> .

<http://w3id.org/polifonia/pilot/meetups/3337272/5/1>
        rdf:type            ont:Sentence ;
        ont:excerpt         "In honour of the taking of Rostov by the First Cavalry, he wrote the song which gained national popularity, \"Budyonny's March\" (1920, lyrics by A.d'Aktilj)." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/3337272/5> ;
        ont:person          <http://dbpedia.org/resource/Semyon_Budyonny> ;
        ont:place           <http://dbpedia.org/resource/Rostov-on-Don> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/3337272/5/0> ;
        ont:subject         <http://dbpedia.org/resource/Dmitry_Pokrass> ;
        ont:timeExpression  "1920" .

<http://w3id.org/polifonia/pilot/meetups/3337272/0/2>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Dmitry Pokrass was recognized in 1975 as a People's Artist of the USSR." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/3337272/0> ;
        ont:person          <http://dbpedia.org/resource/Dmitry_Pokrass> ;
        ont:place           <http://dbpedia.org/resource/Soviet_Union> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/3337272/0/1> ;
        ont:subject         <http://dbpedia.org/resource/Dmitry_Pokrass> ;
        ont:timeExpression  "1975 as" .

<http://w3id.org/polifonia/pilot/meetups/3337272/10/0>
        rdf:type          ont:Sentence ;
