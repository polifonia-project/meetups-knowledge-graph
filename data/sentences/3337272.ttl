<http://w3id.org/polifonia/pilot/meetups/3337272/3/8>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1917 he returned to Kiev, where he found work as an accompanist." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/3337272/3> ;
        ont:person        <http://dbpedia.org/resource/Accompaniment> ;
        ont:place         <http://dbpedia.org/resource/Kyiv> ;
