<http://w3id.org/polifonia/pilot/meetups/3337272/8/1>
        rdf:type            ont:Sentence ;
        ont:excerpt         "From 1923 to 1926, Pokrass served as the principal conductor and musical director of the Moscow theatres \"Oriental Carpets\" and \"Hermitage\"." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/3337272/8> ;
        ont:person          <http://dbpedia.org/resource/Conducting> ;
        ont:place           <http://dbpedia.org/resource/Hermitage_Museum> , <http://dbpedia.org/resource/Moscow> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/3337272/8/0> ;
        ont:subject         <http://dbpedia.org/resource/Dmitry_Pokrass> ;
        ont:timeExpression  "From 1923 to 1926" .

<http://w3id.org/polifonia/pilot/meetups/3337272/3/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Touring cities, he absorbed the music of the suburbs of old Kiev, of military bands, Jewish weddings and celebrations, the cinema, synagogue services, and cheerful Ukrainian dances." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/3337272/3> ;
        ont:place         <http://dbpedia.org/resource/Ukraine> , <http://dbpedia.org/resource/Kyiv> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/3337272/3/1> ;
        ont:subject       <http://dbpedia.org/resource/Dmitry_Pokrass> .

<http://w3id.org/polifonia/pilot/meetups/3337272/0>
        rdf:type  ont:Paragraph ;
