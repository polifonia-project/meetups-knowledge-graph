<http://w3id.org/polifonia/pilot/meetups/3337272/3/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "As a student, he composed romances and songs for actors of a variety show." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/3337272/3> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/3337272/3/3> ;
        ont:subject       <http://dbpedia.org/resource/Dmitry_Pokrass> .

