<http://w3id.org/polifonia/pilot/meetups/243651/0/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Arthur Blakey (October 11, 1919 – October 16, 1990) was an American jazz drummer and bandleader." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/243651/0> ;
        ont:person          <http://dbpedia.org/resource/Bandleader> , <http://dbpedia.org/resource/King_Arthur> ;
