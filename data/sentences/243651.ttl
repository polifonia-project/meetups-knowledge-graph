        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/243651/0/0> ;
        ont:subject         <http://dbpedia.org/resource/Art_Blakey> ;
        ont:timeExpression  "October 16 , 1990" , "October 11 , 1919" .

<http://w3id.org/polifonia/pilot/meetups/243651/6/12>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Golson, as musical director, wrote several jazz standards which began as part of the band book, such as \"I Remember Clifford\", \"Along Came Betty\", and \"Blues March\", and were frequently revived by later editions of the group." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/243651/6> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/243651/6/11> ;
        ont:subject       <http://dbpedia.org/resource/Art_Blakey> .

<http://w3id.org/polifonia/pilot/meetups/243651/2/11>
        rdf:type          ont:Sentence ;
