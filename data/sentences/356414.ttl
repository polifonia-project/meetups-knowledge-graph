        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/356414/64/0> ;
        ont:subject       <http://dbpedia.org/resource/Mohammed_Rafi> .

<http://w3id.org/polifonia/pilot/meetups/356414/57/7>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Three songs were tied for the No. 2 place: Two were sung by Rafi." ;
