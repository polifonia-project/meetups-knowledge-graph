<http://w3id.org/polifonia/pilot/meetups/356414/54/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Mohammed Rafi died at 10:25 pm on 31 July 1980, following a massive heart attack, aged 55." ;
