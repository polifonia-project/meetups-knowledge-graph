        ont:subject         <http://dbpedia.org/resource/Mohammed_Rafi> ;
        ont:timeExpression  "31 July 1980" , "10:25 pm" .

<http://w3id.org/polifonia/pilot/meetups/356414/64/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Rafi's hobbies included playing badminton, carrom and flying kites." ;
