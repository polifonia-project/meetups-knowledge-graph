        rdf:type            ont:Sentence ;
        ont:excerpt         "Foley became involved with this new aspect in the film industry and worked with sound for movies such as, The Phantom of the Opera (1925), The Jazz Singer (1927) and Tarzan and the Tiger (1929)." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/455790/3> ;
        ont:person          <http://dbpedia.org/resource/William_A._Foley> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/455790/3/21> ;
        ont:subject         <http://dbpedia.org/resource/Jack_Foley_(sound_effects_artist)> ;
        ont:timeExpression  "1925" , "1929" , "1927" .

<http://w3id.org/polifonia/pilot/meetups/455790/3/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Foley’s parents were separated as Margaret lived with her father and four sisters." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/455790/3> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/455790/3/1> ;
