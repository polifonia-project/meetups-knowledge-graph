<http://w3id.org/polifonia/pilot/meetups/455790/3/13>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He also wrote and drew comics for the local newspaper." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/455790/3> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/455790/3/12> ;
