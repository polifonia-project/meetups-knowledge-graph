        rdf:_3    <http://w3id.org/polifonia/pilot/meetups/455790/2/2> .

<http://w3id.org/polifonia/pilot/meetups/455790/3/24>
        rdf:type          ont:Sentence ;
        ont:excerpt       "After this movie, Foley solely focused on sound for his career in the film industry." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/455790/3> ;
        ont:person        <http://dbpedia.org/resource/William_A._Foley> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/455790/3/23> ;
        ont:subject       <http://dbpedia.org/resource/Jack_Foley_(sound_effects_artist)> .

<http://w3id.org/polifonia/pilot/meetups/455790/3/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Foley’s biological father worked at the docks, was a volunteer firefighter as well as a singer and songwriter for local pubs." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/455790/3> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/455790/3/3> ;
        ont:subject       <http://dbpedia.org/resource/Jack_Foley_(sound_effects_artist)> .

<http://w3id.org/polifonia/pilot/meetups/455790/3/31>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Jack Foley passed away on November 9, 1967, at the age of 76." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/455790/3> ;
        ont:person          <http://dbpedia.org/resource/William_A._Foley> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/455790/3/30> ;
        ont:subject         <http://dbpedia.org/resource/Jack_Foley_(sound_effects_artist)> ;
        ont:timeExpression  "November 9 , 1967" , "age of 76" .

<http://w3id.org/polifonia/pilot/meetups/455790/3/16>
        rdf:type          ont:Sentence ;
        ont:excerpt       "With this effecting him personally, Foley began contacting several people he knew in the film industry." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/455790/3> ;
        ont:person        <http://dbpedia.org/resource/William_A._Foley> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/455790/3/15> ;
        ont:subject       <http://dbpedia.org/resource/Jack_Foley_(sound_effects_artist)> .

<http://w3id.org/polifonia/pilot/meetups/455790/3/29>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Foley won awards for his work, including the Golden Reel Award." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/455790/3> ;
        ont:person        <http://dbpedia.org/resource/William_A._Foley> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/455790/3/28> ;
        ont:subject       <http://dbpedia.org/resource/Jack_Foley_(sound_effects_artist)> .

<http://w3id.org/polifonia/pilot/meetups/455790/3/9>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1914, Foley and his wife moved to Bishop, California as Foley disliked the New York weather." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/455790/3> ;
        ont:person        <http://dbpedia.org/resource/William_A._Foley> ;
        ont:place         <http://dbpedia.org/resource/New_York_City> , <http://dbpedia.org/resource/Bishop,_California> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/455790/3/8> ;
        ont:subject       <http://dbpedia.org/resource/Jack_Foley_(sound_effects_artist)> .

<http://w3id.org/polifonia/pilot/meetups/455790/3/10>
        rdf:type          ont:Sentence ;
        ont:excerpt       "While in Bishop, during World War I, Foley worked for the American Defense Society guarding the Los Angeles water supply to ensure the water wasn’t poisoned." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/455790/3> ;
        ont:person        <http://dbpedia.org/resource/William_A._Foley> ;
        ont:place         <http://dbpedia.org/resource/Los_Angeles> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/455790/3/9> ;
        ont:subject       <http://dbpedia.org/resource/Jack_Foley_(sound_effects_artist)> .

