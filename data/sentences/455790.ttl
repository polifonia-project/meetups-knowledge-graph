<http://w3id.org/polifonia/pilot/meetups/455790/0/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Jack Donovan Foley (April 12, 1891 – November 9, 1967) was the developer of many sound effect techniques used in filmmaking." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/455790/0> ;
        ont:person          <http://dbpedia.org/resource/Jarrett_Jack> , <http://dbpedia.org/resource/Donovan> , <http://dbpedia.org/resource/William_A._Foley> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/455790/0/0> ;
        ont:subject         <http://dbpedia.org/resource/Jack_Foley_(sound_effects_artist)> ;
        ont:timeExpression  "April 12 , 1891" , "November 9 , 1967" .

<http://w3id.org/polifonia/pilot/meetups/455790/3/5>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Foley grew up with James Cagney, Arthur Murray, and Bret Lahr as they were some of his classmates." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/455790/3> ;
        ont:person        <http://dbpedia.org/resource/King_Arthur> , <http://dbpedia.org/resource/William_A._Foley> , <http://dbpedia.org/resource/Bret_Hart> , <http://dbpedia.org/resource/James_Cagney> ;
        ont:place         <http://dbpedia.org/resource/Murray_River> , <http://dbpedia.org/resource/Lahr> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/455790/3/4> ;
        ont:subject       <http://dbpedia.org/resource/Jack_Foley_(sound_effects_artist)> .

<http://w3id.org/polifonia/pilot/meetups/455790/3/32>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Foley himself calculated that he walked over 5000 miles making sounds for films, and his work and legacy are still remembered to this day." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/455790/3> ;
        ont:person        <http://dbpedia.org/resource/William_A._Foley> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/455790/3/31> ;
        ont:subject       <http://dbpedia.org/resource/Jack_Foley_(sound_effects_artist)> .

<http://w3id.org/polifonia/pilot/meetups/455790/3/17>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Along with the town’s storekeepers, Foley led a publicity campaign to bring the film industry to Bishop to help stimulate the local economy." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/455790/3> ;
        ont:person        <http://dbpedia.org/resource/Diocesan_bishop> , <http://dbpedia.org/resource/William_A._Foley> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/455790/3/16> ;
        ont:subject       <http://dbpedia.org/resource/Jack_Foley_(sound_effects_artist)> .

<http://w3id.org/polifonia/pilot/meetups/455790/3/11>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He also worked at a Bishop hardware store." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/455790/3> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/455790/3/10> ;
        ont:subject       <http://dbpedia.org/resource/Jack_Foley_(sound_effects_artist)> .

<http://w3id.org/polifonia/pilot/meetups/455790/2>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/455790/2/0> ;
