<http://w3id.org/polifonia/pilot/meetups/455790/3/23>
        rdf:type            ont:Sentence ;
        ont:excerpt         "It wasn’t until Show Boat (1929) when Foley’s career took off as he helped create sound effects for the movie which led to the name Foley effects." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/455790/3> ;
        ont:person          <http://dbpedia.org/resource/William_A._Foley> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/455790/3/22> ;
        ont:subject         <http://dbpedia.org/resource/Jack_Foley_(sound_effects_artist)> ;
        ont:timeExpression  "1929" .

<http://w3id.org/polifonia/pilot/meetups/455790/3/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "When Foley was sixteen, Margaret married Michael Gilmartin, the son of Sligo immigrants." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/455790/3> ;
        ont:person        <http://dbpedia.org/resource/William_A._Foley> , <http://dbpedia.org/resource/Margaret_Tudor> ;
        ont:place         <http://dbpedia.org/resource/Sligo> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/455790/3/2> ;
        ont:subject       <http://dbpedia.org/resource/Jack_Foley_(sound_effects_artist)> .

<http://w3id.org/polifonia/pilot/meetups/455790/3/30>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Motion Picture Sound Editors honored Foley after his retirement in the early 1960s." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/455790/3> ;
        ont:person        <http://dbpedia.org/resource/William_A._Foley> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/455790/3/29> ;
        ont:subject       <http://dbpedia.org/resource/Jack_Foley_(sound_effects_artist)> .

<http://w3id.org/polifonia/pilot/meetups/455790/3/15>
        rdf:type          ont:Sentence ;
        ont:excerpt       "This led to a decrease in wages for many of the locals, particularly those who worked in stores." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/455790/3> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/455790/3/14> ;
        ont:subject       <http://dbpedia.org/resource/Jack_Foley_(sound_effects_artist)> .

<http://w3id.org/polifonia/pilot/meetups/455790/3/28>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Foley made sound a critical aspect of movie productions." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/455790/3> ;
        ont:person        <http://dbpedia.org/resource/William_A._Foley> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/455790/3/27> ;
        ont:subject       <http://dbpedia.org/resource/Jack_Foley_(sound_effects_artist)> .

<http://w3id.org/polifonia/pilot/meetups/455790/3/8>
        rdf:type          ont:Sentence ;
        ont:excerpt       "They married in secret as Foley was Catholic, and Rehm was Protestant." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/455790/3> ;
        ont:person        <http://dbpedia.org/resource/William_A._Foley> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/455790/3/7> ;
        ont:subject       <http://dbpedia.org/resource/Jack_Foley_(sound_effects_artist)> .

