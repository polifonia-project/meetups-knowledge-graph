<http://w3id.org/polifonia/pilot/meetups/864356/0/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Wallace Foster Bennett (November 13, 1898 – December 19, 1993) was an American businessman and politician." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/864356/0> ;
        ont:person          <http://dbpedia.org/resource/Wallace_F._Bennett> ;
        ont:place           <http://dbpedia.org/resource/United_States> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/864356/0/0> ;
        ont:subject         <http://dbpedia.org/resource/Wallace_F._Bennett> ;
        ont:timeExpression  "November 13 , 1898" , "December 19 , 1993" .

<http://w3id.org/polifonia/pilot/meetups/864356/10/8>
        rdf:type          ont:Sentence ;
        ont:excerpt       "His views were faced with much criticism." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/864356/10> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/864356/10/7> ;
        ont:subject       <http://dbpedia.org/resource/Wallace_F._Bennett> .

<http://w3id.org/polifonia/pilot/meetups/864356/12/10>
        rdf:type            ont:Sentence ;
        ont:excerpt         "\"Bennett declined to seek re-election in 1974 and resigned on December 20 of that year to let his elected successor, Jake Garn, take office early and gain seniority." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/864356/12> ;
        ont:person          <http://dbpedia.org/resource/Jake_Garn> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/864356/12/9> ;
        ont:subject         <http://dbpedia.org/resource/Wallace_F._Bennett> ;
        ont:timeExpression  "re-election in 1974" , "December 20" .

<http://w3id.org/polifonia/pilot/meetups/864356/8/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He was subsequently re-elected to three more terms." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/864356/8> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/864356/8/3> ;
        ont:subject       <http://dbpedia.org/resource/Wallace_F._Bennett> .

