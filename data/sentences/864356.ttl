<http://w3id.org/polifonia/pilot/meetups/864356/10/7>
        rdf:type          ont:Sentence ;
        ont:excerpt       "\"Bennett Spoke at a Convention of the American Mining Congress in 1963 that declared the coin and silver problem had reached a catastrophic level." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/864356/10> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/864356/10/6> ;
        ont:subject       <http://dbpedia.org/resource/Wallace_F._Bennett> .

<http://w3id.org/polifonia/pilot/meetups/864356/8/3>
        rdf:type            ont:Sentence ;
        ont:excerpt         "In November, he defeated Thomas by a margin of 54%-46%." ;
