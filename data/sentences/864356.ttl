<http://w3id.org/polifonia/pilot/meetups/864356/6/3>
        rdf:type            ont:Sentence ;
        ont:excerpt         "He served in that position until 1950, when he became chairman of the board." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/864356/6> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/864356/6/2> ;
        ont:subject         <http://dbpedia.org/resource/Wallace_F._Bennett> ;
        ont:timeExpression  "position until 1950" .

<http://w3id.org/polifonia/pilot/meetups/864356/14/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "After his departure from the Senate, Bennett returned to Salt Lake City, resumed his business pursuits, and served on a variety of boards." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/864356/14> ;
        ont:place         <http://dbpedia.org/resource/Salt_Lake_City> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/864356/14/0> ;
        ont:subject       <http://dbpedia.org/resource/Wallace_F._Bennett> .

<http://w3id.org/polifonia/pilot/meetups/864356/12/6>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He also voted against the Equal Rights Amendment." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/864356/12> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/864356/12/5> ;
