<http://w3id.org/polifonia/pilot/meetups/864356/10/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "By 1961, the Treasury Stocks had unnecessarily been depleted." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/864356/10> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/864356/10/1> ;
        ont:subject       <http://dbpedia.org/resource/Wallace_F._Bennett> .

