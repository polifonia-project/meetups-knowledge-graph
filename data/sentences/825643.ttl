        rdf:type          ont:Sentence ;
        ont:excerpt       "Boyd carried out interviews to promote the release." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/825643/20> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/825643/20/1> ;
        ont:subject       <http://dbpedia.org/resource/Pattie_Boyd> .

