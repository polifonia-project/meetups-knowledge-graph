        rdf:type            ont:Sentence ;
        ont:excerpt         "Once cured of his addiction in 1974, Clapton again pursued Boyd." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/825643/16> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/825643/16/5> ;
