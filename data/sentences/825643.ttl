<http://w3id.org/polifonia/pilot/meetups/825643/15/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Marriage to Eric Clapton =" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/825643/15> ;
