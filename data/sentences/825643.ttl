<http://w3id.org/polifonia/pilot/meetups/825643/16/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In the late 1960s, Eric Clapton and George Harrison became close friends and began writing and recording music together." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/825643/16> ;
        ont:person        <http://dbpedia.org/resource/Eric_Clapton> , <http://dbpedia.org/resource/George_Gershwin> , <http://dbpedia.org/resource/George_Harrison> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/825643/16/0> ;
        ont:subject       <http://dbpedia.org/resource/Pattie_Boyd> .

<http://w3id.org/polifonia/pilot/meetups/825643/14/8>
        rdf:type            ont:Sentence ;
        ont:excerpt         "The couple were engaged on 25 December 1965, and married on 21 January 1966 in a ceremony at Epsom register office." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/825643/14> ;
        ont:place           <http://dbpedia.org/resource/Epsom> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/825643/14/7> ;
        ont:subject         <http://dbpedia.org/resource/Pattie_Boyd> ;
        ont:timeExpression  "21 January 1966" , "25 December 1965" .

<http://w3id.org/polifonia/pilot/meetups/825643/16/15>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He later admitted to being a \"full-blown\" alcoholic who was often abusive towards his wife." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/825643/16> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/825643/16/14> ;
        ont:subject       <http://dbpedia.org/resource/Pattie_Boyd> .

<http://w3id.org/polifonia/pilot/meetups/825643/22>
        rdf:type  ont:Paragraph ;
