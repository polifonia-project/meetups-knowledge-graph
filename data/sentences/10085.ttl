        ont:subject         <http://dbpedia.org/resource/Edward_Elgar> ;
        ont:timeExpression  "1901" , "1916" .

<http://w3id.org/polifonia/pilot/meetups/10085/7/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "but none the less true and sturdy English quality\"." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/10085/7> ;
