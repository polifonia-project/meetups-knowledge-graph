        ont:timeExpression  "March 1904" .

<http://w3id.org/polifonia/pilot/meetups/10085/18/12>
        rdf:type          ont:Sentence ;
        ont:excerpt       "His new life as a celebrity was a mixed blessing to the highly strung Elgar, as it interrupted his privacy, and he often was in ill-health." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/10085/18> ;
        ont:person        <http://dbpedia.org/resource/Edward_Elgar> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/10085/18/11> ;
        ont:subject       <http://dbpedia.org/resource/Edward_Elgar> .

<http://w3id.org/polifonia/pilot/meetups/10085/19/13>
