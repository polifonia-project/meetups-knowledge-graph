<http://w3id.org/polifonia/pilot/meetups/10085/16/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Richter conducted the premiere, which was marred by a poorly prepared chorus, which sang badly." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/10085/16> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/10085/16/2> ;
