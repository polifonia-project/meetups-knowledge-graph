        rdf:_9    <http://w3id.org/polifonia/pilot/meetups/10085/27/8> .

<http://w3id.org/polifonia/pilot/meetups/10085/18/5>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He had accepted the post reluctantly, feeling that a composer should not head a school of music." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/10085/18> ;
        ont:person        <http://dbpedia.org/resource/Composer> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/10085/18/4> ;
        ont:subject       <http://dbpedia.org/resource/Edward_Elgar> .

<http://w3id.org/polifonia/pilot/meetups/10085/25/17>
        rdf:type            ont:Sentence ;
        ont:excerpt         "After Alice's death, Elgar sold the Hampstead house, and after living for a short time in a flat in St James's in the heart of London, he moved back to Worcestershire, to the village of Kempsey, where he lived from 1923 to 1927." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/10085/25> ;
        ont:person          <http://dbpedia.org/resource/James_the_Great> , <http://dbpedia.org/resource/Edward_Elgar> ;
        ont:place           <http://dbpedia.org/resource/Hampstead> , <http://dbpedia.org/resource/Worcestershire> , <http://dbpedia.org/resource/Kempsey_railway_station> , <http://dbpedia.org/resource/London> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/10085/25/16> ;
        ont:subject         <http://dbpedia.org/resource/Edward_Elgar> ;
        ont:timeExpression  "from 1923 to 1927" .

<http://w3id.org/polifonia/pilot/meetups/10085/22/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Towards the end of the war, Elgar was in poor health." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/10085/22> ;
        ont:person        <http://dbpedia.org/resource/Edward_Elgar> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/10085/22/2> ;
        ont:subject       <http://dbpedia.org/resource/Edward_Elgar> .

<http://w3id.org/polifonia/pilot/meetups/10085/32/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "The variation form was ideal for him at this stage of his career, when his comprehensive mastery of orchestration was still in contrast to his tendency to write his melodies in short, sometimes rigid, phrases." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/10085/32> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/10085/32/3> ;
        ont:subject       <http://dbpedia.org/resource/Edward_Elgar> .

<http://w3id.org/polifonia/pilot/meetups/10085/42/5>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1933 he was promoted within the Royal Victorian Order to Knight Grand Cross (GCVO)." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/10085/42> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/10085/42/4> ;
        ont:subject       <http://dbpedia.org/resource/Edward_Elgar> .

<http://w3id.org/polifonia/pilot/meetups/10085/40/15>
        rdf:type          ont:Sentence ;
        ont:excerpt       "It has never been a great popular favourite, and Kennedy and Reed identify shortcomings in it." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/10085/40> ;
        ont:person        <http://dbpedia.org/resource/John_F._Kennedy> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/10085/40/14> ;
        ont:subject       <http://dbpedia.org/resource/Edward_Elgar> .

<http://w3id.org/polifonia/pilot/meetups/10085/8/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He heard Saint-SaÃ«ns play the organ at the Madeleine and attended concerts by first-rate orchestras." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/10085/8> ;
        ont:person        <http://dbpedia.org/resource/Madeleine_of_Valois> ;
        ont:place         <http://dbpedia.org/resource/First-rate> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/10085/8/0> ;
        ont:subject       <http://dbpedia.org/resource/Edward_Elgar> .

<http://w3id.org/polifonia/pilot/meetups/10085/5>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/10085/5/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/10085/5/1> ;
        rdf:_3    <http://w3id.org/polifonia/pilot/meetups/10085/5/2> .

<http://w3id.org/polifonia/pilot/meetups/10085/19/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Elgar and Reed formed a firm friendship, which lasted for the rest of Elgar's life." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/10085/19> ;
        ont:person        <http://dbpedia.org/resource/Edward_Elgar> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/10085/19/1> ;
        ont:subject       <http://dbpedia.org/resource/Edward_Elgar> .

<http://w3id.org/polifonia/pilot/meetups/10085/39>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/10085/39/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/10085/39/1> ;
        rdf:_3    <http://w3id.org/polifonia/pilot/meetups/10085/39/2> ;
        rdf:_4    <http://w3id.org/polifonia/pilot/meetups/10085/39/3> .

<http://w3id.org/polifonia/pilot/meetups/10085/23/0>
        rdf:type            ont:Sentence ;
