<http://w3id.org/polifonia/pilot/meetups/10085/26/4>
        rdf:type            ont:Sentence ;
        ont:excerpt         "In November 1931, Elgar was filmed by PathÃ© for a newsreel depicting a recording session of Pomp and Circumstance March No. 1 at the opening of EMI's Abbey Road Studios in London." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/10085/26> ;
        ont:person          <http://dbpedia.org/resource/Edward_Elgar> ;
        ont:place           <http://dbpedia.org/resource/London> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/10085/26/3> ;
        ont:subject         <http://dbpedia.org/resource/Edward_Elgar> ;
        ont:timeExpression  "1 at" , "November 1931" .

<http://w3id.org/polifonia/pilot/meetups/10085/25/26>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Later in the series of recordings, Elgar also conducted two newly founded orchestras, Boult's BBC Symphony Orchestra and Sir Thomas Beecham's London Philharmonic Orchestra." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/10085/25> ;
        ont:person        <http://dbpedia.org/resource/Edward_Elgar> , <http://dbpedia.org/resource/Thomas_Beecham> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/10085/25/25> ;
        ont:subject       <http://dbpedia.org/resource/Edward_Elgar> .

<http://w3id.org/polifonia/pilot/meetups/10085/20/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Last major works =" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/10085/20> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/10085/20/0> ;
