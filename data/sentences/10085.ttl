        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/10085/40/13> ;
        ont:subject       <http://dbpedia.org/resource/Edward_Elgar> .

<http://w3id.org/polifonia/pilot/meetups/10085/4>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/10085/4/0> ;
