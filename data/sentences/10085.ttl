        ont:timeExpression  "2 June 1857" .

<http://w3id.org/polifonia/pilot/meetups/10085/16/10>
        rdf:type          ont:Sentence ;
        ont:excerpt       "The DÃ¼sseldorfer Volksblatt wrote, \"A memorable and epoch-making first performance!" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/10085/16> ;
