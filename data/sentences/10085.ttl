        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/10085/6/1> ;
        rdf:_3    <http://w3id.org/polifonia/pilot/meetups/10085/6/2> ;
        rdf:_4    <http://w3id.org/polifonia/pilot/meetups/10085/6/3> ;
        rdf:_5    <http://w3id.org/polifonia/pilot/meetups/10085/6/4> ;
