<http://w3id.org/polifonia/pilot/meetups/192586/5/13>
        rdf:type            ont:Sentence ;
        ont:excerpt         "He considered resigning the post in 1896, when he failed the counterpoint paper in the Cambridge examinations for the degree of doctor of music; he was successful at his second attempt, and the doctorate was conferred in March 1898." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/192586/5> ;
        ont:person          <http://dbpedia.org/resource/Counterpoint> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/192586/5/12> ;
        ont:subject         <http://dbpedia.org/resource/Walford_Davies> ;
        ont:timeExpression  "post in 1896" , "March 1898" .

<http://w3id.org/polifonia/pilot/meetups/192586/7/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "With this appointment, in the view of his biographer, Jeremy Dibble, Davies began to be seen as a prominent figure in British musical life." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/192586/7> ;
        ont:place         <http://dbpedia.org/resource/United_Kingdom> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/192586/7/0> ;
        ont:subject       <http://dbpedia.org/resource/Walford_Davies> .

