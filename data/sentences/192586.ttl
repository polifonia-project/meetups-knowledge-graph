<http://w3id.org/polifonia/pilot/meetups/192586/5/7>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Davies entered for the Cambridge bachelor of music examinations in 1889, but his exercise (a cantata, The Future, to words by Matthew Arnold) failed." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/192586/5> ;
        ont:person        <http://dbpedia.org/resource/Matthew_Arnold> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/192586/5/6> ;
        ont:subject       <http://dbpedia.org/resource/Walford_Davies> .

<http://w3id.org/polifonia/pilot/meetups/192586/6/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "National reputation ===" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/192586/6> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/192586/6/0> ;
        ont:subject       <http://dbpedia.org/resource/Walford_Davies> .

<http://w3id.org/polifonia/pilot/meetups/192586/9/6>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Davies resigned his professorship at Aberystwyth in 1926, but remained as chairman of the National Council of Music for the rest of his life." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/192586/9> ;
        ont:place         <http://dbpedia.org/resource/Aberystwyth> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/192586/9/5> ;
        ont:subject       <http://dbpedia.org/resource/Walford_Davies> .

<http://w3id.org/polifonia/pilot/meetups/192586/5/1>
        rdf:type            ont:Sentence ;
        ont:excerpt         "He was the seventh of nine children of John Whitridge Davies and Susan, née Gregory, and the youngest of four surviving sons." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/192586/5> ;
        ont:person          <http://dbpedia.org/resource/Gregory_of_Nyssa> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/192586/5/0> ;
        ont:subject         <http://dbpedia.org/resource/Walford_Davies> ;
        ont:timeExpression  "seventh of nine" .

<http://w3id.org/polifonia/pilot/meetups/192586/11/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "His widow later married Julian Harold Legge Lambart, Lower Master at Eton College." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/192586/11> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/192586/11/3> ;
        ont:subject       <http://dbpedia.org/resource/Walford_Davies> .

