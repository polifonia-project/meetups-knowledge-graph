        ont:subject       <http://dbpedia.org/resource/Arthur_Godfrey> .

<http://w3id.org/polifonia/pilot/meetups/418568/94/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "76-page booklet, Arthur Godfrey and His Gang (PDF)" ;
