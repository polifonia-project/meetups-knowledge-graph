        rdf:type          ont:Sentence ;
        ont:excerpt       "He also demanded, though there was no contractual requirement, that his cast members refrain from hiring personal managers or booking agents." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/418568/34> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/418568/34/7> ;
