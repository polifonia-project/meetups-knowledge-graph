<http://w3id.org/polifonia/pilot/meetups/418568/10/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In addition to announcing, Godfrey sang and played the ukulele." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/418568/10> ;
        ont:person        <http://dbpedia.org/resource/Godfrey_of_Bouillon> , <http://dbpedia.org/resource/Ukulele> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/418568/10/0> ;
