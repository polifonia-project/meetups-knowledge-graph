        ont:subject       <http://dbpedia.org/resource/Arthur_Godfrey> .

<http://w3id.org/polifonia/pilot/meetups/418568/65/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "He appeared in the movies 4 for Texas (1963), The Glass Bottom Boat (1966), and Where Angels Go, Trouble Follows (1968)." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/418568/65> ;
        ont:place           <http://dbpedia.org/resource/Texas> ;
