        rdf:type          ont:Sentence ;
        ont:excerpt       "\"I'm Looking Over a Four Leaf Clover\"; \"Slap 'Er Down Again, Paw\"; \"Slow Poke\"; and \"The Thing\"." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/418568/20> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/418568/20/11> ;
