        ont:timeExpression  "age of 78" .

<http://w3id.org/polifonia/pilot/meetups/418568/21/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Godfrey had been in pain since the 1931 car crash that damaged his hip." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/418568/21> ;
        ont:person          <http://dbpedia.org/resource/Godfrey_of_Bouillon> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/418568/21/0> ;
        ont:subject         <http://dbpedia.org/resource/Arthur_Godfrey> ;
        ont:timeExpression  "1931 car crash" .

<http://w3id.org/polifonia/pilot/meetups/418568/55/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Following the Julius LaRosa episode, singer-songwriter Ruth Wallis, renowned for her double-entendre songs, recorded \"Dear Mr. Godfrey\", a country tune that implored him to \"hire me and fire me and make a star of me." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/418568/55> ;
        ont:person        <http://dbpedia.org/resource/Ruth_Wallis> , <http://dbpedia.org/resource/Godfrey_of_Bouillon> , <http://dbpedia.org/resource/Double_entendre> , <http://dbpedia.org/resource/Julius_La_Rosa> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/418568/55/0> ;
        ont:subject       <http://dbpedia.org/resource/Arthur_Godfrey> .

<http://w3id.org/polifonia/pilot/meetups/418568/31/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Since Godfrey funded the majority of the airport, it is now known as Leesburg Executive Airport at Godfrey Field." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/418568/31> ;
        ont:person        <http://dbpedia.org/resource/Godfrey_of_Bouillon> ;
        ont:place         <http://dbpedia.org/resource/Leesburg_Executive_Airport> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/418568/31/0> ;
        ont:subject       <http://dbpedia.org/resource/Arthur_Godfrey> .

<http://w3id.org/polifonia/pilot/meetups/418568/65/1>
        rdf:type            ont:Sentence ;
        ont:excerpt         "He briefly co-hosted the 1960–61 debut season of Candid Camera with creator Allen Funt, but that relationship, like so many others, ended abruptly and acrimoniously; Godfrey hosted at least one broadcast without Funt." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/418568/65> ;
        ont:person          <http://dbpedia.org/resource/Allen_Funt> , <http://dbpedia.org/resource/Godfrey_of_Bouillon> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/418568/65/0> ;
        ont:subject         <http://dbpedia.org/resource/Arthur_Godfrey> ;
        ont:timeExpression  "1960–61 debut season" , "one broadcast" .

<http://w3id.org/polifonia/pilot/meetups/418568/75/2>
        rdf:type            ont:Sentence ;
        ont:excerpt         "He was next married to the former Mary Bourke from February 24, 1938, until their divorce in 1982, a year before his death." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/418568/75> ;
        ont:person          <http://dbpedia.org/resource/Mary_I_of_England> , <http://dbpedia.org/resource/Divorce> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/418568/75/1> ;
        ont:subject         <http://dbpedia.org/resource/Arthur_Godfrey> ;
        ont:timeExpression  "February 24 , 1938" , "divorce in 1982" .

