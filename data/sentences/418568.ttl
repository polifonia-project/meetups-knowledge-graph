<http://w3id.org/polifonia/pilot/meetups/418568/5/5>
        rdf:type            ont:Sentence ;
        ont:excerpt         "In 1958, at the age of 78, her sauciness made her a big hit with the audience when she appeared on Groucho Marx's quiz show You Bet" ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/418568/5> ;
        ont:person          <http://dbpedia.org/resource/Groucho_Marx> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/418568/5/4> ;
