<http://w3id.org/polifonia/pilot/meetups/143160/10/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He went on to win an Oscar for his score for The Godfather Part II." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/143160/10> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/143160/10/2> ;
