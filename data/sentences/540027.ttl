        rdf:type          ont:Sentence ;
        ont:excerpt       "As he began to sing, the doors of the club opened wide, and in came Mary, dressed in black with a black gaucho hat, on top of a donkey!" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/540027/21> ;
        ont:person        <http://dbpedia.org/resource/Mary_I_of_England> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/540027/21/4> ;
