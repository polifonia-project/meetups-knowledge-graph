<http://w3id.org/polifonia/pilot/meetups/1571869/11/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "On several occasions he has conducted these scores live in the cinema, as well as in concert halls as the film is being screened." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1571869/11> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1571869/11/3> ;
        ont:subject       <http://dbpedia.org/resource/Carl_Davis> .

<http://w3id.org/polifonia/pilot/meetups/1571869/3/6>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Other radio and TV commissions followed and Davis's UK career was launched." ;
