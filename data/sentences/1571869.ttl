<http://w3id.org/polifonia/pilot/meetups/1571869/6/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Davis also worked for television producer Jeremy Isaacs in providing the original music for the documentary history series The World at War (1973) for Thames Television and Cold War (1998)." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1571869/6> ;
        ont:person          <http://dbpedia.org/resource/Jeremy_Isaacs> , <http://dbpedia.org/resource/Television_producer> , <http://dbpedia.org/resource/Miles_Davis> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1571869/6/0> ;
        ont:subject         <http://dbpedia.org/resource/Carl_Davis> ;
        ont:timeExpression  "1973" , "1998" .

<http://w3id.org/polifonia/pilot/meetups/1571869/1/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He also collaborated with Paul McCartney in the creation of the Liverpool Oratorio." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1571869/1> ;
        ont:person        <http://dbpedia.org/resource/Paul_McCartney> ;
        ont:place         <http://dbpedia.org/resource/Liverpool> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1571869/1/0> ;
        ont:subject       <http://dbpedia.org/resource/Carl_Davis> .

<http://w3id.org/polifonia/pilot/meetups/1571869/5/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Davis achieved early prominence with the title music for the BBC's anthology play series The Wednesday Play and later for Play for Today." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1571869/5> ;
        ont:person        <http://dbpedia.org/resource/Miles_Davis> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1571869/5/0> ;
        ont:subject       <http://dbpedia.org/resource/Carl_Davis> .

