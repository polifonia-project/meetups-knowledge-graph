<http://w3id.org/polifonia/pilot/meetups/980182/1/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Driscoll was dismissed from Rainbow shortly after their debut album, entitled Ritchie Blackmore's Rainbow, was recorded." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/980182/1> ;
        ont:person        <http://dbpedia.org/resource/Ritchie_Blackmore> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/980182/1/0> ;
        ont:subject       <http://dbpedia.org/resource/Gary_Driscoll> .

<http://w3id.org/polifonia/pilot/meetups/980182/2/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Driscoll was found murdered in Ithaca, New York on June 8, 1987." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/980182/2> ;
        ont:place           <http://dbpedia.org/resource/Ithaca,_New_York> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/980182/2/0> ;
        ont:subject         <http://dbpedia.org/resource/Gary_Driscoll> ;
        ont:timeExpression  "June 8 , 1987" .

