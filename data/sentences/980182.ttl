<http://w3id.org/polifonia/pilot/meetups/980182/0/3>
        rdf:type            ont:Sentence ;
        ont:excerpt         "They were eventually discovered by Deep Purple bassist Roger Glover who went on to produce two of Elf's three studio albums." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/980182/0> ;
        ont:person          <http://dbpedia.org/resource/Roger_Glover> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/980182/0/2> ;
        ont:subject         <http://dbpedia.org/resource/Gary_Driscoll> ;
        ont:timeExpression  "two of" , "three studio" .

<http://w3id.org/polifonia/pilot/meetups/980182/1/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "It is speculated that firing Gary was simply due to his R&B style of drumming, which did not sit well with Blackmore." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/980182/1> ;
        ont:person        <http://dbpedia.org/resource/Ritchie_Blackmore> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/980182/1/1> ;
        ont:subject       <http://dbpedia.org/resource/Gary_Driscoll> .

<http://w3id.org/polifonia/pilot/meetups/980182/2/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Details of his murder remain blurred and his murder unsolved." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/980182/2> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/980182/2/0> ;
        ont:subject       <http://dbpedia.org/resource/Gary_Driscoll> .

