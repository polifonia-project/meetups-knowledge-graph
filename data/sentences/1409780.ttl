        ont:subject       <http://dbpedia.org/resource/Daniel_Santos_(singer)> .

<http://w3id.org/polifonia/pilot/meetups/1409780/14/7>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Late in 1956, Santos recorded with a Venezuelan record label named \"Discomoda\"." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1409780/14> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1409780/14/6> ;
        ont:subject       <http://dbpedia.org/resource/Daniel_Santos_(singer)> .

