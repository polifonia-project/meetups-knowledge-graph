<http://w3id.org/polifonia/pilot/meetups/1409780/12/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "By this time Santos' fame had grown and he decided to travel to Cuba, establishing a residence in Havana in 1946." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1409780/12> ;
        ont:place         <http://dbpedia.org/resource/Havana> , <http://dbpedia.org/resource/Cuba> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1409780/12/0> ;
        ont:subject       <http://dbpedia.org/resource/Daniel_Santos_(singer)> .

<http://w3id.org/polifonia/pilot/meetups/1409780/0/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Daniel Santos (June 6, 1916 – November 27, 1992) was a Puerto Rican singer and composer of boleros, and an overall performer of multiple Caribbean music genres, including guaracha, plena and rumba." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1409780/0> ;
        ont:person          <http://dbpedia.org/resource/Composer> , <http://dbpedia.org/resource/Daniel_Santos_(singer)> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1409780/0/0> ;
        ont:subject         <http://dbpedia.org/resource/Daniel_Santos_(singer)> ;
        ont:timeExpression  "June 6 , 1916" , "November 27 , 1992" .

<http://w3id.org/polifonia/pilot/meetups/1409780/11/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "===" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1409780/11> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1409780/11/0> ;
        ont:subject       <http://dbpedia.org/resource/Daniel_Santos_(singer)> .

<http://w3id.org/polifonia/pilot/meetups/1409780/8/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He was sent to Maui Island, after completing his basic military training in Kentucky." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1409780/8> ;
        ont:place         <http://dbpedia.org/resource/Kentucky> , <http://dbpedia.org/resource/Maui> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1409780/8/3> ;
        ont:subject       <http://dbpedia.org/resource/Daniel_Santos_(singer)> .

<http://w3id.org/polifonia/pilot/meetups/1409780/12/13>
        rdf:type          ont:Sentence ;
        ont:excerpt       "While in prison he composed a single named El Preso and was asked to write Amnistía as part of a campaign to promote the well-being of inmates." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1409780/12> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1409780/12/12> ;
        ont:subject       <http://dbpedia.org/resource/Daniel_Santos_(singer)> .

<http://w3id.org/polifonia/pilot/meetups/1409780/3/5>
        rdf:type          ont:Sentence ;
        ont:excerpt       "A member of the Trío Lírico was passing by and heard him sing." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1409780/3> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1409780/3/4> ;
        ont:subject       <http://dbpedia.org/resource/Daniel_Santos_(singer)> .

<http://w3id.org/polifonia/pilot/meetups/1409780/14/5>
        rdf:type          ont:Sentence ;
        ont:excerpt       "During his third presentation Santos lost his voice without completing the scheduled show, he tried to explain the situation to the public but was unable to calm them down and a riot erupted." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1409780/14> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1409780/14/4> ;
        ont:subject       <http://dbpedia.org/resource/Daniel_Santos_(singer)> .

<http://w3id.org/polifonia/pilot/meetups/1409780/19/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Despite his health he made final presentations in some of Puerto Rico's municipalities where he received recognitions in San Juan and Ponce." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1409780/19> ;
        ont:place         <http://dbpedia.org/resource/San_Juan,_Puerto_Rico> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1409780/19/3> ;
        ont:subject       <http://dbpedia.org/resource/Daniel_Santos_(singer)> .

<http://w3id.org/polifonia/pilot/meetups/1409780/15>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/1409780/15/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/1409780/15/1> ;
        rdf:_3    <http://w3id.org/polifonia/pilot/meetups/1409780/15/2> ;
        rdf:_4    <http://w3id.org/polifonia/pilot/meetups/1409780/15/3> .

<http://w3id.org/polifonia/pilot/meetups/1409780/10/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Santos became active in the Puerto Rican Independence Movement and identified himself with the Puerto Rican Nationalist Party and its president Pedro Albizu Campos after he was discharged from the military because of the prejudice which he experienced within the Army." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1409780/10> ;
        ont:person        <http://dbpedia.org/resource/Pedro_Albizu_Campos> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1409780/10/0> ;
        ont:subject       <http://dbpedia.org/resource/Daniel_Santos_(singer)> .

<http://w3id.org/polifonia/pilot/meetups/1409780/18/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He continued to sing the songs which he had written during his career until he finally retired and established a residence in Florida." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1409780/18> ;
        ont:place         <http://dbpedia.org/resource/Florida> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1409780/18/3> ;
        ont:subject       <http://dbpedia.org/resource/Daniel_Santos_(singer)> .

<http://w3id.org/polifonia/pilot/meetups/1409780/19/14>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He is buried at what is virtually Puerto Rico's national pantheon Santa María Magdalena de Pazzis Cemetery in Old San Juan, geographically quite close to where Albizu Campos and Pedro Flores were interred." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1409780/19> ;
        ont:person        <http://dbpedia.org/resource/Pedro_Albizu_Campos> , <http://dbpedia.org/resource/Pedro_Flores_Garcia> ;
        ont:place         <http://dbpedia.org/resource/Old_San_Juan> , <http://dbpedia.org/resource/Magdalena_Department> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1409780/19/13> ;
        ont:subject       <http://dbpedia.org/resource/Daniel_Santos_(singer)> .

<http://w3id.org/polifonia/pilot/meetups/1409780/6/4>
        rdf:type            ont:Sentence ;
        ont:excerpt         "On one occasion, he was singing \"Amor Perdido\" (Lost Love), without knowing that the composer of the song Pedro Flores was in the audience." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1409780/6> ;
        ont:person          <http://dbpedia.org/resource/Composer> , <http://dbpedia.org/resource/Pedro_Flores_Garcia> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1409780/6/3> ;
        ont:subject         <http://dbpedia.org/resource/Daniel_Santos_(singer)> ;
        ont:timeExpression  "one occasion" .

<http://w3id.org/polifonia/pilot/meetups/1409780/12/20>
        rdf:type          ont:Sentence ;
        ont:excerpt       "As many people during those days, he viewed Fidel Castro's fight against Batista very favorably." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1409780/12> ;
        ont:person        <http://dbpedia.org/resource/Fidel_Castro> , <http://dbpedia.org/resource/Fulgencio_Batista> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1409780/12/19> ;
        ont:subject       <http://dbpedia.org/resource/Daniel_Santos_(singer)> .

<http://w3id.org/polifonia/pilot/meetups/1409780/9>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/1409780/9/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/1409780/9/1> .

<http://w3id.org/polifonia/pilot/meetups/1409780/12/5>
        rdf:type          ont:Sentence ;
        ont:excerpt       "This exposition came after he established friendships with local public figures." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1409780/12> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1409780/12/4> ;
        ont:subject       <http://dbpedia.org/resource/Daniel_Santos_(singer)> .

<http://w3id.org/polifonia/pilot/meetups/1409780/8/9>
        rdf:type          ont:Sentence ;
        ont:excerpt       "After the war concluded Santos returned to New York, where he received a tribute upon his arrival." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1409780/8> ;
        ont:place         <http://dbpedia.org/resource/New_York_City> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1409780/8/8> ;
        ont:subject       <http://dbpedia.org/resource/Daniel_Santos_(singer)> .

<http://w3id.org/polifonia/pilot/meetups/1409780/12/18>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Santos, known for his Puerto Rican independentist preferences, was never in the good graces of the dictator." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1409780/12> ;
        ont:person        <http://dbpedia.org/resource/Dictator> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1409780/12/17> ;
        ont:subject       <http://dbpedia.org/resource/Daniel_Santos_(singer)> .

<http://w3id.org/polifonia/pilot/meetups/1409780/19/9>
        rdf:type          ont:Sentence ;
        ont:excerpt       "After this he was involved in several relationships with women of several Latin American countries, on occasions having more than one consecutively." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1409780/19> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1409780/19/8> ;
        ont:subject       <http://dbpedia.org/resource/Daniel_Santos_(singer)> .

<http://w3id.org/polifonia/pilot/meetups/1409780/3>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/1409780/3/0> ;
        rdf:_10   <http://w3id.org/polifonia/pilot/meetups/1409780/3/9> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/1409780/3/1> ;
        rdf:_3    <http://w3id.org/polifonia/pilot/meetups/1409780/3/2> ;
        rdf:_4    <http://w3id.org/polifonia/pilot/meetups/1409780/3/3> ;
        rdf:_5    <http://w3id.org/polifonia/pilot/meetups/1409780/3/4> ;
        rdf:_6    <http://w3id.org/polifonia/pilot/meetups/1409780/3/5> ;
        rdf:_7    <http://w3id.org/polifonia/pilot/meetups/1409780/3/6> ;
        rdf:_8    <http://w3id.org/polifonia/pilot/meetups/1409780/3/7> ;
        rdf:_9    <http://w3id.org/polifonia/pilot/meetups/1409780/3/8> .

<http://w3id.org/polifonia/pilot/meetups/1409780/10/5>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Later that year he visited the Dominican Republic, where he had legal problems and was jailed briefly." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1409780/10> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1409780/10/4> ;
        ont:subject       <http://dbpedia.org/resource/Daniel_Santos_(singer)> .

<http://w3id.org/polifonia/pilot/meetups/1409780/12/12>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Santos was subsequently pardoned by Prío Socarrás, but he asked to remain in jail twelve additional days to spend the Christmas celebration with some of the inmates." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1409780/12> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1409780/12/11> ;
        ont:subject       <http://dbpedia.org/resource/Daniel_Santos_(singer)> .

<http://w3id.org/polifonia/pilot/meetups/1409780/8/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "This incident produced two mannerisms that Santos eventually adopted in his singing style: chopped delivery (almost syllable by syllable, as suggested by Flores) and stretched last vowel in the last verse of each stanza, in almost every song he recorded afterwards." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1409780/8> ;
        ont:person        <http://dbpedia.org/resource/Vowel> ;
        ont:place         <http://dbpedia.org/resource/Flores_Department> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1409780/8/2> ;
        ont:subject       <http://dbpedia.org/resource/Daniel_Santos_(singer)> .

<http://w3id.org/polifonia/pilot/meetups/1409780/3/4>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Santos moved into a small apartment, where, one day, he started to sing \"Te Quiero, Dijiste\" (You said 'I Love You')." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1409780/3> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1409780/3/3> ;
        ont:subject         <http://dbpedia.org/resource/Daniel_Santos_(singer)> ;
        ont:timeExpression  "one day" .

<http://w3id.org/polifonia/pilot/meetups/1409780/19/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "During this time Santos was also suffering from mental illnesses, including memory loss due to Alzheimer's disease." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1409780/19> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1409780/19/2> ;
        ont:subject       <http://dbpedia.org/resource/Daniel_Santos_(singer)> .

<http://w3id.org/polifonia/pilot/meetups/1409780/14/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Here he was contracted to perform in a theater named \"Apolo\", where he worked with a band named the \"Costa Rica Swing Boys\"." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1409780/14> ;
        ont:place         <http://dbpedia.org/resource/Apolo_Municipality> , <http://dbpedia.org/resource/Costa_Rica> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1409780/14/3> ;
        ont:subject       <http://dbpedia.org/resource/Daniel_Santos_(singer)> .

<http://w3id.org/polifonia/pilot/meetups/1409780/14>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/1409780/14/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/1409780/14/1> ;
        rdf:_3    <http://w3id.org/polifonia/pilot/meetups/1409780/14/2> ;
        rdf:_4    <http://w3id.org/polifonia/pilot/meetups/1409780/14/3> ;
        rdf:_5    <http://w3id.org/polifonia/pilot/meetups/1409780/14/4> ;
        rdf:_6    <http://w3id.org/polifonia/pilot/meetups/1409780/14/5> ;
        rdf:_7    <http://w3id.org/polifonia/pilot/meetups/1409780/14/6> ;
        rdf:_8    <http://w3id.org/polifonia/pilot/meetups/1409780/14/7> .

<http://w3id.org/polifonia/pilot/meetups/1409780/18/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Santos continued performing during the following decades, making presentations in several Latin American countries." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1409780/18> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1409780/18/2> ;
        ont:subject       <http://dbpedia.org/resource/Daniel_Santos_(singer)> .

<http://w3id.org/polifonia/pilot/meetups/1409780/19/13>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Daniel Santos died on November 27, 1992, aged 76, at his ranch, \"Anacobero's Ranch\" in Ocala, Florida." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1409780/19> ;
        ont:person          <http://dbpedia.org/resource/Daniel_Santos_(singer)> ;
        ont:place           <http://dbpedia.org/resource/Ocala,_Florida> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1409780/19/12> ;
        ont:subject         <http://dbpedia.org/resource/Daniel_Santos_(singer)> ;
        ont:timeExpression  "November 27 , 1992" .

<http://w3id.org/polifonia/pilot/meetups/1409780/6/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "His chores included singing, waiting on tables and on occasions he was the master of ceremonies for which he was paid a salary of thirty dollars." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1409780/6> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1409780/6/2> ;
        ont:subject       <http://dbpedia.org/resource/Daniel_Santos_(singer)> .

