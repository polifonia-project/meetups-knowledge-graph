        rdf:_8    <http://w3id.org/polifonia/pilot/meetups/1409780/8/7> ;
        rdf:_9    <http://w3id.org/polifonia/pilot/meetups/1409780/8/8> .

<http://w3id.org/polifonia/pilot/meetups/1409780/12/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He participated and sang for several other stations, including RHC-Cadena Azul and CMQ, where he participated in a program named Cascabel." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1409780/12> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1409780/12/3> ;
        ont:subject       <http://dbpedia.org/resource/Daniel_Santos_(singer)> .

<http://w3id.org/polifonia/pilot/meetups/1409780/21>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/1409780/21/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/1409780/21/1> ;
        rdf:_3    <http://w3id.org/polifonia/pilot/meetups/1409780/21/2> .

<http://w3id.org/polifonia/pilot/meetups/1409780/8/8>
        rdf:type          ont:Sentence ;
        ont:excerpt       "While in the military Santos teamed up with Juanito Jiménez as a part of a duo dubbed Los Cumbancheros." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1409780/8> ;
        ont:person        <http://dbpedia.org/resource/Juanito_(footballer,_born_1954)> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1409780/8/7> ;
        ont:subject       <http://dbpedia.org/resource/Daniel_Santos_(singer)> .

<http://w3id.org/polifonia/pilot/meetups/1409780/12/17>
        rdf:type            ont:Sentence ;
        ont:excerpt         "On March 10, 1952, Fulgencio Batista organized a successful coup d'état and took control of the island's government." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1409780/12> ;
        ont:person          <http://dbpedia.org/resource/Fulgencio_Batista> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1409780/12/16> ;
        ont:subject         <http://dbpedia.org/resource/Daniel_Santos_(singer)> ;
        ont:timeExpression  "March 10 , 1952" .

<http://w3id.org/polifonia/pilot/meetups/1409780/3/9>
        rdf:type            ont:Sentence ;
        ont:excerpt         "He returned to Puerto Rico only to return once more to Manhattan after he unsuccessfully tried to acquire a job as a singer at WKAQ, which was one of the island's main radio stations." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1409780/3> ;
        ont:place           <http://dbpedia.org/resource/Manhattan> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1409780/3/8> ;
        ont:subject         <http://dbpedia.org/resource/Daniel_Santos_(singer)> ;
        ont:timeExpression  "one of" .

<http://w3id.org/polifonia/pilot/meetups/1409780/19/8>
        rdf:type            ont:Sentence ;
        ont:excerpt         "In 1947, he married Cuban socialite Eugenia Pérez Portal, who gave him his first son in 1948, Daniel Jr." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1409780/19> ;
        ont:person          <http://dbpedia.org/resource/Socialite> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1409780/19/7> ;
        ont:subject         <http://dbpedia.org/resource/Daniel_Santos_(singer)> ;
        ont:timeExpression  "son in 1948" .

<http://w3id.org/polifonia/pilot/meetups/1409780/19>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/1409780/19/0> ;
        rdf:_10   <http://w3id.org/polifonia/pilot/meetups/1409780/19/9> ;
        rdf:_11   <http://w3id.org/polifonia/pilot/meetups/1409780/19/10> ;
        rdf:_12   <http://w3id.org/polifonia/pilot/meetups/1409780/19/11> ;
        rdf:_13   <http://w3id.org/polifonia/pilot/meetups/1409780/19/12> ;
        rdf:_14   <http://w3id.org/polifonia/pilot/meetups/1409780/19/13> ;
        rdf:_15   <http://w3id.org/polifonia/pilot/meetups/1409780/19/14> ;
        rdf:_16   <http://w3id.org/polifonia/pilot/meetups/1409780/19/15> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/1409780/19/1> ;
        rdf:_3    <http://w3id.org/polifonia/pilot/meetups/1409780/19/2> ;
