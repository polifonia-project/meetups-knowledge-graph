        rdf:_9    <http://w3id.org/polifonia/pilot/meetups/1409780/19/8> .

<http://w3id.org/polifonia/pilot/meetups/1409780/2>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/1409780/2/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/1409780/2/1> .

<http://w3id.org/polifonia/pilot/meetups/1409780/15/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "A self proclaimed \"anti-communist\", he explained that he had supported the Cuban Revolution because its leadership had assured him that it was not communist." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1409780/15> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1409780/15/2> ;
        ont:subject       <http://dbpedia.org/resource/Daniel_Santos_(singer)> .

<http://w3id.org/polifonia/pilot/meetups/1409780/10/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Two months later he began singing Mexican music and boleros at Greenwich Village." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1409780/10> ;
        ont:place         <http://dbpedia.org/resource/Greenwich_Village> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1409780/10/3> ;
        ont:subject       <http://dbpedia.org/resource/Daniel_Santos_(singer)> .

<http://w3id.org/polifonia/pilot/meetups/1409780/8/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Santos recalled in an interview once that he had to hold back tears while recording the song, since his draft papers had just arrived and he would soon have to live a situation similar to what the song's lyrics described, but that a friend started mocking him at the control booth, to which he decided to curse him on the spot, trading the word mama'o (an extremely vulgar term in Puerto Rican Spanish) for mamá (mother)." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1409780/8> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1409780/8/1> ;
        ont:subject       <http://dbpedia.org/resource/Daniel_Santos_(singer)> .

<http://w3id.org/polifonia/pilot/meetups/1409780/12/11>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Later that year Santos was arrested after becoming involved in a fight where he accidentally injured a woman after trying to defend himself." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1409780/12> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1409780/12/10> ;
        ont:subject       <http://dbpedia.org/resource/Daniel_Santos_(singer)> .

<http://w3id.org/polifonia/pilot/meetups/1409780/3/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "When he was fifteen years old he began looking for work in Manhattan." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1409780/3> ;
        ont:place         <http://dbpedia.org/resource/Manhattan> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1409780/3/2> ;
        ont:subject       <http://dbpedia.org/resource/Daniel_Santos_(singer)> .

<http://w3id.org/polifonia/pilot/meetups/1409780/14/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Later that year he visited Ecuador for the first time in his artistic career." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1409780/14> ;
        ont:place         <http://dbpedia.org/resource/Ecuador> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1409780/14/2> ;
        ont:subject       <http://dbpedia.org/resource/Daniel_Santos_(singer)> .

<http://w3id.org/polifonia/pilot/meetups/1409780/19/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "When the police arrived at the scene they transported him to a local hospital, Santos was released two days after." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1409780/19> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1409780/19/1> ;
        ont:subject       <http://dbpedia.org/resource/Daniel_Santos_(singer)> .

<http://w3id.org/polifonia/pilot/meetups/1409780/13>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/1409780/13/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/1409780/13/1> ;
        rdf:_3    <http://w3id.org/polifonia/pilot/meetups/1409780/13/2> .

<http://w3id.org/polifonia/pilot/meetups/1409780/12/9>
        rdf:type          ont:Sentence ;
        ont:excerpt       "This year also marked Santos' debut with La Sonora Matancera, where he served as vocalist." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1409780/12> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1409780/12/8> ;
        ont:subject       <http://dbpedia.org/resource/Daniel_Santos_(singer)> .

<http://w3id.org/polifonia/pilot/meetups/1409780/7/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "==" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1409780/7> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1409780/7/1> ;
        ont:subject       <http://dbpedia.org/resource/Daniel_Santos_(singer)> .

<http://w3id.org/polifonia/pilot/meetups/1409780/8/10>
        rdf:type            ont:Sentence ;
        ont:excerpt         "There he recorded \"Linda\", written specially for him by Flores for one of Santos' old girlfriends." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1409780/8> ;
        ont:person          <http://dbpedia.org/resource/Linda_McCartney> ;
        ont:place           <http://dbpedia.org/resource/Flores> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1409780/8/9> ;
        ont:subject         <http://dbpedia.org/resource/Daniel_Santos_(singer)> ;
        ont:timeExpression  "one of" .

<http://w3id.org/polifonia/pilot/meetups/1409780/18/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "While he performed with the Sonora Matancera, Santos suffered a heart attack while he was sleeping in a hotel located in La Reforma after eating dinner." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1409780/18> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1409780/18/1> ;
        ont:subject       <http://dbpedia.org/resource/Daniel_Santos_(singer)> .

<http://w3id.org/polifonia/pilot/meetups/1409780/19/12>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Santos' last marriage was with Ana Rivera, who was originally of Puerto Rico." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1409780/19> ;
        ont:person        <http://dbpedia.org/resource/Diego_Rivera> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1409780/19/11> ;
        ont:subject       <http://dbpedia.org/resource/Daniel_Santos_(singer)> .

<http://w3id.org/polifonia/pilot/meetups/1409780/6/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1938, Santos was working at the Cuban Casino Cabaret in Manhattan, which was normally visited by Puerto Ricans and other Latinos." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1409780/6> ;
        ont:place         <http://dbpedia.org/resource/Manhattan> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1409780/6/1> ;
        ont:subject       <http://dbpedia.org/resource/Daniel_Santos_(singer)> .

<http://w3id.org/polifonia/pilot/meetups/1409780/7>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/1409780/7/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/1409780/7/1> ;
        rdf:_3    <http://w3id.org/polifonia/pilot/meetups/1409780/7/2> .

<http://w3id.org/polifonia/pilot/meetups/1409780/12/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "These included a special presentation titled Alegrias de Hatuey, which was broadcast by \"Radio Progreso\", a radio station." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1409780/12> ;
        ont:person        <http://dbpedia.org/resource/Hatuey> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1409780/12/2> ;
        ont:subject       <http://dbpedia.org/resource/Daniel_Santos_(singer)> .

<http://w3id.org/polifonia/pilot/meetups/1409780/20>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/1409780/20/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/1409780/20/1> .

<http://w3id.org/polifonia/pilot/meetups/1409780/12/16>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Among several other contemporary records were: El juego de la vida, El 5 y 6, El ajiaco, El niño majadero, Ramoncito campeón and El tíbiri tábara." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1409780/12> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1409780/12/15> ;
        ont:subject         <http://dbpedia.org/resource/Daniel_Santos_(singer)> ;
        ont:timeExpression  "5 y" .

<http://w3id.org/polifonia/pilot/meetups/1409780/8/7>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Nevertheless, he was sent to Okinawa towards the end of the war." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1409780/8> ;
        ont:place         <http://dbpedia.org/resource/Okinawa_Prefecture> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1409780/8/6> ;
        ont:subject       <http://dbpedia.org/resource/Daniel_Santos_(singer)> .

<http://w3id.org/polifonia/pilot/meetups/1409780/3/8>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Santos debuted with them on September 13, 1930; he sang in various social events and was paid a dollar for every song that he sang." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1409780/3> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1409780/3/7> ;
        ont:subject         <http://dbpedia.org/resource/Daniel_Santos_(singer)> ;
        ont:timeExpression  "September 13 , 1930" .

<http://w3id.org/polifonia/pilot/meetups/1409780/19/7>
        rdf:type            ont:Sentence ;
        ont:excerpt         "His first marriage took place in 1934, where he married Lucy Montilla when he was eighteen years old." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1409780/19> ;
        ont:place           <http://dbpedia.org/resource/Montilla> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1409780/19/6> ;
        ont:subject         <http://dbpedia.org/resource/Daniel_Santos_(singer)> ;
        ont:timeExpression  "place in 1934" .

<http://w3id.org/polifonia/pilot/meetups/1409780/18>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/1409780/18/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/1409780/18/1> ;
        rdf:_3    <http://w3id.org/polifonia/pilot/meetups/1409780/18/2> ;
        rdf:_4    <http://w3id.org/polifonia/pilot/meetups/1409780/18/3> ;
        rdf:_5    <http://w3id.org/polifonia/pilot/meetups/1409780/18/4> .

<http://w3id.org/polifonia/pilot/meetups/1409780/1>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/1409780/1/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/1409780/1/1> .

<http://w3id.org/polifonia/pilot/meetups/1409780/9/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "International performances ===" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1409780/9> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1409780/9/0> ;
        ont:subject       <http://dbpedia.org/resource/Daniel_Santos_(singer)> .

<http://w3id.org/polifonia/pilot/meetups/1409780/15/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He left Cuba abruptly, never to return, when Castro and Guevara failed to convince him that the Cuban Revolution was \"nationalistic\"." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1409780/15> ;
        ont:person        <http://dbpedia.org/resource/Che_Guevara> , <http://dbpedia.org/resource/Fidel_Castro> ;
        ont:place         <http://dbpedia.org/resource/Cuba> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1409780/15/1> ;
        ont:subject       <http://dbpedia.org/resource/Daniel_Santos_(singer)> .

<http://w3id.org/polifonia/pilot/meetups/1409780/10/3>
        rdf:type            ont:Sentence ;
        ont:excerpt         "On March 1946, Santos inaugurated a bar and restaurant named Borinquen, and administrated the establishment for some weeks." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1409780/10> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1409780/10/2> ;
        ont:subject         <http://dbpedia.org/resource/Daniel_Santos_(singer)> ;
        ont:timeExpression  "March 1946" .

<http://w3id.org/polifonia/pilot/meetups/1409780/19/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He was attended by some bystanders and residents of the neighborhood who called the New York City Police Department." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1409780/19> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1409780/19/0> ;
        ont:subject       <http://dbpedia.org/resource/Daniel_Santos_(singer)> .

<http://w3id.org/polifonia/pilot/meetups/1409780/8/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Santos recorded \"Despedida\" (My Good-bye), a farewell song written by Flores from the viewpoint of an Army recruit who had to leave behind his girlfriend and his ailing mother, which became a hit." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1409780/8> ;
        ont:place         <http://dbpedia.org/resource/Flores> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1409780/8/0> ;
        ont:subject       <http://dbpedia.org/resource/Daniel_Santos_(singer)> .

<http://w3id.org/polifonia/pilot/meetups/1409780/12/10>
        rdf:type          ont:Sentence ;
        ont:excerpt       "His first successful single was titled Bigote de Gato, based on an area of Havana named \"Luyano\", which was infamous for serving as the home for fortune tellers." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1409780/12> ;
        ont:place         <http://dbpedia.org/resource/Havana> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1409780/12/9> ;
        ont:subject       <http://dbpedia.org/resource/Daniel_Santos_(singer)> .

<http://w3id.org/polifonia/pilot/meetups/1409780/3/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Santos joined his high school's choir, but he dropped out of high school in his second year and moved out of his parents' apartment." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1409780/3> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1409780/3/1> ;
        ont:subject       <http://dbpedia.org/resource/Daniel_Santos_(singer)> .

<http://w3id.org/polifonia/pilot/meetups/1409780/21/2>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Mi Vida Entera (2020) written by his daughter Danilú Santos-Price,; Vengo a decirle adiós a los muchachos (1989) by Josean Ramos; La importancia de llamarse Daniel Santos (1988), by Luis Rafael Sánchez, El Inquieto Anacobero, by Salvador Garmendia." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1409780/21> ;
        ont:person          <http://dbpedia.org/resource/Daniel_Santos_(singer)> , <http://dbpedia.org/resource/Salvador_Garmendia> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1409780/21/1> ;
        ont:subject         <http://dbpedia.org/resource/Daniel_Santos_(singer)> ;
        ont:timeExpression  "2020" , "1988" , "1989" .

<http://w3id.org/polifonia/pilot/meetups/1409780/14/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "The tour concluded with a presentation in New York and he is returned to Cuba." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1409780/14> ;
