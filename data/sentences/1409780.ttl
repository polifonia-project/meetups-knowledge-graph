        ont:excerpt       "Santos donated the profits from \"Sierra Maestra\" to the Cuban Revolution." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1409780/12> ;
        ont:place         <http://dbpedia.org/resource/Sierra_Maestra> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1409780/12/21> ;
        ont:subject       <http://dbpedia.org/resource/Daniel_Santos_(singer)> .

<http://w3id.org/polifonia/pilot/meetups/1409780/12/7>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He also made five presentations in theaters, among them the Martí Theater." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1409780/12> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1409780/12/6> ;
        ont:subject       <http://dbpedia.org/resource/Daniel_Santos_(singer)> .

