        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/1409780/16/1> .

<http://w3id.org/polifonia/pilot/meetups/1409780/14/6>
        rdf:type          ont:Sentence ;
        ont:excerpt       "While in Ecuador he composed two successful singles, Cataplum pa' and Cautiverio." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1409780/14> ;
        ont:place         <http://dbpedia.org/resource/Ecuador> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1409780/14/5> ;
        ont:subject       <http://dbpedia.org/resource/Daniel_Santos_(singer)> .

