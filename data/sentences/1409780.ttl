<http://w3id.org/polifonia/pilot/meetups/1409780/12/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "At the moment Cuba was experiencing economic growth and Santos developed an interest in the island." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1409780/12> ;
        ont:place         <http://dbpedia.org/resource/Cuba> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1409780/12/0> ;
        ont:subject       <http://dbpedia.org/resource/Daniel_Santos_(singer)> .

<http://w3id.org/polifonia/pilot/meetups/1409780/8/5>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In Maui, he was assigned to a US Army infantry unit which was used to replenish casualties in the Pacific theatre." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1409780/8> ;
        ont:place         <http://dbpedia.org/resource/Maui> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1409780/8/4> ;
        ont:subject       <http://dbpedia.org/resource/Daniel_Santos_(singer)> .

<http://w3id.org/polifonia/pilot/meetups/1409780/5/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "===" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1409780/5> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1409780/5/0> ;
        ont:subject       <http://dbpedia.org/resource/Daniel_Santos_(singer)> .

<http://w3id.org/polifonia/pilot/meetups/1409780/0/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Over the course of his career he adopted several names created by the public and became known as \"El Jefe\" and \"El Inquieto Anacobero\"." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1409780/0> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1409780/0/0> ;
        ont:subject       <http://dbpedia.org/resource/Daniel_Santos_(singer)> .

<http://w3id.org/polifonia/pilot/meetups/1409780/12/14>
        rdf:type          ont:Sentence ;
        ont:excerpt       "This was followed by several successful productions with Sonora Matancera." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1409780/12> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1409780/12/13> ;
        ont:subject       <http://dbpedia.org/resource/Daniel_Santos_(singer)> .

<http://w3id.org/polifonia/pilot/meetups/1409780/16/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "On July 22, 1972 he performed in the inauguration ceremony of El Balcon del Pueblo, a building owned by Radio Cristal, which was located at Guayaquil." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1409780/16> ;
        ont:place           <http://dbpedia.org/resource/Guayaquil> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1409780/16/0> ;
        ont:subject         <http://dbpedia.org/resource/Daniel_Santos_(singer)> ;
        ont:timeExpression  "July 22 , 1972" .

<http://w3id.org/polifonia/pilot/meetups/1409780/11/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Santos in Cuba =" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1409780/11> ;
        ont:place         <http://dbpedia.org/resource/Cuba> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1409780/11/0> ;
        ont:subject       <http://dbpedia.org/resource/Daniel_Santos_(singer)> .

<http://w3id.org/polifonia/pilot/meetups/1409780/3/6>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He then knocked on Santos' door." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1409780/3> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1409780/3/5> ;
        ont:subject       <http://dbpedia.org/resource/Daniel_Santos_(singer)> .

<http://w3id.org/polifonia/pilot/meetups/1409780/19/5>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Santos was also invited to participate in a music festival in Cuba, where he was supposed to receive a homage, but he was unable to attend." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1409780/19> ;
        ont:place         <http://dbpedia.org/resource/Cuba> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1409780/19/4> ;
        ont:subject       <http://dbpedia.org/resource/Daniel_Santos_(singer)> .

<http://w3id.org/polifonia/pilot/meetups/1409780/16>
        rdf:type  ont:Paragraph ;
