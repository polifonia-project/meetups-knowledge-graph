        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/851595/1/0> ;
        ont:subject       <http://dbpedia.org/resource/Prabhat_Ranjan_Sarkar> .

<http://w3id.org/polifonia/pilot/meetups/851595/18/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "The Manipura Chakra: at the level of the navel (controls the luminous factor and is associated with Pancreas)." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/851595/18> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/851595/18/0> ;
