<http://w3id.org/polifonia/pilot/meetups/851595/13/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Hiranyamaya Kosa (\"golden level\") or \"Subtle Causal Mind\": is the subtlest layer." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/851595/13> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/851595/13/0> ;
