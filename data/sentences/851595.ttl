<http://w3id.org/polifonia/pilot/meetups/851595/2/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Sarkar was born during the full moon of the Indian month of Vaeshakh (Buddha Purnima), on 21 May 1921 (at 6:07 in the morning) to Lakshmi Narayan Sarkar, a homoeopathic doctor." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/851595/2> ;
        ont:person          <http://dbpedia.org/resource/Gautama_Buddha> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/851595/2/0> ;
        ont:subject         <http://dbpedia.org/resource/Prabhat_Ranjan_Sarkar> ;
        ont:timeExpression  "21 May 1921" , "6:07 in" .

<http://w3id.org/polifonia/pilot/meetups/851595/27/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Social and political philosophy" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/851595/27> ;
