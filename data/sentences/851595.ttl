<http://w3id.org/polifonia/pilot/meetups/851595/28/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Law of Social Cycle =" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/851595/28> ;
