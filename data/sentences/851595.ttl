        rdf:type            ont:Sentence ;
        ont:excerpt         "In his series of discourses Talks on Prout, given in Ranchi in July 1961, Sarkar makes a distinction between the terms \"culture\" and \"customs\"." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/851595/38> ;
