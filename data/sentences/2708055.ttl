        rdf:type            ont:Sentence ;
        ont:excerpt         "His early work blends non-western instruments with 1960s folk revival, and has been cited as important in the development of psychedelic music." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/2708055/0> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/2708055/0/1> ;
