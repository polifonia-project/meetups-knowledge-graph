<http://w3id.org/polifonia/pilot/meetups/2708055/2/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "His parents were divorced in 1941, shortly after his birth." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2708055/2> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2708055/2/0> ;
