        rdf:type            ont:Sentence ;
        ont:excerpt         "Langenhoven is regarded as one of the most prolific and most versatile Afrikaans writers still today, his Collected Works comprising 16 volumes." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1228021/11> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1228021/11/0> ;
