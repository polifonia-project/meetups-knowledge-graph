<http://w3id.org/polifonia/pilot/meetups/1228021/13/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Nelson Mandela recounts that in 1964 while in prison he read a book by Langenhoven called Shadows of Nazareth about the trial of Jesus Christ that deeply affected him." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1228021/13> ;
        ont:person          <http://dbpedia.org/resource/Jesus> , <http://dbpedia.org/resource/Nelson_Mandela> ;
        ont:place           <http://dbpedia.org/resource/Nazareth> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1228021/13/0> ;
        ont:subject         <http://dbpedia.org/resource/Cornelis_Jacobus_Langenhoven> ;
        ont:timeExpression  "1964 while" .

<http://w3id.org/polifonia/pilot/meetups/1228021/5/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "This boulder known as Herrie's Stone (\"Herrie se Klip\", in Afrikaans), has been declared a provincial heritage site." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1228021/5> ;
        ont:place         <http://dbpedia.org/resource/Boulder> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1228021/5/3> ;
        ont:subject       <http://dbpedia.org/resource/Cornelis_Jacobus_Langenhoven> .

<http://w3id.org/polifonia/pilot/meetups/1228021/12/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "The place he and his family lived in, called the Arbeidsgenot, (meaning \"the pleasure of work\" or \"the joy of labour\") has been turned into a house museum." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1228021/12> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1228021/12/0> ;
