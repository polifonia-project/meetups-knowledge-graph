<http://w3id.org/polifonia/pilot/meetups/17902/67/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Euler insisted that knowledge is founded in part on the basis of precise quantitative laws, something that monadism and Wolffian science were unable to provide." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/17902/67> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/17902/67/0> ;
