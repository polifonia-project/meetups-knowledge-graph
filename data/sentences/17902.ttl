        ont:subject         <http://dbpedia.org/resource/Leonhard_Euler> ;
        ont:timeExpression  "1707–1773" , "7 January 1734" .

<http://w3id.org/polifonia/pilot/meetups/17902/24/1>
        rdf:type            ont:Sentence ;
