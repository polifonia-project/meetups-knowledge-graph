        ont:timeExpression  "one of" .

<http://w3id.org/polifonia/pilot/meetups/17902/4/7>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1723, he received a Master of Philosophy with a dissertation that compared the philosophies of René Descartes and Isaac Newton." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/17902/4> ;
