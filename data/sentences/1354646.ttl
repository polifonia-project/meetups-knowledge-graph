        rdf:type          ont:Sentence ;
        ont:excerpt       "Our entire orchestra family is profoundly disappointed when reporters report the news in such an ungracious way." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1354646/13> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1354646/13/2> ;
        ont:subject       <http://dbpedia.org/resource/Christoph_Eschenbach> .

