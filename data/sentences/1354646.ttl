        ont:subject         <http://dbpedia.org/resource/Christoph_Eschenbach> ;
        ont:timeExpression  "20 February 1940" .

<http://w3id.org/polifonia/pilot/meetups/1354646/11/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In the weeks prior to his departure, Philadelphia Inquirer music critics Peter Dobrin and David Patrick Stearns had contrasting articles whether or not he should be retained, with Dobrin suggesting that Eschenbach should move on and Stearns arguing that Eschenbach should remain." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1354646/11> ;
        ont:person        <http://dbpedia.org/resource/Nicolae_Dobrin> , <http://dbpedia.org/resource/Saint_Patrick> ;
        ont:place         <http://dbpedia.org/resource/Philadelphia> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1354646/11/0> ;
        ont:subject       <http://dbpedia.org/resource/Christoph_Eschenbach> .

<http://w3id.org/polifonia/pilot/meetups/1354646/24/7>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He is scheduled to stand down from the Konzerthausorchester Berlin post at the close of the 2022-2023 season." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1354646/24> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1354646/24/6> ;
        ont:subject       <http://dbpedia.org/resource/Christoph_Eschenbach> .

