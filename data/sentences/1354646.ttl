        rdf:type            ont:Sentence ;
        ont:excerpt         "Christoph Eschenbach (German: [ˈkʁɪstɔf ˈɛʃn̩bax]; born 20 February 1940) is a German pianist and conductor." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1354646/0> ;
        ont:person          <http://dbpedia.org/resource/Christoph_Eschenbach> , <http://dbpedia.org/resource/Conducting> ;
