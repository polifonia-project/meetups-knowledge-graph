<http://w3id.org/polifonia/pilot/meetups/1354646/20/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He is an exciting interpreter." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1354646/20> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1354646/20/3> ;
        ont:subject       <http://dbpedia.org/resource/Christoph_Eschenbach> .

<http://w3id.org/polifonia/pilot/meetups/1354646/20/14>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Even two years ago, at my last visit to Verizon Hall, the atmosphere was palpably unpleasant." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1354646/20> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1354646/20/13> ;
        ont:subject       <http://dbpedia.org/resource/Christoph_Eschenbach> .

<http://w3id.org/polifonia/pilot/meetups/1354646/27/1>
        rdf:type          ont:Sentence ;
