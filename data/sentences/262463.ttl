        ont:person        <http://dbpedia.org/resource/Rob_Halford> ;
        ont:place         <http://dbpedia.org/resource/Tipton> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/262463/3/17> ;
        ont:subject       <http://dbpedia.org/resource/Glenn_Tipton> .

<http://w3id.org/polifonia/pilot/meetups/262463/14/34>
        rdf:type            ont:Sentence ;
        ont:excerpt         "\"One day when I'm good enough to, I wouldn't even call it a film score, but do music for film, I'd like that opportunity." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/262463/14> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/262463/14/33> ;
