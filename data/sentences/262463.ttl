        rdf:type            ont:Sentence ;
        ont:excerpt         "In the mid-1980s, both guitarists started to use the complex technique of sweep-picking, which can be notably heard on the title track of their 1990 album Painkiller." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/262463/11> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/262463/11/3> ;
