        ont:timeExpression  "25 on" .

<http://w3id.org/polifonia/pilot/meetups/262463/0/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Glenn Raymond Tipton (born 25 October 1947) is an English musician." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/262463/0> ;
        ont:person          <http://dbpedia.org/resource/Raymond_of_Poitiers> ;
