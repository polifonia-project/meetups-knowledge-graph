        rdf:type          ont:Sentence ;
        ont:excerpt       "Steve Winwood was a great all round vocalist, guitarist and keyboard player and I remember trying to learn 'Georgia' on the piano after I had seen them play." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/262463/14> ;
