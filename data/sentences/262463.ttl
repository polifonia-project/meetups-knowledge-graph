        ont:subject       <http://dbpedia.org/resource/Glenn_Tipton> .

<http://w3id.org/polifonia/pilot/meetups/262463/46/1>
        rdf:type            ont:Sentence ;
        ont:excerpt         "He stated that Tipton is one of his earliest guitar influences." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/262463/46> ;
        ont:place           <http://dbpedia.org/resource/Tipton> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/262463/46/0> ;
        ont:subject         <http://dbpedia.org/resource/Glenn_Tipton> ;
        ont:timeExpression  "one of" .

<http://w3id.org/polifonia/pilot/meetups/262463/3/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He would then play on a Rickenbacker until he was able to afford a Fender Stratocaster." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/262463/3> ;
        ont:person        <http://dbpedia.org/resource/Fender_Stratocaster> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/262463/3/0> ;
        ont:subject       <http://dbpedia.org/resource/Glenn_Tipton> .

<http://w3id.org/polifonia/pilot/meetups/262463/31/3>
        rdf:type            ont:Sentence ;
        ont:excerpt         "For the Epitaph tour, he switched to using ENGL Invaders that are modified to use 6L6 power tubes." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/262463/31> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/262463/31/2> ;
        ont:subject         <http://dbpedia.org/resource/Glenn_Tipton> ;
