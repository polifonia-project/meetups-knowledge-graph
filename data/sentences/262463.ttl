        ont:subject         <http://dbpedia.org/resource/Glenn_Tipton> ;
        ont:timeExpression  "25 October 1947" .

<http://w3id.org/polifonia/pilot/meetups/262463/23>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/262463/23/0> .

<http://w3id.org/polifonia/pilot/meetups/262463/20/1>
        rdf:type            ont:Sentence ;
        ont:excerpt         "These include a 1960s Fender Stratocaster up until about 1978." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/262463/20> ;
        ont:person          <http://dbpedia.org/resource/Fender_Stratocaster> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/262463/20/0> ;
        ont:subject         <http://dbpedia.org/resource/Glenn_Tipton> ;
        ont:timeExpression  "1960s Fender" .

<http://w3id.org/polifonia/pilot/meetups/262463/5/5>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Both of these albums experimented with new sounds that distinguished them from the records with Halford." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/262463/5> ;
        ont:person        <http://dbpedia.org/resource/Rob_Halford> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/262463/5/4> ;
        ont:subject       <http://dbpedia.org/resource/Glenn_Tipton> .

<http://w3id.org/polifonia/pilot/meetups/262463/3/13>
        rdf:type          ont:Sentence ;
        ont:excerpt       "On Sad Wings of Destiny, Tipton showed off more of his guitar work on songs like \"Tyrant\", \"Dreamer Deceiver\" and \"Victim of Changes\"." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/262463/3> ;
        ont:place         <http://dbpedia.org/resource/Tipton> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/262463/3/12> ;
        ont:subject       <http://dbpedia.org/resource/Glenn_Tipton> .

<http://w3id.org/polifonia/pilot/meetups/262463/12/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1997, Tipton released his solo album, Baptizm of Fire, featuring a host of well-known musicians including Billy Sheehan, Cozy Powell and Don Airey, among others where he showed his technical guitar playing abilities." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/262463/12> ;
        ont:person        <http://dbpedia.org/resource/Don_Airey> , <http://dbpedia.org/resource/Cozy_Powell> , <http://dbpedia.org/resource/Billy_Sheehan> ;
        ont:place         <http://dbpedia.org/resource/Tipton> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/262463/12/0> ;
        ont:subject       <http://dbpedia.org/resource/Glenn_Tipton> .

<http://w3id.org/polifonia/pilot/meetups/262463/44/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Sun Kil Moon released a song titled \"Glenn Tipton\" on their album Ghosts of the Great Highway." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/262463/44> ;
        ont:person        <http://dbpedia.org/resource/Glenn_Tipton> ;
        ont:place         <http://dbpedia.org/resource/Great_Highway> , <http://dbpedia.org/resource/Kil_Municipality> , <http://dbpedia.org/resource/Moon> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/262463/44/0> ;
        ont:subject       <http://dbpedia.org/resource/Glenn_Tipton> .

<http://w3id.org/polifonia/pilot/meetups/262463/14/29>
        rdf:type          ont:Sentence ;
        ont:excerpt       "They were so prolific and the endless melodies they invented will live forever\"." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/262463/14> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/262463/14/28> ;
        ont:subject       <http://dbpedia.org/resource/Glenn_Tipton> .

