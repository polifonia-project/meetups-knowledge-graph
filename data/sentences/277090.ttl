        ont:person          <http://dbpedia.org/resource/Gene_Ammons> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/277090/11/5> ;
        ont:subject         <http://dbpedia.org/resource/Dodo_Marmarosa> ;
        ont:timeExpression  "One album" .

<http://w3id.org/polifonia/pilot/meetups/277090/7/41>
        rdf:type            ont:Sentence ;
        ont:excerpt         "He left Shaw's band for the final time during one concert after they had twice played Shaw's hit, \"Frenesí\", Marmarosa had threatened to leave if Shaw called for it again, and the leader had done so after the audience requested a third playing." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/277090/7> ;
        ont:person          <http://dbpedia.org/resource/George_Bernard_Shaw> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/277090/7/40> ;
        ont:subject         <http://dbpedia.org/resource/Dodo_Marmarosa> ;
        ont:timeExpression  "one concert" .

