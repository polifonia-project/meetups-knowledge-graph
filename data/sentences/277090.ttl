<http://w3id.org/polifonia/pilot/meetups/277090/9/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "For around a decade, Marmarosa was much less active as a musician." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/277090/9> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/277090/9/0> ;
        ont:subject       <http://dbpedia.org/resource/Dodo_Marmarosa> .

<http://w3id.org/polifonia/pilot/meetups/277090/7/8>
        rdf:type            ont:Sentence ;
        ont:excerpt         "He then moved to Charlie Barnet's big band, where he stayed from October 1943 to March 1944." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/277090/7> ;
        ont:person          <http://dbpedia.org/resource/Charlie_Barnet> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/277090/7/7> ;
        ont:subject         <http://dbpedia.org/resource/Dodo_Marmarosa> ;
        ont:timeExpression  "March 1944" , "October 1943" .

<http://w3id.org/polifonia/pilot/meetups/277090/7/36>
        rdf:type          ont:Sentence ;
        ont:excerpt       "The piano was his life." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/277090/7> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/277090/7/35> ;
        ont:subject       <http://dbpedia.org/resource/Dodo_Marmarosa> .

<http://w3id.org/polifonia/pilot/meetups/277090/13/11>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Marmarosa also encouraged DeFranco to take up bebop." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/277090/13> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/277090/13/10> ;
        ont:subject       <http://dbpedia.org/resource/Dodo_Marmarosa> .

