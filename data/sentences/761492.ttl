        ont:subject         <http://dbpedia.org/resource/Margaret_Truman> ;
        ont:timeExpression  "1934 election" .

<http://w3id.org/polifonia/pilot/meetups/761492/8/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Truman's professional acting debut occurred April 26, 1951." ;
