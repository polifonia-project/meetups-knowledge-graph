<http://w3id.org/polifonia/pilot/meetups/761492/6/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "She sang professionally for the next decade, appearing with major American orchestras and giving several national concert tours." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/761492/6> ;
        ont:place         <http://dbpedia.org/resource/United_States> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/761492/6/0> ;
        ont:subject       <http://dbpedia.org/resource/Margaret_Truman> .

<http://w3id.org/polifonia/pilot/meetups/761492/6/18>
        rdf:type          ont:Sentence ;
        ont:excerpt       "The Post was then forced to acknowledge the letter, which drew international headlines, becoming a minor scandal for the Truman administration." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/761492/6> ;
        ont:person        <http://dbpedia.org/resource/Harry_S._Truman> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/761492/6/17> ;
        ont:subject       <http://dbpedia.org/resource/Margaret_Truman> .

