        ont:subject       <http://dbpedia.org/resource/Margaret_Truman> .

<http://w3id.org/polifonia/pilot/meetups/761492/6/12>
        rdf:type          ont:Sentence ;
        ont:excerpt       "She is flat a good deal of the time." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/761492/6> ;
