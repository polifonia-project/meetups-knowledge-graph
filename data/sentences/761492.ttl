<http://w3id.org/polifonia/pilot/meetups/761492/8/2>
        rdf:type            ont:Sentence ;
        ont:excerpt         "On March 17, 1952, Truman was guest soloist on The Railroad Hour in a presentation of the operetta Sari." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/761492/8> ;
        ont:person          <http://dbpedia.org/resource/Harry_S._Truman> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/761492/8/1> ;
        ont:subject         <http://dbpedia.org/resource/Margaret_Truman> ;
        ont:timeExpression  "March 17 , 1952" .

<http://w3id.org/polifonia/pilot/meetups/761492/3/3>
        rdf:type            ont:Sentence ;
        ont:excerpt         "In June 1944, she christened the battleship USS Missouri at Brooklyn Navy Yard, and spoke again in 1986 at the ship's recommissioning." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/761492/3> ;
        ont:place           <http://dbpedia.org/resource/Brooklyn_Navy_Yard> , <http://dbpedia.org/resource/Missouri> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/761492/3/2> ;
        ont:subject         <http://dbpedia.org/resource/Margaret_Truman> ;
        ont:timeExpression  "1986 at" , "June 1944" .

<http://w3id.org/polifonia/pilot/meetups/761492/6/8>
        rdf:type            ont:Sentence ;
        ont:excerpt         "She performed on stage, radio, and television through 1956." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/761492/6> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/761492/6/7> ;
        ont:subject         <http://dbpedia.org/resource/Margaret_Truman> ;
        ont:timeExpression  "television through 1956" .

<http://w3id.org/polifonia/pilot/meetups/761492/12>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/761492/12/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/761492/12/1> ;
        rdf:_3    <http://w3id.org/polifonia/pilot/meetups/761492/12/2> .

<http://w3id.org/polifonia/pilot/meetups/761492/0/9>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1957, Truman abandoned her singing career to pursue a career as a journalist and radio personality, when she became the co-host of the program Weekday with Mike Wallace." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/761492/0> ;
        ont:person        <http://dbpedia.org/resource/Radio_personality> , <http://dbpedia.org/resource/Harry_S._Truman> , <http://dbpedia.org/resource/Mike_Wallace> , <http://dbpedia.org/resource/Journalist> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/761492/0/8> ;
        ont:subject       <http://dbpedia.org/resource/Margaret_Truman> .

<http://w3id.org/polifonia/pilot/meetups/761492/25>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/761492/25/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/761492/25/1> ;
        rdf:_3    <http://w3id.org/polifonia/pilot/meetups/761492/25/2> ;
        rdf:_4    <http://w3id.org/polifonia/pilot/meetups/761492/25/3> .

<http://w3id.org/polifonia/pilot/meetups/761492/6/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Some of her credits include concert appearances with the Los Angeles Philharmonic at the Hollywood Bowl, the National Symphony Orchestra, the NBC Symphony Orchestra, the Pittsburgh Symphony, the Philadelphia Orchestra, and the Saint Louis Symphony among others." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/761492/6> ;
        ont:place         <http://dbpedia.org/resource/Hollywood_Bowl> , <http://dbpedia.org/resource/St._Louis> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/761492/6/1> ;
        ont:subject       <http://dbpedia.org/resource/Margaret_Truman> .

<http://w3id.org/polifonia/pilot/meetups/761492/0/10>
        rdf:type          ont:Sentence ;
        ont:excerpt       "She also wrote articles as an independent journalist, for a variety of publications in the 1960s and 1970s." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/761492/0> ;
        ont:person        <http://dbpedia.org/resource/Journalist> , <http://dbpedia.org/resource/Independent_politician> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/761492/0/9> ;
        ont:subject       <http://dbpedia.org/resource/Margaret_Truman> .

<http://w3id.org/polifonia/pilot/meetups/761492/6/19>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Reviewers after that felt more free to be honest in their reviews of her performances, with mixed criticism for her singing thereafter." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/761492/6> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/761492/6/18> ;
        ont:subject       <http://dbpedia.org/resource/Margaret_Truman> .

<http://w3id.org/polifonia/pilot/meetups/761492/5>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/761492/5/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/761492/5/1> .

<http://w3id.org/polifonia/pilot/meetups/761492/0/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "She was a favorite with the media." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/761492/0> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/761492/0/2> ;
        ont:subject       <http://dbpedia.org/resource/Margaret_Truman> .

