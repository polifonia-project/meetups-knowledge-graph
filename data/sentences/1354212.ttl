<http://w3id.org/polifonia/pilot/meetups/1354212/28/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Many of his works were published, includes those for piano, choir, voice, organ, and orchestra:" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1354212/28> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1354212/28/0> ;
        ont:subject       <http://dbpedia.org/resource/Robert_Nathaniel_Dett> .

<http://w3id.org/polifonia/pilot/meetups/1354212/13/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "They also performed on CBS radio broadcasts." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1354212/13> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1354212/13/1> ;
        ont:subject       <http://dbpedia.org/resource/Robert_Nathaniel_Dett> .

