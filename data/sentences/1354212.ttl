        rdf:type            ont:Sentence ;
        ont:excerpt         "Honorary Doctorate (1926) from Oberlin College." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1354212/26> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1354212/26/0> ;
        ont:subject         <http://dbpedia.org/resource/Robert_Nathaniel_Dett> ;
        ont:timeExpression  "1926" .

<http://w3id.org/polifonia/pilot/meetups/1354212/9/5>
        rdf:type          ont:Sentence ;
        ont:excerpt       "His interest in composition had to accommodate his demands of teaching and administration." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1354212/9> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1354212/9/4> ;
        ont:subject       <http://dbpedia.org/resource/Robert_Nathaniel_Dett> .

<http://w3id.org/polifonia/pilot/meetups/1354212/11/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He also became involved with the National Association of Teachers in Colored Schools." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1354212/11> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1354212/11/1> ;
        ont:subject       <http://dbpedia.org/resource/Robert_Nathaniel_Dett> .

<http://w3id.org/polifonia/pilot/meetups/1354212/5/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "His position as a major pianist-composer was earned in 1914." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1354212/5> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1354212/5/0> ;
        ont:subject       <http://dbpedia.org/resource/Robert_Nathaniel_Dett> .

<http://w3id.org/polifonia/pilot/meetups/1354212/20/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1919 he recorded two selections for Broome Special Phonograph Records, \"Mammy\" from Magnolia Suite and \"Barcarolle\" from In the Bottoms." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1354212/20> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1354212/20/2> ;
        ont:subject       <http://dbpedia.org/resource/Robert_Nathaniel_Dett> .

<http://w3id.org/polifonia/pilot/meetups/1354212/27/0>
        rdf:type          ont:Sentence ;
