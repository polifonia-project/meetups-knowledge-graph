        ont:subject       <http://dbpedia.org/resource/Robert_Nathaniel_Dett> .

<http://w3id.org/polifonia/pilot/meetups/1354212/59/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "\"The Emancipation of Negro Music\"." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1354212/59> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1354212/59/0> ;
        ont:subject       <http://dbpedia.org/resource/Robert_Nathaniel_Dett> .

<http://w3id.org/polifonia/pilot/meetups/1354212/12/2>
        rdf:type            ont:Sentence ;
        ont:excerpt         "It was conducted by Eugene Goosens in its premiere on May 7, 1937, with a chorus of 350 and the Cincinnati Symphony Orchestra at the Cincinnati May Festival in Ohio." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1354212/12> ;
        ont:person          <http://dbpedia.org/resource/Eugene_Aynsley_Goossens> ;
        ont:place           <http://dbpedia.org/resource/Ohio> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1354212/12/1> ;
        ont:subject         <http://dbpedia.org/resource/Robert_Nathaniel_Dett> ;
        ont:timeExpression  "chorus of 350" , "May 7 , 1937" .

<http://w3id.org/polifonia/pilot/meetups/1354212/6/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "On December 27, 1916, Dett married Helen Elise Smith." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1354212/6> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1354212/6/0> ;
        ont:subject         <http://dbpedia.org/resource/Robert_Nathaniel_Dett> ;
        ont:timeExpression  "December 27 , 1916" .

<http://w3id.org/polifonia/pilot/meetups/1354212/4/8>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Sometimes, his arrangements were criticized for being \"inauthentic\" due to their similarities with Western classical music." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1354212/4> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1354212/4/7> ;
        ont:subject       <http://dbpedia.org/resource/Robert_Nathaniel_Dett> .

