        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/3986/141/4> ;
        ont:subject         <http://dbpedia.org/resource/Benjamin_Franklin> ;
        ont:timeExpression  "50 bill" , "from 1914 and 1918" , "100 bill" .

<http://w3id.org/polifonia/pilot/meetups/3986/15/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Franklin conceived the idea of a subscription library, which would pool the funds of the members to buy books for all to read." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/3986/15> ;
        ont:person        <http://dbpedia.org/resource/Benjamin_Franklin> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/3986/15/0> ;
        ont:subject       <http://dbpedia.org/resource/Benjamin_Franklin> .

<http://w3id.org/polifonia/pilot/meetups/3986/24/6>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1763, he was appointed as the last royal governor of New Jersey." ;
