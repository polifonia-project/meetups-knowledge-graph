        ont:place         <http://dbpedia.org/resource/New_Jersey> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/3986/24/5> ;
        ont:subject       <http://dbpedia.org/resource/Benjamin_Franklin> .

<http://w3id.org/polifonia/pilot/meetups/3986/84/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "To understand this phenomenon more clearly Franklin conducted experiments." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/3986/84> ;
        ont:person        <http://dbpedia.org/resource/Benjamin_Franklin> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/3986/84/0> ;
        ont:subject       <http://dbpedia.org/resource/Benjamin_Franklin> .

<http://w3id.org/polifonia/pilot/meetups/3986/47/11>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Franklin spent two months in German lands in 1766, but his connections to the country stretched across a lifetime." ;
