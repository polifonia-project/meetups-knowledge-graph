        ont:subject         <http://dbpedia.org/resource/Benjamin_Franklin> ;
        ont:timeExpression  "17 in" .

<http://w3id.org/polifonia/pilot/meetups/3986/93/7>
        rdf:type          ont:Sentence ;
        ont:excerpt       "I see of this truth—that God governs in the affairs of men." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/3986/93> ;
