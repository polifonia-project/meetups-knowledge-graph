<http://w3id.org/polifonia/pilot/meetups/3986/93/6>
        rdf:type          ont:Sentence ;
        ont:excerpt       "I have lived, Sir, a long time and the longer I live, the more convincing proofs" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/3986/93> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/3986/93/5> ;
