<http://w3id.org/polifonia/pilot/meetups/3986/132/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He also composed music, notably a string quartet in early classical style." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/3986/132> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/3986/132/0> ;
