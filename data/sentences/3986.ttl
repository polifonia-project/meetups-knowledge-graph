<http://w3id.org/polifonia/pilot/meetups/3986/60/5>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He held an honorary position and seldom engaged in debate." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/3986/60> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/3986/60/4> ;
