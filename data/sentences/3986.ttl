<http://w3id.org/polifonia/pilot/meetups/3986/120/5>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1790, Quakers from New York and Pennsylvania presented their petition for abolition to Congress." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/3986/120> ;
        ont:place         <http://dbpedia.org/resource/Pennsylvania> , <http://dbpedia.org/resource/New_York_City> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/3986/120/4> ;
