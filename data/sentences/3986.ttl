<http://w3id.org/polifonia/pilot/meetups/3986/63/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In poor health during the signing of the US Constitution in 1787, he was rarely seen in public from then until his death." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/3986/63> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/3986/63/0> ;
