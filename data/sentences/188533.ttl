<http://w3id.org/polifonia/pilot/meetups/188533/0/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Ronald Frederick Lane (1 April 1946  4 June 1997) was an English musician, songwriter, and producer who is best known as the bass guitarist and founding member of two prominent English rock and roll bands: Small Faces (196569) and subsequently Faces (196973)." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/188533/0> ;
        ont:person          <http://dbpedia.org/resource/Frederick_Lane> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/188533/0/0> ;
        ont:subject         <http://dbpedia.org/resource/Ronnie_Lane> ;
        ont:timeExpression  "196973" , "196569" , "4 June 1997" , "member of two" , "1 April 1946" .

<http://w3id.org/polifonia/pilot/meetups/188533/24>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/188533/24/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/188533/24/1> .

<http://w3id.org/polifonia/pilot/meetups/188533/8/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "According to McLagan, Lane would later regret leaving Faces." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/188533/8> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/188533/8/3> ;
        ont:subject       <http://dbpedia.org/resource/Ronnie_Lane> .

<http://w3id.org/polifonia/pilot/meetups/188533/10/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1973, Lane moved to Fishpool Farm in the village of Hyssington, Montgomeryshire, Wales, just over the border from England." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/188533/10> ;
        ont:place         <http://dbpedia.org/resource/Hyssington> , <http://dbpedia.org/resource/Montgomeryshire> , <http://dbpedia.org/resource/Fishpool> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/188533/10/0> ;
        ont:subject       <http://dbpedia.org/resource/Ronnie_Lane> .

<http://w3id.org/polifonia/pilot/meetups/188533/13/5>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In late 1976 he joined a short-lived reformation of Small Faces but quit after two rehearsals, to be replaced by Rick Wills (who later played alongside former Small Faces drummer Kenney Jones in the Jones Gang)." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/188533/13> ;
        ont:person        <http://dbpedia.org/resource/Rick_Wills> , <http://dbpedia.org/resource/Kenney_Jones> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/188533/13/4> ;
        ont:subject       <http://dbpedia.org/resource/Ronnie_Lane> .

<http://w3id.org/polifonia/pilot/meetups/188533/17/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He toured Japan but his health continued to decline." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/188533/17> ;
        ont:place         <http://dbpedia.org/resource/Japan> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/188533/17/3> ;
        ont:subject       <http://dbpedia.org/resource/Ronnie_Lane> .

