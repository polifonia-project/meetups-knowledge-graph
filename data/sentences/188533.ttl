        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/188533/1/0> ;
        ont:subject       <http://dbpedia.org/resource/Ronnie_Lane> .

<http://w3id.org/polifonia/pilot/meetups/188533/4/5>
        rdf:type          ont:Sentence ;
        ont:excerpt       "When shopping for a Harmony bass guitar, Lane visited the J60 Music Bar in Manor Park, London, where he met employee Steve Marriott." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/188533/4> ;
        ont:person        <http://dbpedia.org/resource/Steve_Marriott> ;
        ont:place         <http://dbpedia.org/resource/Manor_Park,_London> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/188533/4/4> ;
