        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/188533/15/3> ;
        ont:subject       <http://dbpedia.org/resource/Ronnie_Lane> .

<http://w3id.org/polifonia/pilot/meetups/188533/4/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Initially playing lead guitar, Lane quickly switched to bass." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/188533/4> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/188533/4/3> ;
