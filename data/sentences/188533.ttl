        rdf:type            ont:Sentence ;
        ont:excerpt         "At least a dozen successful songs credit Lane, and he co-wrote all but one of the tracks on their 1968 concept album Ogdens' Nut Gone Flake." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/188533/6> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/188533/6/3> ;
