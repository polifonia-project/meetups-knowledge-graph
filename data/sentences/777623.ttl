<http://w3id.org/polifonia/pilot/meetups/777623/7/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Seger Ellis gradually retired and took up residence in Houston, where he died in a retirement home, on September 29, 1995." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/777623/7> ;
        ont:person          <http://dbpedia.org/resource/Seger_Ellis> ;
        ont:place           <http://dbpedia.org/resource/Houston> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/777623/7/0> ;
        ont:subject         <http://dbpedia.org/resource/Seger_Ellis> ;
        ont:timeExpression  "September 29 , 1995" .

<http://w3id.org/polifonia/pilot/meetups/777623/2/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Ellis began his career as pianist playing live for a local Houston radio station (later known as KPRC) in the early 1920s." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/777623/2> ;
        ont:person        <http://dbpedia.org/resource/Monta_Ellis> ;
        ont:place         <http://dbpedia.org/resource/Houston> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/777623/2/0> ;
        ont:subject       <http://dbpedia.org/resource/Seger_Ellis> .

<http://w3id.org/polifonia/pilot/meetups/777623/31>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/777623/31/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/777623/31/1> .

<http://w3id.org/polifonia/pilot/meetups/777623/6/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "After moving back to Texas, he began to be less active as a performer and more involved in song writing." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/777623/6> ;
        ont:place         <http://dbpedia.org/resource/Texas> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/777623/6/0> ;
        ont:subject       <http://dbpedia.org/resource/Seger_Ellis> .

