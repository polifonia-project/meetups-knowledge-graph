        rdf:type          ont:Sentence ;
        ont:excerpt       "Life and career ==" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/777623/1> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/777623/1/0> ;
        ont:subject       <http://dbpedia.org/resource/Seger_Ellis> .

<http://w3id.org/polifonia/pilot/meetups/777623/16>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/777623/16/0> .

<http://w3id.org/polifonia/pilot/meetups/777623/29>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/777623/29/0> .

<http://w3id.org/polifonia/pilot/meetups/777623/5/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1939, Ellis reorganized and his new band featured the conventional four-man reed section." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/777623/5> ;
        ont:person        <http://dbpedia.org/resource/Monta_Ellis> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/777623/5/0> ;
        ont:subject       <http://dbpedia.org/resource/Seger_Ellis> .

<http://w3id.org/polifonia/pilot/meetups/777623/0/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He also made a few brief film appearances, most notably in collaboration with director Ida Lupino." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/777623/0> ;
        ont:person        <http://dbpedia.org/resource/Ida_Lupino> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/777623/0/0> ;
        ont:subject       <http://dbpedia.org/resource/Seger_Ellis> .

<http://w3id.org/polifonia/pilot/meetups/777623/3/6>
        rdf:type          ont:Sentence ;
        ont:excerpt       "On these records, Ellis sang in a bittersweet alto, with which he was uncomfortable early in his career, believing his voice to be too high." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/777623/3> ;
        ont:person        <http://dbpedia.org/resource/Monta_Ellis> ;
