<http://w3id.org/polifonia/pilot/meetups/559173/9/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Berigan's business troubles drove him to declare bankruptcy in 1939, and shortly after to join Tommy Dorsey as a featured jazz soloist." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/559173/9> ;
        ont:person          <http://dbpedia.org/resource/Tommy_Dorsey> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/559173/9/0> ;
        ont:subject         <http://dbpedia.org/resource/Bunny_Berigan> ;
        ont:timeExpression  "bankruptcy in 1939" .

<http://w3id.org/polifonia/pilot/meetups/559173/4/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "During this time (late 1935 and throughout 1936), he began to record regularly under his own name, and he continued to back singers such as Bing Crosby, Mildred Bailey, and Billie Holiday." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/559173/4> ;
        ont:person        <http://dbpedia.org/resource/Bing_Crosby> , <http://dbpedia.org/resource/Mildred_Bailey> , <http://dbpedia.org/resource/Billie_Holiday> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/559173/4/0> ;
        ont:subject       <http://dbpedia.org/resource/Bunny_Berigan> .

<http://w3id.org/polifonia/pilot/meetups/559173/2/7>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He joined the staff of CBS radio network musicians in early 1931." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/559173/2> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/559173/2/6> ;
        ont:subject       <http://dbpedia.org/resource/Bunny_Berigan> .

