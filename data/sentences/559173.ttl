        rdf:type            ont:Sentence ;
        ont:excerpt         "His 1937 recording of \"I Can't Get Started\" was used in the film Save the Tiger (1973), the Roman Polanski film Chinatown (1974), and a Martin Scorsese short film, The Big Shave (1967)." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/559173/11> ;
        ont:person          <http://dbpedia.org/resource/Roman_Polanski> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/559173/11/3> ;
        ont:subject         <http://dbpedia.org/resource/Bunny_Berigan> ;
        ont:timeExpression  "1973" , "1967" , "1974" , "1937 recording" .

<http://w3id.org/polifonia/pilot/meetups/559173/6/6>
        rdf:type          ont:Sentence ;
        ont:excerpt       "This network radio show helped further popularize jazz as the swing era reached its peak." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/559173/6> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/559173/6/5> ;
        ont:subject       <http://dbpedia.org/resource/Bunny_Berigan> .

