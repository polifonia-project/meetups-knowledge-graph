        ont:excerpt       "Berigan did not do either." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/559173/9> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/559173/9/5> ;
