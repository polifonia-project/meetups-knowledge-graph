<http://w3id.org/polifonia/pilot/meetups/1148248/2/3>
        rdf:type            ont:Sentence ;
        ont:excerpt         "In 1973, his first album, Erkin Koray, was released, consisting of a collection of singles from 1967 to 1973." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1148248/2> ;
        ont:person          <http://dbpedia.org/resource/Erkin_Koray> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1148248/2/2> ;
        ont:subject         <http://dbpedia.org/resource/Erkin_Koray> ;
        ont:timeExpression  "from 1967 to 1973" .

