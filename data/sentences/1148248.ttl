        rdf:type          ont:Sentence ;
        ont:excerpt       "In addition to his own compositions, he rearranged multiple well known Turkish folk songs." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1148248/0> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1148248/0/3> ;
