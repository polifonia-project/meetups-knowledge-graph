<http://w3id.org/polifonia/pilot/meetups/3152159/2/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Sylvester Louis Ossman was born in Hudson, New York, and made his first recordings in 1893." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/3152159/2> ;
        ont:place         <http://dbpedia.org/resource/New_York_City> , <http://dbpedia.org/resource/Hudson_River> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/3152159/2/0> ;
        ont:subject       <http://dbpedia.org/resource/Vess_Ossman> .

<http://w3id.org/polifonia/pilot/meetups/3152159/3/5>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He temporarily ceased recording in 1913 but resumed in late 1915." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/3152159/3> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/3152159/3/4> ;
        ont:subject       <http://dbpedia.org/resource/Vess_Ossman> .

