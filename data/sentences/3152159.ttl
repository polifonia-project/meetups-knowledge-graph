        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/3152159/4/1> .

<http://w3id.org/polifonia/pilot/meetups/3152159/1/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Biography ==" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/3152159/1> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/3152159/1/0> ;
        ont:subject       <http://dbpedia.org/resource/Vess_Ossman> .

<http://w3id.org/polifonia/pilot/meetups/3152159/3/13>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He was buried in Valhalla Cemetery in St. Louis, Missouri." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/3152159/3> ;
        ont:place         <http://dbpedia.org/resource/Missouri> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/3152159/3/12> ;
        ont:subject       <http://dbpedia.org/resource/Vess_Ossman> .
