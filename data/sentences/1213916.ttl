        ont:person          <http://dbpedia.org/resource/John_Peel> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1213916/5/7> ;
        ont:subject         <http://dbpedia.org/resource/Bridget_St_John> ;
        ont:timeExpression  "1969 debut album" .

<http://w3id.org/polifonia/pilot/meetups/1213916/5/13>
        rdf:type          ont:Sentence ;
        ont:excerpt       "This album was a significant step up from her debut, and contained string arrangements mostly by Geesin himself, particularly striking on the opening track \"A Day A Way\" and \"Seagull- Sunday.\"" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1213916/5> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1213916/5/12> ;
        ont:subject       <http://dbpedia.org/resource/Bridget_St_John> .

