        rdf:type            ont:Sentence ;
        ont:excerpt         "After studying the viola for two years and then the trumpet for two years, St John bought a guitar with 20 pounds her grandmother gave her shortly before she finished high school." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1213916/3> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1213916/3/1> ;
        ont:subject         <http://dbpedia.org/resource/Bridget_St_John> ;
        ont:timeExpression  "guitar with 20" , "trumpet for two" , "viola for two" .

