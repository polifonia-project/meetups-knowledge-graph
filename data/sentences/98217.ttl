        ont:person          <http://dbpedia.org/resource/Shel_Silverstein> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/98217/68/0> ;
        ont:subject         <http://dbpedia.org/resource/Shel_Silverstein> ;
        ont:timeExpression  "2017" .

