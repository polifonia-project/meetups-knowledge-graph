<http://w3id.org/polifonia/pilot/meetups/2447748/2/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "He became a Salvation Army officer in 1949, in the Harrow Corps., and in 1951 he married Joyce Foster, who had become a Salvation Army officer from the Hastings Citadel in 1949." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/2447748/2> ;
        ont:person          <http://dbpedia.org/resource/David_Foster> , <http://dbpedia.org/resource/James_Joyce> ;
        ont:place           <http://dbpedia.org/resource/Hastings> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/2447748/2/0> ;
        ont:subject         <http://dbpedia.org/resource/Ray_Steadman-Allen> ;
        ont:timeExpression  "officer in 1949" .

