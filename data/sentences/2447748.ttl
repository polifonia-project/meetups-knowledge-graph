        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/2447748/8/0> .

<http://w3id.org/polifonia/pilot/meetups/2447748/4/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "First published in 1980, this volume has been reprinted due to continued demand from composers, arrangers, and university music departments alike." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2447748/4> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2447748/4/1> ;
        ont:subject       <http://dbpedia.org/resource/Ray_Steadman-Allen> .

<http://w3id.org/polifonia/pilot/meetups/2447748/3/2>
        rdf:type            ont:Sentence ;
        ont:excerpt         "His creativity was given totally to God and was instrumental in guiding Salvation Army music into uncharted territory, particularly when the International Music Editorial Department was under his leadership between 1967 and 1980." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/2447748/3> ;
        ont:person          <http://dbpedia.org/resource/God> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/2447748/3/1> ;
        ont:subject         <http://dbpedia.org/resource/Ray_Steadman-Allen> ;
        ont:timeExpression  "between 1967 and 1980" .

<http://w3id.org/polifonia/pilot/meetups/2447748/2>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/2447748/2/0> .

<http://w3id.org/polifonia/pilot/meetups/2447748/1/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In the event, Bantock died, and Ray Steadman-Allen joined the Music Editorial Department of The Salvation Army." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2447748/1> ;
        ont:person        <http://dbpedia.org/resource/Ray_Steadman-Allen> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2447748/1/1> ;
        ont:subject       <http://dbpedia.org/resource/Ray_Steadman-Allen> .

<http://w3id.org/polifonia/pilot/meetups/2447748/0/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "When they were appointed to London in 1937, he obtained a job at International Headquarters as office boy to General Evangeline Booth, daughter of The Salvation Army's founder." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2447748/0> ;
        ont:person        <http://dbpedia.org/resource/Evangeline_Booth> ;
        ont:place         <http://dbpedia.org/resource/London> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2447748/0/1> ;
        ont:subject       <http://dbpedia.org/resource/Ray_Steadman-Allen> .

<http://w3id.org/polifonia/pilot/meetups/2447748/7>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/2447748/7/0> .

<http://w3id.org/polifonia/pilot/meetups/2447748/9/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In his later years, Ray Steadman-Allen became affectionately known as 'RSA'." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2447748/9> ;
        ont:person        <http://dbpedia.org/resource/Ray_Steadman-Allen> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2447748/9/0> ;
        ont:subject       <http://dbpedia.org/resource/Ray_Steadman-Allen> .

<http://w3id.org/polifonia/pilot/meetups/2447748/4/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He wrote a book called Colour and Texture in the Brass Band Score which was published by The Salvation Army." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2447748/4> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2447748/4/0> ;
        ont:subject       <http://dbpedia.org/resource/Ray_Steadman-Allen> .

<http://w3id.org/polifonia/pilot/meetups/2447748/8/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Steadman-Allen also encouraged new compositional talent, particularly brass band composers." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2447748/8> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2447748/8/0> ;
        ont:subject       <http://dbpedia.org/resource/Ray_Steadman-Allen> .

<http://w3id.org/polifonia/pilot/meetups/2447748/3/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Lord of the Sea created a furore." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2447748/3> ;
        ont:place         <http://dbpedia.org/resource/Furore> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2447748/3/0> ;
        ont:subject       <http://dbpedia.org/resource/Ray_Steadman-Allen> .

