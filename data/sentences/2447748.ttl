<http://w3id.org/polifonia/pilot/meetups/2447748/7/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "and patron of the London Musicological Research Society." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2447748/7> ;
        ont:place         <http://dbpedia.org/resource/London> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2447748/7/0> ;
        ont:subject       <http://dbpedia.org/resource/Ray_Steadman-Allen> .

<http://w3id.org/polifonia/pilot/meetups/2447748/6/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "As well as completing his Doctorate in Music, Ray Steadman-Allen held several honorary fellowships, was the President of the National College of Music, Vice President of the National Association of Brass Band Conductors" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2447748/6> ;
        ont:person        <http://dbpedia.org/resource/President_of_the_United_States> , <http://dbpedia.org/resource/Ray_Steadman-Allen> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2447748/6/0> ;
        ont:subject       <http://dbpedia.org/resource/Ray_Steadman-Allen> .

<http://w3id.org/polifonia/pilot/meetups/2447748/1/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He was examined for a music diploma by Sir Granville Bantock who invited him to apply for a job in music after the war." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2447748/1> ;
        ont:person        <http://dbpedia.org/resource/Granville_Bantock> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2447748/1/0> ;
        ont:subject       <http://dbpedia.org/resource/Ray_Steadman-Allen> .

<http://w3id.org/polifonia/pilot/meetups/2447748/13/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "External links ==" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2447748/13> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2447748/13/0> ;
