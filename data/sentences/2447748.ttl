<http://w3id.org/polifonia/pilot/meetups/2447748/5/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Besides well over 200 brass band works published by The Salvation Army, Ray Steadman-Allen wrote numerous choral works with a large number of compositions and arrangements in manuscript form, often completed for recordings or special concert presentations." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/2447748/5> ;
        ont:person          <http://dbpedia.org/resource/Ray_Steadman-Allen> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/2447748/5/0> ;
        ont:subject         <http://dbpedia.org/resource/Ray_Steadman-Allen> ;
        ont:timeExpression  "200 brass band" .

<http://w3id.org/polifonia/pilot/meetups/2447748/0/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He was born in the Salvation Army 'Mother's Hospital', Clapton, while his Salvation Army Officer parents were living in the Horfield area of Bristol." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2447748/0> ;
        ont:person        <http://dbpedia.org/resource/Mother> ;
        ont:place         <http://dbpedia.org/resource/Bristol> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2447748/0/0> ;
        ont:subject       <http://dbpedia.org/resource/Ray_Steadman-Allen> .

