        rdf:_5    <http://w3id.org/polifonia/pilot/meetups/106366/9/4> .

<http://w3id.org/polifonia/pilot/meetups/106366/6/22>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Blake performed with Gregory Hines on the television program Saturday Night Live on March 10, 1979 (Season 4, Episode 14)." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/106366/6> ;
        ont:person          <http://dbpedia.org/resource/Gregory_Hines> , <http://dbpedia.org/resource/William_Blake> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/106366/6/21> ;
        ont:subject         <http://dbpedia.org/resource/Eubie_Blake> ;
        ont:timeExpression  "March 10 , 1979" , "Season 4" , "Episode 14" .

<http://w3id.org/polifonia/pilot/meetups/106366/10>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/106366/10/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/106366/10/1> .

<http://w3id.org/polifonia/pilot/meetups/106366/11/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Nomination for a Grammy Award for The Eighty-Six Years of Eubie Blakein the category of \"Best Instrumental Jazz Performance, Small Group or Soloist with Small Group\"" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/106366/11> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/106366/11/0> ;
        ont:subject       <http://dbpedia.org/resource/Eubie_Blake> .

<http://w3id.org/polifonia/pilot/meetups/106366/6/4>
        rdf:type            ont:Sentence ;
        ont:excerpt         "She died later that year, at the age of 58." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/106366/6> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/106366/6/3> ;
        ont:subject         <http://dbpedia.org/resource/Eubie_Blake> ;
        ont:timeExpression  "age of 58" .

<http://w3id.org/polifonia/pilot/meetups/106366/4/10>
        rdf:type          ont:Sentence ;
        ont:excerpt       "According to Blake, he also worked the medicine show circuit and was employed by a Quaker doctor." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/106366/4> ;
        ont:person        <http://dbpedia.org/resource/William_Blake> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/106366/4/9> ;
        ont:subject       <http://dbpedia.org/resource/Eubie_Blake> .

<http://w3id.org/polifonia/pilot/meetups/106366/3>
