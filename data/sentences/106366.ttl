        ont:subject       <http://dbpedia.org/resource/Eubie_Blake> .

<http://w3id.org/polifonia/pilot/meetups/106366/6/5>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Of his loss, Blake said, \"In my life I never knew what it was to be alone." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/106366/6> ;
        ont:person        <http://dbpedia.org/resource/William_Blake> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/106366/6/4> ;
        ont:subject       <http://dbpedia.org/resource/Eubie_Blake> .

<http://w3id.org/polifonia/pilot/meetups/106366/4/11>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He played a Melodeon strapped to the back of the medicine wagon." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/106366/4> ;
        ont:person        <http://dbpedia.org/resource/Diatonic_button_accordion> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/106366/4/10> ;
