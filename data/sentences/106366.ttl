        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/106366/6/12> ;
        ont:subject       <http://dbpedia.org/resource/Eubie_Blake> .

<http://w3id.org/polifonia/pilot/meetups/106366/4/14>
        rdf:type          ont:Sentence ;
        ont:excerpt       "It was not committed to paper, however, until 1915, when he learned to write musical notation." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/106366/4> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/106366/4/13> ;
