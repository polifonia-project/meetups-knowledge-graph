<http://w3id.org/polifonia/pilot/meetups/106366/4/24>
        rdf:type            ont:Sentence ;
        ont:excerpt         "In the 1920s he recorded for the Victor and Emerson labels among others." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/106366/4> ;
        ont:person          <http://dbpedia.org/resource/Ralph_Waldo_Emerson> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/106366/4/23> ;
        ont:subject         <http://dbpedia.org/resource/Eubie_Blake> ;
        ont:timeExpression  "In the 1920s" .

<http://w3id.org/polifonia/pilot/meetups/106366/2/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Eubie Blake was born February 7, 1887, at 319 Forrest Street, in Baltimore, Maryland." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/106366/2> ;
        ont:person          <http://dbpedia.org/resource/Eubie_Blake> ;
        ont:place           <http://dbpedia.org/resource/Maryland> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/106366/2/0> ;
        ont:subject         <http://dbpedia.org/resource/Eubie_Blake> ;
        ont:timeExpression  "February 7 , 1887" .

<http://w3id.org/polifonia/pilot/meetups/106366/1/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "==" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/106366/1> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/106366/1/0> ;
        ont:subject       <http://dbpedia.org/resource/Eubie_Blake> .

<http://w3id.org/polifonia/pilot/meetups/106366/9/4>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Blake was reported to have said, on his birthday in 1979, \"If I'd known I was going to live this long, I would have taken better care of myself\", but this has been attributed to others and has appeared in print at least as early as 1966." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/106366/9> ;
        ont:person          <http://dbpedia.org/resource/William_Blake> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/106366/9/3> ;
        ont:subject         <http://dbpedia.org/resource/Eubie_Blake> ;
        ont:timeExpression  "birthday in 1979" .

<http://w3id.org/polifonia/pilot/meetups/106366/4/5>
        rdf:type          ont:Sentence ;
        ont:excerpt       "When Blake was seven, he received music lessons from a neighbor, Margaret Marshall, an organist for the Methodist church." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/106366/4> ;
        ont:person        <http://dbpedia.org/resource/Margaret_Anne_Marshall> , <http://dbpedia.org/resource/William_Blake> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/106366/4/4> ;
        ont:subject       <http://dbpedia.org/resource/Eubie_Blake> .

<http://w3id.org/polifonia/pilot/meetups/106366/6/15>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1977 he played Will Williams in the Jeremy Kagan biographical film Scott Joplin." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/106366/6> ;
        ont:person        <http://dbpedia.org/resource/Scott_Joplin> , <http://dbpedia.org/resource/Jeremy_Kagan> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/106366/6/14> ;
        ont:subject       <http://dbpedia.org/resource/Eubie_Blake> .

<http://w3id.org/polifonia/pilot/meetups/106366/0/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "James Hubert \"Eubie\" Blake (February 7, 1887 – February 12, 1983) was an American pianist, lyricist, and  composer of ragtime, jazz, and popular music." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/106366/0> ;
        ont:person          <http://dbpedia.org/resource/Composer> ;
        ont:place           <http://dbpedia.org/resource/United_States> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/106366/0/0> ;
        ont:subject         <http://dbpedia.org/resource/Eubie_Blake> ;
        ont:timeExpression  "February 7 , 1887" , "February 12 , 1983" .

<http://w3id.org/polifonia/pilot/meetups/106366/4/16>
        rdf:type          ont:Sentence ;
        ont:excerpt       "The band played ragtime music, which was still quite popular." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/106366/4> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/106366/4/15> ;
        ont:subject       <http://dbpedia.org/resource/Eubie_Blake> .

<http://w3id.org/polifonia/pilot/meetups/106366/9>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/106366/9/0> ;
