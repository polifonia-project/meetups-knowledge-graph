        rdf:type          ont:Sentence ;
        ont:excerpt       "Also in 1978, he played Warren Beatty's valet, Bentley, in Heaven Can Wait." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2830223/7> ;
        ont:person        <http://dbpedia.org/resource/Warren_Beatty> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2830223/7/5> ;
        ont:subject       <http://dbpedia.org/resource/Hamilton_Camp> .

<http://w3id.org/polifonia/pilot/meetups/2830223/8/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He was the voice of Fenton Crackshell, aka GizmoDuck, on the Disney animated series DuckTales and its spinoff Darkwing Duck." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2830223/8> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2830223/8/0> ;
        ont:subject       <http://dbpedia.org/resource/Hamilton_Camp> .

