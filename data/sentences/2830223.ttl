        rdf:type          ont:Sentence ;
        ont:excerpt       "He became a child actor in films and onstage." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2830223/2> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2830223/2/0> ;
