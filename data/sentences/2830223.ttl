<http://w3id.org/polifonia/pilot/meetups/2830223/30/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Hamilton Camp at Find a Grave" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2830223/30> ;
