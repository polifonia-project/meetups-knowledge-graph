<http://w3id.org/polifonia/pilot/meetups/2830223/5/1>
        rdf:type            ont:Sentence ;
        ont:excerpt         "As a singer, Camp had a minor hit with the song \"Here's to You,\" which peaked at number 76 on the Billboard Hot 100 in 1968." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/2830223/5> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/2830223/5/0> ;
        ont:subject         <http://dbpedia.org/resource/Hamilton_Camp> ;
        ont:timeExpression  "Hot 100" , "76 on" .

