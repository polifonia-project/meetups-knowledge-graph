        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2830223/30/0> ;
        ont:subject       <http://dbpedia.org/resource/Hamilton_Camp> .

<http://w3id.org/polifonia/pilot/meetups/2830223/7/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He guest-starred on television shows such as The Rat Patrol, The Monkees, M*A*S*H, Soap, The Mary Tyler Moore Show, The Twilight Zone, Starsky and Hutch, Cheers, The Andy Griffith Show, Bewitched, Gomer Pyle, U.S.M.C., Three's Company and Lois & Clark: The New Adventures of Superman, as the older H. G. Wells." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2830223/7> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2830223/7/0> ;
        ont:subject       <http://dbpedia.org/resource/Hamilton_Camp> .

<http://w3id.org/polifonia/pilot/meetups/2830223/2/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He originally performed under the names Robin Camp and Bob Camp, later changing his name to Hamilton after joining the Subud spiritual movement." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2830223/2> ;
        ont:person        <http://dbpedia.org/resource/Hamilton_Camp> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/2830223/2/1> ;
        ont:subject       <http://dbpedia.org/resource/Hamilton_Camp> .

<http://w3id.org/polifonia/pilot/meetups/2830223/6/1>
        rdf:type            ont:Sentence ;
        ont:excerpt         "He also performed with the Chicago comedy troupe The Second City and the San Francisco satirical comedy troupe the Committee and appeared in a number of stage productions, including a 2004 production of A Midsummer Night's Dream at the Hollywood Bowl." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/2830223/6> ;
        ont:place           <http://dbpedia.org/resource/San_Francisco> , <http://dbpedia.org/resource/Chicago> , <http://dbpedia.org/resource/Hollywood_Bowl> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/2830223/6/0> ;
        ont:subject         <http://dbpedia.org/resource/Hamilton_Camp> ;
        ont:timeExpression  "2004 production" .

