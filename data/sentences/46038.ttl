        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/46038/27/0> ;
        ont:subject       <http://dbpedia.org/resource/Jeremy_Bentham> .

<http://w3id.org/polifonia/pilot/meetups/46038/37/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "After he learned more about American law and realised that most of it was state-based, he promptly wrote to the governors of every single state with the same offer." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/46038/37> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/46038/37/1> ;
