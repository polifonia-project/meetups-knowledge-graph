<http://w3id.org/polifonia/pilot/meetups/251481/3/9>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He also had to check that the parts for building the planes were where they needed to be on the production line." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/251481/3> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/251481/3/8> ;
