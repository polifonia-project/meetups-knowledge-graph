        ont:person          <http://dbpedia.org/resource/Lindsay_Anderson> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/251481/12/3> ;
        ont:subject         <http://dbpedia.org/resource/Arthur_Lowe> ;
        ont:timeExpression  "1968" .

<http://w3id.org/polifonia/pilot/meetups/251481/19/1>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Graham Lord wrote, in his 2002 biography, that \"almost every actor who worked with Arthur considered him to be outstanding\"." ;
