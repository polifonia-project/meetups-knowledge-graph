<http://w3id.org/polifonia/pilot/meetups/251481/16/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He was taken, unconscious, to Birmingham General Hospital, where he died at about 5 a.m. the following morning." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/251481/16> ;
        ont:place         <http://dbpedia.org/resource/Birmingham_General_Hospital> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/251481/16/2> ;
        ont:subject       <http://dbpedia.org/resource/Arthur_Lowe> .

<http://w3id.org/polifonia/pilot/meetups/251481/8/6>
        rdf:type            ont:Sentence ;
        ont:excerpt         "During this time Lowe began a romantic relationship with Joan Cooper (1922–1989), a married actress in the company whose husband also began an affair at about the same time." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/251481/8> ;
        ont:person          <http://dbpedia.org/resource/Joan_Cooper> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/251481/8/5> ;
        ont:subject         <http://dbpedia.org/resource/Arthur_Lowe> ;
        ont:timeExpression  "1922–1989" .

<http://w3id.org/polifonia/pilot/meetups/251481/12/10>
        rdf:type            ont:Sentence ;
        ont:excerpt         "He employed a multitude of voices on the BBC animated television series Mr. Men (1974), in which he was the narrator." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/251481/12> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/251481/12/9> ;
