        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/251481/12/0> ;
        ont:subject       <http://dbpedia.org/resource/Arthur_Lowe> .

<http://w3id.org/polifonia/pilot/meetups/251481/12/13>
        rdf:type            ont:Sentence ;
        ont:excerpt         "He bought Amazon as a houseboat in 1968, but realised her potential and took her back to sea in 1971; this vessel is still operating in the Mediterranean." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/251481/12> ;
        ont:place           <http://dbpedia.org/resource/Amazon_River> , <http://dbpedia.org/resource/Mediterranean_Sea> ;
