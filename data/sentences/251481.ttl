        ont:subject       <http://dbpedia.org/resource/Arthur_Lowe> .

<http://w3id.org/polifonia/pilot/meetups/251481/14/9>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Around this time Lowe was making many television commercials, with no fewer than nineteen in 1981 alone." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/251481/14> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/251481/14/8> ;
        ont:subject         <http://dbpedia.org/resource/Arthur_Lowe> ;
        ont:timeExpression  "nineteen in 1981" .

