<http://w3id.org/polifonia/pilot/meetups/251481/3/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Lowe's father, a tall man known as \"Big Arthur\", worked for the Great Central Railway, later absorbed into the London and North Eastern Railway." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/251481/3> ;
        ont:person        <http://dbpedia.org/resource/King_Arthur> ;
        ont:place         <http://dbpedia.org/resource/London> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/251481/3/0> ;
