<http://w3id.org/polifonia/pilot/meetups/1451518/36/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "The art work was worth an estimated $2 million." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1451518/36> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1451518/36/0> ;
