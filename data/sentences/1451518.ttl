        ont:timeExpression  "July 5 , 2011" , "April 25 , 1928" .

<http://w3id.org/polifonia/pilot/meetups/1451518/10/6>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Twombly often inscribed on paintings the names of mythological figures during the 1960s." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1451518/10> ;
        ont:person        <http://dbpedia.org/resource/Cy_Twombly> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1451518/10/5> ;
        ont:subject       <http://dbpedia.org/resource/Cy_Twombly> .

<http://w3id.org/polifonia/pilot/meetups/1451518/17/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 2008, a specially curated selection of Twombly's photographic work was exhibited in Huis Marseille, the Museum for Photography, Amsterdam; the exhibition was opened by Sally Mann." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1451518/17> ;
        ont:person        <http://dbpedia.org/resource/Sally_Mann> , <http://dbpedia.org/resource/Cy_Twombly> ;
        ont:place         <http://dbpedia.org/resource/Amsterdam> , <http://dbpedia.org/resource/Huis_Marseille,_Museum_for_Photography> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1451518/17/2> ;
        ont:subject       <http://dbpedia.org/resource/Cy_Twombly> .

<http://w3id.org/polifonia/pilot/meetups/1451518/5/5>
        rdf:type          ont:Sentence ;
        ont:excerpt       "The poet and rector of the College, Charles Olson, had a great influence on him." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1451518/5> ;
        ont:person        <http://dbpedia.org/resource/Poet> , <http://dbpedia.org/resource/Charles_Olson> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1451518/5/4> ;
        ont:subject       <http://dbpedia.org/resource/Cy_Twombly> .

<http://w3id.org/polifonia/pilot/meetups/1451518/1/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Twombly is said to have influenced younger artists such as Anselm Kiefer, Francesco Clemente, and Julian Schnabel." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1451518/1> ;
        ont:person        <http://dbpedia.org/resource/Cy_Twombly> , <http://dbpedia.org/resource/Francesco_Clemente> , <http://dbpedia.org/resource/Anselm_Kiefer> , <http://dbpedia.org/resource/Julian_Schnabel> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1451518/1/0> ;
        ont:subject       <http://dbpedia.org/resource/Cy_Twombly> .

<http://w3id.org/polifonia/pilot/meetups/1451518/30/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Twombly's will, written under U.S. law, allocated the bulk of the artist's art and cash to the Cy Twombly Foundation." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1451518/30> ;
        ont:person        <http://dbpedia.org/resource/Cy_Twombly> ;
        ont:place         <http://dbpedia.org/resource/United_States> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1451518/30/0> ;
        ont:subject       <http://dbpedia.org/resource/Cy_Twombly> .

<http://w3id.org/polifonia/pilot/meetups/1451518/6/5>
        rdf:type            ont:Sentence ;
        ont:excerpt         "From 1955 through 1956, he taught at the Southern Seminary and Junior College in Buena Vista, Virginia, currently known as Southern Virginia University; during the summer vacations, Twombly would travel to New York to paint in his Williams Street apartment." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1451518/6> ;
        ont:person          <http://dbpedia.org/resource/Cy_Twombly> ;
        ont:place           <http://dbpedia.org/resource/Buena_Vista,_Virginia> , <http://dbpedia.org/resource/New_York_City> , <http://dbpedia.org/resource/Virginia> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1451518/6/4> ;
        ont:subject         <http://dbpedia.org/resource/Cy_Twombly> ;
        ont:timeExpression  "From 1955 through 1956" .

<http://w3id.org/polifonia/pilot/meetups/1451518/18/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "The Last Paintings, Twombly's most recent solo exhibition, began in Los Angeles in early 2012." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1451518/18> ;
        ont:person        <http://dbpedia.org/resource/Cy_Twombly> ;
        ont:place         <http://dbpedia.org/resource/Los_Angeles> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1451518/18/2> ;
        ont:subject       <http://dbpedia.org/resource/Cy_Twombly> .

<http://w3id.org/polifonia/pilot/meetups/1451518/10/17>
        rdf:type          ont:Sentence ;
        ont:excerpt       "His later sculptures exhibit a similar blend of emotional expansiveness and intellectual sophistication." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1451518/10> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/1451518/10/16> ;
        ont:subject       <http://dbpedia.org/resource/Cy_Twombly> .

