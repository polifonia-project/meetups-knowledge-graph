<http://w3id.org/polifonia/pilot/meetups/1451518/25/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "In 1995, The Four Seasons entered the permanent collection of the Museum of Modern Art as a gift from the artist." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/1451518/25> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/1451518/25/0> ;
