<http://w3id.org/polifonia/pilot/meetups/1451518/0/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Edwin Parker \"Cy\" Twombly Jr. (; April 25, 1928 – July 5, 2011)" ;
