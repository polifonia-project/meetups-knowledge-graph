        ont:subject         <http://dbpedia.org/resource/Cy_Twombly> ;
        ont:timeExpression  "From 1955 to 1959" .

<http://w3id.org/polifonia/pilot/meetups/1451518/42/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Further reading ==" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/1451518/42> ;
