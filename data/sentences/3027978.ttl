<http://w3id.org/polifonia/pilot/meetups/3027978/12/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "His successful investments in companies and business ventures during the last decades have brought Meier assets which are estimated around 150-200 million Swiss franc." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/3027978/12> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/3027978/12/0> ;
