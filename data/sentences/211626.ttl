<http://w3id.org/polifonia/pilot/meetups/211626/6/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "\"Disco Lady\" was the first certified platinum single (two million copies sold) by the RIAA." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/211626/6> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/211626/6/2> ;
