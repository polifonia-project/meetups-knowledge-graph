<http://w3id.org/polifonia/pilot/meetups/211626/3/4>
        rdf:type            ont:Sentence ;
        ont:excerpt         "A few years later, after Cooke had established his independent SAR Records, Taylor signed on as one of the label's first acts and recorded \"Rome Wasn't Built In A Day\" in 1962." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/211626/3> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/211626/3/3> ;
        ont:subject         <http://dbpedia.org/resource/Johnnie_Taylor> ;
        ont:timeExpression  "one of" .

<http://w3id.org/polifonia/pilot/meetups/211626/15/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "His highly complex personal life was revealed after his death." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/211626/15> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/211626/15/3> ;
        ont:subject       <http://dbpedia.org/resource/Johnnie_Taylor> .

