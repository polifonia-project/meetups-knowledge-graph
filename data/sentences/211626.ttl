<http://w3id.org/polifonia/pilot/meetups/211626/4/4>
        rdf:type            ont:Sentence ;
        ont:excerpt         "In 1970, Taylor married Gerlean Rocket and they remained married until his death in 2000." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/211626/4> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/211626/4/3> ;
        ont:subject         <http://dbpedia.org/resource/Johnnie_Taylor> ;
        ont:timeExpression  "death in 2000" .

