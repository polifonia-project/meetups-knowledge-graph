<http://w3id.org/polifonia/pilot/meetups/442116/19/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Most of these songs were sung in English." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/442116/19> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/442116/19/1> ;
        ont:subject       <http://dbpedia.org/resource/Peter_Dawson_(bass-baritone)> .

<http://w3id.org/polifonia/pilot/meetups/442116/14/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "After the First World War, audio technology was improving, and he recut many of his more popular titles during the early 1920s." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/442116/14> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/442116/14/2> ;
        ont:subject       <http://dbpedia.org/resource/Peter_Dawson_(bass-baritone)> .

