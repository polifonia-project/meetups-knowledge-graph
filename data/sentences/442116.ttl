        rdf:type            ont:Sentence ;
        ont:excerpt         "During one of these performances, after winning a large amount playing poker in the wings, he hurried on at his call, and accidentally scattered his winnings over the stage." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/442116/8> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/442116/8/2> ;
