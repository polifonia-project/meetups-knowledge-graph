        ont:place         <http://dbpedia.org/resource/Dawson_City> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/442116/10/4> ;
        ont:subject       <http://dbpedia.org/resource/Peter_Dawson_(bass-baritone)> .

<http://w3id.org/polifonia/pilot/meetups/442116/6/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Then, aged 19, he won a prize for the best bass solo in a competition at Ballarat, Victoria, and began taking up concert engagements." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/442116/6> ;
        ont:place         <http://dbpedia.org/resource/Ballarat> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/442116/6/2> ;
        ont:subject       <http://dbpedia.org/resource/Peter_Dawson_(bass-baritone)> .

