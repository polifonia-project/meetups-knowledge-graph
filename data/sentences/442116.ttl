        rdf:_3    <http://w3id.org/polifonia/pilot/meetups/442116/19/2> .

<http://w3id.org/polifonia/pilot/meetups/442116/25/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Dawson was inducted into the Hall of Fame in 1991." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/442116/25> ;
        ont:place         <http://dbpedia.org/resource/Dawson_City> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/442116/25/1> ;
        ont:subject       <http://dbpedia.org/resource/Peter_Dawson_(bass-baritone)> .

<http://w3id.org/polifonia/pilot/meetups/442116/18/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "He sang Elgar roles in exemplary fashion, too, including \"Oh My Warriors\" and \"The Sword Song\" (\"Leap, Leap, to the Light\") from Caractacus." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/442116/18> ;
        ont:person        <http://dbpedia.org/resource/Edward_Elgar> , <http://dbpedia.org/resource/Caratacus> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/442116/18/1> ;
        ont:subject       <http://dbpedia.org/resource/Peter_Dawson_(bass-baritone)> .

<http://w3id.org/polifonia/pilot/meetups/442116/13/3>
        rdf:type          ont:Sentence ;
        ont:excerpt       "His first efforts were made for Edison-Bell on wax cylinders in 1904." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/442116/13> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/442116/13/2> ;
