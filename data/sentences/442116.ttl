<http://w3id.org/polifonia/pilot/meetups/442116/7/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In addition to Italian and French opera, he grew to admire the German music dramas of Richard Wagner." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/442116/7> ;
        ont:person        <http://dbpedia.org/resource/Richard_Wagner> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/442116/7/1> ;
        ont:subject       <http://dbpedia.org/resource/Peter_Dawson_(bass-baritone)> .

<http://w3id.org/polifonia/pilot/meetups/442116/10/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "However, the Armistice of 1918 intervened before he saw frontline service." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/442116/10> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/442116/10/3> ;
        ont:subject       <http://dbpedia.org/resource/Peter_Dawson_(bass-baritone)> .

<http://w3id.org/polifonia/pilot/meetups/442116/6/2>
        rdf:type            ont:Sentence ;
        ont:excerpt         "At the age of 17 he joined a church choir and received singing lessons from C.J. Stevens." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/442116/6> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/442116/6/1> ;
        ont:subject         <http://dbpedia.org/resource/Peter_Dawson_(bass-baritone)> ;
        ont:timeExpression  "age of 17" .

