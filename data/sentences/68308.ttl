<http://w3id.org/polifonia/pilot/meetups/68308/0/4>
        rdf:type            ont:Sentence ;
        ont:excerpt         "For his contributions to the television industry, Albertson was honored with a star on the Hollywood Walk of Fame in 1977 at 6253 Hollywood Boulevard." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/68308/0> ;
        ont:place           <http://dbpedia.org/resource/Hollywood> , <http://dbpedia.org/resource/Hollywood_Boulevard> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/68308/0/3> ;
