<http://w3id.org/polifonia/pilot/meetups/68308/3/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Albertson dropped out of high school, ending his formal education after a single year." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/68308/3> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/68308/3/3> ;
        ont:subject       <http://dbpedia.org/resource/Jack_Albertson> .

