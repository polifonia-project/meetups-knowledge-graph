<http://w3id.org/polifonia/pilot/meetups/68308/17/3>
        rdf:type            ont:Sentence ;
        ont:excerpt         "He also appeared in a 1969 episode of the TV series The Virginian entitled Girl in the Shadows." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/68308/17> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/68308/17/2> ;
        ont:subject         <http://dbpedia.org/resource/Jack_Albertson> ;
        ont:timeExpression  "1969 episode" .

<http://w3id.org/polifonia/pilot/meetups/68308/9/2>
        rdf:type            ont:Sentence ;
        ont:excerpt         "He won an Academy Award for Best Supporting Actor for his role in the 1968 film The Subject Was Roses." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/68308/9> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/68308/9/1> ;
        ont:subject         <http://dbpedia.org/resource/Jack_Albertson> ;
        ont:timeExpression  "1968 film" .

