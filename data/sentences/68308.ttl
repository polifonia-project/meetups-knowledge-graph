        rdf:type            ont:Sentence ;
        ont:excerpt         "He had recurring roles in Ensign O'Toole (1962–63) and Run, Buddy, Run (1966)." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/68308/16> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/68308/16/2> ;
