<http://w3id.org/polifonia/pilot/meetups/68308/2/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "His father abandoned his mother before Jack was born, and the boy was raised by his stepfather, Alex Erlich, a barber." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/68308/2> ;
        ont:person        <http://dbpedia.org/resource/Jonathan_Erlich> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/68308/2/3> ;
        ont:subject       <http://dbpedia.org/resource/Jack_Albertson> .

<http://w3id.org/polifonia/pilot/meetups/68308/20/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "both filmed in 1981 and released posthumously." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/68308/20> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/68308/20/3> ;
        ont:subject       <http://dbpedia.org/resource/Jack_Albertson> .

<http://w3id.org/polifonia/pilot/meetups/68308/18/3>
        rdf:type            ont:Sentence ;
        ont:excerpt         "He earned an Emmy Award for that role in 1976, which was his second, his first one was for an appearance on the variety show Cher in 1975." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/68308/18> ;
        ont:person          <http://dbpedia.org/resource/Cher> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/68308/18/2> ;
