        rdf:type          ont:Sentence ;
        ont:excerpt       "Verdon was a mental health-care advocate; later in life, she openly spoke about the positive effects of mental-health counseling." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/55608/16> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/55608/16/8> ;
