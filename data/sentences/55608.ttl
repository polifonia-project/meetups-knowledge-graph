<http://w3id.org/polifonia/pilot/meetups/55608/0/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "She won four Tony Awards for her musical comedy performances, and served as an uncredited choreographer's assistant and specialty dance coach for theater and film." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/55608/0> ;
        ont:person        <http://dbpedia.org/resource/Choreography> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/55608/0/0> ;
        ont:subject       <http://dbpedia.org/resource/Gwen_Verdon> .

<http://w3id.org/polifonia/pilot/meetups/55608/12/5>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Called simply Fosse, the revue was conceived and directed by Richard Maltby Jr. and Ann Reinking and choreographed by Reinking and Chet Walker." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/55608/12> ;
        ont:person        <http://dbpedia.org/resource/Richard_Maltby_Jr.> , <http://dbpedia.org/resource/Ann_Reinking> , <http://dbpedia.org/resource/Chet_Walker> , <http://dbpedia.org/resource/Bob_Fosse> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/55608/12/4> ;
        ont:subject       <http://dbpedia.org/resource/Gwen_Verdon> .

<http://w3id.org/polifonia/pilot/meetups/55608/26>
        rdf:type  ont:Paragraph ;
        rdf:_1    <http://w3id.org/polifonia/pilot/meetups/55608/26/0> ;
        rdf:_2    <http://w3id.org/polifonia/pilot/meetups/55608/26/1> .

<http://w3id.org/polifonia/pilot/meetups/55608/4/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "In 1942, Verdon's parents asked her to marry family friend and tabloid reporter James Henaghan after he got her pregnant at 17 years old, and she quit her dancing career to raise their child." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/55608/4> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/55608/4/0> ;
        ont:subject         <http://dbpedia.org/resource/Gwen_Verdon> ;
        ont:timeExpression  "pregnant at 17" .

<http://w3id.org/polifonia/pilot/meetups/55608/16/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "They had a daughter, Nicole, in 1963." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/55608/16> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/55608/16/3> ;
        ont:subject       <http://dbpedia.org/resource/Gwen_Verdon> .

<http://w3id.org/polifonia/pilot/meetups/55608/7/5>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Verdon received a pay increase and her first Tony Award for her performance." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/55608/7> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/55608/7/4> ;
        ont:subject       <http://dbpedia.org/resource/Gwen_Verdon> .

<http://w3id.org/polifonia/pilot/meetups/55608/3/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "By age six, she was dancing on stage." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/55608/3> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/55608/3/0> ;
        ont:subject         <http://dbpedia.org/resource/Gwen_Verdon> ;
        ont:timeExpression  "By age six" .

<http://w3id.org/polifonia/pilot/meetups/55608/20>
