        ont:place           <http://dbpedia.org/resource/Woodstock,_Vermont> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/55608/20/0> ;
        ont:subject         <http://dbpedia.org/resource/Gwen_Verdon> ;
        ont:timeExpression  "October 18 , 2000" .

<http://w3id.org/polifonia/pilot/meetups/55608/9/0>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Verdon won another Tony for her performance in the musical New Girl in Town as a hard-luck girl fleeing from her past as a prostitute." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/55608/9> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/55608/9/0> ;
        ont:subject       <http://dbpedia.org/resource/Gwen_Verdon> .

<http://w3id.org/polifonia/pilot/meetups/55608/4/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In 1945, she appeared as a dancer in the movie musical Blonde From Brooklyn." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/55608/4> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/55608/4/0> ;
        ont:subject       <http://dbpedia.org/resource/Gwen_Verdon> .

<http://w3id.org/polifonia/pilot/meetups/55608/16/5>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Fosse's extramarital affairs put a strain on their marriage, and by 1971, Verdon and Fosse were separated, but never divorced." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/55608/16> ;
        ont:person        <http://dbpedia.org/resource/Bob_Fosse> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/55608/16/4> ;
        ont:subject       <http://dbpedia.org/resource/Gwen_Verdon> .

<http://w3id.org/polifonia/pilot/meetups/55608/12/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "After originating the role of Roxie opposite Chita Rivera's Velma Kelly in Chicago, Verdon focused on film acting, playing character roles in movies such as The Cotton Club (1984), Cocoon (1985) and Cocoon: The Return (1988)." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/55608/12> ;
        ont:person          <http://dbpedia.org/resource/Chita_Rivera> ;
        ont:place           <http://dbpedia.org/resource/Chicago> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/55608/12/0> ;
        ont:subject         <http://dbpedia.org/resource/Gwen_Verdon> ;
        ont:timeExpression  "1985" , "1984" , "1988" .

<http://w3id.org/polifonia/pilot/meetups/55608/8/0>
        rdf:type            ont:Sentence ;
        ont:excerpt         "Verdon's biggest critical and commercial success was her following show, George Abbott's Damn Yankees (1955), based on the novel The Year the Yankees Lost the Pennant." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/55608/8> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/55608/8/0> ;
        ont:subject         <http://dbpedia.org/resource/Gwen_Verdon> ;
        ont:timeExpression  "1955" .

<http://w3id.org/polifonia/pilot/meetups/55608/3/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "She went on to study multiple dance forms, ranging from tap, jazz, ballroom and flamenco to Balinese." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/55608/3> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/55608/3/0> ;
        ont:subject       <http://dbpedia.org/resource/Gwen_Verdon> .

