<http://w3id.org/polifonia/pilot/meetups/149935/17/3>
        rdf:type            ont:Sentence ;
        ont:excerpt         "She died in Überlingen, one of the best preserved medieval sites, on the shore of Lake Constance on 5 March 1980 at the age of 82 and was interred at Bayreuth." ;
        ont:inParagraph     <http://w3id.org/polifonia/pilot/meetups/149935/17> ;
        ont:place           <http://dbpedia.org/resource/Berlingen,_Switzerland> , <http://dbpedia.org/resource/Bayreuth> , <http://dbpedia.org/resource/Lake_Constance> ;
        ont:prevSentence    <http://w3id.org/polifonia/pilot/meetups/149935/17/2> ;
        ont:subject         <http://dbpedia.org/resource/Winifred_Wagner> ;
        ont:timeExpression  "one of" , "age of 82" , "5 March 1980" .

<http://w3id.org/polifonia/pilot/meetups/149935/12/4>
        rdf:type          ont:Sentence ;
        ont:excerpt       "She corresponded with Hitler for nearly two decades." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/149935/12> ;
        ont:person        <http://dbpedia.org/resource/Adolf_Hitler> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/149935/12/3> ;
        ont:subject       <http://dbpedia.org/resource/Winifred_Wagner> .

