<http://w3id.org/polifonia/pilot/meetups/149935/17/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "\"To have met him [Hitler],\" she declared, \"is an experience I would not have missed.\"" ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/149935/17> ;
        ont:person        <http://dbpedia.org/resource/Adolf_Hitler> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/149935/17/0> ;
        ont:subject       <http://dbpedia.org/resource/Winifred_Wagner> .

<http://w3id.org/polifonia/pilot/meetups/149935/12/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "According to Gottfried Wagner, Winifred Wagner's grandson, she never admitted the error of her ways." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/149935/12> ;
        ont:person        <http://dbpedia.org/resource/Winifred_Wagner> , <http://dbpedia.org/resource/Gottfried_Wagner> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/149935/12/1> ;
        ont:subject       <http://dbpedia.org/resource/Winifred_Wagner> .

<http://w3id.org/polifonia/pilot/meetups/149935/11/2>
        rdf:type          ont:Sentence ;
        ont:excerpt       "In the late 1930s, she served as Hitler's personal translator during treaty negotiations with Britain." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/149935/11> ;
        ont:person        <http://dbpedia.org/resource/Adolf_Hitler> ;
        ont:place         <http://dbpedia.org/resource/United_Kingdom> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/149935/11/1> ;
        ont:subject       <http://dbpedia.org/resource/Winifred_Wagner> .

<http://w3id.org/polifonia/pilot/meetups/149935/15/1>
        rdf:type          ont:Sentence ;
        ont:excerpt       "Her grandson Gottfried Wagner later recalled that" ;
