        rdf:type          ont:Sentence ;
        ont:excerpt       "Davis' first major post was as associate conductor of the BBC Scottish Symphony Orchestra, beginning in 1970." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/2771033/3> ;
