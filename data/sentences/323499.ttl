        rdf:type          ont:Sentence ;
        ont:excerpt       "Cahn became a member of the Songwriters Hall of Fame in 1972." ;
        ont:inParagraph   <http://w3id.org/polifonia/pilot/meetups/323499/6> ;
        ont:prevSentence  <http://w3id.org/polifonia/pilot/meetups/323499/6/0> ;
