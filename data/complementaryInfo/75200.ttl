        mtp:hasdod             "1810-07-19" ;
        mtp:thumbnail          "http://commons.wikimedia.org/wiki/Special:FilePath/Grassi,_Josef_Mathias_-_Luise_von_Mecklenburg-Strelitz.jpg?width=300" .

