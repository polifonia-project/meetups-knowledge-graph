        mtp:thumbnail          "http://commons.wikimedia.org/wiki/Special:FilePath/Jai_Singh_and_Shivaji.jpg?width=300" .

<http://dbpedia.org/resource/Krishna_Kumari_(actress)>
        rdf:type               mtp:Participant ;
        rdfs:label             "Krishna Kumari (actress)" ;
        rdfs:seeAlso           core:Person ;
        mtp:hasAbstract        "T. Krishna Kumari (6 March 1933 – 24 January 2018) was an Indian actress. She worked predominantly in Telugu films, in addition to Tamil and Kannada films in the 1950s and 1960s. She was the sister of noted actress Sowkar Janaki. She starred in over 150 films in her career, most notably Pichi Pullayya (1953), Bangaru Papa (1955), Vinayaka Chaviti (1957), Pelli Kanuka (1960), Devanthakudu (1960), Bharya Bhartalu (1961), Vagdanam (1961), Kula Gotralu (1962), Chaduvukunna Ammayilu (1963), Bandipotu (1963), Punarjanma (1963 film), Aggi Pidugu (1964), Doctor Chakravarty (1964), Gudi Gantalu (1964), Antastulu (1965), Chikkadu Dorakadu (1967),Tikka Sankarayya (1968), Neramu Siksha (1973)." ;
        mtp:hasDBPediaEntity   <http://dbpedia.org/resource/Krishna_Kumari_(actress)> ;
        mtp:hasWikidataEntity  "http://www.wikidata.org/entity/Q6437449" ;
