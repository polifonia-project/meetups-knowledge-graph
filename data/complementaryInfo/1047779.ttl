        mtp:thumbnail          "http://commons.wikimedia.org/wiki/Special:FilePath/Bill_Fay_By_Erica_Parrott_2012.jpg?width=300" .

<http://dbpedia.org/resource/Glenn_Kotche>
        rdf:type               mtp:Participant ;
