        mtp:hasType          "HT" .

<http://w3id.org/polifonia/pilot/meetups/864138/13/10>
        rdf:type             mtp:Meetup ;
        mtp:hasEvidenceText  "In May 2001 Pekka Pohjola released Views, on which he toned down the rock-solid guitar-based sound of Urban Tango (1982) and Space Waltz (1985), instead focusing more on jazz and pop-classical arrangements, leaning heavily on strings and brass arrangements." ;
        mtp:hasParticipant   meetup:d3511c4559e84e4a8566ec9afa963c1a ;
        mtp:hasPurpose       meetup:f2c12d36f3dc1d7b9bb8808f79856174 ;
