        mtp:hasdob             "1935-10-01" ;
        mtp:thumbnail          "http://commons.wikimedia.org/wiki/Special:FilePath/Julie_Andrews_Park_Hyatt,_Sydney,_Australia_2013.jpg?width=300" .

