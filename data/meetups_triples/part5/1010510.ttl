        mtp:hasdob             "1905-06-21" ;
        mtp:hasdod             "1980-04-15" ;
        mtp:thumbnail          "http://commons.wikimedia.org/wiki/Special:FilePath/Sartre_1967_crop.jpg?width=300" .

