        mtp:hasdob             "1880-05-16" ;
        mtp:hasdod             "1954-05-29" ;
        mtp:thumbnail          "http://commons.wikimedia.org/wiki/Special:FilePath/Anne_O'Hare_McCormick.jpg?width=300" .

