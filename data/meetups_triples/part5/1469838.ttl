        mtp:hasdob             "1350-01-01" ;
        mtp:hasdod             "1430-11-05" ;
        mtp:thumbnail          "http://commons.wikimedia.org/wiki/Special:FilePath/Seal_of_Vytautas_the_Great.jpg?width=300" .

