        mtp:hasdob             "1832-01-27" ;
        mtp:hasdod             "1898-01-14" ;
        mtp:thumbnail          "http://commons.wikimedia.org/wiki/Special:FilePath/LewisCarrollSelfPhoto.jpg?width=300" .

