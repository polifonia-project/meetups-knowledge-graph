        mtp:hasdob             "1927-07-03" ;
        mtp:hasdod             "2011-11-27" ;
        mtp:thumbnail          "http://commons.wikimedia.org/wiki/Special:FilePath/Ken_Russell_in_1971.jpg?width=300" .

