        mtp:hasdob             "0339-01-01" ;
        mtp:hasdod             "0397-01-01" ;
        mtp:thumbnail          "http://commons.wikimedia.org/wiki/Special:FilePath/AmbroseOfMilan_(cropped).jpg?width=300" .

