<http://dbpedia.org/resource/Leigh_Harline>
        rdf:type               mtp:Participant ;
        rdfs:label             "Leigh Harline" ;
        rdfs:seeAlso           core:Person ;
        mtp:hasAbstract        "Leigh Adrian Harline (March 26, 1907 – December 10, 1969) was an American film composer and songwriter. He was known for his \"musical sophistication that was uniquely 'Harline-esque' by weaving rich tapestries of mood-setting underscores and penning memorable melodies for animated shorts and features.\"" ;
        mtp:hasDBPediaEntity   <http://dbpedia.org/resource/Leigh_Harline> ;
        mtp:hasWikidataEntity  "http://www.wikidata.org/entity/Q1367518" ;
        mtp:hasdob             "1907-03-26" ;
