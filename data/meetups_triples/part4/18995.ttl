        mtp:hasdob             "1947-11-15" ;
        mtp:hasdod             "" ;
        mtp:thumbnail          "http://commons.wikimedia.org/wiki/Special:FilePath/Bill_Richardson_at_the_LBJ_Library_(3).jpg?width=300" .

<http://dbpedia.org/resource/Reel>
